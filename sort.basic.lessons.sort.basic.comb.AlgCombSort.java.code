public void combSort() {
  double gap = getValueCount();
  int ecart = getValueCount();;
  boolean swap;
 	int i = 0;
  do{
    swap=true;
    if(gap>1){
      gap = gap / 1.3;
    }
    i = 0;
    ecart = (int)gap;
    while(i+gap < getValueCount()){
      if(isSmaller(i+ecart,i)){
        swap(i,i+ecart);
        swap=false;
        i++;
      }
    }
  }while(gap>1 || swap);
}
