def selectionSort() {
  
  
  var test=false;
  var j=0;
  for(j<-0 to getNombreValeurs()-2)
  {
    var min=getValeur(j)
    var ind=j;
    
    for(i<-j+1 to getNombreValeurs()-1)
    {
      var m=getValeur(i)
      if(min>m)
      {
        min=m
        ind=i;
      }
    }
    if(j!=ind)
    	echange(j,ind)
  }
}