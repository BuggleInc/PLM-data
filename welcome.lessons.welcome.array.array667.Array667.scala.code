def array667(nums:Array[Int]): Int = {
  var six1:Boolean = false
  var six2:Boolean = false
  var compteur = 0
  for (i <- nums)
  {
    if (i==6)
    {
    	six1=true
    	if (i+1==7 || i+1==6)
      {
      	six2=true
      	compteur +=1
      }
      else
      	six1=false
    }    	
  }
  return compteur
}
  