var beg : Int = 0
var isOver : Boolean = true
def cocktailSort2() {
  var end : Int = getValueCount()-2
  while (isOver == true){
    isOver = false
    for (i <- beg to end){
      if (isSmaller(i,i+1)== false){
        swap(i,i+1)
        end = end - 1
        isOver = true
      }
    }
    for ( i <- end to beg){
      if (isSmaller(i,i+1)== false){
        swap(i,i+1)
        beg = beg +1
        isOver = true
    }
  }
}
}