def gnomeSort() 
{
  var iSuiv = 0
  var jSuiv = 1
  

  while (jSuiv <= getNombreValeurs()-1)
  {
    if (iSuiv<0)
    {
      iSuiv+=1
      jSuiv+=1
    }
    
  
    if (plusPetit(iSuiv,jSuiv))
    {
      
      iSuiv+=1
      jSuiv+=1
    }

    else
    {
      echange(iSuiv,jSuiv)
      iSuiv=iSuiv -1
      jSuiv= jSuiv -1

    }

  }
  
}