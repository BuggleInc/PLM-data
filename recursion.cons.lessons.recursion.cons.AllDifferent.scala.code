def allDifferent(l:List[Int]): Boolean = {
	if (l.tail == Nil)
	{
		return true
	}
	else if (isMember(l.tail, l.head))
	{
		return false
	}
	else allDifferent(l.tail)
}

def isMember(l:List[Int], v:Int): Boolean = {
	if (l.length == 0)
	{
		return false;
	}
	else if (l.head == v)
	{
		return true
	}
	else isMember(l.tail, v)
}
