def testPos() : Boolean ={
  var h: Int= getMondeHauteur()-1
  var l: Int = getMondeLargeur()-1
  if (getX == l & getY == h) {
    return true
  }
  return false
}



def deplace(){
  if (getY < getMondeHauteur-1){
    setPos(getX, getY+1)
  }
  else{
    setPos(getX+1, 0)
  }
}



