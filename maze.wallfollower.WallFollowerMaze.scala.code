override def run() 
{
  while(!porteBiscuit())
  {
    keepHandOnSideWall()
    if (estSurBiscuit())
    	{
        prendBiscuit()
      }
  }
}

def keepHandOnSideWall()
{
  if(murGauche() && !estFaceMur())
  	{
      avance()
    }
  else
  	{
      gauche()
    }
}


def murGauche():Boolean=
{
  gauche()
  var mur=estFaceMur()
  droite()
  return mur
}


