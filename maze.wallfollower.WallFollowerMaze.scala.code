override def run() {
  brushDown()
  while(!isOverBaggle()){

    	while(!isFacingWall() && wallToLeft()){
        forward()
      }
    if(isFacingWall()){
      if(wallToLeft()){
        right()
      }
      else{
        left()
        forward()
        //left()
        //forward()
      }
    }
   else{
     left()
     forward()
   }
    
  }
  pickupBaggle()
}

def wallToLeft() : Boolean = {
  left()
  var mur : Boolean = isFacingWall()
  right()
  return mur
}