var nbBiscuits:Int = 0
while (!sortieTrouvee()) {
  do {
    avance()
    if (estSurBiscuit()) {
      nbBiscuits = nbBiscuits + 1
    }
  } while (!crossing())  
  if (nbBiscuits >= 3) {
    gauche()
  } else {
    droite()
  }
  nbBiscuits = 0
}
avance()

