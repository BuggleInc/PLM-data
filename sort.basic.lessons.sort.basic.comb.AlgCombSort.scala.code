def combSort() {
  var ecart:Int = getNombreValeurs()
  var i:Int = 0
  var ok:Boolean = false
  while(ecart>1 || ok == false){
    ok = true
    if(ecart >1){ecart = (ecart.toDouble / 1.3).toInt}
    i=0
    while(i+ecart < getNombreValeurs()-1){
      if(plusPetit(i+ecart,i+ecart)){
        echange(i,i+ecart);
        ok = false
      }
      i+=1
    }
  }
}