def posFinale():Boolean={
	if ((getMondeHauteur()-1==getY())&&(getMondeLargeur()-1==getX())){
  	return true
  }
  return false
}

override def run() {

	var compteur = 0
  
	while (!posFinale()){
    
		ecritMessage(compteur)
    if (compteur%getMondeHauteur() != 0){
    	setPos(getX(),getY()+1)
    	compteur += 1
  	} else {
    	setPos(getX()+1,0)
  	}
  
	}

}