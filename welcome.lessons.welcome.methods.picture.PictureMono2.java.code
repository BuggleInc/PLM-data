public void run() {
	for(int k=0;k<4;k++)
	makeV();
}
public void brush()
{
	brushDown();
	brushUp();
}
public void makeV()
{
	for(int i =0;i<2;i++)
	forward();
	brush();
	forward();
	left();
	forward();
	brush();
	backward();
	right();
	forward();
	brush();
	for(int j=0;j<2;j++)
	forward();
     left();
}
