def cocktailSort() {
	val l = getNombreValeurs()-2
	var fin = false
	do {
		var a = 0
		for (i<- 0 to l) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1); a=a+1
			}
		}
		for ( i<- l to 0 by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);a=a+1
			}
		}
		if (a==0) {
			fin = true
		}
	} while (!fin)
}
