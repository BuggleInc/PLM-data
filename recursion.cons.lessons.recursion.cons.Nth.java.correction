$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; import lessons.recursion.cons.universe.*;import java.util.Vector;public class nth extends BatEntity { public RecList cons(int head, RecList tail){  return new RecList(head, tail);} @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( nth( (RecList)t.getParameter(0), (Integer)t.getParameter(1) ) ); }
	int nth(RecList seq, int n) {
		if (n == 1) return seq.head;
		return nth(seq.tail, n-1);
	}
