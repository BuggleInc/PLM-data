def solve() {
  for(i<-0 to getSize()/2-1){
    
    if(getColor(i)<getColor(getSize()-1-i)){
			swap(i,getSize()-1-i);
    }
    if(getColor(i)==getColor(getSize()-1-i)){
			swap(i,getSize()-i);
    }
    
  }
  
  for(i<-0 to getSize()-2){
    
    if(getColor(i)<getColor(i+1)){
			swap(i,i+1);
    }
    
  }
}