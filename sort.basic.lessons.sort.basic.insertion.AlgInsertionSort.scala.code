def insertionSort() {
  var lgr:Int= getNombreValeurs()
  var v=getValeur(1:Int)
  var c:Int=0
  for (i<-1 to lgr-1) {
    v=getValeur(i:Int)
    for  (j<- 0 to i-1){
      if (!plusPetitQue(i-j:Int, v:Int)){
        setValeur((i-j):Int, getValeur(i-j-1:Int):Int)
        c=i-j
      }
    setValeur(c,v)
    }
  }
}