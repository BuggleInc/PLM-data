def insertionSort() {
  var v = 0
  var ecritV=false
  for (i<-1 to getNombreValeurs()-1){
    v = getValeur(i)
    ecritV=false
    for(j<-i-1 to 0 by -1){  
      if (!plusPetitQue(j,v) && !ecritV ){
        copie(j,j+1)
      } else if (!ecritV){
        setValeur(j,v)
        ecritV=true
      }
    }
  }
}