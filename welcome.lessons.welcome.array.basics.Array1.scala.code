override def run() {
	var T=new Array[Color](getMondeHauteur)
	for (i<-0 to T.length) {
		T(i)=getCouleurSol
		avance
	}

	def marque(c:Color){
		setCouleurBrosse(c)
		baisseBrosse
		leveBrosse
	}	
	
	var Largeur=getMondeLargeur
	for(i<-1 to Largeur){
		recule(getMondeHauteur)
		droite
		avance
		gauche
		for (j<-0 to T.length){
			marque(T(i))
			avance
		}
	}
}
