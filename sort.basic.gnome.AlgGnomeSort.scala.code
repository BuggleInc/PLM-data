def gnomeSort() 
{
  var iSuiv = 0
  var jSuiv = 1
  var iPrece = 0

  while (jSuiv <= getNombreValeurs()-1)
  {
    
    if (iPrece<1)
  {
    iPrece+=1
    iSuiv+=1
    jSuiv+=1
  }
    if (plusPetit(iSuiv,jSuiv))
    {
      iPrece+=1
      iSuiv+=1
      jSuiv+=1
    }

    else
    {
      echange(iSuiv,jSuiv)
      iPrece=iPrece -1
      iSuiv=iSuiv -1
      jSuiv= jSuiv -1

    }

  }
  
}