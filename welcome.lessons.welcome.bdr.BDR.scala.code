var lettre =  ""
var finir = false
while (estSurMessage()) {
	lettre = litMessage()
	if (lettre == "R"){
		droite()
		avance()
	} else if (lettre == "L"){
		gauche()
		avance()
	} else if (lettre == "I") {
		retourne()
		avance()
	} else if (lettre == "A") {
		avance()
	} else if (lettre == "B") {
		avance(2)
	} else if (lettre == "C") {
		avance(3)
	} else if (lettre == "Z") {
		recule(1)
	} else if (lettre == "Y") {
		recule(2)
	} else if (lettre == "X") {
		recule(3)
	}
	
	
	
} 

