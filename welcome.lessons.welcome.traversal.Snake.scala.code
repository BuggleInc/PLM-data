override def run() {
  baisseBrosse()
  while(posFinale() == false){serpent()}
}

def serpent(){
  if(!estFaceMur()){avance()}
  if(estFaceMur() && getDirection() == Direction.EST){avance();gauche()}
  if(estFaceMur() && getDirection() == Direction.OUEST){avance();droite()}
}

def posFinale(){
  var x:Boolean = false
  if(estFaceMur() && getDirection() == Direction.EST){
    gauche()
    if(estFaceMur()){x = true}
  }
  if(estFaceMur() && getDirection() == Direction.OUEST){
    droite()
    if(estFaceMur()){x = true}
  }
  return(x)
}
