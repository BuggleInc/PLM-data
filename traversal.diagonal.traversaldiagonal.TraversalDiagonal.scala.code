var diag = 0;
override def run() {
  
  def changeDiag(x : Int, y : Int) {
    
    if (diag < 6) {
      setPos(0 , diag + 1);
    } else if (diag >= 6) {
      setPos(getX()-1, 6);
    }
    
  }
  
  def diagonale(x : Int, y : Int) {
    
    if ((x == 0) && (y == 0)) {
      
      changeDiag(x , y);
      diag += 1;
    } else {
      
      setPos(getX()+1 , getY()+1)
      
    }
    
    
  }
  
  
  
  
}