def array667(nums:Array[Int]): Int = {
  var compteur =0
  var test=0
  var prem=nums(0)
  var deu=nums(0)
  for (compteur <- 1 to (nums.length-1)){
    prem=nums(compteur)
    deu=nums(compteur-1)
    if (prem==deu && prem==6){
      test=test+1
    }
    if (prem==7 && deu==6){
      test = test+1
    }
  }
}