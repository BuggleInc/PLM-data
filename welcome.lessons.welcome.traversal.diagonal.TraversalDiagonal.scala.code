var diag = 0;
override def run() {
	var x=0
	val gML=getMondeLargeur-1
	val gMH=getMondeHauteur-1	
	var i=0
	var j=0
	var y=0
	var z=1
	while (diag!=2*gML){
		do {
			if (diag<gML+1) setPos(i,j)
			else setPos(j,i)
			ecritMessage(x)
			x+=1
			j-=z
			i+=z
		}while (i<=diag-y)
		if (diag<gML+1) {i=0;j=diag+1}
		else {j=diag-gML+1;i=gMH;y=2*gML-diag;z= -1}
		diag+=1
	}
}
