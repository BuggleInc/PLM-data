override def run() {
  while (!estSurBiscuit){
    if (keepHandOnSideWall()){
      if (!estFaceMur){
        avance()
      }else{
        droite()
        if (!estFaceMur){
          droite()
          avance()
        }
        avance()
      }
    }else{
      gauche()
      avance()
    }
  }
}

keepHandOnSideWall() : Boolean ={
  gauche()
  if (estFaceMur){
    droite()
    return true
  }else{
    droite()
    return false
  }
  return false
}