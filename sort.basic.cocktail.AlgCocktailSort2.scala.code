def cocktailSort2() {
  var beg = 0
  var end = getValueCount() - 2
  var swaped = false
  
  do {
    swaped = false
    
    for(i <- beg to end) {
      if(isSmaller(i + 1, i)) {
        swap(i, i + 1)
        end -= 1
        swaped = true
      }
    }
    
    for(i <- end to beg by -1) {
      if(isSmaller(i + 1, i)) {
        swap(i, i + 1)
        beg += 1
        swaped = true
      }
    }
  } while(swaped)
}