def max1020(a:Int, b:Int):Int = {
  if (a < 10 && a > 20 && b < 10 && b > 20)
  	{return 0}
  if (a >= 10 && a <= 20 && (b <10 || b >20))
  	{return a}
  if (b >= 10 && b <= 20 && (a <10 || a >20))
  	{return b}
  if ((a >= 10 && a <= 20) && (b >= 10 && b <= 20))
  	{if (a >= b)
    	{return a}
  	else 
      {return b}
    
    }
  

}