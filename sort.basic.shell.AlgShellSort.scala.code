def shellSort()  {
  var ecart = getNombreValeurs()/2
  while (ecart>0) {
    var v = 0
    var mem = 0
    for (i<-1 to getNombreValeurs()-1) {
      v=getValeur(i)
      mem=i
      for (j<-1 to i) {
        if (plusPetitQue(i-j,v)==false) {
          copie(i-j,i-j+1)
          mem = i-j
        }
      }
      setValeur(mem,v)
    }
  }
}