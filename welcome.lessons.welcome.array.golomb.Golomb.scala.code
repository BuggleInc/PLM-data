def golomb(num:Int): Int = {
  
  if (num>1)
  	return -1;
  
  var a= new Array[Int](100)
  var last=1
  a(0)=0
  a(1)=1
  for (i<-2 to a.length-1){
    if (a(i) == 0) {
      a(i) = a(i-1)+1
      last=last+1
    }
    for (j<-1 to a(i)){
      a(last)=i
      last=last+1
    }
  }
  return a(num)
}