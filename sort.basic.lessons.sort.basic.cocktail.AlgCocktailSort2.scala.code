def cocktailSort2() {
  var debut=1
  var fin=getNombreValeurs()-1
  var test=false
  while (!test){
    test = true
    for (i <- debut to fin){
      if(plusPetit(i,i-1)){
        test = false
        echange(i-1,i)
      }
    }
    fin -=1
    for (i <- fin to debut by -1) {
        if (plusPetit(i,i-1)){
        test = false
        echange(i-1,i)
    }
    }
    debut+=1
  }
}