def alarmClock(day:Int, vacation:Boolean): String = {
  var z:String="7:00"
  var w:String="10:00"
  var y:String="off"
  if (day!=0 && day !=6 && !vacation){
    return z
  } else if (day==0 && !vacation){
    return w
  } else if (day==6 && !vacation){
    return w
  } else if (day!=0 && day !=6 && vacation){
    return w
  } else {
    return y
  }
}