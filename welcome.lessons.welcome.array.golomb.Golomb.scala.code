def golomb(num:Int): Int = {
  
	var Tableau = new Array[Int](1)
  var occurence = 2
  Tableau(0) = 1
  var indice = 2
  
  for (i <- 2 to num){
    if (occurence == 0){
      occurence = Tableau(i-1)
      indice = indice + 1
    }
    occurence = occurence - 1
    Tableau = AugmenterTaille(Tableau)
    Tableau(i-1) = indice
  }
  return Tableau(num)
}

    
def AugmenterTaille(tab:Array[Int]){
  
  var newTab = tab + new Array[Int](1)
  return newTab
  
}