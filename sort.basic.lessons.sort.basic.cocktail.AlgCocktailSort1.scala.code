def cocktailSort() {
	var sorted : Boolean = false;
	do {
		sorted = true;
		for (i <- 0 to getNombreValeurs()-2) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				sorted = false;
			}
		}
		for (j <- getNombreValeurs()-2 to 0 by -1) {
			if (!plusPetit(j,j+1)) {
				echange(j,j+1);
				sorted = false;
			}
		}
	} while (!sorted)
}
