override def run() {
  var compteur = 0
  ecritMessage(""+compteur)
  while (getX() != getMondeLargeur() && getY() != getMondeHauteur()){
    if (getY() != getMondeHauteur() -1){
      setPos(getX(), getY() + 1);
      compteur += 1;
      ecritMessage(""+compteur);
    }
    else{
      setPos(getX() + 1, 0);
      compteur += 1;
      ecritMessage(""+compteur);
    }
  }
}