override def run() {
  lire()
  for(i<- 1 to getMondeLargeur()-1){
    gauche()
    avance()
    droite()
  }
}

def lire(){
  var n:Int=getMondeHauteur()
	var couleurs=new Array[Color](n)
  for (i<- 1 to n-1){
    couleurs(i)=getCouleurSol()
    avance()
  }
  couleurs(n)=getCouleurSol()
  recule(n-1)
}