def combSort() {
  var ecart : Int = getNombreValeurs()-1
  var inverse = true
  do{
    if (ecart>1){
      ecart=(ecart.toDouble/1.3).toInt
    }
    inverse = false
    var i=0
    for (i+ecart<getNombreValeurs()-1){
      if (!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        inverse=true
        i=i+1
      }
    }
  }while (inverse) || ecart>1
}