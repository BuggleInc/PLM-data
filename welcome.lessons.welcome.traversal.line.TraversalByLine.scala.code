override def run() {
var x = 0
	var y = 0
	var mess = 0
	
	ecritMessage(mess)
	
	
	while (getY() != getMondeHauteur() - 1 || getX() != getMondeLargeur() - 1)
	{
		if (getX() == getMondeLargeur() - 1)
		{
			y = y + 1 
			x = 0
			setPos(x , y )
			mess = mess + 1  
			ecritMessage(mess)		  
		}

		x = x + 1
		setPos(x , y)
		mess = mess + 1
		ecritMessage(mess)	
		
	}

}
