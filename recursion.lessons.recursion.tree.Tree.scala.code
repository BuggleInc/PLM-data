def tree(steps:Int, lenght:Double, angle:Double, shrink:Double)    {
current(steps)
if(steps==0) {return()}
	else{
		avance(lenght)
		droite(angle)
		tree(steps-1,lenght*shrink,angle,shrink)
		gauche(2*angle)
		tree(steps-1,lenght*shrink,angle,shrink)
		current(steps)
		droite(angle)
		recule(lenght)
	}
}	
		
		
		
		
		


