def inOrderEqual(a:Int, b:Int, c:Int, equalOk:Boolean):Boolean = {
	(a,b,c,equalOk) match {
		case (w,x,y,z) if (equalOk==false && a<b && b<c) => return true
		case (w,x,y,z) if (equalOk==true && a==b) => return true
		case (w,x,y,z) if (equalOk==true && a==c) => return true
		case (w,x,y,z) if (equalOk==true && b==c) => return true
		case (_,_,_,_) => return false
	}
}
