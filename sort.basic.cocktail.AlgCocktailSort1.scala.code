def cocktailSort() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var echangefait=false
  do {
    echangefait=false
    for (i<- debut to fin){
      if (plusPetit(i,i+1)==false){
        echange(i,i+1);
        echangefait=true;
      }
    }
   for (i<-fin to debut){
     if (plusPetit(i,i+1)==false){
       echange(i,i+1);
       echangefait=true;
     }
   }
  } while (echangefait==true)
}