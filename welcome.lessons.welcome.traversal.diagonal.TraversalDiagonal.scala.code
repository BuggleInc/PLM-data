
override def run() {
  var comp = 0;
  var col = 0;
  var lign = 0;
  while (!(positionFinale())) {
    if (getY == 0) {
      col = col +1;
    } else {
      lign = lign +1;
    }
    setPos(lign-col,col);  
  }
}



def positionFinale():Boolean = {
  var res = false;
  if (getX()==6  && getY()==6) {
    res = true;
  }
  return res
}