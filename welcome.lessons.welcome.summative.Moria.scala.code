def Moria() {
  retourne()
  while(getCouleurSol()!=Color.green){
    if (estFaceMur==true && estSurBiscuit==false){
    droite()
    avance()
    gauche()
    avance()
  }
    else rameneBiscuit()
  }
}

def rameneBiscuit(){
  while (!estSurBiscuit()){
    avance()
  }
  prendBiscuit()
  retourne()
  while (!estSurBiscuit()){
    avance
  }
  recule()
  poseBiscuit()
}