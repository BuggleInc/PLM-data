def insertionSort() {
	val longueur:Int = getNombreValeurs();
	var tmp:Int = 0;
	var lastCase:Int = 0;
	for (i <- 0 to longueur -1) {
		tmp = getValeur(i);
		for (j<- 0 to i) {
			if (!plusPetit(getValeur(i-j),tmp)){
				setValeur(i,getValeur(i-j));
			}
			lastCase = i-(j-1);
		}
		 setValeur(lastCase,tmp);
	}
	
}
