def cocktailSort3() {
  var debut = 0
	var fin = getNombreValeurs()-2
	var mem = 0
  do {
    mem=1
    for (i<-debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
        mem=0
      }
    }
    if (mem==1) {
      return
    }
    mem=1
    fin=fin-1
    for (i<-debut to fin) {
      if (plusPetit(fin-i+1,fin-i)) {
        echange(fin-i+1,fin-i)
        mem=0
      }
    }
    if (mem==1) {
      return
    }
    debut=debut+1
  } while (mem==0)
}