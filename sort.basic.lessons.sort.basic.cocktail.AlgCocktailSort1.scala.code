def cocktailSort()
{
  var max = getNombreValeurs() - 2
  var inversion = false
 
  for (l <- 0 to getNombreValeurs()/2)
  {
    inversion = false
    
    for (i <- 0 to max)
    {
      if (plusPetit(i + 1, i))
      {
    		echange(i, i + 1)
        inversion = true
      }
    }
    
    for (i <- max to 0)
    {
      if (plusPetit(i + 1, i))
      {
    		echange(i, i + 1)
        inversion = true
      }
    }
    
    max -= 1
  }
}