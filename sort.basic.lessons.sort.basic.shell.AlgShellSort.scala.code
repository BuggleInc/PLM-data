def shellSort()  {
  var lgr = getNombreValeurs()
  var ecart = lgr/2
  while(ecart>0){
  var v = 0
  for (i<-lgr/2  to lgr-1){
    v = getValeur(i)
    var j = i-ecart
    while(j>=0 && !plusPetitQue(j,v)){
      copie(j,j+ecart)
      j-=ecart
    }
    
    setValeur(j+1,v)
    ecart = ecart/2
    
  }
  }

}