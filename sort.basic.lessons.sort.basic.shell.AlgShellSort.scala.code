def shellSort()  {
  var ecart=getNombreValeurs()/2
  
  while(ecart>0){
  
    
    for (i<-ecart+1 to getNombreValeurs()-1){
      var j=i
      var memory=getValeur(i)
      while (j-ecart>=0 && !plusPetitQue(j-ecart, memory)){
        copie(j-ecart, j)
        j=j-ecart
      }
      setValeur(j, memory) 
    }
  
  ecart=ecart/2  
  }
  
}