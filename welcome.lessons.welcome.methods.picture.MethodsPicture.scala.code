override def run() {

def marquerSol(c : Color) {
	setCouleurBrosse(c : Color);
	baisseBrosse();
	leveBrosse();
}
def faireV(c : Color) {
	avance();
	marquerSol(c : Color);
	avance();
	gauche();
	avance();
	marquerSol(c : Color);
	recule();
	droite();
	avance();
	marquerSol(c : Color);
	avance();
	gauche();
}
def faireCarre() {
	faireV(Color.GREEN);
	faireV(Color.YELLOW);
	faireV(Color.RED);
	faireV(Color.BLUE);
	for (i <- 1 to 5) {
		avance();
	}
}
for (j <- 1 to 9) {
	faireCarre();
}
}
