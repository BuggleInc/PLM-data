def combSort() {
  var nbValeurs = getNombreValeurs()
  var ecart = nbValeurs
  var auMoinsUnEchange = 0
  while(ecart > 1 || auMoinsUnEchange == 1)
  {
    auMoinsUnEchange = 0
    if(ecart>1)
    	ecart = (ecart.toDouble / 1.3).toInt
    var i = 0
    while (i+ecart < nbValeurs)
    {
      if (plusPetit(i, i+ecart))
      {
        echange(i, i+ecart)
        auMoinsUnEchange = 1
      }
      i += 1
    }
  }
}