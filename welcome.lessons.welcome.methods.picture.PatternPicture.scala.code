override def run() {
  for (i<- 1 to 2){
    for (i<- 1 to 2){
      faireMotif()
      for (i <- 1 to 4){
        avance()
      }
    gauche()
    for (i<- 1 to 4){
      avance()
    }
    droite()
    }
  }
}


def faireRed() {
  setCouleurBrosse(Color.red)
