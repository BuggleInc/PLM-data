def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  baisseCrayon()
	if (steps==0){
    cache()
    gauche(angle)
   	recule(length)
   	gauche(angle)
    avance(length)
  }
  else {
    
    avance(length)
    droite(angle)
  	
  	
  	current(steps)
  	tree(steps-1,length*shrink,angle,shrink)
    
  }
  	
  	
 
}