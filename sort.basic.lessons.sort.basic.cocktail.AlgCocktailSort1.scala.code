def cocktailSort() {
  var switch=true
  while (switch) {
    switch=false
    for (k <- 0 to getNombreValeurs()-2) {
      if (plusPetit(k+1,k)) {echange(k,k+1);switch=true}
    }
    
    for (k <- getNombreValeurs()-2 to 0 by -1) {
      if (plusPetit(k+1,k)) {echange(k,k+1);switch=true}
    }
    if (!switch) {return}
  }
}