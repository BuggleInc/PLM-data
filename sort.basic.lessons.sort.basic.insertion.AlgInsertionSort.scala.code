def insertionSort() {
	

var n=getNombreValeurs()
  var T:Array[Int](getNombreValeurs())
  var i=0 //indice de parcours
  var j=0 //indice pour savoir ou on va inserer
  var v=0 // ou on va stocker la valeur de T(i)

  i=2
  v=T(i) // valeur de la case i dans une variable v
  while(i<n)
  {
  	j=i-2
  	while(j>0 && v<T(j))
  	{
  		T(j+1)=T(j)
  		j=j-1
  	}
  	T(j+1)=v
  	i=i+1
  }
}