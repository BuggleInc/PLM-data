def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = 
{
	if (noDoubles && die1==die2)
	{
		if (die1=6 && !die2=6)
		{
			die1=1
			return die1+die2
		} else if (die2=6 && !die1=6)
		{
			die2=1
			return die1+die2
		
		} else {
			return (die1=die1+1 || die2=die2+1)
		}
	} else {
		return (die1+die2)
	}
}
