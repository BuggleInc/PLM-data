override def run() {

def positionFinale(x:Int, y:Int):Boolean = {
	if (x==getMondeLargeur()-1 && y==getMondeHauteur()-1) {
		return true
	}
	else {
		return false
	}
}

	var x=0
	var y=0
	while (!positionFinale(x, y)) {
		x = x
		y = y + 1;
		if (y == getMondeHauteur()-1) {
			y = 0
		}
		setPos(x, y)		
		writeMessage("x");
	}
}
