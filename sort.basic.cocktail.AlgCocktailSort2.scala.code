def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs() - 2
	var l = getNombreValeurs()
  while (debut <= l/2  +1) {
    for (i <- debut to fin){
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
      }
    }
    fin -= 1
    for (i <- fin to debut) {
      if (plusPetit(i,i+1)) {
        echange(i,i+1)
      }
    }
    debut += 1
  }
}

