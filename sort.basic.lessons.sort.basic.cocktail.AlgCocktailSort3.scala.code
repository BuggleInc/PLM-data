def cocktailSort3() {
	var b:Int=0
	var c:Int=getNombreValeurs()-2
	var x:Boolean=true
	do {x=false
		for(i<-b to c) {
			if (!plusPetit(i,i+1)){
			echange(i,i+1)
			x=true
			}
			}
			if (x==false) return
			c -= 1
			
		for(i<-c to b by -1) {
			if (!plusPetit(i,i+1)){
			echange(i,i+1)
			x=true
			}
			}
			if (x==false) return
			b += 1
		
		
	} 
	while(x)}

}
