void solve() {
  
  int b = 0;
  int bl = 0;
  int r = getTaille();
  int i = 0;
  
  while(bl<r)
  {
  	if(getCouleur(i)==BLEU)
      {
      echange(b,bl);
      b++;
      bl++;
    }
    else if(getCouleur(i)==BLANC)
      {
     bl++;
    }
    else
      {
      echange(bl,r);
      r--;
    }

    i++;
	}
}