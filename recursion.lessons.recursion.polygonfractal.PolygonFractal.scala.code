def polygonFractal (levels:Int, sides:Int, length:Double, shrink:Double) {
	if(levels==0){
		avance(length*shrink)
		droite(360/sides)
		avance(length*shrink)
		droite(360/sides)
		avance(length*shrink)
		droite(360/sides)
		avance(length*shrink)
		droite(360/sides)
		avance(length*shrink)
		droite(360/sides)
	}else{
		polygonFractal(levels-1, sides, length, shrink)
	}
}
