def isFacingTrail():Boolean = {
  if(getCouleurSol() == Color.green){
    avance()
    return true
  }
  while(1==1){
    while(estFaceMur()){
      droite()
      gauche()
    }
    droite()
    avance()
    if(getCouleurSol() == Color.green){
      return true
    }
    recule()
  }
  return false
}