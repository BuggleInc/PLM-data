



fini = False

while not fini:
    action  = False
    for i in range(getNombreValeurs()-1):
        if plusPetit(i+1,i):
            echange(i+1,i)
            action = True
    for i in range(getNombreValeurs()-1,0,-1):

        if plusPetit(i,i-1):
            echange(i,i-1)
            action = True
    if action == False:
        fini = True
        






