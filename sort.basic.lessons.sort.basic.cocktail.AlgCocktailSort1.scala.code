def cocktailSort() {
  var a:Boolean = false
  for (i <- 0 to getNombreValeurs-2) {
    a = false
    for (j <- 0 to getNombreValeurs()-2-i) {
      if (getValeur(j)>getValeur(j+1)) {
        echange(j,j+1)
        a = true
      }
    }
    for (j <- getNombreValeurs()-2-i to 0) {
      if (getValeur(j)>getValeur(j+1)) {
        echange(j,j+1)
        a = true
      }
    }
    if (a == false) {return}
  }
}