def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  var vitmax1:Int = 60
  var vitmax2:Int = 80
  if (isBirthday) {
    vitmax1 = vitmax1 + 5
    vitmax2 = vitmax2 + 5
  }
  if (speed <= vitmax1) {
    return 0
  } else if (vitmax1 + 1 <= speed <= vitmax2) {
    return 1
  } else {
    return 2
  }
