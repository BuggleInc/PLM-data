def bubbleSort2() {
  var n: Int= getNombreValeurs() 
  var estTriee:Boolean=false
  while (estTriee==false){
    estTriee=true
    for (j<-0 to n-1){
      for (i<- 0 to n-2){
        if (plusPetit(i+1,i) ) {
          echange(i,i+1)
          estTriee=false
        }
      }
      n=n-1
    }
  }
}