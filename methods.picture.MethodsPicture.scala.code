override def run() {
  for (k<-1 to 3) {

    for (j<-1 to 3) {

      faireV(Color.YELLOW)
      faireV(Color.RED)
      faireV(Color.BLUE)
      faireV(Color.GREEN)
      avance(5);
    }
    gauche();
    avance(5);
    droite();
  }

}
def faireV(c:Color) {
