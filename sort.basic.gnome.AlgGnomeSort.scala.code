def gnomeSort() {
  var i=0;
  while(i!=getNombreValeurs()-1){
    var fin=false;
    while(fin!=true){
      if(plusPetit(i, i+1)){
        i=i+1
      }
      else{
        echange(i,i+1);
        fin=true;
        i=i-1
      }
    }
    fin=false;
    if(i==1){
      i=i+1;
    }
    while(fin!=true){
      if(plusPetit(i-1, i)){
        i=i-1
      }
      else{
        echange(i-1,i);
        fin=true;
        i=i+1
      }
    }
  }
}