def cocktailSort3() {
	var debut = 0;
	var swapedAsc = true;
	var swapedDsc = true;
	var fin = getNombreValeurs - 2;
	var i = 0;
	do{
		swapedAsc = false;
		swapedDsc = false;
		for(i <- debut to fin){
			if(plusPetit(i+1, i)){
				swapedAsc = true;
				echange(i+1, i);
			}
		}
		fin -= 1;
		i = fin;
		while(i >= debut){
			if(plusPetit(i+1, i)){
				swapedDsc = true;
				echange(i+1, i);
			}
			i -= 1;
		}
		debut += 1;
	}while(swapedAsc && swapedDsc);
}
