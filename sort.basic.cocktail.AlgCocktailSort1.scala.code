def cocktailSort() {
  var swaped = false
  
  do {
    swaped = false
    
    for(i <- 0 to getValueCount() - 2) {
      if(isSmaller(i + 1, i)) {
        swap(i, i + 1)
      	swaped = true
      }
    }
    
    for(i <- getValueCount() - 2 to 0 by -1) {
      if(isSmaller(i + 1, i)) {
        swap(i, i + 1)
        swaped = true
      } 
    }
  } while(swaped)
}