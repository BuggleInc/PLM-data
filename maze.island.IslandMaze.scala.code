override def run() {
  var etat = 0;
  etat match {
    case 0 => courirNord(); 
    case 1 => suiveurEst();
    
  }
}


def courirNord() {
  var etat = 0;
  while (!estFaceMur) {
    forward();
  }
  if (estFaceMur) {
    etat = 1
  }
}


def suiveurEst() {
  while (!estSurBiscuit) {
    	keepHandOnSideWall();
  }
  prendBiscuit();
}


def keepHandOnSideWall() {
  left();
  while (estFaceMur()) {
    right();
  }
  forward();
}