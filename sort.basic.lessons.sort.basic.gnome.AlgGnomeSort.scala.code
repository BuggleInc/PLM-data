def gnomeSort() {
  var pos:Int =0;
  
  while( pos !=getNombreValeurs-1){
    if(plusPetit(pos+1,pos)){
      echange(pos,pos+1);
      if(pos>0){
        pos=pos-1;
      }else if(pos==0){
        pos=pos+1;
      }
    }else if(pos!=getNombreValeurs-1){
      pos=pos+1;
    }
    
   
  }
  
  
  
  
}
