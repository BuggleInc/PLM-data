def deplacerBiscuit() {
  prendBiscuit();
  retourne();
  while(!estSurBiscuit) {
    avance();
  }
  recule();
  poseBiscuit();
  retourne();
}

def sortir() {
  retourne();
  while (getCouleurSol() != Color.GREEN) {
    if (estSurBiscuit) {
      deplacerBiscuit();
    } else if (estFaceMur) {
      droite();
      avance();
      gauche();
    } else {
      avance();
    }
  }
}