public void run() {
	for (int z = 0; z < getWorldWidth() * getWorldHeight() - 1; z++) {		
		int x = ((Math.floor((Math.sqrt(8 * z + 1)+1)/2)*Math.floor((Math.sqrt(8 * z + 1)+3)/2))/2) - z - 1;
		int y = z - ((Math.floor((Math.sqrt(8 * z + 1)+1)/2)*Math.floor((Math.sqrt(8 * z + 1)-1)/2))/2);
		setPos(x,y);
		writeMessage(z);
	}
}