def combSort() {
  var used = true;
  val len = getValueCount();
  var gap = len;
  var i = 0;
  do{
    used = false;
    if (gap > 1) {
      gap = (gap.toDouble / 1.3).toInt
    }
    i = 0;
    while (i+gap < len){
      if(!isSmaller(i, i+gap)){
        swap(i,i+gap);
        used = true ;
      }
      i+=1;
    }
  }while (gap > 1 || used)
}