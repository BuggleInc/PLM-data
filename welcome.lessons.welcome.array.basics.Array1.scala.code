override def run() 
{
var hauteur: Int= getMondeHauteur()
var largeur: Int= getMondeLargeur()
var T = new Array[Color](hauteur)
def marquer()
{ 
	baisseBrosse();
	leveBrosse();
}
def getMotif()
{
for (k <- 0 to (hauteur-1))
	{
		avance();
		T(k)=getCouleurSol() 
	} 	 
}
for (i <- 0 to largeur-1  )
		{
		getMotif();
	  	setCouleurBrosse(T(0))
		marquer();
		avance();
		}
		
}



