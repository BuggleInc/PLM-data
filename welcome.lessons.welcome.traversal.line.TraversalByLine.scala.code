override def run() {
	var hauteur=getMondeHauteur();
	var largeur=getMondeLargeur();
	var num =0 ;
	var x=0 ;
	var y=0 ;
	while ( !(getX()==largeur) && !(getY()==hauteur-1))
{
	if (x !=6 )
	{
	ecritMessage(num)
	setPos(x+1,y)
	x=x+1
	num=num+1
	}
	
	if (y!=6 && x==6)
	{
	ecritMessage(num)
	setPos(0,y+1)
	y=0
	y=y+1
	}
	
}
ecritMessage(num)
}


