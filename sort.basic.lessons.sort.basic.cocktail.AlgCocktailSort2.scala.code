def cocktailSort2() {
var estRange = 1 // on initialise à "est pas range"
var debut = 0
var fin = getNombreValeurs()-2
while(estRange == 1)
{
	estRange = 0 // on suppose que le dernier tour l'a rangé, si c'est vrai on reste à 0
	for(i <- fin to debut by -1)
	{
		if(getValeur(i) < getValeur(i+1))
		{
		echange(i,i+1)	
		estRange = 1
		}
	debut = debut + 1		
	}

	for( i<- debut to fin )
	{
		if(getValeur(i) > getValeur(i+1))
		{
		echange(i,i+1)	
		estRange = 1	
		}
	fin = fin - 1
	}

}
}
