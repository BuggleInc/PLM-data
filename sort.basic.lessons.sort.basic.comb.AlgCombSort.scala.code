def combSort() {
  var ecart : Int = getNombreValeurs()-1
  var inverse = true
  do{
    if (ecart>1){
      ecart=(ecart.toDouble/1.3).toInt
    }
    inverse = false
    for (i<-ecart to getNombreValeurs()-1){
      if (!plusPetit(i-ecart,i)){
        echange(i-ecart,i)
        inverse=true        
      }
    }
    
  }while (ecart>1 || !inverse )
}