def cocktailSort() {

  var L =getNombreValeurs()
  for (k<- L-2 to 0 by -1 ){
    
    L=L-1
    for(i<-0 to k) {
    	if (getValeur(i) > getValeur(i+1)){
      	echange(i,i+1)
  }      
      }     
  for (l<-0 to L-2){
    	if (getValeur(l) > getValeur(l+1)){
      	echange(l,l+1)
    
  }
