def gnomeSort() {
  var n:Int=getNombreValeurs()
  var i : Int=1
  while(i<n-1){
    if (i==0){if(!plusPetit(i,i+1)){
      echange(i,i+1)
      i=i+1
    }else {
      i=i+1
    }
             }else {if(!plusPetit(i,i+1)){
      echange(i,i+1)
      i=i-1
    }else {
      i = i+1
    }
                   }
  }
}