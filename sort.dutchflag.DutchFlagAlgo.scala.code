def solve() {
  while (isSorted()) {
    for (x <- 0 to getSize()) {
      if (getColor(x)==BLUE) {
        swap(x,0)
      } else { 
        if (getColor(x)==RED) {
          swap(x,getSize()-1)
        }
      }
    }
  }
}

def insertionSort() {
  var v:Int=0
  var j:Int=0
  for (i <- 1 to getValueCount()-1) {
    v=getValue(i)
    j=i
    while ((j>0) && (getValue(j-1)>v) ) {
      setValue(j, getValue(j-1))
      j=j-1
    }
    setValue(j,v)
  }
}