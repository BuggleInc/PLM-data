def cocktailSort2() {
	var d:Int=0;
	var f:Int=getNombreValeurs()-2;
	var t:Boolean=false;

	while (t==true) {
		t=false;
		for (i<-d to f) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				t=true;
			}
		}
		for (i<-f to d by -1) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				t=true;
			}
		}
	}
}
