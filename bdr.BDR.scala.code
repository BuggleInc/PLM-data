var fini:Boolean = estSurMessage()
var A:String =""
while(fini==true){
  A=litMessage();
  if(A=="R"){
  	droite();
    avance();
  }
  if(A=="L"){
  	gauche();
    avance();
  }
  if(A=="I"){
  	retourne();
    clearMessage();
  }
  if(A=="A"){
    avance();
  }
  if(A=="B"){
    avance(2);
  }
  if(A=="C"){
    avance(3);
  }
  if(A=="Z"){
  	recule();
  }
  if(A=="Y"){
  	recule(2);
  }
  if(A=="X"){
  	recule(3);
  }
}