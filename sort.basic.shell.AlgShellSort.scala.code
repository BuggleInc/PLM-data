def shellSort()  {
  var ecart:Int=(getNombreValeurs().toDouble/2).toInt
  while (ecart>2){
  
  for (j<- 1 to getNombreValeurs()-1 ){
    var i:Int=j
    var a:Int=getValeur(i)
    while(i>ecart && a<getValeur(i-ecart) ){
      setValeur(i,getValeur(i-ecart))
      i-=ecart
    }
    setValeur(i,a)
  }
    ecart = (ecart.toDouble / 2).toInt
}
}