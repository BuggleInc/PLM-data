def hunt(c:Color) {
  while(!porteBiscuit()){
  if(estSurBiscuit()) prendBiscuit()
  else if(isFacingTrail(c)) {
    avance()
    baisseBrosse()
    leveBrosse()
  }
  else gauche()
  }}

def isFacingTrail(c:Color):Boolean = {
  if(estFaceMur()) return false
  avance()
  var rep = getCouleurSol() == c
  recule()
  return rep
}