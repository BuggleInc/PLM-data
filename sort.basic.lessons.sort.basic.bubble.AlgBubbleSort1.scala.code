
def bubbleSort()  {
  var taille = getNombreValeurs();
  var parcours = true;
  while (parcours)
  {parcours = false
    for(j<-1 to taille - 1)
    {
      
      if (getValeur(j)<getValeur(j-1))
          {
            echange(j,j-1)
            parcours = true
          }
    }
  }
}
          
