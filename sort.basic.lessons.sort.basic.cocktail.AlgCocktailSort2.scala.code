def cocktailSort2() {
var n=getNombreValeurs()
var compteur=1
var tictac=1

	
 while(compteur!=0){
 	compteur=0
	for (k<- 0+tictac to n-1-tictac){
		if (plusPetit(k+1,k)){
		    	echange(k,k+1)
		    	compteur=1
		}
	}
	for (k<- n-1-tictac to 0+tictac-1 by -1){
		if (plusPetit(k+1,k)){
			echange(k,k+1)
			compteur=1
		}
	}
	tictac=tictac+1
}
}

