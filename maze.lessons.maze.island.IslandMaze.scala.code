override def run() {
  while(!estSurBiscuit()){
    while(getY()!=0){
    	while(!estFaceMur){
      	avance()
    	}
      var n = 0
    	while(estFaceMur && (n%2 == 0) && (n%4 != 0)){
      	gauche()
        n = n+1
    	}
      while(!estFaceMur){
      	avance()
      }
      while(n!=0){
        gauche()
        n = n-1
      }
    }
  	GaucheMur()
  }
  prendBiscuit()
}

def GaucheMur(){
  gauche()
