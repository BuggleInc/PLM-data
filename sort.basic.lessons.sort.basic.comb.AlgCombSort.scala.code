def combSort() {
  var ecart = getNombreValeurs()
  var rep = false
  var n = getNombreValeurs()
  var i = 0
  
  do {
    rep=false
    
    if (ecart >1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i=0
    
    while (i+ecart < n ){
      
      if (getValeur(i)>getValeur(i+ecart)){
        echange(i,i+ecart)
        rep = true
      }
      i+=1
    }
    
    
    
  } while ( rep || ecart > 1)
}