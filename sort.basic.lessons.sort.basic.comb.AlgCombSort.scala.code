def combSort() {
var ecart=getNombreValeurs()-2;
var i=0;
var p=0;
do {
	if (ecart>1) {
		ecart = (ecart.toDouble / 1.3).toInt
		i=0
		p=0
		while ((i+ecart) < (getNombreValeurs()-1)) {
			if (getValeur(i+ecart)<getValeur(i)) {
				echange(i, i+ecart);	
			}
				i=i+1;p=1;
		}	
	}
}
while ((ecart > 1) || p !=1)
}

