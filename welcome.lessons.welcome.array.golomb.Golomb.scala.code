def golomb(num:Int): Int = {
  var pasDInspiration:Int = 2 // les valeurs croissantes de la suite //
  var compt:Int = 1 // compteur du nombre d'occurences de chaque valeur (initialisation bizarre mais sous contr?le) //
  var nums = new Array[Int](num) // un joli tableau <3 //
  nums(0) = 1
  nums(1) = 2
  for (i <- 2 to num - 1) {
    if (compt != nums(pasDInspiration-1)) {
      nums(i) = pasDInspiration
      compt = compt + 1
    } else {
      pasDInspiration = pasDInspiration + 1
      nums(i) = pasDInspiration
      compt = 1
    }
  }
  return nums(num - 1)
}