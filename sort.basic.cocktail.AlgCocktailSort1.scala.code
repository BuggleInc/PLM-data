def cocktailSort() {
  var lgr=getNombreValeurs()
  
  for (i<-0 to lgr-2){
    var pos1=i
    var pos2=i+1
    if (plusPetit(pos2,pos1)){
    	echange(pos1,pos2)
    }
  }
  
  for (i<-lgr-2 to 0 by -1){
    var pos1=i
    var pos2=i+1
    if (plusPetit(pos1,pos2)){
    	echange(pos1,pos2)
    }
  }
}