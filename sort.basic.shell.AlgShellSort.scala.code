def shellSort()  {
  var x:Int=getNombreValeurs()
  var v=0
  var y=0
  var ecart=x/2
  while (ecart>0){
  for (i<-1 to x-1) {
    y=i
    v=getValeur(i)
    for (j<-i-1 to ecart by -ecart) {
      if (plusPetitQue(j,v)==false) {  
        setValeur(j+1,getValeur(j))
        y=j
      }
    }
    setValeur(y,v)
   }
}
}