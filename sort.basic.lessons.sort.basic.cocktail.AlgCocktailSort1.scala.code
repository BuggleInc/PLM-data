def cocktailSort() {
	var taille: Int = getNombreValeurs();
	var tri: Boolean = false;
	do{
		tri = false;
		for(i <- 1 to taille - 1){
			if(!plusPetit(i - 1, i)){
				echange(i - 1, i);
				tri = true;
			}
			if(plusPetit(taille - i, taille - i  - 1)){
				echange(taille - i, taille - i - 1);
				tri = true;
			}
		}

	}while(!tri)
}
