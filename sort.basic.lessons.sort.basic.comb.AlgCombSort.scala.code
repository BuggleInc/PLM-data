def combSort() {
  var gap = getValueCount()
  var len=getValueCount()
  var i=0
  var swapped = false
  do {
    swapped =false
    if (gap>1){
      gap = (gap.toDouble/1.3).toInt
    }
    i=0
    while (i+gap<len){
      if (isSmaller(i,i+gap)){
        swap(i,i+gap)
        swapped = true
      }
      i=i+1
    }
  } while(gap>1 || swapped == true)
}