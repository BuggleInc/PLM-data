def cocktailSort() {

	var swapped = true
	while(swapped)
	{
		swapped=false
		for (i <- 0 to getNombreValeurs()-2)
		{
			if (plusPetit(i+1:Int, i:Int) )
			{
				echange(i:Int, i+1:Int)
				swapped=true
				
			}
			
			
		}
		for (i <- getNombreValeurs()-2 to 0 by -1 )
		{
			if (plusPetit(i+1:Int, i:Int) )
			{
				echange(i:Int, i+1:Int)
				swapped=true
				
			}
			
		
		

	
	}
}
	
