def cocktailSort3() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var inverse = true
  
  do{
    inverse = false
    for (k<-debut to fin){
      if (!plusPetit(k,k+1)){
        echange(k,k+1)
        inverse=true
      }
    }
    if (!inverse){
      return
    }
    fin-=1
    inverse=false
    for (k<-fin to debut by -1){
      if (!plusPetit(k,k+1)){
        echange(k,k+1)
        inverse=true
      }
    }
    if (!inverse){
      return
    }
    debut+=1
  }while (inverse)
}