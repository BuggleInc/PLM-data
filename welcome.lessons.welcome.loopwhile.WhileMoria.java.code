public void run() { 
	while(getGroundColor()!=Color.green) {
		while(!isFacingWall()) {
			left();
			left();
			forward();
			while(!isOverBaggle()) {
				forward();
			}
			pickUpBaggle();
			left();
			left();
			while(!isOverBaggle()) {
				forward();
			}
			backward();
			dropBaggle();
		}
		right();
		forward();
		left();
		forward();
	}
}
