def solve() {
  while(!estTrie()){
    insertionSort()
  }
}

def insertionSort() {
  var n:Int=getTaille()
  for ( k <- 1 to (n-1)){
    var stock:Int=getCouleur(k)
    var l:Int=0
    var h=n-1
    if (stock==BLEU){
      while(getCouleur(l)==BLEU){
        l+=1
      }
      echange(k,l)
    } else {
      if(stock==ROUGE){
        while(getCouleur(h)==ROUGE){
          h-=1
        }
        echange(k,h)
      }
    }

  }
}