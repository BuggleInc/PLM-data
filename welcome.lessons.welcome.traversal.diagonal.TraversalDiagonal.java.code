
public void run() {
	int counter = 0;
	int start = 0;
	for(int diag = 0; diag < getWorldHeight(); diag++){	
		counter = drawDiag(diag, counter);
	}

}


int drawDiag(int diag, int counter){
	int ccounter = counter;
	for(int x = 0; x <= diag; x++){
		setPos(x, diag - x);
		writeMessage(ccounter);
		ccounter++;
	}
	return ccounter;
}

int drawDown(int diag){
	if(diag > getWorldHeight()){
		start = diag - getWorldHeight();
	}
}

