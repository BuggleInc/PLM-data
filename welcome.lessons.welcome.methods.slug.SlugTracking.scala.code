def isFacingTrail():Boolean = {
avance()
var x=true
while(!estSurBiscuit) {
	if (getCouleurSol() != Color.green) {
	recule()
	gauche()
	avance()
	}
	else if (estFaceMur) {
		x=false
		recule()
		gauche()
	}
	else x=true
	recule()
	avance()
	baisseBrosse()
	leveBrosse()
	avance()
}
return x
}        
