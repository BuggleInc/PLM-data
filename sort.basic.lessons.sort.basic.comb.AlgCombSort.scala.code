def combSort() 
{
  var taille = getNombreValeurs()
  var ecart = getNombreValeurs()
  var permute = true
  do 
  { 
    permute = false
    if (ecart>1)
    {
     ecart = (ecart.toDouble / 1.3).toInt
    }
    var i = 0
    while (i+ecart<taille) 
    {
      
      if (!plusPetit(i,i+ecart)) 
      {
        echange(i,i+ecart)
        permute = true
      }
      i+=1
    }
    
    
  } while (ecart>1 || permute)
  
  
  
  
}