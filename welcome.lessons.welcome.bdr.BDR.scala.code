var finished: Boolean = false

while (!finished){
  var task = readMessage()
  task match {
    case "R" => right();forward()
    case "L" => left();forward()
    case "I" => back();forward()
    case "A" => forward()
    case "B" => forward(2)
    case "C" => forward(3)
    case "Z" => backward()
    case "Y" => backward(2)
    case "X" => backward(3)
    case "" =>  finished = true
    
  }
}
