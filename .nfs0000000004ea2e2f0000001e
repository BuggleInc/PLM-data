def haveBaggle():Boolean = {
  if (estSurBiscuit())
  {
    return true
  }
	else{
    return false
  }
  }



var vuBiscuit = false

while(!vuBiscuit)
	{
    for(compteur<-1 to 6)
    	{
        if (haveBaggle()) {
          vuBiscuit = true
        }
        avance()
        
    
       
      }
    if(!vuBiscuit){
      recule(6);droite();avance();gauche()
    }
    else {
         recule(6)
       }
    
  }