def shellSort()  {
  var lgr = getNombreValeurs()
  var diffcase = 1
  var ecart = lgr
  while (ecart>1){
    ecart=ecart/2 
  for (i <- 1 to lgr-1){
    diffcase = 1
    var v = getValeur(i)
  while(i-diffcase*ecart >= 0 && v<getValeur(i-(diffcase*ecart))){  
    setValeur(i-(diffcase*ecart)+ecart,getValeur(i-(diffcase*ecart)))
    diffcase=diffcase+1
  }
    setValeur(i-(diffcase*ecart)+ecart,v)
  }
}
  	
}