/**
 * doSide : effectue une partie du motif puis revient à la position d'origine.
 */

void doSide(Color c) {
	setCouleurBrosse(Color.c);
	gauche();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
	droite();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
}

public void run() {
	doSide (color.RED);
}
