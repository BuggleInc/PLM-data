def haveBaggle():
    seenBaggle = False
    steps = 0
    while seenBaggle == False :
        forward()
        steps = steps + 1
        if isOverBaggle() == True:
            seenBaggle = True
        if steps == 6:
            while steps <= 0:
                backward()
                steps = steps - 1
        right()
        forward()
        left()
    return seenBaggle
    
