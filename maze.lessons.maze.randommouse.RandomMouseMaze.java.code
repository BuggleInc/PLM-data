public void run() {
	mouseMaze();
}

public void mouseMaze(){
	
	do{
		int random = random3();

		if(random == 0){
			left();
		}else if(random == 1){
			right();
		}else if(random == 2 && !isFacingWall()){
				forward();
		}

		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());

}//end mouseMaze
