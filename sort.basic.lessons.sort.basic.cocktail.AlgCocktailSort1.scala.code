def cocktailSort() {
  var boole=false
  do{
    boole=false
  	for (i <- 0 to getValueCount()-2){
    	if(isSmaller(i+1,i)){
      	swap(i,i+1)
      	boole=true
      }
    }
    for (i <- getValueCount()-2 to 0){
    	if(isSmaller(i+1,i)){
      	swap(i,i+1)
      	boole=true
      }
    } 
   }
  while(boole)
}
