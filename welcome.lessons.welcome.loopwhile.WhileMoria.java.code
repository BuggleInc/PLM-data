// isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle(). 
public void run() { 
	// Turn around!
	right();
	right();

	// Do while not facing the wall on exit
	while (!isFacingWall()) {

		// Find first baggle
		while (!isOverBaggle()) {
			forward();
		}

		// Move baggle
		pickupBaggle();
		while (!isOverBaggle()) {
			backward();
		}
	
		// Move forward one cell (so in a free space)
		forward();
		dropBaggle();
	}
}