public void run() { 
	while(!isFacingWall()) {
		left();
		left();
		forward();
		boolean testWall = wall();
		while(!isOverBaggle()) {
			forward();
			testWall = wall();
		}
		if(testWall) {
			break;
		}
		pickUpBaggle();
		left();
		left();
		while(!isOverBaggle()) {
			forward();
		}
		backward();
		dropBaggle();
	}
}

public boolean wall() {
	if(isFacingWall()) {
		right();
		forward();
		left();
		forward();
		return true;
	}
	return false;
}
