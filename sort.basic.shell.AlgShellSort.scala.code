def shellSort()  {
  var n:Int=getNombreValeurs()
  var ecart:Int=n
  while (ecart>0){
    ecart=(ecart.toDouble/2.0).toInt
    insertion(ecart)
  }
  
}

def insertion(e:Int) {
  var n:Int=getNombreValeurs()
  for ( k <- e to (n-1)){
    var stock:Int=getValeur(k)
    var l:Int=k-e
    while ((l>=0)&&(stock<getValeur(l))){
      echange(l+e,l)
      l=l-e
    }
  }
}