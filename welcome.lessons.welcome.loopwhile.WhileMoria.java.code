public void run() { 
	boolean hasBaggle = false;
	while(!isFacingWall()) {
		if(hasBaggle) {
			forward();
			if(isOverBaggle()) {
				backward();
				dropBaggle();
				hasBaggle = false;
			}
		}
		else {
			backward();
			if(isOverBaggle()){
				pickUpBaggle();
				hasBaggle = true;
			}
		}
	}
}
