def findExit(H:Int, L:Int):Array[Int] ={
  /* Renvoie la sortie d'un monde de taille (x,y) donn√©e */
  var x = 0
  var y = 0
  while (!hasBaggle(x,y) && x<= L-1){
    x = x+1
    y=0
    while (!hasBaggle(x,y) && y<= H-1){
      y = y + 1 
    }
  }
  return Array(x,y)
  
}