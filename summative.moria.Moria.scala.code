var isOver : Boolean = false
def Moria() {
  while (isOver == false ){
    backward()
    if (isFacingWall==true && isOverBaggle==false){
      isOver = true
    }
    else {
      while(isOverBaggle()==false){
       backward() 
      }
      pickupBaggle()
      forward()
      while (isOverBaggle==false){
        forward()
      }
      backward()
      dropBaggle()
    }
  }
  left()
  forward()
  left()
  forward()
}