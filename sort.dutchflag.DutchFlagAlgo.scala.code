def solve()
{
  var n = getTaille()-1;
  var dernier = 0;
  var x = 0;
  var v = 0;
  var j = 0;
  var currCouleur = BLEU;
  
  
  for (i <- 1 to n)
  {
    v = getCouleur(i);
    x = v+1;
    j = i;
    
    while (j>0 && x>v)
    {
      x = getCouleur(j-1);
      if (x > v)
      {
        echange(j, j-1);
      }
      j-=1;
    }
  }
}