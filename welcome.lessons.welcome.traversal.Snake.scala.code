override def run() {
  while (!finale())
  {
    serpent()
  }
}

def finale():Boolean={
  if (estFaceMur){
    return true
  }
}

def serpent() {
  if (!estFaceMur())
  {
    avance()
  }
  else
  {
    if (getDirection()==Direction.EST)
    {
      gauche()
      avance()
      droite()
    }
    else
    {
      droite()
      avance()
      gauche()
    }
  }
}

