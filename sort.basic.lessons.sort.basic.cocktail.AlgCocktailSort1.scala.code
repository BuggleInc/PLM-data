def cocktailSort() {
  var chg = true;
  while(chg){
    chg=false;
    var i = 0;
    for(i <- 0 to getNombreValeurs()-2){
      if(plusPetit(i+1,i)){
        echange(i+1,i);
          chg=true;
      }
    }
    for(j <-getNombreValeurs()-2 to 0 by -1){
      if(plusPetit(j+1,j)){
        echange(j+1,j);
        chg=true;
      }
    }
  }
}
