def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  
  if (isBirthday==false){
    if (speed<=60){
      return 0
    }else if (speed>60 & speed<81){
      return 1
    }else if (speed>80){
      return 2
    }
  }else {
    if (speed<=65){
      return 0
    }else if (speed>65 & speed<86){
      return 1
    }else if (speed>85){
      return 2
    }
    
  }
}