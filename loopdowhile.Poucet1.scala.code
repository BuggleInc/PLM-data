var step = 0; 

while  (!exitReached()){
  for (stepper <-1 to 5){
    forward()
  }  
  if (step < 3){
    right()
    step = 0
    for (stepper <- 1 to 5){
      forward()
      if(isOverBaggle()){
        step =step + 1
      } 
    }
  }else {
    left()
    step = 0
    for (stepper <- 1 to 5){
      forward()
      if(isOverBaggle()){
        step =step + 1
      } 
    }
  }
}