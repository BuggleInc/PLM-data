public void run() {
	back();
	while (!isFacingWall && !isOverBaggle()) {	
	if (isOverBaggle()) {
			pickupBaggle();
			back();
			while (!isOverBaggle()) {
				forward();
			}
			back();
			forward();
			dropBaggle();
			forward();
		}else{
			forward();
		}
	}
	right();
	forward();
	left();
	forward();
}
