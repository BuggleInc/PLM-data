def cocktailSort3() {
	var fin:Int = getNombreValeurs - 2
	var debut:Int = 0
	var E:Boolean = false
	do {
		E = false
		for(i <- debut to fin) {
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				E = true
			}
		}
		fin -= 1
		if(E == false) {
			return
		}
		for(i <- fin to debut by -1) {
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				E = true
			}
		}
		debut += 1
		if(E == false) {
			return
		}
	}
	while(E)
}
