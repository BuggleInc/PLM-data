public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){

	do{	
		//go north
		while(!isFacingWall() && getDirection() == Direction.NORTH){
			forward();
		}
		
		if(isFacingWall()){
			right();
		}else{
			forward();
			left();			
		}

		//pick up baggle
		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
		
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall
