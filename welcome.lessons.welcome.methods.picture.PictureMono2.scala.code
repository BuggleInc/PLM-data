override def run() {
	def faireV() {
		avance()
		baisseBrosse()
		leveBrosse()
		avance()
		gauche()
		avance()
		baisseBrosse()
		leveBrosse()
		droite()
		droite()
		avance()
		gauche()
		avance()
		baisseBrosse()
		leveBrosse()
		avance()
	}
	def faireMotif() {
		for (i <- 1 to 4) {
			faireV()
			gauche()
		}
	}
	def faireMetaMotif() {
		for (i <- 1 to 3) {
			faireMotif()
