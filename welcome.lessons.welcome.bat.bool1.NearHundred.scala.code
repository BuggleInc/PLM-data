def nearHundred(n:Int): Boolean = { var c1:Int=0
                                   var c2:Int=0
                                   c1=Math.abs(100-n)
                                   c2=Math.abs(200-n)
                                   
                                   if (c1<=10||c2<=10){
                                     return true
                                   }
                                   return false}
                                  