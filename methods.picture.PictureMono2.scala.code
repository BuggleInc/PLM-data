override def run() {
  setPos(0, 6)
  for (i <- 0 to 2){
    for (j <- 1 to 3){
      faireMotif()
      setPos(i*7,getY() + 7)
    }
  }

  def faireMotif(){
    avance()
    avance()
    marquerSol()
    avance()
    gauche()
    avance()
    marquerSol()
    recule()
    droite()
    avance()
    marquerSol()
    avance()
    avance()
    gauche()
  }

  def marquerSol(){
    baisseBrosse()
    leveBrosse()
  }
}