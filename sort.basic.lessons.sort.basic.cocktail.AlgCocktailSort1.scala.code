def cocktailSort() {
  var swapped=false
  for (j<-0 to getValueCount()-1){
  	for (i <-0 to getValueCount()-2) {
    	if (!isSmaller(i,i+1)) {
      	swap(i,i+1);swapped=true
    	}
  	}
  	for (i<-getValueCount()-2 to 0 by -1) {
    	if (!isSmaller(i,i+1)) {
      	swap(i,i+1);swapped=true
    	}
   	}
  	if (!swapped) {return}
  }
}