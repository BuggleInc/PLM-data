public void combSort() {
var ecart = getNombreValeurs()
var i
var inverse = false

do {
	if (ecart>1)
		ecart = (ecart.toDouble / 1.3).toInt
	i = 0

     inverse = false
	while (i+ecart < getNombreValeurs()) {
		if (plusPetit(i+ecart,i)) {
			echange(i+ecart,i)
			inverse = true
		}
		i+=1
			
	}
	
	
	
} while (ecart > 1 || inverse) 

}
