while isOverMessage():
    x = readMessage()

    if x == "R":
        right()
        forward()   

    if x == "L":
        left()
        forward()

    if x == "I":
        right()
        right()
        forward()

    if x == "A":
        forward()

    if x == "B":
        forward(2)

    if x == "C":
        forward(3)

    if x == "Z":
        backward()

    if x == "Y":
        backward(2)
  
    if x == "X":
        backward(3)
