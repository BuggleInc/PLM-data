def caughtSpeeding(speed, isBirthday):
    if isBirthday==False:
        if speed<=60:
            return 0
        elif 60<speed<81:
            return 1
        elif speed>=81:
            return 2
    else:
        if speed<=65:
            return 0
        elif 65<speed<86:
            return 1
        elif speed>=86:
            return 2
    