def icyHot(temp1:Int, temp2: Int): Boolean = {
  if(temp1 <100 && temp1 >0){
    return false

  }else{
    if(temp2 <100 && temp2 >0){
    return false
    }else{
      if(temp1>100 && temp2<0){
        return true
      }else{
        if(temp2>100 && temp1<0){
          return true
        }else{
          return false
        }
      }
    }
  }
}