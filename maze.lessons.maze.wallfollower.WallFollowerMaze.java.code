public void run() {
	while(!isOverBaggle()) {
		if(keepHandOnSideWall()){
			if(!isFacingWall()){
				forward();
			}
		}
		else if(!isFacingWall()){
			forward();
		}
		else {
			right();
		}
	}
}

boolean keepHandOnSideWall(){
	left();
	if(isFacingWall()) {
		right();
		return true;
	}
	return false;
}
