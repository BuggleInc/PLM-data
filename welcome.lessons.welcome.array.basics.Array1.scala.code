override def run() {
  var H=getMondeHauteur()
  var L=getMondeLargeur()
  var T= new Array[int](H)
  for (i <- 0 to H-1){
    T(i)=getCouleurSol()
    avance()
  }
  gauche();avance();droite();avance()
  for (j <- 1 to L-1){
    for (k <- 0 to H-1){
      setCouleurBrosse(T(k))
      baisseBrosse()
      leveBrosse()
      avance()
  } 
}
