def combSort() {
  var ecart=getValueCount()
  var echange=true
  do {
    if (ecart>1){
      ecart=(ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    }
    var k=0
    while (k+ecart<getValueCount()){
      if (!isSmaller(k,k+1)){
        swap(k+1,k)
        echange=true
      }else{
        echange=false
      }
      k=k+1
    }
  }while (ecart >=1 || echange==false)
}