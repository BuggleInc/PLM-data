def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {

		(speed, isBirthday) match {
  case speed && isBirthday==true <65 => return 0
  case speed && isBirthday==true <=85 && speed>65  => return 1
  case speed && isBirthday==true >86 => return 2
  
  case speed && isBirthday==false <60 => return 0
  case speed && isBirthday==false <=80 && speed>60  => return 1
  case speed && isBirthday==false >81 => return 2
	}


	
}