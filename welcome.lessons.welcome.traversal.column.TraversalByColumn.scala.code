override def run() {
  var i=0
	while(posFinale()==false){
    ecritMessage(i+"")
    nextPos()
  }
}

def posFinale(): Boolean={
  if (getX()==getMondeLargeur & getY()==getMondeHauteur())
  	return true
  return false
}

def nextPos(){
  if (getY()==getMondeHauteur()){
    setPos(getX()+1,0)
  }
  else{
    setY(getY()+1)
  }
}