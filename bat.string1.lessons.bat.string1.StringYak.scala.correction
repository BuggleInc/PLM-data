$package
import plm.universe.bat.{BatEntity,BatWorld,BatTest}; 
import plm.universe.World; 
class stringYak extends BatEntity { 
   override def run(t: BatTest) {
   t.setResult( stringYak( t.getParameter(0).asInstanceOf[String]) )
   }
def stringYak(str:String):String = { 
  var res = ""
  var i=0
  while (i<str.length) {
    if (i+2<str.length  && str(i) == 'y' && str(i+2)=='k')
      i += 2
    else
      res += str(i)
    i+=1
  }
  return res
} }