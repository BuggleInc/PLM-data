def combSort() {
	var ecart = getNombreValeurs()
	var swapped = true

	do{
		swapped = false
		if(ecart>1){
			ecart = (ecart.toDouble/1.3).toInt
		}
		var i = 0
		while(i+ecart<getNombreValeurs()){
			if(plusPetit(i+ecart, i)){
				echange(i, i+ecart)
				swapped = true
			}
			i +=1
		}
	}while(ecart>=1 || swapped)
}
