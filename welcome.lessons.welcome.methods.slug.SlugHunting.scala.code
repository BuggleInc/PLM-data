def hunt() {
	while (!estSurBiscuit) {
	if (isFacingTrail()) {
		baisseBrosse();
		leveBrosse();
		avance();
	} else {
		gauche();
	}
}
prendBiscuit();
}

def isFacingTrail():Boolean = {
if (estFaceMur()) {
	return false
}
avance();
if (getCouleurSol()==Color.green) {
	recule();
	return true
} else {
	recule();
	return false
}
}
