def bubbleSort2() {
	var nb = getNombreValeurs()
	var x=1
	while (x!=0){
		x=0
		for(i<-0 to nb-2){
			if (getValeur(i+1)<getValeur(i)){
				echange(i,i+1)
				x=x+1
			}
		}
	}
}
