def golomb(num:Int): Int = {
	var S=new Array[Int](num+1)
	var rempli=1
	var pos=2
	S(0)=0
	S(1)=1
	S(2)=2
	while (rempli<=(num)){
		for(i<-rempli+1 to rempli+S(pos)){
			if(rempli+S(pos)<=num){
				S(i)=pos
			}
			else{
				rempli=num+1
			}
		}
		rempli=rempli+S(pos)
		pos=pos+1
	}
	return S(num+1)
}
