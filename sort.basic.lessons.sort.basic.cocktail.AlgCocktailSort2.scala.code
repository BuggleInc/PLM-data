def cocktailSort2() {
  var n = 2
  for (i <- 1 to getNombreValeurs()){
    for (i <- 0 to getNombreValeurs()-n-1){
      if(plusPetit(i+1:Int,i:Int)){
        echange(i:Int,i+1:Int)
      }
    }
    for (i <- n to getNombreValeurs()){
      if(plusPetit(getNombreValeurs()-i+1:Int,getNombreValeurs()-i:Int)){
        echange(getNombreValeurs()-i+1:Int,getNombreValeurs()-i:Int)
      }
    }
    n = n+1
  }
}