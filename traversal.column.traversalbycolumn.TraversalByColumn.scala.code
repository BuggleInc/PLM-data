def fini():Boolean={
  if ((getMondeHauteur() == getX()) && (getMondeLargeur() == getY())){
    return true
  }
  return false
}


override def run() {
  var cpt = 0
  for(i<- 0 to getMondeHauteur()-1){
    for(j<- 0 to getMondeLargeur()-1){
      setPos(i,j);
      ecritMessage();
      cpt += 1;
    }
  }
}