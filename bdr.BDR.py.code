finished = True
while (finished):
  if readMessage() == 'R':
    right()
    forward()
  elif readMessage() == 'L':
    left()
    forward()
  elif readMessage() == 'I':
    back()
    forward()
  elif readMessage() == 'A':
    forward()
  elif readMessage() == 'B':
    forward(2)
  elif readMessage() == 'C':
    forward(3)
  elif readMessage() == 'Z':
    backward()
  elif readMessage() == 'Y':
    backward(2)
  elif readMessage() == 'X':
    backward(3)
  else:
    finished = False
