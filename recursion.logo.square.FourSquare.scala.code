override def run() {
  
  setColor(Color.black)
  var n : Int = 100
  drawSquare(n)
  right(90)
  drawSquare(n)
  right(90)
  drawSquare(n)
  right(90)
  drawSquare(n)
  
  
}

def drawSquare(nbPas:Int):Unit = {
  
  penDown()
  
  for(i<-0 to 4){
    
    forward(nbPas)
    left(90)
    
    
  }
  
  penUp()
  
}
