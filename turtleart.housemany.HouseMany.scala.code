override def run() {
  houseTree()
  leveCrayon()
  gauche(90);avance(200);droite(90)
  avance(75)
  baisseCrayon()
  house()
  leveCrayon()
  droite(90);avance(150);gauche(90)
  baisseCrayon()
  house()
  leveCrayon()
  gauche(90);avance(150);droite(90)
