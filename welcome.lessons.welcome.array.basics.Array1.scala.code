override def run() {
  var h=getMondeHauteur()
  var l=getMondeLargeur()
  var T=new Array[Color](h)
  for (i<-0 to (h-1)){
    T(i)=getCouleurSol()
    avance()
  }
  replique(T,l)      
}

def replique(T:Array[Color],l:Int){
  for (j<-0 to l-1){
    avance()
    gauche()
    avance()
    droite()
    for (k<-0 to T.length){
      setCouleurBrosse(T(k))
      baisseBrosse()
      leveBrosse()
      avance()
    }
  }
}