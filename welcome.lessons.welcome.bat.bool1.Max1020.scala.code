def max1020(a:Int, b:Int):Int = { 
  var c:Int = Math.max(a,b)
  if (a>=10 && a<=20) {
    if (b<10 || b>20) {
      return a
    }
    else {
      return Math.max(a,b)
    }
  }
  if (b>=10 && b<=20) {
    if (a<10 || a>20) {
      return b
    }
    else {
      return Math.max(a,b)
    }
  }
  return 0
}