def shellSort()  {
  for (i<- 1 to n){
  	var ecart: Int=getNombreValeurs()/2
  	var l: Int=i
    var v: Int=getValeur(i)
    while (ecart>0){
      ecart/=2
    for (j<- i-ecart to 0 by -ecart){
          if (!plusPetitQue(j,v)){
        			copie(j,j+1)
        			l=j
      }
    }
    setValeur(l,v)
  }
  
}
}