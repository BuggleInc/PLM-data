def cocktailSort2() {

	var end = 0
	var debut = 0
	var fin = getNombreValeurs()-2

	do {
		for( k <-debut to fin) {
			end=0
			
			for (i<-debut to fin) {
				if (plusPetit(i+1, i)) {
					echange(i+1, i)
					end+=1
				}
			}
			fin -= 1

			for (i<-fin to debut by -1) {
				if (plusPetit(i+1, i)) {
					echange(i+1, i)
					end+=1
				}
			}
			debut += 1
		}
	} while (end!=0)

}
