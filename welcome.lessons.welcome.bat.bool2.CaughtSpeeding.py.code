def caughtSpeeding(speed, isBirthday):

    if isBirthday and ((speed)<=65):
        return 0
    elif isBirthday and ((speed)>=61+5 and (speed+5)<=80+5):
        return 1
    elif isBirthday and ((speed)>=81+5):
        return 2
    
    elif not isBirthday and (speed<=60):
        return 0
    elif not isBirthday and (speed>=61 and speed<=80):
        return 1
    elif not isBirthday and (speed>=81):
        return 2
