def in3050(a:Int, b:Int):Boolean = {
  (a,b) match {
    case (a,b) if estEntre(30,40,a)&&estEntre(30,40,b)==>true
    case (a,b) if estEntre(40,50,a)&&estEntre(40,50,b)==>true
    case (_,_) => false
  }
}

def estEntre(a:Int,b:Int,x:Int):Boolean ={ //a<b
  (x) match {
    case x if x<a => false
    case x if x>b => false
    case _ => true
  }
}