def cocktailSort3() {
  var lgr=getNombreValeurs()
  var debut=0
  var fin=lgr-2
  var parcours:Boolean = true
  do{
    parcours=false
    for (i<-debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        parcours=true
      }
    }
    if (!parcours){return}
    fin=fin-1
    parcours=false
    for (k<-fin to debut by -1){
      if (plusPetit(k+1,k)){
        echange(k+1,k)
        parcours=true
      }
    }
    debut=debut+1
    if (!parcours){return}
  }while (parcours)
}