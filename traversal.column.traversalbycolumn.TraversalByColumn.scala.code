override def run() {
  def test():Boolean{
    var x=getX()
    ver y=getY()
    if (x==(getMondeLargeur() && y==getMondeHauteur()){
      return true
    }
    else{return false}
  }
  
  def deplacement(){
    var h=getMondeHauteur()
    var cpt=0
    ecritMessage(cpt)
    while (!test()){
    if (getY()==h){
      setPos(getX()+1,0)
    }
    else{
      setPos(getX(),getY()+1)
    }
      ecritMessage(cpt)
      cpt+=1
    
    }}
}