override def run() {
  baisseBrosse()
  avance()
  while (arrivee()==true){
    if(estFaceMur()){
      if (getDirection() == Direction.EST){
        gauche()
        avance()
        gauche()
    	}else if (getDirection() == Direction.OUEST){
        droite()
        avance()
        droite()
      }
  	}else{
      avance()
    }
  } 
}

def arrivee(){
  if(estFaceMur()){
		if (getDirection() == Direction.EST){
      gauche()
      if(estFaceMur()){
        droite()
        return true
      }else{
        return false
      }
    }else if (getDirection() == Direction.OUEST){
      droite()
      if(estFaceMur()){
        gauche()
      	return true  
      }else{
        return false
      }
    }
  }else{
    return false
  }
}