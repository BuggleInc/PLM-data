override def run() {
  var c:Color=Color.black;
    for(i<-1 to 4){
      qcircle(c)
      i match{
        case 1 => c=Color.red;
        case 2=> c=Color.yellow;
        case 3=> c=Color.magenta;
        case _=> c=Color.black
      }
      
}
}
    def qcircle(c:Color){
    	setColor(c)
    	for(j<-1 to 90){
    		forward(100);
        penUp();
    		backward(100);
        penDown();
    		right(1);
      }
    }