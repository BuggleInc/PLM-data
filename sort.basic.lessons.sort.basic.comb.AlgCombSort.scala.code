def combSort() {
	var ecart = getNombreValeurs()
	var swapped = false
	do{
		swapped = false
	
		if(ecart>1){
			ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
		}
		var i = 0
		while(ecart+i<getNombreValeurs()){
			if(plusPetit(i+ecart,i)){
				echange(i,i+ecart)
				swapped = true
		}
	}
	}while(ecart>1 | swapped = true)
}