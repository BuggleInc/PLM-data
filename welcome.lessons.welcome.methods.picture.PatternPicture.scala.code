override def run() {
  faireLigne()
  gauche()
  avance(4)
  droite()
  faireLigne()
  avance(4)
}

def petitDessinRG(c: Color) {
  avance()
  setCouleurBrosse(c)
  baisseBrosse
  leveBrosse
  gauche()
  avance()
  gauche()
  avance()
  baisseBrosse
  leveBrosse
  gauche()
  avance()
  gauche()
  avance(2)
}

def petitDessinYB(c: Color) {
  
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
  avance()
  gauche()
  avance()
  baisseBrosse()
  leveBrosse()
  recule()
  droite()
  avance()
}

def faireMotif() {
  petitDessinRG(Color.RED)
  petitDessinYB(Color.BLUE)
  recule(4)
  gauche()
  avance(2)
  droite()
  petitDessinYB(Color.YELLOW)
  petitDessinRG(Color.GREEN)
  droite()
  avance(2)
  gauche()
}

def faireLigne() {
  faireMotif()
  faireMotif()
}