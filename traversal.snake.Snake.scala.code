override def run() {
  brushDown();
  while (!estFaceMur) {
    forward();
  }
  if (getDirection() == Direction.EST) {
    left(); 
    if (!estFaceMur) {
      left(); forward();
    }
  }
  if (getDirection() == Direction.OUEST) {
    right();
    if (!estFaceMur) {
      right(); forward(); 
    }
  }
}