public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){
	do{
					forward();
			left();
		if(isFacingWall() ){
			right();
		}//else{
		//	forward();
		//	left();			
		//}

		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall
