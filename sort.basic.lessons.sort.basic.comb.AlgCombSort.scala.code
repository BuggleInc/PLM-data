def combSort() {
  var ecart = getNombreValeurs();
  var enOrdre = true ;
  while(ecart>1 || enOrdre){
    enOrdre =false ;
    if ( ecart > 1)
    ecart = (ecart.toDouble / 1.3).toInt
    var i = 0 ;
    while ( i+ecart<getNombreValeurs()){
      if ( plusPetit(i+1,i)){
        echange(i,i+1);
        enOrdre = true ;
      }
      i+=1;
    }

  }
}