override def run() {
 	var murG=false
  var Nord=(getDirection())==Direction.NORD
  
  while((!estFaceMur())&&(Nord)){
    avance()
  }  
  
  while(!murG){
    gauche()
    if (estFaceMur()){
      murG=true
    }
  }
  droite()
  while(!estSurBiscuit()){
    keepHandOnSideWall()
  }
  prendBiscuit()
}



def keepHandOnSideWall(){
  gauche()
  if (!estFaceMur()){
    avance()
  }
  else{
    droite()
    droite()
    keepHandOnSideWall()
  }
}