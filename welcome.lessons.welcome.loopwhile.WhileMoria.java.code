public void run() { 
	while(!isOverBaggle() && !isFacingWall()){
		backward();
			if (isOverBaggle()){
				pickupBaggle();
				while(isCarryingBaggle() && !isOverBaggle()){
					forward();
				}
				if(isOverBaggle()){
					backward();
					dropBaggle();
					backward();


				}
			}

	}
						if (isFacingWall()){
						right();
						forward();
						left();
						forward();
					}
}
