def solve() {
  var apresBleu=0;
  var avantBlanc = getTaille()-1;
  var blancRouge = getTaille()-1;
  while(apresBleu <= avantBlanc){
    if(getCouleur(apresBleu)==BLEU){
      apresBleu += 1;
    }else{
      echange(apresBleu,avantBlanc);
      avantBlanc -= 1;
    }
  }
  avantBlanc += 1;
  while(avantBlanc <= blancRouge){
    if(getCouleur(avantBlanc)==BLANC){
      avantBlanc += 1;
    }else{
      echange(avantBlanc,blancRouge);
      blancRouge -= 1;
    }
  }
}