override def run() {
  var cpt = 0
  do
  {
    ecritMessage(cpt)
    move()
    cpt = cpt+1
  }while(!finalPos())
  ecritMessage(cpt)
}

def finalPos():Boolean = 
{
  return (getMondeHauteur()==getY()+1)&&(getMondeLargeur()==getX()+1)
}
def move()
{
  if(getMondeLargeur()==getX()+1)
    setPos(0,getY()+1)
	else
  	setX(getX()+1)
}