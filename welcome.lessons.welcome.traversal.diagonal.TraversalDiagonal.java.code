
public void run() {
	int counter = 0;
	int start = 0;
	for(int diag = 0; diag < getWorldHeight(); diag++){
		counter = drawDiag(start,diag, counter);
		start++;
	}
	/*
	for(int diag = 0; diag < getWorldHeight(); diag++){
		
		counter = drawDiag(start, diag, counter);
	}
	*/
	
}


int drawDiag(int start, int diag, int counter){
	int ccounter = counter;
	for(int x = start; x <= diag; x++){
		setPos(x, diag - x);
		writeMessage(ccounter);
		ccounter++;
	}
	return ccounter;
}



