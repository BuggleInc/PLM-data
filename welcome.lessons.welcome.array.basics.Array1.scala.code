override def run() {
	val X= getMondeHauteur()
	var T = new Array[Color](X);
	for( i<-0 to T.length-1){
		avance();
		var T(i):=getCouleurSol()
	}
	gauche()
	avance()
	droite()
	avance()
	for(i<-T){
		avance()	
		CouleurBrosse()=T(i)
		baisseBrosse()	
	}
	for(i<-1 to 4){
		gauche();
		avance();
		droite();
		avance();
		for(i<-T){
			avance()
			CouleurBrosse()=T(i)
			baisseBrosse()		
		}
	}		
}	
