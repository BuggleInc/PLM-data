# while I did not find the exit, take the next corridor to decide whether I should turn left or right at the next intersection

exitReached = False
while not exitReached or not isGroundOrange():
  exitReached = True
  x=0
  for i in range (5):
    forward()
    if isOverBaggle()=True :
      x=x+1
  if x>=3:
    left()
  elif x<=2:
    right()
  