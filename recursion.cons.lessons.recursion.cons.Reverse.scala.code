def reverse(l:List[Int]): List[Int] = {
	if (l == Nil)
	{
		return Nil
	}
	else return last(l)::reverse(butLast(l))
	
}

def butLast(l:List[Int]): List[Int] = {
	if (l.tail.length == 0 )
	{
		return Nil
	}

	else return l.head :: butLast(l.tail)
}

def last(l:List[Int]): Int = {
	if (l.tail.length == 0)
		return l.head
	else last(l.tail)
}
