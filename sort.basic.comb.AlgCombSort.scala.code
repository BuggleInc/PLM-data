def combSort() {
  var a = true
  var ecart: Double = getNombreValeurs()
  do{
    a= false
    if(ecart>1){
      ecart=ecart /1.3
    }
    var i=0
    while(i+ecart< getNombreValeurs){
      if(i>i+ecart){
        echange(i,i+ecart)
        a=true
      }
      
      i+=1
    }
  }while(ecart>1 || a)
}