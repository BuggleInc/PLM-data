
def fin () : Boolean = {
  return getX() == 6 && getY() == 6
}


override def run() {
  var i = 0
  var d = 1
  while (!fin()){
    ecritMessage(i)
    i += 1
    if ((d==2 && getX()==0) || (d==1 && getX()== 6)){
      if (d==2) {setPos(0,getY()+1);d=1}
      else {setPos(6,getY()+1);d=2}
    }
    
    else {d match {
      case 1 => setPos(getX()+1,getY())
      case 2 => setPos(getX()-1,getY())
      }}
  };ecritMessage(i)
}

