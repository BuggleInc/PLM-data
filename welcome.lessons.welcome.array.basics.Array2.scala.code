override def run() {
	var T = new Array[Color](getMondeHauteur)
	T(0)=getCouleurSol()
	for(c <- 0 to T.length - 2) {
		avance()
		T(c) = getCouleurSol
	}
	recule(getMondeHauteur()-1)
	for(c2 <- 0 to T.length - 2) {
		gauche()
		avance()
		droite()
		for(c3 <- 0 to T.length - 1) {
			setCouleurBrosse (T(c3))
			baisseBrosse()
			leveBrosse()
			avance()
		}
		recule(getMondeHauteur()-1)
	}
	recule()
}
