boolean haveBaggle() {
	boolean seenBaggle = false;
	right();
	while (!isFacingWall()){
		left();
		int progressY = 0;
		while (progressY < 6){
			if (isOverBaggle()){
				seenBaggle = true;
			}
			forward();
			progressY++;
		}
		while (progressY != 0) {
			backward();
			progressY--;
		}
		right();
		forward();
	}
	return seenBaggle;
}
