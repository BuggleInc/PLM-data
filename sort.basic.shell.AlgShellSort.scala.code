def shellSort()  {
  var ecart=((getNombreValeurs().toDouble)/2).toInt
  while(ecart>0){
   for(i<- 1 to getNombreValeurs()-1){
    var v = getValeur(i)
    var j=i-1
    while((j-ecart)>0 && getValeur(j-ecart)>v ){
 			copie(j-ecart,j)
      j=j-ecart
      
    }
    setValeur(j,v)
    
   }
   ecart=(ecart.toDouble /2).toInt
  }
}
