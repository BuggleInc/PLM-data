override def run() {
  
  setColor(Color.black)
  var n : Int = 100
  drawSquare(n)
  println(getX())
  moveTo(-100,0)
  drawSquare(n)
  moveTo(-100,-100)
  drawSquare(n)
  moveTo(0,-100)
  drawSquare(n)
  
  
}

def drawSquare(nbPas:Int):Unit = {
  
  penDown()
  
  for(i<-0 to 4){
    
    forward(nbPas)
    left(90)
    
    
  }
  
  penUp()
  
}
