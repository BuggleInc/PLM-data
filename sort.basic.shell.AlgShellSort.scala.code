def shellSort()  {
  var i=0
  var v=0
  var c=0
  var ecart=getNombreValeurs()/2
  var test=false
  for(i <- 1 to getNombreValeurs()-1){
    v=getValeur(i)
    test=plusPetitQue(i-ecart,v)
    c=0
    while(test==false && i-1-c>=0){
      copie(i-c*ecart,i-(c+1)*ecart)
      c+=1
      if(i-c*ecart>=0){
      	test=plusPetitQue(i-c*ecart,v)
      }
    }
    setValeur(i-c*ecart,v)
  }
}