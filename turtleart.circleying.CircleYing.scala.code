override def run()
{
  val rayon = 50;
  val demiRayon = rayon/2;
  
  demiCercle(demiRayon);
  
  leveCrayon();
  demiCercle(demiRayon);
  droite(90);
	avance(rayon);
  gauche(90);
  
  baisseCrayon();
  cercle(rayon);
  leveCrayon();
  
  demiCercle(demiRayon);
  baisseCrayon();
  demiCercle(demiRayon)
}

def demiCercle(rayon:Int)
{
  val cote = rayon*2*3.14/360;
 
  for (i <- 1 to 180)
  {
    avance(cote);
    droite(1);
	}
}