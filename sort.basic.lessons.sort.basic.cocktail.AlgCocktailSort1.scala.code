def cocktailSort() {
	var échangé = true
		while ( échangé == true ) {
			échangé = false
			for ( i <- 0 to getNombreValeurs()-2 ) {
				if (plusPetit(i,i+1)==false ) {
					echange(i,i+1)
					échangé = true
				}
			}
			for ( i <- getNombreValeurs()-2 to 0 by -1 ) {
				if (plusPetit(i,i+1)==false ) {
					echange(i,i+1)
					échangé = true
				}
			}
		}
}
