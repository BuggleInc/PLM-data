def combSort() {
	var ecart=getNombreValeurs()
	var inverse=true
	do
	{
		inverse=false
		if(ecart>1) ecart = (ecart.toDouble / 1.3).toInt
		i=0
		while(i+ecart<getNombreValeurs()){
			if(plusPetit(i+ecart,i){
				echange(i,i+ecart)
				inverse=true
			}
			
		}
	} while((ecart>1) || (inverse))
}
