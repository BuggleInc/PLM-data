override def run() {
 while (!arrive()) {
 	baisseBrosse()
 	if (!estFaceMur())
 	  {avance()}
 	 		
 }
}


def arrive() {
	if (estFaceMur())
		{ if (getDirection() == Direction.EST)
			  {gauche()
			  estFaceMur()
			   droite()}
			else { if (getDirection == Direction.OUEST)
					 {droite()
					 estFaceMur()
					 gauche}}
		}
	else false
}
