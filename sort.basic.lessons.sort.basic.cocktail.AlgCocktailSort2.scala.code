def cocktailSort2() {
	var debut = 0 
	var fin = getNombreValeurs -2
	var inverse = true
	do {
		for (i <- debut to fin) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1)
				inverse = true
			}
		}
		fin -= 1
		for (i <- fin to debut by -1) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1)
				inverse = true
			}
		}
		fin -= 1
		
	} 
}
