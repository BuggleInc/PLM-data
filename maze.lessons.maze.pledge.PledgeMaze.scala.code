var c=0

override def run() {
	while (!estSurBiscuit()) {
		while (!estFaceMur()) {
			avance();
		}
		Gauche();
		while(c!=0) {
			if (!estFaceMur) {
				while (!estFaceMur()) {
					avance();
				}
			} else {
				Droite();
				Droite();
				if (!estFaceMur) {
					while (!estFaceMur()) {
						avance();
					}
				}
			}
		}
	}
}

def Gauche() {
	gauche();
	c=c+1
}

def Droite() {
	droite();
	c=c-1
}
