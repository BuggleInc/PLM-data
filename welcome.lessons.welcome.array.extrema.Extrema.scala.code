def extrema(num:Int): Int = {
    if num.length>0 {
        var min=num(0);
        var max=num(0);
        for(i -> 1 to num.length-1){
            if(min>num(i))
                min=num(i);
            il(max<num(i))
                max=num(i);
        }
        return max-min;
    }else
        return 0;
  }
