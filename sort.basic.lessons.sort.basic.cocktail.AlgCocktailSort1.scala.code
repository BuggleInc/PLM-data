def cocktailSort() {

	var length = getNombreValeurs();
	var tri1 = true;
	var tri2 = true;
	do {
		
		for(i<-0 to length-2) {
			if(plusPetit(i, i+1) == false) {
				echange(i, i+1);
				tri1 = false;
			}
		}
		for(i<-length-2 to 0) {
			if(plusPetit(i, i+1) == false) {
				echange(i, i+1);
				tri2 = false;
			}
		}
	} while(tri1 == false || tri2 == false)
}
