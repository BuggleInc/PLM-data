def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
	if(a*b<0){
    if(negative==false){
      return true
    }
    if (negative==true){
      if(a<0 && b<0){
        return true
      }
      else {
        return false
      }
    }
    else{
      return false
    }
  }
  else{
    return false
  }
}