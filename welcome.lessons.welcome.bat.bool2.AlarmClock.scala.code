def alarmClock(day:Int, vacation:Boolean): String = {
  if (vacation){
    if ((day>=1)&&(day<=5)){
    	return "10:00"
    } else if (day<=1){
    		return "off"
    }
  } else if (!vacation){
    	if ((day>=1)&&(day<=5)){
    		return "7:00"
  		} else if (day<=1){
    			return "10:00"
  		}
		}
}
