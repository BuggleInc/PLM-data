def max2Value(nums:Array[Int]): Int = {
  var c = nums(0)
  var b = nums(1)
  var max1 = Math.max(b,c)
  var max2 = Math.min(c,b)

    for (i<- 2 to nums.length){
    if (nums(i)>=max1 && nums(i)>max2){
      max2 = max1
      max1 = nums(i)
    }else if (nums(i)>max2 && nums(i)<max1){
      max2 = nums(i)
    }
    
  }
  
  return max2
}