override def run() {


	def posFinale():Boolean={
		var H = getMondeHauteur();
		var L = getMondeLargeur();
	if( getX()==L && getY()==H){
		return true;
	}else{
		return false;
	}
}
var c = 0;
while(!posFinale()){
		ecritMessage(c+"");
		c=c+1;
		if(getY()==6){
			setPos(getX()+1,0)
		}else{
			setPos(getX(),getY()+1)
		}
}

}
