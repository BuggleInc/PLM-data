int max1020(int a, int b) {
	if(between(a,10,20)){
		if(between(b,10,20))
			return max(a,b);
		else return a;
	} else {
		if(between(b,10,20)){
			return b;
		} else {
			return 0;
		}
	}
			
}

int between(int a, int b, int c){
	return(b <= a && a <= c);
}
int max(int a, int b){
	if(a > b)
		return a;
	else return b;
}
