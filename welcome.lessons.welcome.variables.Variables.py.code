nbPas = int(0)
dejaFait = int(0)
while estSurBiscuit() == False:
    avance()
    nbPas = nbPas + 1
if estSurBiscuit() == True:
    prendBiscuit()
while nbPas != dejaFait:
    if nbPas == 0:
        poseBiscuit()
    else:
        while nbPas != dejaFait:
            recule()
            dejaFait = dejaFait + 1 
        poseBiscuit()
  

