var diag = 0;
override def run() {
  var cpt:Int=0
  while (!positionFinale()){
    ecritMessage(cpt)
    cpt=cpt+1
    if (murGauche()){
      setPos(getY()+1,0)
    }else{
    	setPos(getX()-1,getY()+1)
   	}
  }
}

def positionFinale():Boolean={
  return getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1
}
  
def murGauche():Boolean={
  gauche()
  var reponse:Boolean=estFaceMur()
  droite()
  return reponse
}
