public void run() {
	colByCol();
}

void colByCol(){
int width = getWorldWidth();
int height = getWorldHeight();
int count = 0;

	for(int x = 0; x < width-1; x++){
		for(int y = 0; y < height-1; y++){
			if(!finalPos()){
				setPos(x, y);
				count++;
				writeMessage(count - 1);
			}			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth() && getY() == getWorldHeight()){
		return true;
	}

	return false;
}//end finalPos
