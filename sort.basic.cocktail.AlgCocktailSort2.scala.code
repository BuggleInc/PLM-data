def cocktailSort2() {


	var flag:Boolean = true;
	var j:Int = 0;
	var debut:Int = 0;
	var fin:Int = getNombreValeurs - 2
	
  do{
		flag = false;
		for (i <- debut to fin){
			if(getValeur(i) > getValeur(i + 1)) {
				echange(i,i + 1);
				flag = true;
			}
		}
		fin -= 1;
		
		for (i <- fin to debut by -1){
			if(getValeur(i) > getValeur(i + 1) ) {
				echange(i, i + 1);
				flag = true;
			}
		}
		debut += 1;
		
	}while(flag)
}