def combSort() {
  
  var ecart:Double = getNombreValeurs()
  var swapped:Boolean = true
  while (ecart > 1 || swapped) {
    
    if (ecart > 1) {
      
      ecart = ecart / 1.3
    }
    var i:Int = 0
    while (i + ecart < getNombreValeurs()-2) {
      
      if (plusPetit(i, i+1) == false) {
        
        echange(i, i+1)
        swapped = false
      }
      i = i + 1  
    } 
  }
}