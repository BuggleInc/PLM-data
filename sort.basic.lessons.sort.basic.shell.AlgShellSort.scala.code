def shellSort()  {
  var lgr = getNombreValeurs()
  var ecart = (lgr/2).toInt
  while(ecart>0){
  	var v = 0
  	for (i<- ecart-1 to getNombreValeurs()-1){
    	v = getValeur(i)
    	var j = i-ecart
    	while(j>=0 && !plusPetitQue(j,v)){
      	copie(j,j+ecart)
      	j-= ecart
      
    	}	
    
    	setValeur(j+ecart,v)
    	
  }
    ecart = (ecart/2).toInt
}
}