$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class cigarParty extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( cigarParty((Integer)t.getParameter(0), (Boolean)t.getParameter(1)) ); }
	boolean cigarParty(int cigars, boolean isWeekend) {
		return (isWeekend && cigars >= 40) || (!isWeekend && (cigars >= 40) && (cigars <= 60));
	}
 }