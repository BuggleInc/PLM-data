public void run() {
	keepHandOnSideWall();
}


void keepHandOnSideWall(){
	faceEmptyAndLeftWall();
	forward();
	faceEmpty();
	forward();
}


void faceEmpty(){
	while(isFacingWall())
		left();
}

void faceEmptyAndLeftWall(){
	while(isFacingWall() && !wallOnLeft())
		left();
}

boolean wallOnLeft(){
	left();
	boolean a = isFacingWall();
	right();
	return a;
}
