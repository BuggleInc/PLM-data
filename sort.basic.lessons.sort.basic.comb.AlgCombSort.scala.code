def combSort() {
  var ecart=getNombreValeurs()
  var i:Int=0
  var parcours:Boolean=true
  do{
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i=0
    parcours=false
    while (i+ecart<getNombreValeurs()){
      if (plusPetit(i+ecart,i)){
        echange(i+ecart,i)
        parcours=true
      }
      i+=1
    }
  } while (ecart>1 || (parcours))
}