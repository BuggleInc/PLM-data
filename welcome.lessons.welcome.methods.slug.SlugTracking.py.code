def isFacingTrail():
    while estSurBiscuit()==False and porteBiscuit()==False:
        if estFaceMur()==False:
            baisseBrosse()
            leveBrosse()
            forward()
        else:
            left()
        if getCouleurSol()!=Color.green:
            backward()
            left()
    if estSurBiscuit()==True:
        prendBiscuit()