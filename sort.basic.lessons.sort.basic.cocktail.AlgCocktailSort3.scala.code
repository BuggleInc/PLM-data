def cocktailSort3() {
	var tri: Boolean = false;
	var debut: Int = 1;
	var fin: Int = getNombreValeurs() - 1;
	do{
		tri = false;
			for(i <- debut to fin){
				if(!plusPetit(i - 1, i)){
					echange(i - 1, i);
					tri = true;
				}
			}
			if(!tri){
				return;
			}
			for(i <- fin to debut + 1 by -1){
				if(plusPetit(i, i - 1)){
					echange(i, i - 1);
					tri = true;
				}
			}
		debut = debut + 1;
		fin = fin - 1;
	}while(tri)
}
