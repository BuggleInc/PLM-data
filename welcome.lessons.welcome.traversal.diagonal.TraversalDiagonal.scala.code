override def run() {
  var cpt = 0
  var num = 0
  for(i<-0 to 12){
    num = diago(num,cpt)
  }
  
}

def diago(numV:Int,cptV:Int):Int = {
  var num = numV
  var cpt = cptV
  for(i<- 1 to num%getMondeHauteur()){
    ecritMessage(cpt)
    cpt = cpt+1
    setPos(getX()+1,getY()-1)
  }

  return num
}