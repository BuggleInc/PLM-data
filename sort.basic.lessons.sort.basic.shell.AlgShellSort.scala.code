def shellSort()  {
	var ecart:Int=getNombreValeurs()/2;
	while (ecart>0) {
	var v=0;
	var count=0;
	for (i <- ecart to getNombreValeurs()-1) {
		v=getValeur(i); // valeur a trier
		count=i;
		while ((count>0) && (getValeur(i)>v)) { // fais de la place pour la valeur
			setValeur(count-ecart,getValeur(count));
			count=count-ecart
			}
		setValeur(count+ecart, v); // place la valeur a sa place dans la partie triée
	}
	ecart=ecart/2;
	}
}