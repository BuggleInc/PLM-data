public void run() {
	keepHandOnsidewall();
}

public void keepHandOnsidewall(){
int spins = 4;
boolean d = false;

	do{
		while(!d){
			left();
			if(isFacingWall() && getDirection() == Direction.WEST){
				right();
				d = true;
			}
		}
		
		if(!isFacingWall()){
			forward();
			d = true;
		}/*else{
			right();
		}*/
		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall
