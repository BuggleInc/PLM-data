public void run() { 
	for(int laps = 0; laps < 7; laps++){
		for(int sides = 0; sides < 4; sides++){
			for(int steps = 0; steps < 4; steps++)
				forward();
			left();
			for(int steps = 0; steps < 2; steps++)
				forward();	
			right();
			for(int steps = 0; steps < 4; steps++)
				forward();
			right();
			for(int steps = 0; steps < 2; steps++)
				forward();	
			left();
			for(int steps = 0; steps < 4; steps++)
				forward();
			left();
		}
	}
}
