override def run() 
{
  var count=1
  while(!posFinale)
  	{
      goNext()
      ecritMessage(count)
      count+=1
    }
}


def posFinale():Boolean=
{
  return((getMondeHauteur()==(getY()+1))&&(getMondeLargeur()==getX()+1))
}


def goNext()
{
  if (estFaceMur())
  	{
    	setPos(getX()+1,0)
  	}
  else
  	{
      setPos(getX(),getY()+1)
    }
}