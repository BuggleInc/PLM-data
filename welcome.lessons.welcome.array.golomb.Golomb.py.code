def golomb(num):
    a=[]
    for b in range(num):
        a.append(0)
    a[1]=1
    return len(a)
    for n in range(2,len(a)):
       nValeurs=0
       for i in a:
           if i == n-1:
               nValeurs +=1
       if nValeurs < n-1:
           a[n]=a[n-1]
       elif nValeurs == n-1:
           a[n] = a[n-1]+1
    return a[num-1]