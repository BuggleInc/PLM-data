override def run() {

	var EST=false
	
	def finish():Boolean = {
		if (estFaceMur()) {
		if (getDirection() == Direction.EST) {gauche();EST=true}
		else {droite();EST=false}
		if (estFaceMur()) {return true}
		}
		if (EST) {droite()}
		else {gauche()}
		return false
	}

	def serpent() {
		if (!estFaceMur()) {avance()}
		else {
			if (getDirection() == Direction.EST) {gauche();avance();gauche()}
			else {droite();avance();droite()}
		}
	}

	baisseBrosse()
	while (!finish()) {serpent()}
	
}
