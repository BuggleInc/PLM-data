var compt:Int = 0

override def run() {
  
  
  var posFin:Boolean = false
  
  while(posFin != true){
    avancer()
    
    if(getX() == getMondeLargeur() && getY() == getMondeHauteur()){
      ecritMessage(compt)
      posFin = true;
    }
  }

  }


def avancer() {
  for (x <- 0 to getMondeLargeur()-1){
    for (y <- 1 to getMondeHauteur()){
      ecritMessage(compt)
      compt = compt + 1;
      setPos(x, y)
    }
  }
}