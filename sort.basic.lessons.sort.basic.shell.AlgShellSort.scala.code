def shellSort()  {
 var ecart=getNombreValeurs()/2
 while(ecart>0) {
 for (i<-1 to getNombreValeurs()-1){
 var v=getValeur(i)
 var boucle=i
 while ((boucle-ecart)>0 && getValeur(boucle-ecart)>=v) {
    setValeur(boucle,getValeur(boucle-ecart))
    boucle=boucle-ecart
    }
    setValeur(boucle,v)
  }
  ecart= (ecart)/2 
 } 
}