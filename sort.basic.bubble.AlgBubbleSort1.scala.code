def bubbleSort() {
  var n:Int =getValueCount();
	var finished:Boolean = false
  while (!finished) {
    finished = true;
    for(i<-0 to n-2) {
      if (!isSmaller(i,i+1)) {
        swap(i,i+1);
        finished = false;
      }
    }
  }
}