def shellSort()  {
  do{
    var ecart=getNombreValeurs()/2
    for(i <-0 to getNombreValeurs()-2){
    	var tampon = getValeur(i+1)
    	var j = i
    	while(j-ecart>=0 && !plusPetitQue(j-ecart,tampon)){
      	copie(i-j,i-j+1)
      	j=j-ecart
    }
    
    setValeur(j,tampon)
  	
      
    }
  }while(ecart>1)


}