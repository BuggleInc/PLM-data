def bubbleSort3()  {
  var swapped=false;
  for (i<-getValueCount()-1 to 0 by -1;j<-0 to i-1 ) {
    	swapped=false
    
      for (j <-0 to i-1){
      if (!isSmaller(j,j+1))  		  
          {
            swap(j,j+1);
            swapped=true;
          }
      }
    
    if (!swapped) {return}
    
  }
}