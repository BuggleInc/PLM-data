def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
	return Ado(a) + Ado(b) + Ado(c) +Ado(d);

}





def Ado(z:Int): Int ={
	if (ado(z)){
		return 1;
	}else{
		return 0;
	}
}











def  ado(z:Int):Boolean ={
	if ( z==13){
		return true;
	}else{
		if ( z==19){
			return true;
		}else{
			if ( z>13 && z<19){
				return true;
			}else{
				return false;
			}
		}
	}
}
