def bubbleSort2()
{
  var sortie:Boolean = false
  var compteur:Int = 0
  
  while(!sortie)
  {    
    for(i <- 0 to getNombreValeurs()-2-compteur)
    {
      if(plusPetit(i+1,i))
      {
        echange(i,i+1)
      }
    }
    compteur = compteur + 1
    
    if(compteur == getNombreValeurs()-1)
    {
      sortie = true
    }
  }
}
