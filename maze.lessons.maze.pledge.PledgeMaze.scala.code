override def run() {
  var sommeAngle = 0
  var depart=true
  while(!estSurBiscuit()){
    if(isDirectionFree(Direction.NORTH)&&sommeAngle==0){
      avance()
    } else {
        sommeAngle = KeepHandOnSideWall(sommeAngle)
        depart=false
  }
  }
  droite()
  prendBiscuit()
}

    
    
    
