def cocktailSort() {
  var fin=false
    while(fin==false){
      fin = true
      for(i <- 0 to getNombreValeurs()-2){
        if(!plusPetit(i,i+1)){
          echange(i,i+1)
          fin=false
      }
      fin=true
      for(i <- getNombreValeurs()-1 to -1,-1){
        if(!plusPetit(i+1,i)){
          echange(i,i+1)
          fin=false 
        
      }
    }
}
