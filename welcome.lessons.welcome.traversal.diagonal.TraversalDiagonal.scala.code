
override def run() {
var diag=1;
var x=0
ecritMessage(x)

for (i<-0 to 5){

setPos(0,diag)
while (estFaceMur==false){
	x=x+1
	ecritMessage(x)
     
	setPos(getX()+1,getY()-1)
	

	
}
x=x+1
ecritMessage(x)
setPos(0,diag+1)
diag=diag+1
}

for (i<-0 to 5){

setPos(diag,0)
while (estFaceMur==false){
	x=x+1
	ecritMessage(x)
     
	setPos(getX()-1,getY()+1)
	

	
}
x=x+1
ecritMessage(x)
setPos(diag-1,0)
diag=diag-1
}

}