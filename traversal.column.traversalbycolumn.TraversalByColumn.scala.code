override def run() {
  var cp:Int = 0
  while (!fin) {
    for (i <- 0 to (getMondeLargeur() - 1)) {
      for (j <- 0 to (getMondeHauteur() - 1)) {
        setPos(i,j)
        ecritMessage(cp)
        cp += 1
      }
    }
  }
  }


def fin():Boolean = {
  if (getX() == ((getMondeLargeur() - 1)) && (getY() == (getMondeHauteur() -1))) {
    return true
  } else {
    return false
  }
}

