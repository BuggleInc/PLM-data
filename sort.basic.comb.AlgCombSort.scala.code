def combSort() {
  var i=0
  var c=1
  var ecart=getNombreValeurs()
  do {
    c=0
  	if (ecart>1) {
    	ecart= (ecart.toDouble/1.3).toInt
      echange(0:Int,3:Int)
  	} 
  	i=0
    while (i+ecart<getNombreValeurs()){
      echange(2:Int,5:Int)
      if (plusPetit(i+ecart:Int,i:Int)) {
        echange(i+ecart:Int,i:Int)
        i+=1
        c=1
      }
    }
  } while (ecart>1 ||c==1)
}