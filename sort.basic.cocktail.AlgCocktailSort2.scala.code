def cocktailSort2() {
  var Start : Integer = 0
  var Stop : Integer = getNombreValeurs
  var Swap : Boolean = true
  while (Swap == true) {
    Swap = false
    for(i <- Start to Stop) {
      if (!plusPetit(i,i+1)) {
        Swap = true
        echange(i,i+1)
      }
    	Stop-=1
    }
    for(i <- Stop to Start by -1) {
      if (!plusPetit(i,i+1)) {
        Swap = true
        echange(i,i+1)
  		}
    	Start+=1
		}
  }
}