def cocktailSort2() {
  
  var sorted = false
  
  var fin = getNombreValeurs() -2 
  var deb = 0
  
  
  while(!sorted){
    sorted=true
    for(i<-deb to fin){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        sorted=false
        
        fin -= 1
      }
    }
    for(i<-fin to deb by -1){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        sorted=false
        deb += 1 
      }
    }
    
    
  }
}