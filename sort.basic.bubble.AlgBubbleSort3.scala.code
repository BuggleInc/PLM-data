def bubbleSort3()  {

  for (c <- 0 to getNombreValeurs()-1) {
    
    for (x <- 0 to getNombreValeurs()-c-2) {
      
      var compteur = 0
      
      if (!plusPetit(x,x+1)) {
        
        echange(x,x+1)
        
      } else {compteur = compteur + 1}
      
      if (compteur == getNombreValeurs()) {return}
      
    }
    
  }
 
}