override def run() {faireMotif()}

def marquerSol(){baisseBrosse();leveBrosse()}

def faireDiago(c:Color){setCouleurBrosse(c);marquerSol();avance();gauche();avance();marquerSol();gauche();avance()}

def faireMotif(){avance();gauche();faireDiago(Color.RED);gauche();avance();avance();faireDiago(Color.BLUE);gauche();avance();gauche();recule();recule();gauche();avance();avance();droite();faireDiago(Color.YELLOW);gauche();avance();gauche();avance();avance();avance();gauche();faireDiago(Color.GREEN);gauche();recule();recule();recule();gauche();recule();recule();droite()}

def reposition(){avance();gauche();avance();avance();avance();droite()}


