override def run() {
var c:Int=0


	do {
		for (x<-0 to getMondeHauteur()-1)
		{
			setPos(x,0)
			ecritMessage(c)
			c=c+1
		}
		for (y<-1 to 3)
		{
			for (x<-getMondeHauteur()-1 to 0)
			{
				setPos(x,y)
				ecritMessage(c)
				c=c+1
			}
			for (x<-0 to getMondeHauteur()-1 )
			{
				setPos(x,y)
				ecritMessage(c)
				c=c+1
			}
		}
		}
	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-2
		)
	
}
