public void run() {
	mouseMaze();
}

public void mouseMaze(){
int random;

	do{
		random = random3();

		if(random == 0){
			left();
		}else if(random == 1){
			right();
		}else if(random == 2){
			if(!isFacingWall()){
				forward();
			}
		}

		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());

}//end mouseMaze