public void run() { 
	back();
	while(!isFacingWall() && !isOverBaggle() ){
		forward();
			if (isOverBaggle()){
				pickupBaggle();
				while(isCarryingBaggle() && !isOverBaggle()){
					backward();
				}
				if(isOverBaggle()){
					forward();
					dropBaggle();
					forward();
				}
			}

	}
/*					if (isFacingWall()){
						left();
						forward();
						right();
						forward();
					}*/
}
