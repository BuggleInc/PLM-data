def hunt() {
  while(!isOverBaggle()){
    isFacingTrail()
}

def isFacingTrail():Boolean ={
    if(isFacingWall()){
      return false
    }else{
      forward();
    if(getCouleurSol()==Color.green){
      brushDown();
      brushUp();
      backward();
      return true
    }else{
      backward();
      return false
    }
    }
}
}