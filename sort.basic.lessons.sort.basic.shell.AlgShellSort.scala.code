def shellSort()  {
  var ecart = getValueCount()/2
  while (ecart > 0) {
    var v= 0
  	for (i<-1 to getValueCount()-1) {
    	v=getValeur(i)
    	var j = i
    	while (j>0 && getValeur(j-ecart)>v) {
      		copy(j-ecart,j)
       	 j=j-ecart
    	}
    	setValue(j,v)
  	}
    ecart=ecart-1
  }
}

