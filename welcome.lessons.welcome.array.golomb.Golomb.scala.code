def golomb(num:Int): Int = {
	var c = 2
	var T = new Array[Int](num)
	var r = 1
	var e = 0

	T(0) = 1
	T(1) = 2

	while (c < num)
	{
		if (T(c) == r && c + r -1 < num)
		{
			for (e <- c to c + r -1 )
			{
				T(e) = c
				
			}
			c = c +r 
			T(r) = r + 1
			T(c) = r
		}
		if (T(c) == r && c + r -1 > num)
		{
			for (e <- c to num - 1)
			{
				T(e) = c
				
			}
			c = num
		}
		c = c +1 
		
	}

	return T(num - 1)
	

	
} 
