override def run() {
	var c=0
	while (!estSurBiscuit()) {
		while (!estFaceMur()) {
			avance();
		}
		keepHandOnSideWall()
	}

	def keepHandOnSideWall() {
	if(!estFaceMur()) {
		gauche();
		if(estFaceMur()) {
			droite();
			avance();
		} else {
			avance();
		}
	} else {
		gauche()
		if(estFaceMur()) {
			retourne();
			if (estFaceMur()) {
				droite();
				avance();
			} else {
			avance();
			}
		} else {
			avance();
		}
	}
}
}
