def cocktailSort3() {
  var n = getNombreValeurs()
  var d = false
  var debut = 0
  var fin = n-2
  do {
    d = false
    for (i <- debut to fin) {
      if (!plusPetit(i,i+1)) {
        echange(i,i+1);d = true
      } 
    }; fin -= 1; if (!d) {return}
    for (i <- fin to debut by -1) {
      if (!plusPetit (i,i+1)) {
        echange(i+1,i);d = true
      } 
  }; debut += 1; if (!d) {return}
  }
  while (d)}