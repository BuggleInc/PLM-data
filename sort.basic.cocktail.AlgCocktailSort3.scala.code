def cocktailSort3() {
  val n=getNombreValeurs()
  var debut = 0
  var fin = 0
  var chgt = 0
  for(i <- 0 to n/2) {
    for (i <- debut to (n-2-fin)) {
      if (plusPetit(i+1,i)) {echange(i,i+1)
                            }
      else chgt +=1
      if(chgt == n-1) return
    }
    fin +=1
    chgt = 0
    for(i <- (n-2-fin) to debut by (-1)){
      if (plusPetit(i+1,i)) {echange(i,i+1)
                            }
      else chgt += 1
      if(chgt == n-1) return
    }
    debut +=1
    chgt = 0
  }
}


