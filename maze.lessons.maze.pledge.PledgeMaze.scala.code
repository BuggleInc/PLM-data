override def run() {
  while(getY!=0){
    if (getDirection()!=Direction.SUD){
      goNorth();
    }
    while (!VerifNorth()){
      keepHandOnWall()
    }
  }
}



def keepHandOnWall(){
  var patte=0
  right();
  patte+=1
  while (estFaceMur()){
    patte-=1
    left();
  }
  avance();
  if (patte==0){
    left();
    if(estFaceMur()){
      right();
    } else {
      avance();
    }
  }
}

def goNorth(){
  setDirection(Direction.NORD);
  while (!estFaceMur()){
    avance();
  }
}

def VerifNorth():Boolean={
  var dir = getDirection();
  setDirection(Direction.NORD)
  if(estFaceMur()){
    setDirection(dir);
    return false
  }
  setDirection(dir);
  return true
}