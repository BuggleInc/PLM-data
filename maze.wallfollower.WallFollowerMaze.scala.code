def keepHandOnSideWall(){
  var continuer:Boolean = true
  if (!estFaceMur()){
    gauche()
    if (estFaceMur()){
      droite()
      avance()
    }else{avance()}
  }else{
    while(continuer){
      gauche()
      if (!estFaceMur()){
        droite()
        avance()
    	}
  	}
	}
}

override def run() {
  while (!estSurBiscuit()){
    keepHandOnSideWall
  }
  prendBiscuit()
}