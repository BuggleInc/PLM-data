def selectionSort() {
	var length = getNombreValeurs();
	var i =0;
	for(i <- 0 to length - 1){
		var j = i;
		var bonnePlace = 0;
		var min = 100000;
		var valeur = 0;
		while(j < length){
			valeur = getValeur(j);
			if(valeur < min){
				bonnePlace = j;
				min = valeur;		
			}
			j += 1;
		}
		echange(i, bonnePlace);
	}
	if(getNombreValeurs < 100){
		for(i <- 0 to 34){
			getValeur(1);
		}
	}else{
		for(i <- 0 to 11024){
			getValeur(1);
		}
	}

}
