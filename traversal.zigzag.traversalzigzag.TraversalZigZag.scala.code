override def run() {
  var x = getX()
  var y = getY()
  var cpt = 0
  for (i <- 1 to 48){
    ecritMessage(cpt)
    if(x==getMondeLargeur()-1){
      setPos(x,y+1)
      cpt = cpt +1
      x=x
      y=y+1
    }else{
      if(y%2==0){
      	setPos(x+1,y)
      	cpt = cpt +1
      	x=x+1
      	y=y
      }else{
        setPos(x-1,y)
      	cpt = cpt +1
      	x=x-1
      	y=y
      }
    }
  }
  ecritMessage(cpt)
}