override def run() {
    def positionFinale():Boolean={
    var c=0
    while ((getX()!=getWorldWidth()-1) || getY()!=getWorldHeight()-1){
      writeMessage(c)
      if (getY()!=6){
        setPos(getX()+1,getY())
        c=c+1
      }
      else {
        setPos(0,getY()+1)
        c=c+1
      }
    }
    writeMessage(c)
    return(true)
  }
  
positionFinale()
}