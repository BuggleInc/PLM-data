def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (isBirthday){
    if(speed-5 <=60){
      return 0
    }else if (speed-5 <=80){
      return 1
    }else {
      return 2
    }
  }else {
    if(speed <=60){
      return 0
    }else if (speed <=80){
      return 1
    }else {
      return 2
  }
  }
}