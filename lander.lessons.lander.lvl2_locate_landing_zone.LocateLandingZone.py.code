def getLandingZone():
    lastPoint = getGround()[0]
    for point in getGround():
        print ("point:"+str(point.x())+","+str(point.y())+"\n")
        if (point != lastPoint):
            if lastPoint.y() == point.y():
                print "found"
                return (lastPoint,point)
        lastPoint = point
    print("not found")

