  var lgr:Int=getNombreValeurs();
  var ecart:Int=lgr
  var i:Int=1
  var a=1
  while (ecart>1||a==1)
  	a=0
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i=0
    while (i+ecart < lgr) {
      if (!plusPetit(i,i+ecart)) {echange(i,i+ecart);a=1}
      i+=1
    } 

                
}