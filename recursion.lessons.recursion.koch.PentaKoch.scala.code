def pentaKoch(levels:Int, length:Double) {
if(levels==0){
	avance(length)
}else{
	pentaKoch(levels-1,length*0.4)
	left(72+90)
	pentaKoch(levels-1,length*0.4)
	right(90+72)
	pentaKoch(levels-1,length*0.4)
	right(90+72)
	pentaKoch(levels-1,length*0.4)
	right(90+72)
	pentaKoch(levels-1,length*0.4)
	left(72)
}
}
