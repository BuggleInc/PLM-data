def cocktailSort2() {
  var n = 2
  var t = 0
  var i = 2
  while(t == 0){
    t = 1
    for (j <- 0 to getNombreValeurs()-i-2){
      if(plusPetit(j+1:Int,j:Int)){
        echange(j:Int,j+1:Int)
        t = 0
      }
    }
    for (j <- i+1 to getNombreValeurs()){
      if(plusPetit(getNombreValeurs()-j+1:Int,getNombreValeurs()-j:Int)){
        echange(getNombreValeurs()-j+1:Int,getNombreValeurs()-j:Int)
        t = 0
      }
    }
    i = i+1
  }
}