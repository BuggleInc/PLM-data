def combSort() {
	var lgr: Int = getNombreValeurs();
	var ecart: Float = lgr/1.0;
	var chgt: Boolean = true;
	var i: Int = 0;
	while (chgt == true | ecart > 1){
		if (ecart > 1){
			ecart = ecart/1.3;
		}
		i=0;
		while (i+ecart < lgr){
			if (plusPetit(i+ecart,i)){
				echange(i+ecart,i);
				i=i+1;
			}	
		}
	}
	
	

}
