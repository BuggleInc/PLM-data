public void run() { 
	while(!isOverBaggle()){
		backward();
		while(isOverBaggle()){
			pickUpBaggle();
			while(isCarryingBaggle()){
				while(!isOverBaggle()){
					forward();
					if(isOverBaggle()){
						backward();
						dropBaggle();
					}else {
						forward();
					}
					
				}
				
			}
			
		}
	}
}
//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().
