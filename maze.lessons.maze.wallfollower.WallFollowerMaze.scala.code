def keepHandOnSideWall()
{
  var ok = false
  
  for(i <- 0 to 1)
  {
    if (!ok)
    {
      gauche()
      
      if (!estFaceMur())
      {
        avance()
        droite()
        ok = true
      }
    }
  }
}

override def run()
{
  while(!estSurBiscuit())
  {
