def faireMotif() {
  
  faireRed()
  
  avance();avance()
  
  faireBlue()
  
  gauche();avance();avance();droite()
  
  faireGreen()
  
  recule();recule()
  
  faireYellow
  
  gauche;recule();recule();droite()
  
}

def faireDoubleMotif(){
  
  faireMotif()
  
  for(i<- 1 to 4){
    avance()
  }
  
  faireMotif()
  
  for(i<- 1 to 4){
    recule()
    
}