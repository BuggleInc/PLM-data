def combSort() {
  var ecart = getNombreValeurs()
  var swapped= false
  do {
    swapped = false
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    
    for (k<- ecart to getNombreValeurs()-2) {
      if (!plusPetit(k,k+1)) {
        echange(k,k+1)
        swapped=true
      }
      
    }
  }while(ecart>1 || swapped  )
}
