var diag = 0;
override def run() {
  var y=0
  setPos(0,4)
  y=parcoursDiag(4,1)
}

  
/*
00
01 10
02 11 20
03 12 21 30
04 13 22 31 40
*/
  
def parcoursDiag(diag:Int,cpt:Int): Int={
  var y=diag
  var x=0
  while (y!=0){
    x+=1
    y-=1
    setPos(x,y)
    ecritMessage(cpt)
    
  }
  return cpt
}