void goAndGet(){
int stepFwd = 0;
int stepBwd = 0;

	while(!isOverBaggle()){
		forward();
		stepFwd++;
		
		if(isOverBaggle()){
			pickupBaggle();
			while(stepBwd < stepFwd){
				backward();
				stepBwd++;
			}
			if(stepFwd == stepBwd){
				dropBaggle();
				stepFwd = 0;
				stepBwd = 0;
			}
		}
	}
}
