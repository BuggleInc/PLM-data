def combSort() {
  var ecart = getNombreValeurs()
  if(ecart>1){
    ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
  }
  var i = 0
  var tri = true
  while(i+ecart<getNombreValeurs()-2 && tri){
    tri= false
    if(plusPetit(i,i +ecart)){
      echange(i,i+ecart)
      i+=1
      tri = true
    }
    if(!tri || ecart<=1){
      return
    }
  }
}