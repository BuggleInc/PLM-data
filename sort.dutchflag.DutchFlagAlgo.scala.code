def solve() {
  var i=0
  var debut=0
  var fin = getTaille()-1
	var avantBlanc= fin
  while ((debut<avantBlanc)&&(debut<fin)){
    var c= getCouleur(i)
    c match{
      case BLEU => (if (i> debut){
        				echange(i, debut)
        				debut +=1
      				})
      case BLANC => while (getCouleur(avantBlanc)==BLANC){
        					avantBlanc -=1
      					}
      					echange(i, avantBlanc)
      					avantBlanc -=1
      case ROUGE => while (getCouleur(fin)==ROUGE){
        					echange(fin-1, avantBlanc-1)
        					fin -=1
        					avantBlanc -=1
      					}
      					echange(i, fin-1)
      					fin-=1
    }
	}
}