override def run() {
  var x=0
  var y=0
  val x2=getMondeLargeur()
  val y2=getMondeHauteur()
  for (i<-0 to (x2-1)*(y2-1)){
    ecritMessage(i)
    x=x+1
    if (x==x2) {x=0;y=y+1}
    setPos(x,y)
  }
}

def arrive ():Boolean ={
  val x=getX()
  val y=getY()
  val x2=getMondeLargeur()
  val y2=getMondeHauteur()
  return ((x==x2)&&(y==y2))
}