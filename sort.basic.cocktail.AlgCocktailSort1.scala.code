def cocktailSort() 
{
  var echanger=false
  do
  {
   	echanger=false
    for (i<-0 to getNombreValeurs()-2)
    	{
        if (plusPetit(i+1,i))
        	{
            echange(i,i+1)
            echanger=true
          }
      }
    for (j<-getNombreValeurs()-2 to 0 by -1)
    	{
        if (plusPetit(j+1,j))
        	{
            echange(j,j+1)
            echanger=true
          }
      }
  }
  while(echanger)
}
