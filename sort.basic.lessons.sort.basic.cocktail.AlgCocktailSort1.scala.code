def cocktailSort() {
  var trie: Boolean=false
  while(trie==false){
    trie = true
    for (i<-0 to getNombreValeurs()-2){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)

      }
    }
    for (i<-0 to getNombreValeurs()-2){
      if(plusPetit(getNombreValeurs()-i-1,getNombreValeurs()-i-2)){
        echange(i,i+1)
        trie = false
      }
    }
  }

}

