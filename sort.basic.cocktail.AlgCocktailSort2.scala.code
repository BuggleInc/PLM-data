def cocktailSort2() {
  var n=getNombreValeurs()-2
	var t=0
  var ec=true
  do{
    ec=false
    for (i<- t to n){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        ec=true}}
    n-=1
    for (i<- n to t by -1){
      if (plusPetit(i+1,i)){
        echange(i+1,i);ec=true
      }
    }
    t=t+1
    
  }while (ec)
}