def cocktailSort3() {
  var swapped = false
  var debut = 0
  var fin = getNombreValeurs-2
  do {
  swapped = false
  for(i <- debut to fin) {
    if (!plusPetit(i,i+1)){
      echange(i,i+1)
      swapped = true
    }
  }
  fin -= 1
  if (swapped) {
  for(k <- fin to debut by -1) {
    if (!plusPetit(k,k+1)){
      echange(k,k+1)
      swapped = true
    } 
  }
  debut +=1
  }
	}	while(swapped)
}