def cocktailSort() {

	var length = getNombreValeurs();
	var tri = true;
	do {
		tri = false;
		for(i<-0 to length-2) {
			if(plusPetit(i, i+1) == false) {
				echange(i, i+1);
				tri = false;
			}
		}
		for(i<-length-2 to 0) {
			if(plusPetit(i, i+1) == false) {
				echange(i, i+1);
				tri = false;
			}
		}
	} while(tri == true)
}
