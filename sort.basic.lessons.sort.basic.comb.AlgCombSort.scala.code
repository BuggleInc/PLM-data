def combSort() {
  var ecart:Int = getNombreValeurs()
  var x:Boolean = false
  while(ecart > 1 && !x){
    x = true
    if(ecart > 1){
      ecart = (ecart.toDouble/1.3).toInt
    }
    var i:Int = 0
    while(i+ecart < getNombreValeurs()){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        i += 1
        x = false
      }
    }
  }
}