def combSort() {
  var ecart = getNombreValeurs()
  var aInverse = true
  var i = 0
  val n = getNombreValeurs()
  do {
    if (ecart > 1) {
      ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    }
    i = 0
    aInverse = false
    while (i+ecart < n) {
      if (getValeur(i+ecart) < getValeur(i)) {
        echange(i+ecart,i)
        aInverse = true
        i += 1
      }
    }
  } while ((ecart > 1) || aInverse)
}