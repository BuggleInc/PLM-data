def insertionSort() {for (i<- 1 to getNombreValeurs()-1)
                     {var v=getValeur(i)
                     for (k<- v to 1 by -1)
                     {if (plusPetit(v,getValeur(i-1)))
                      {copie(i-1, i)
                      var k=i}
                       copie(v,k) }
                     }
}