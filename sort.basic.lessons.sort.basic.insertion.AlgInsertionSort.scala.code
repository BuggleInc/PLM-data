def insertionSort() {

var i : Int = 1;
var value : Int;
var indiceInserer : Int;

while(i < (getNombreValeurs()-1))
{
value = getValeur(i);
indiceInserer = i-1;
while((indiceInserer>=0) & (value<=getValeur(indiceInserer)))
{setValeur(indiceInserer,indiceInserer+1);
indiceInserer = indiceInserer - 1;
}
setValeur(indiceInserer+1,value);
i=i+1;


 
}
	
	
	
	
}
 

