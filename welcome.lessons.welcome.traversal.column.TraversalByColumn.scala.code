override def run() {
  while(!posFinale()){
    var i:Int = 0
    ecritMessage(i)
    posSuivante()
  }
}

def posFinale():Boolean ={
  var z:Boolean = false
  if(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1){z = true}
  return(z)
}

def posSuivante(){
  if(getY() < getMondeHauteur()-1){setY(getY()+1;i += 1)}
  else if(getY() == getMondeHauteur()-1){setPos(getX()+1,0); i += 1}
  ecritMessage(i)
}