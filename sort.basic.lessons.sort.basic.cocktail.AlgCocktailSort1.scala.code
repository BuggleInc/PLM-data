def cocktailSort() {
  var n = getNombreValeurs();
  var c = 1;
  while ( c != 0 ){
    c = 0;
    for ( i <- 0 to n-2 ){
      if (plusPetit(i+1:Int,i:Int)){
        echange(i+1,i);
        c += 1;
      }
    }
    for ( i <- 1 to n-1 ){
      if (plusPetit(n-i:Int,n-i-1:Int)){
        echange(n-i-1,n-i);
        c += 1;
      }
    }
  }
}
