def setCouleurSol(couleur):
    setCouleurBrosse(couleur)
    baisseBrosse()
    leveBrosse()
def step(rule, colors):
    a=getCouleurSol()
    i=0
     while a!=colors[i]:
        i=i+1
    setCouleurSol(colors[i+1]):
    if rule[i]=='L':
        gauche()
    elif  rule[i]=='R':
        droite()
    avance() 
