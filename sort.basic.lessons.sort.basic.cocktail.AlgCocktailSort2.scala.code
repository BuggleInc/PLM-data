def cocktailSort2() {
var swapped = true
var debut = 0 
var fin = getNombreValeurs()-2

while (swapped) {
swapped = false
for (i <- debut to fin){
	if (plusPetit(i+1,i)) {
	echange(i,i+1)	
	
	swapped=true
	}
	fin -= 1
}


for (i <- fin to debut by -1){
	if (plusPetit(i+1,i)) {
	echange(i,i+1)
		
	swapped=true
	}
	debut += 1
}
}
}
