override def run() {
  var i:Int = 0
  ecritMessage(i)
  while(!posFinale()){
    i += 1
    posSuivante()
    ecritMessage(i)
  }
}

def posFinale():Boolean ={
  var z:Boolean = false
  if(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1){z = true}
  return(z)
}

def posSuivante(){
  if(getY() < getMondeHauteur()-1){setY(getY()+1)}
  else if(getY() == getMondeHauteur()-1){setPos(getX()+1,getMondeLargeur-1)}
}