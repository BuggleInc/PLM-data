public void cocktailSort() {
	boolean swapped;
	do {
		swapped = false;
		for(int i = 0; i < getValueCount() - 1; i++)
			if(isSmaller(i,i+1)){
				swap(i+1, i);
				swapped = true;
			}
		for(int i = getValueCount() - 1; i >= 0 ; i--)
			if(isSmaller(i+1, i)){
				swap(i, i+1);
				swapped = true;
			}
	} while(swapped);
}	
