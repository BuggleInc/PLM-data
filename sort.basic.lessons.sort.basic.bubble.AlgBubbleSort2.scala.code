def bubbleSort2() 
{
	var i : Int=0;
	var j : Int=0;
	var plusPetitTrouve : Int=0;
		
		while (i<getNombreValeurs())
		{
		 j=i+1;

		 while(j<getNombreValeurs())
		 {
		 	  if (plusPetit(j,i))
			  {
			  	plusPetitTrouve = j;
			  }
			  
			  j=j+1;
		 }	  
		 echange(i,plusPetitTrouve)
	
			 
			 i=i+1; 
		}
}
