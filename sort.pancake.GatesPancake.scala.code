def solve() {
  var nbCrepes = getTaillePile()
  var t=0
  var tmoins1 = 0
  var tplus1 = 0
  var bool = true
  while(!estTriee()){
    t = getRayonCrepe(0)
    tplus1 = trouverTPlus1(t)
    tmoins1 = trouverTMoins1(t)
    bool = casA(t,tplus1,tmoins1)
    if (!bool){
    bool = casB(t,tplus1,tmoins1)
   	}
	}
}
//On se met d'accord : t c'est le rayon de la première crepe
// tplus1 et tmoins1 sont les indices des crepes vérifiants les tailles respectives

//Fonctions de bases
def estLibre(rang:Int): Boolean={
  if (rang>0){
    if (getRayonCrepe(rang - 1) == getRayonCrepe(rang) - 1){return false}
  }
  if (rang < getTaillePile() - 1){
  	if (getRayonCrepe(rang + 1) == getRayonCrepe(rang) + 1){return false}
  }
  return true
}

def trouverTPlus1(t:Int):Int={
  var i = 0
  for (i <- 1 to getTaillePile()-1){
    if (getRayonCrepe(i) == t+1){return i}
  }
  return 9999
}

def trouverTMoins1(t:Int):Int={
	var i=0
  for (i <- 1 to getTaillePile()-1){
    if (getRayonCrepe(i) == t-1){return i}
  }
  return 9999
}

def estPremier(rang:Int):Boolean={
  if (!estLibre(rang) && (rang==0 || estLibre(rang-1))){
    return true
  }
  else { return false }
}

//LES CAS
//
def casA(t:Int,tplus1:Int,tmoins1:Int):Boolean={
  if (estLibre(0) && tplus1 != 9999 && estLibre(tplus1)){
    retourne(tplus1)
    return true
  }
  if (estLibre(0) && tmoins1 != 9999 && estLibre(tmoins1)){
    retourne(tmoins1)
    return true
  }
  return false
}
//
def casB(t:Int,tplus1:Int,tmoins1:Int):Boolean={
  if (estLibre(0) && tplus1 != 9999 && estPremier(tplus1)){
    retourne(tplus1+1)
    return true
  }
  if (estLibre(0) && tmoins1 != 9999 && estPremier(tmoins1)){
    retourne(tmoins1+1)
    return true
  }
  
  return false
	
}
