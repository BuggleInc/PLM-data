def cocktailSort2() {
  var debut:Int = 0
  var fin:Int = getNombreValeurs()-2
  var continue:Int = 1
  while (continue != 0){
    continue = 0
  	for (i <- debut to fin){
    	if (plusPetit(i+1,i)){echange(i,i+1)}
      continue = continue + 1
  	}
    fin = fin - 1
  	for (i <- fin to debut by -1){
    	if (plusPetit(i,i-1)){echange(i-1,i)}
    	continue = continue +1
  	}
    debut = debut + 1
  }
}