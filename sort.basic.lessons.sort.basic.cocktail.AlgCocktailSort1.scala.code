def cocktailSort() 
{
	var modifie=false
	do
	{
		modifie=false
		for(k<-0 to getNombreValeurs()-2)
		{
			if(getValeur(k)>getValeur(k+1))
			{
				echange(k,k+1)
				modifie=true
			}
		}
		for(k<-getNombreValeurs() -2 to 0 by -1)
		{
			if(getValeur(k)<getValeur(k-1))
			{
				echange(k,k-1)
				modifie=true
			}
		}
	}
	while(modifie)
}
