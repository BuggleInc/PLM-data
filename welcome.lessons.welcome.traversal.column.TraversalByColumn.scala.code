override def run() {
  var comp = 0;
  var col = 0;
  var lign = 0;
  while(!(positionFinale())) {
    ecritMessage(comp);
    comp = comp +1;
  	if (lign==6) {
      lign=0;
      col=col+1;
      setPos(col,lign);
    } else {
      lign = lign + 1;
      setPos(col,lign);
    }
  }
  ecritMessage(comp);
}

def positionFinale():Boolean = {
  var res = false;
  if (getX()==6  && getY()==6) {
    res = true;
  }
  return res
}