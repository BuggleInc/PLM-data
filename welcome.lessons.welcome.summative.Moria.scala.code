def trouverBiscuit(){
  setDirection(Direction.EAST)
  while (!estSurBiscuit()){
    avance()
  }
  prendBiscuit()
  setDirection(Direction.WEST)
  while (!estSurBiscuit()){
    avance()
  }
  poseBiscuit()
  setDirection(Direction.EAST)
  avance()
}

def sortie(): Boolean={
  if (estFacemur())
  	return true
  return false
}


while (sortie()==false) {
  trouverBiscuit()
}
setDirection(Direction.SOUTH)
avance()
setDirection(Direction.WEST)
avance()