override def run() {
    while(!estSurBiscuit) {
    	var x:Int = random3()
    		if (x==0 && !estFaceMur) {avance()}
    		else if (x==1) {
    			gauche();
    			while (estFaceMur) {gauche()};
    			avance();
    		} else if (x==2) {
    			droite();
    			while (estFaceMur) {droite()};
    			avance();
    		}
    }
    prendBiscuit();
}
