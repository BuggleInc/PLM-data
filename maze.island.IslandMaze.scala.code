override def run() {
  setDirection(Direction.NORTH)
  while (!estSurBiscuit()) {
    if (!estFaceMur() && getDirection() == Direction.NORTH) { 
      avance() 
    } else { keepHandOnSideWall() }
  }
  prendBiscuit()
}

def keepHandOnSideWall() {
  gauche()
  if (!estFaceMur()) {
    avance()
  } else  {
    while (estFaceMur()) {
      droite()
    }
    avance()
  }
}