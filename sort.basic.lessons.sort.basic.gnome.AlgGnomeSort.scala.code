def gnomeSort() 
{
  var taille = getNombreValeurs()
  var avance = 1
  while (avance < taille-1)
  {
    if (avance == -1)
    {
      avance = 0
    }
    if (plusPetit(avance,avance+1))
    {
      avance = avance + 1
    }
    else
    {
      echange(avance,avance+1)
      avance = avance-1
    }
  }
}