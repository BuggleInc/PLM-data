def alarmClock(day:Int, vacation:Boolean): String = {
	if (vacation==false){
    
    if ( day == 5 || day ==1 || day ==2 || day == 3 || day ==4 ){
      return "7:00"
    }   
    if ( day == 6 || day ==0 ) {
      return "10:00"
    } 
    
  }
  else {
    if ( day == 5 || day ==1 || day ==2 || day == 3 || day ==4 ){
      return "10:00"
    }   
    if ( day == 6 || day ==0 ) {
      return "off"
    } 
  }
}