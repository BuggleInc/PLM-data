def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if (!isBirthday && speed <= 60) {
		return 0
	} else { 
		if (!isBirthday && speed >= 61 && speed <= 80) {
			return 1
		} else {
			if (!isBirthday && speed > 80) {
				return 2	
			}
		} else {
			if (isBirthday && speed <= 65) {
				return 0
			} else { 
				if (isBirthday && speed >= 66 && speed <= 85) {
					return 1
				} else {
					if (isBirthday && speed > 85) {
						return 2	
					}
				}
			}
		}
	}	
}