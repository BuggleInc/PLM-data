override def run() {
  
  while (!isFacingWall()){
    forward()
  }
  
  right()
  for (i <- 1 to 3){
  	if (isFacingWall()){
   	 left()
  	}
  }
  
  while (!isOverBaggle()){
    keepHandOnSideWall()
		forward()
  }
  
  
  pickupBaggle()
}



def keepHandOnSideWall(){
	left()
  for (i <- 1 to 3){
  	if (isFacingWall()){
   	 right()
  	}
  }
  
}