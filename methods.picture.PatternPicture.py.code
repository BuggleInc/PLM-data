def pattern(color1,color2,color3,color4):
  setBrushColor(color1)
  for i in range(2):
    forward()
    brushDown();getBrushColor();brushUp()
    left()
    forward()
    left()
  setBrushColor(color2)
  forward();forward()
  brushDown();getBrushColor();brushUp()
  forward()
  left()
  forward()
  brushDown();getBrushColor();brushUp()
  for i in range (2):
    left();forward()
  

pattern(Color.red,Color.blue,Color.green,Color.yellow)