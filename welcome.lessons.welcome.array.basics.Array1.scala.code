override def run() {

	var couleur = new Array[Color](getMondeHauteur());
	
	for(i <- 0 to getMondeHauteur()-1) {
		avance();
		couleur(i) = getCouleurSol();
	}
	for(l <- 0 to getMondeLargeur()-2) {
		gauche();
		avance();
		droite();
		
		for(j <- l to getMondeHauteur()-1) {
			avance();
			setCouleurBrosse(couleur(j));
			baisseBrosse();
			leveBrosse();
		}
		
	}
}
