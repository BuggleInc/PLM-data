def max2Value(nums:Array[Int]): Int = {
  var max1=0
  var max2=0
  if (nums(1)>nums(0)){
    max1=nums(1)
    max2=nums(0)
  } else{
    max1=nums(0)
    max2=nums(1)
  }
  for (i<-2 to nums.length){
    if (nums(i)>max1){
      max2=max1
      max1=nums(i)
    } else if (nums(i)>max2){
      max2=nums(i)
    }
  }
  return max2
}