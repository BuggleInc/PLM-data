def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var pm = 0
  do {
    pm=0
    for (i <- debut to fin){
      if (plusPetit(i,i+1)==true) {
        pm=1
        echange(i,i+1)
        fin-=1
      }
    }
   for (j <-fin to debut){
     if (plusPetit(j,j+1)==true){
       pm=1
       echange(j,j+1)
       debut+=1
     }
   }
  } while (pm==1)
}