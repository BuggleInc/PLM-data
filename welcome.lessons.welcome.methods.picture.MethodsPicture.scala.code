override def run() {

def marquerSol(c : Color) {
	setCouleurBrosse() = c;
	baisseBrosse();
	leveBrosse();
}
def faireV(c : Color) {
	avance();
	marquerSol(c : Color);
	avance();
	gauche();
	avance();
	marquerSol(c : Color);
	recule();
	droite();
	avance();
	marquerSol(c : Color);
	avance();
	gauche();
}
def faireCarre() {
	faireV(Color.GREEN);
	faireV(Color.YELLOW);
	faireV(Color.RED);
	faireV(Color.BLUE);
}
}
