def combSort() {
  var lgr=getNombreValeurs()
  var ecart=lgr
  var i=0
  var m=1
  while (ecart>1 || m!=0){
    m=0
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i=0
    while (i+ecart<lgr){
      m=0
      if (plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        m+=1
      }
      i+=1
    }
  }
}