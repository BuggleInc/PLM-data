def isFacingTrail():Boolean = {
    // Write your code here
  var iz = false
  do {
    forward()
    if (getGroundColor() == Color.green){
      brushDown()
      brushUp()
    }
    else {
      backward()
      left()
    }
    if (isFacingWall()){
      left()
    }
    if (isOverBaggle() == true) {
      iz = true
    }
  } while (iz == false)
  return true
}