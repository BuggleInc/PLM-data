public void run() { 
	boolean testWall = false;
	while(!isFacingWall() || !testWall) {
		left();
		left();
		forward();
		testWall = wall();
		while(!isOverBaggle()) {
			forward();
			testWall = wall();
		}
		if(!testWall) {
			pickUpBaggle();
			left();
			left();
			while(!isOverBaggle()) {
				forward();
			}
			backward();
			dropBaggle();
		}
	}
}

public boolean wall() {
	if(isFacingWall()) {
		right();
		forward();
		left();
		forward();
		
		return true;
	}
	return false;
}
