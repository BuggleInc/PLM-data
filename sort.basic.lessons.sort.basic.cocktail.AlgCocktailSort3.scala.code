def cocktailSort3() {
	 var debut:Int = 0;
 	var fin:Int = getNombreValeurs()-2;
 	var test:Boolean=true;
 	do{
 		test=false;
 		for(i<-debut to fin by 1){
 			if(plusPetit(i+1,i)){
 				echange(i,i+1);
 				test=true;
 		}
 	}
 	if(test==false)return;
 	fin-=1;
 	for(i<-fin to debut by -1){
 		if(plusPetit(i+1,i)){
 			echange(i,i+1);
 			test=true;
 		}
 	}
 	//if(test==false)return;
 	debut+=1;
 }while(test)
}
