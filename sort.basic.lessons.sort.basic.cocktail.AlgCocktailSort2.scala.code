def cocktailSort2() {
  var swapped= false
  var j=0
  do {
    swapped=false
    for (i <- 0 to getValueCount()-2-j)
      if (! isSmaller(i,i+1)){
      swap(i,i+1)
        swapped=true;
        j=j+1
    }
    for (i<- getValueCount()-2 to j by -1)
      if (! isSmaller(i,i+1)){
        swap(i,i+1)
        swapped=true;
        j=j+1
      }
    
   
  }  while (swapped);
}
}