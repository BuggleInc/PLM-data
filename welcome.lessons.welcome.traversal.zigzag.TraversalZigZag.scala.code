override def run() {
	var hauteur=getMondeHauteur();
	var largeur=getMondeLargeur();
	var num =0 ;
	var x=0 ;
	var y=0 ;
	while ( !(getX()==largeur-1) && !(getY()==hauteur))
{

	if (x ==0 )
	{
		for ( i <- 0 to 6 ) 
		{
	ecritMessage(num)
	setPos(x+i,y)
	x=x+1
	num=num+1
		}
	}
	
	if (y!=6 && x==6)
	{
	ecritMessage(num)
	setPos(x,y+1)
	x=0
	y=y+1
	num=num+1
	}

}
ecritMessage(num);
}
