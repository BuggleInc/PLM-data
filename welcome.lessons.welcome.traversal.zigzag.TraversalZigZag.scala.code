override def run() {
  var hauteur=getMondeHauteur()
  var largeur=getMondeLargeur()
  var compteur=0
  for (i<-0 to hauteur-1){
      if (i==hauteur-1){
        for (j<-0 to largeur-1){
          setPos(j,largeur-1-i)
          ecritMessage(compteur)
          compteur+=1
        }
      }else{
        for (j<-0 to largeur-1){
          setPos(j,i)
          ecritMessage(compteur)
          compteur+=1
        }
      }
  }
}

