  while (!fonctionTest){
    for (i <- 0 to L-1){
      for (j <- 0 to H-1){
        setPos(L-1-i,L-1-i)
      }
    }
    
  }
}


def fonctionTest() : Boolean={
  var L=getMondeLargeur()
  var H=getMondeHauteur()
  var X=getX()
  var Y=getY()
  if ((X,Y)!=(H-1,L-1)){return false} 
  else {return true}}