def arrayFront9(nums:Array[Int]): Boolean = {
  var n = nums.length
  var o = false
  if (nums==null)
  {
    o=false
  }
  if (n<=4)
  {
    for (i <- 0 to n)
    {
      if (nums(i)==9)
      {
        o=true
      }
    }
  }else
  {
    for (i <- 0 to 3){
      if (nums(i)==9)
      {
        o=true
      }
    }
  }
  return o
}