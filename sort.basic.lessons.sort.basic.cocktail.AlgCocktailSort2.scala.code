def cocktailSort2() {
  var debut:Int = 0
  var fin:Int = getNombreValeurs()-2 
    for (i <- debut to fin) {
      
     if (plusPetit(i, i+1) == false) {
       
       echange(i, i+1)
       fin = fin - 1
     }  
    }
    for (j <- fin to debut by -1) {
      
      if (plusPetit(j, j+1) == false) {
        
        echange(j, j+1)
        debut = debut + 1
      }  
    } 
} 