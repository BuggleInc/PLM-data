override def run() {
  var x = getX()
  var y = getY()
  var num = 0
  do{
    ecritMessage(num)
    if(y < getMondeHauteur()-1){
      y = y+1
      setPos(x,y)
      num = num+1
    }else {
      if(x<getMondeLargeur()-1){
         x = x +1
      		y = 0
      		setPos(x,y)
      		num = num +1
      }
    }
  }while(!posFinale(x,y))
  
}

def posFinale(x:Int,y:Int): Boolean ={
    return ((getX() == getMondeLargeur()) && (getY()== getMondeHauteur()))
}