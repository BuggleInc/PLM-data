while(estSurMessage())
{
	if(getIndication() == 'A')
		avance();
	if(getIndication() == 'Z')
		recule();
	if(getIndication() == 'L')
	{
		gauche();
		avance();
	}
	if(getIndication() == 'B')
	{
		avance();
		avance();
	}
	if(getIndication() == 'R')
	{
		droite();
		avance();
	}
	if(getIndication() == 'X')
	{
		recule();
		recule();
	}
}
