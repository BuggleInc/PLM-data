def cocktailSort() {
	var verif=false;
	do {
		verif=false;
		for (i <- 0 to getNombreValeurs()-2 by 1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				verif=true;
			}
		}
		for (i <- getNombreValeurs()-2 to 0 by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				verif=true;
			}
		}
	} while(verif)
}
