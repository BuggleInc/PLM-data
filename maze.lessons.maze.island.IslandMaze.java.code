public void run() {
//	keepHandOnSideWall();
	goNorth();
	keepHandOnSideWall();
	goNorth();
}

public void keepHandOnSideWall(){
	//first, look for a LH wall
	//left();
	
	do{	
		if(isFacingWall()){
			right();
		}else{
			forward();
			left();			
		}

		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall

public void goNorth(){
	while(!isFacingWall()){
		forward();
	}
}
