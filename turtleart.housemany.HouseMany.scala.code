override def run() {
  drawHouse();forward(70)
  drawHouse();forward(70)
  drawHouse();
  for (i <- 0 to 2) {
    right(90);forward(50);left(50);
    drawHouse()
  }
}

def drawHouse() {
  penDown()
  for (i <- 0 to 3) {
    forward(30);right(90)
  }
  forward(30);right(30);forward(30);right(120);forward(30);right(30)
  forward(30);right(90);forward(30);right(90); penUp()
}