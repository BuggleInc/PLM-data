def shellSort()  {
  var valeur = 0;
  var test = 0;
  var ecart:Int = getNombreValeurs()/2
 	while (ecart>0) {
  	for (i <- ecart to getNombreValeurs-1){
    	valeur = getValeur(i);
    	test = i-ecart;
   		while (test>=0 && getValeur(test)>valeur){ 
      		setValeur(test+ecart,getValeur(test));
      		test=test-ecart;
    	}
    	setValeur(test+ecart,valeur)
  	}
    ecart = ecart/2
	}
}  
