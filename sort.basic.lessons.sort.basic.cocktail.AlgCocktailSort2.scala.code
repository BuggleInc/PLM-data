def cocktailSort2() {
	var lgr : Int = getNombreValeurs();
	var debut : Int = 1;
	var fin : Int = lgr -2;
	var eff : Int = 1;
	
	while(eff == 1){
		eff = 0;
		var i : Int = debut;
		
		while(i <= fin) {
			if(plusPetit(i+1, i)){
				echange(i+1, i);
				eff = 1;
			}
			i = i+1;
		}
		fin -= 1;
		
		i = fin;
		while(i >= debut){
			if(plusPetit(i+1, i)){
				echange(i+1, i);
				eff = 1;
			}
			i = i-1;
		}
		debut += 1;
	}
}

/*
debut=0; fin=lgr-2
Faire
  Pour tout i dans [debut,fin], faire:
    si i et i+1 doivent être échangées, le faire
  fin -= 1 (cela veut dire fin = fin -1)
  Pour tout i dans [fin, debut] en descendant, faire:
    si i et i+1 doivent être échangées, le faire
  debut += 1 (ce qui veut dire debut = debut + 1)
tant qu'au moins l'un des parcours a inversé un élément
 */