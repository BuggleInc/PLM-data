def combSort() {
	var gap = getValueCount()
  var term = false
  
  do{
    if(gap > 1){
      gap = (gap.toDouble / 1.3).toInt
    }
    var i = 0
    
    while(i+gap < getValueCount()){
      term = false
      if(isSmaller(i+gap,i)){
        swap(i,i+gap)
        i += 1
        term = true
      }
    }
  } while((gap > 1) || (term == true))
}