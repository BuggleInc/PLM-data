def cocktailSort3() 
{

var inverse: Boolean=true
var debut: Int=0
var fin: Int=getNombreValeurs()-2

do{
	inverse=false
	for (i<- debut to fin)
	{
		if (plusPetit(i+1,i))
		{
			echange(i,i+1)
			inverse=true
		}
	}
	if (inverse==false)
	{
		return
	}
	fin=fin-1
	
	for (j<- fin to debut by -1)
	{
		if (plusPetit(j+1,j))
		{
			echange(j,j+1)
			inverse=true
		}
		if (inverse==false)
		{
			return
		}
	}
	debut=debut+1
	
}while (inverse)

	
}
