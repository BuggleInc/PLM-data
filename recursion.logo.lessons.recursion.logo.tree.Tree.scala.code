def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps < 1){return}
  current(steps)
  avance(length)
  droite(angle)
  tree(steps-1,length*shrink,angle,shrink)
  current(steps)
  gauche(angle*2)
  tree(steps-1,length*shrink,angle,shrink)
  current(steps)
  droite(angle)
  recule(length)
  
}