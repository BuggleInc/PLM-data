var m= getMondeLargeur()
var T= new Array[Color](n)
	for (k <- 0 to n-1){
		T(k)=getCouleurSol()
		avance()
	}

	for (j <- 0 to m-2){
		avance()
			gauche()
			avance()
			droite()
		for (i <- 0 to n-1){
			setCouleurBrosse(T(i))
			baisseBrosse()
			leveBrosse()
			
		}
			
	}
}
