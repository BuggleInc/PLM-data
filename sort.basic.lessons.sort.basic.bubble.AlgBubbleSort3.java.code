public void bubbleSort3() {
    int length = getValueCount();
    boolean swap = false;
    for(int i = length-2; i>-1; i--) {
        swap = false;
        for(int j = 0; j<=i; j++) {
            if(!isSmaller(j,j+1)) {
                swap(j,j+1);
                swap = true;
            }
        }
        if(!swap) {
            i=0;
        }
    }
}
