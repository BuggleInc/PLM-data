def bubbleSort3() {
   var nbreparcours=0
   var changement=true  
  while (nbreparcours<getNombreValeurs()){
    changement=false
  	for (compteur<-0 to getNombreValeurs()-2-nbreparcours ){
    	if (plusPetit(compteur+1,compteur)==true){
        echange(compteur+1,compteur)
        changement=true
      }
    }
    nbreparcours=nbreparcours+1
  }
}