def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
	if (isMorning== true&&isMom== true&&isAsleep==false) {
		return true
	}
	else if (isAsleep==true&&isMom==true&&isMorning==false) {
		return false
	}
	else if (isMorning== true&&isMom== false&&isAsleep==false) {
		return false
	}
	else if (isMorning== false&&isMom== false&&isAsleep==true) {
		return false
	}
	else if (isMorning== true&&isMom== true&&isAsleep==true) {
		return false
	}
	else return true
}
