var diag = 0;
override def run() {
  var cpt:Int = 0
  for (i <- 0 to getMondeHauteur()) {
    setPos(0,i)
    ecritMessage(cpt)
    cpt+=1
    while(getX()!=i) {
      setPos(getX()+1,getY()-1)
      ecritMessage(cpt)
      cpt+=1
    }
  }
  for (i <- 1 to getMondeLargeur()) {
    setPos(i, getMondeHauteur()-1)
    ecritMessage(cpt)
    cpt+=1
    while(getY()!=i) {
      setPos(getX()+1,getY()-1)
      ecritMessage(cpt)
      cpt+=1
		}
  }
}