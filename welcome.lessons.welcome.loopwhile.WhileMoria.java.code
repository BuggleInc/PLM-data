public void run() {

	int c = 0;
	while ( getGroundColor() != Color.green ) {

		if ( c==0 ) {
		right(); right();
		c++;
		}

		while ( !isOverBaggle() )
			forward();

		if ( isOverBaggle() )	{
			pickupBaggle();
			right(); right();

			while ( !isOverBaggle() )
				forward();
		}

		if ( isOverBaggle() ) {
			right(); right();
			forward();
			dropBaggle();
			forward();
		}
		
		
	}
}
