def max2Value(nums:Array[Int]): Int = {
  var taille = nums.length
  
  var max1 : Int = 0
   var max2: Int = 0
  if (nums(0) >= nums(1))
  {max1 = nums(0)
   max2 = nums(1)}
  else 
  {
   max1 = nums(1)
   max2 = nums(0)
  }
  
  for ( i <- 0 to taille - 1)
  	{
      if (nums(i)>=max1)
      {
        max2 = max1;
        max1 = nums(i)
        
      }
    }
  return max2 }