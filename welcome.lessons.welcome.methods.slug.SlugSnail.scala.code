def hunt(col: Color) {
  avance()
  while(!porteBiscuit()){
    if (isFacingTrail(col)){
      baisseBrosse()
      avance()
      if (estSurBiscuit()){
        prendBiscuit()
      }
      leveBrosse()
    }else{
      gauche()
    }
  }
}

def isFacingTrail(col: Color):Boolean = {
  
  if (estFaceMur()){
    return false
  }else{
  	avance()
  	if (getCouleurSol()==col){
    	recule()
    	return true
  	}else { 
  	recule()
  	return false}
}
}
