def test():Boolean= {
  if ((getX()==getMondeHauteur()-1) & (getY()==getMondeLargeur()-1)) {
    return true
  }
  return false
}


def exo() {
  var w=0 
  while (!test()) {
    if (getY() == getMondeHauteur -1){
      setPos(getX()+1,0)
      w=w+1
    }
    ecritMessage(w)
    setPos(0,getY()+1)
    w=w+1
   
  }
}






override def run() { exo()

}