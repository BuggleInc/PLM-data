override def run() {
  var posfinale=false
  setCouleurBrosse(Color.blue)
  baisseBrosse()
  while(!posfinale)
    rep()
  	movesnake()
}





def movesnake(){
  while(!estFacemur())
  	avance()
  if(getDirection()==Direction.OUEST){
    droite()
    avance()
    droite()
  }
  
}






def rep(){
  getdirection()
  if(estFaceMur()==true){
    
  	posfinale=true
  }
  return posfinale
}