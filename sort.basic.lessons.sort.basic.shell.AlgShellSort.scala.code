def insertionSort() {
  var ecart = getNombreValeurs()/2;
  while(ecart>0){
    for(i <- 1 to getNombreValeurs()-1){
      var v = getValeur(i);
      var j = i;
      while(j!=0 && !plusPetitQue(j-ecart,v)){
        copie(j-1,j);
        j -= 1;
      }
      setValeur(j,v);
    }
    ecart /= 2;
  }
}