def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
  if (isAsleep==true){
    return false
  }
  if (isMorning==true && isAsleep==false && isMom==true){
    return true
  }
  if (isMorning==true && isAsleep==false && isMom==false){
    return false
  }
  else{
    return true
  }
}