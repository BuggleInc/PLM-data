override def run() {
  var nb = 0;
  var diag = 0;
  def serpent(diag:Int):Int={
    if(getY()==0 && diag<=7){ setPos(0,getX()+1); return diag+1}
    else if(getX()==0 && diag>7){ 
      setPos(diag-getMondeLargeur-1,getMondeHauteur-1); return diag+1}
    else{ setPos(getX()+1,getY-1);return diag } 
    }
    
    
    do{
      ecritMessage(nb)
      nb+=1
      diag = serpent(diag)      
    }while(getX()!=getMondeLargeur -1 || getY()!=getMondeHauteur-1 )
    
  }
