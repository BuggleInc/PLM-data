def selectionSort() {
  
  
  var test=false;
  var j=0;
  while(!test)
  {
    var min=getValeur(0)
    var ind=0;
    test=true;
    
    for(i<-j to getNombreValeurs()-1)
    {
      if(min>getValeur(i))
      {
        min=getValeur(i)
        ind=i;
        test=false
      }
    }
    if(!test)
    {
      echange(j,ind)
      j+=1
    }
  }
}