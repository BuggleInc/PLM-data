def cocktailSort2() {
var debut=0
var fin=getNombreValeurs()-2
var v=0
do {
	v=0
	for (i<- debut to fin) {
		if (plusPetit(i,i+1)==false) {
				echange(i, i+1)
				v=v+1
			}
	}
	fin-=1

	for (j<- fin to debut by -1) {
		if (plusPetit(j, j-1)==true) {
			echange (j,j-1)
			v=v+1
		}
	}
	debut+=1
}
 while (v!=0)
}
