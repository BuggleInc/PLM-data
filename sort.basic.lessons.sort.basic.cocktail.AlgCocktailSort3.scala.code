def cocktailSort3() {
  var s = false
  var ss = false
  var debut = 0
  var fin = getNombreValeurs()-2
  do{
    s=false
    ss=false
    for (i<- debut to fin){
      if (!plusPetit(i, i+1)){
        echange(i, i+1)
        s=true
      }
    }
    if (!s) return
    fin = fin-1
    for (i<- fin to debut by -1){
      if (!plusPetit(i, i+1)){
        echange(i, i+1)
        ss=true
      }
    }
    if (!ss) return
    debut = debut +1
  }while (s && fin-debut>1)
}
