def arrayFront9(nums:Array[Int]): Boolean = {
	if (nums.length >= 1 && nums(0) == 9) {
    return true
  }
  else if (nums.length >= 2 && nums(1) == 9) {
    return true
  }
  else if (nums.length >= 3 && nums(2) == 9) {
    return true
  }
  else if (nums.length >=4 && nums(3) == 9) {
    return true
  }
  else {
    return false
  }
}