override def run() {
  var cpt=1
  val h=getMondeHauteur()
  
  ecritMessage("0")
  while (!posFinale()){
    if ((getY())==(h-1)){
    setPos((getX()+1),0)
    ecritMessage(cpt.toString)
  }
  else{
    setPos(getX(),getY()+1)
    ecritMessage(cpt.toString)
  }
    cpt+=1
  }
    
  
  }


def posFinale():Boolean={
  val l=getMondeLargeur()
  val h=getMondeHauteur()
  
  return ((getX()==(l-1))&&(getY()==(h-1)))
}