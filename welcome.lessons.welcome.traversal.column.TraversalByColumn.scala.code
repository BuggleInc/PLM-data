override def run() {
  var xf:Int = getWorldWidth() - 1
  var yf:Int = getWorldHeight() - 1
  
  while (! (xf == getX() && yf == getY()) ){
    if (getX() == xf && getY() == yf){
      return 
      
  	}else if (getY() + 1 <= yf){
      setPos(getX(), getY() + 1)
      
    }else if (getY() + 1 > yf){
      setPos(getX() + 1, 0)
    }
}