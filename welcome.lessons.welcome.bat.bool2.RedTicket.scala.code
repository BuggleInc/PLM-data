def redTicket(a:Int, b:Int, c:Int):Int = 
{if (a==2 && b== 2 && c==2) {return 10} else {if (a==b && b==c) {return 5} else {if(b!=a && c!=a) {return 1} else {return 0}}}}

def redTicket2(a:Int,b:Int,c:Int):Int = {(a,b,c) match {
  case (2,2,2) =>return 10
  case (b,c,a) =>return 5
  case (b,_,_) =>return 1
  case (c,_,_) =>return 1
  case (_,_,_) =>return 0
}}
