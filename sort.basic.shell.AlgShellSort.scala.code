def shellSort()  {
  
  var ecart = getNombreValeurs() / 2;
  var k = 0;
  var i = 0;
  
  
  for (i <- 0 to getNombreValeurs()) {
    
      while (ecart > 0) {

      if (plusPetit(i - (k+1) * ecart, i - k * ecart) == false) {
        echange(i - (k+1) * ecart, i - k * ecart);
      }
      k += 1
    }
    
  }
  
  
  
}