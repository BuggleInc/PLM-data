def switch(indiceHaut:Int) {
  
  //switch la cr√®pe indiceHaut avec indicaHaut+1, sans toucher au reste
  
  retourne(indiceHaut+2)
  retourne(2)
  retourne(indiceHaut+2)
  
}

def plusPetit(indiceHaut:Int): Boolean = {
  
  //Renvoie si indiceHaut est plus petit que indiceHaut+1
  return getRayonCrepe(indiceHaut)<getRayonCrepe(indiceHaut+1)
  
}
def solve() {
  
  for (c <- 0 to getTaillePile()-1) {
    
    for (x <- getTaillePile() to c+1 by -1) {
      
      if (!plusPetit(x)) {
        
        switch(x)
        
      }
      
    }
    
  }
  
}