override def run() {
  var cpt:Int = 0
  while(!positionFinale) {
    ecritMessage(cpt)
    if(getX()<(getMondeHauteur()-1)) {
      setPos(getX()+1, getY())
    } else setPos(0,getY()+1)
    cpt+=1
  }
  ecritMessage(cpt)
}

def positionFinale():Boolean = {
  if (getX()==(getMondeHauteur()-1) && getY()==(getMondeLargeur()-1)) return true
  return false
}
