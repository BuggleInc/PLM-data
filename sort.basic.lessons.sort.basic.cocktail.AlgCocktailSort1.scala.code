def cocktailSort(){
  var a=true
  var b=true
  while (a==true|b==true){
  	for (i <- 0 to getNombreValeurs()/2){
    	if(plusPetit(i+1,i)){
       		echange(i,i+1)
        	a=true
    }else{
        a=false
      }
}
    for (i <- getNombreValeurs()/2 to 0 by -1){
    	if(plusPetit(i+1,i)){
       		echange(i,i+1)
        	b=true
        
    }else{
        b=false
      }
}

}
}