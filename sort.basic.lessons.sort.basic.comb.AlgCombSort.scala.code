def combSort() {
  var ecart : Int = getNombreValeurs()-1
  var inverse = true
  do{
    if (ecart>1){
      ecart=(ecart.toDouble/1.3).toInt
    }
    inverse = false
    for (i<-0 to getNombreValeurs()-1-ecart){
      if (!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        inverse=true
      }
    }
    
  }while (ecart>1 || !inverse )
}