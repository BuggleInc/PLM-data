def insertionSort(e:Int) {
  val n = getNombreValeurs()
  var v = 0
  var k = 0
  var m = 0
  for (i<- e-1 to n-1) {
    k = i
    v = getValeur(i)
    var b = false
    var j = i-e
    while (j>= 0 && !b){if (getValeur(j) > v) {setValeur(j+e,getValeur(j));k = j} else {b = true};j-=e
  }
   setValeur(k,v)
  }}
def shellSort()  {
  var ecart = getNombreValeurs()/2
  while (ecart > 0) {
    insertionSort(ecart)
    ecart/=2
    
  }
}
