def bubbleSort()  {

	var swapped:Boolean = true
	while (swapped) {
  
  	swapped = false
    var j:Int = 0
  	for (i <- 0 to getNombreValeurs()-(2+j)) {
    
    	if (plusPetit(i, i+1) == false) {
      
      	echange(i, i+1)
      	swapped = true
        j = j + 1
    	}
 	 	} 
	}
}