def faireV(c:Color) {
	setCouleurBrosse(c);
	avance();
	baisseBrosse();
	leveBrosse();
	avance();
	gauche();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
	droite();
	avance();
	baisseBrosse();
	leveBrosse();
	avance();
}

override def run() {
	faireV(Color.yellow);
	gauche();
	faireV(Color.red);
	gauche();
	faireV(Color.blue);
	gauche();
	faireV(Color.green);
	gauche()
}
