def cocktailSort3() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var compt = 0
  
  do{
    compt==0
    for (k<-debut to fin){
      if (!plusPetit(k,k+1)){
        echange(k,k+1)
        compt=compt+1
      }
    }
    if (compt==0){
        return
    }    
    fin-=1
    compt==0
    for (k<-fin to debut by -1){
      if (!plusPetit(k,k+1)){
        echange(k,k+1)
        compt=compt+1
      }
    }
    if (compt==0){
      return
    }
    debut+=1
  }while (compt!=0)
}