def combSort()
{
  var n = getNombreValeurs();
  var isEchange = false;
	var ecart = n+0.1;
  var i = 0;
  
  do
  {
    isEchange = false;
    if (ecart>1) ecart /= 1.3;
    
    i = 0;
    while (i+ecart<n)
    {
      if (plusPetit(i+1, i))
      {
        echange(i, i+1);
        isEchange = true;
      }
      i+=1;
    }
  }
  while (ecart > 1 || isEchange)
}