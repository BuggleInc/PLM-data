var diag = 0;
override def run() {
  var cpt: Int =0
  ecritMessage(cpt);
  while (getMondeHauteur()-1 != getY() || getMondeLargeur()-1 != getX()) {
    cpt += 1;
    if (diag<7) {
      setPos(0,diag+1); ecritMessage(cpt)
    } else {
      setPos(diag-6,0)
    }
    while (getX!=getMondeLargeur()-1 || getY() !=0) {
      setPos(getX()+1,getY()-1); ecritMessage(cpt);
    }
      diag += 1
  }
}
