def insertionSort() {
  var compt = 0 
  var v = 0
  var a = 0
  for (i<- 1 to getNombreValeurs()-1) {
    v = getValeur(i)
    a = getValeur(i-1)
    compt = 0
    while (a>v && i-compt > 0) {
      setValeur(i-compt,a)
      compt += 1
      if (i-compt>0) a = getValeur(i-compt-1)
    }
    setValeur(i-compt-1,v)
  }
}