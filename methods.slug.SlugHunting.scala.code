def hunt() {
  while (isFacingTrail()==true){
    brushDown()
    forward()
    brushUp()
    forward()
  }
   if (isOverBaggle()==true){
      pickupBaggle()
    }
}

def isFacingTrail():Boolean = {
  if (isFacingWall()==true){
    return false ;
  }
  else {
    forward()
    if (getGroundColor()==Color.green){
      backward()
      return true;
    }
    else {
      backward()
      return false ;
    }
  }
}