def cocktailSort2() {
  var fini = false;
  var n = getNombreValeurs();
  do {

    fini = true ;
    for (i<- getNombreValeurs()-n to n-2){
      if (!plusPetit(i,i+1)){
        echange(i,i+1);
        fini = false
      }
    }
    for (i<- getNombreValeurs()-n to n-2){
      if (!plusPetit(n - i,n - i + 1)){
        echange(n - i,n - i + 1);
        fini = false
      }
    }
    n -= 1;
   
  } while(!fini)

}