def cocktailSort() 
{

  var taille = getNombreValeurs()
  var permute:Boolean = true
  while (permute)
  {
    permute = false
    for (i<-0 to taille-1)
    {
      if (!plusPetit(i,i+1)) 
      {
        echange(i,i+1)
        permute = true
      }
    }
    for (i<-taille-1 to 0 by -1)
    {
      if (!plusPetit(i,i-1)) 
      {
        echange(i,i-1)
        permute = true
      }
    }
    if (!permute)
    {
      return
    }
  }
}