def posTest():Boolean = { 
  if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {
  	return true
  }
  return false
}



def colonne() {
	var j=0
  var cpt=0
  while (!posTest()) {
    for(i<-1 to 6) {
      setPos(i,j)
      cpt=cpt+1
      ecritMessage(cpt:Int)
    
    }
    
    j=j+1
  }
}







override def run() {
  colonne()
}