override def run() {
  var cpt=0
  var x=0
  var y=0
  ecritMessage(cpt)
  do{
    if (getY()==getMondeHauteur()){
      x=x+1
      y=0
      setPos(x,y)
      cpt+=1
      ecritMessage(cpt)
    } else {
      y+=1
      setPos(x,y)
      cpt+=1
      ecritMessage(cpt)
    }
  } while (fin()==false)
}

def fin(){
  if (getX()==getMondeLargeur() && getY()==getMondeHauteur()){
    return true
  } else return false
}

