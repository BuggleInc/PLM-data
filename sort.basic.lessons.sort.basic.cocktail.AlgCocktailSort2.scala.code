


def cocktailSort2() {
var debut : Int=0 
var fin : Int=getNombreValeurs() -2
var swap: Boolean=false


do { 
	swap=false
	for (i<- debut to fin) {
		if (plusPetit (i+1,i)) {
			echange (i,i+1)
			fin=fin-1
			swap=true
		}
	}

	for(i<-fin to debut by -1) {
		if (plusPetit (i+1,i)) {
			echange (i,i+1)
			debut=debut-1
			swap=true
		
	}
   } 
} while (swap)
}
