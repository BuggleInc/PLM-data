def combSort() {
  var n:Int = getNombreValeurs()
  var ecart:Int = n
  var nbEchanges:Int = 0
  do {
    nbEchanges = 0
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i:Int = 0
    while (i+ecart < n) {
      if (plusPetit(i+ecart, i)) {
        echange(i,i+ecart)
        nbEchanges = nbEchanges+1
      }
      i = i+1
    }
  } while (ecart>1 || nbEchanges!=0)
}