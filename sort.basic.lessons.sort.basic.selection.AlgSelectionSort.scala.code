def selectionSort() {
  var smallest = getValue(0)
  var mem = 0
  for (i<-0 until getValueCount()-1){
    var a = getValue(0)
    a = getValue(1)
    a = getValue(0)
    for (j<-i until getValueCount()){
      if (getValue(j)<smallest){
        smallest=getValue(j)
        mem = j
      }
    }
    swap(i,mem)
    smallest = getValue(i+1)
    mem = i+1
  }
}