def stringYak(str:String):String = 
{
	var chaine = ""

	for(i <- 0 to str.length-4)
	{
		if(i==0 && str.substring(i,3)=="yak")
			chaine += str(i)
		else if(i==str.length-1 && str.substring(i,3)=="yak")
			chaine += str(i)
		else if(str.substring(i,3)!="yak")
			chaine += str(i)
			
	}

	return chaine
}
