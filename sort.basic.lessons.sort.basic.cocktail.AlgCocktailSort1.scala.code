def cocktailSort() {
  var lgr : Int = getNombreValeurs() -2
  var estTriee:Boolean=false
  while (estTriee==false)
  estTriee=true
  for (i <-0 to lgr){
    if (plusPetit(i+1,i)){
      echange(i,i+1)
      estTriee=false
    }
  }
  for (j<- (lgr to 0 by -1)){
    if (plusPetit(j+1,j)){
      echange(j,j+1)
      estTriee=false
    }    
  }
  
}