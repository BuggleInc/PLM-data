override def run() {
	retourne();
	var hauteur=getMondeHauteur();
	var largeur=getMondeLargeur();
	var num =0 ;
	var x=0 ;
	var y=0 ;
	while ( !(getX()==largeur) && !(getY()==hauteur-1))
{
	if (y != 6 )
	{
	ecritMessage(num)
	setPos(x,y+1)
	y=y+1
	num=num+1
	}
	
	if (estFaceMur() && x!=6)
	{
	ecritMessage(num)
	setPos(x+1,0)
	y=0
	x=x+1
	}
}

}
