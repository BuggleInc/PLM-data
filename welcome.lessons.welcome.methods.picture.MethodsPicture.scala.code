override def run() {
  carreVColor()
}
  

def faireVolre(c:Color){
  setCouleurBrosse(c)
  avance()
  baisseBrosse()
  leveBrosse()
  avance()
  gauche()
  avance()
  baisseBrosse()
  leveBrosse()
  recule()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  avance()
  gauche()
}

def carreVColor(){
  faireVolre(Color.YELLOW)
  faireVolre(Color.RED)
  faireVolre(Color.BLUE)
  faireVolre(Color.GREEN)
}