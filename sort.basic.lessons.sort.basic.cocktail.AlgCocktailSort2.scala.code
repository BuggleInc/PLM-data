def cocktailSort2() {

	var debut=0; var fin=getNombreValeurs()-2
	var swapped = true
	while(swapped)
	{
		swapped=false
		for (i <- debut to fin)
		{
			if (plusPetit(i+1:Int, i:Int) )
			{
				echange(i:Int, i+1:Int)
				
				fin = fin -1
				swapped=true
			}
		}
		for (i <- fin to debut by -1 )
		{
			if (plusPetit(i+1:Int, i:Int) )
			{
				echange(i:Int, i+1:Int)
				
				debut=debut+1
				swapped=true
			}
	}
}
}
