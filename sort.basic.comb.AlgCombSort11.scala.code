def combSort() {

  var gap = getValueCount()
  var x = getValueCount()
  var i=0
  var echange = true

  do{

    echange = false;
    i=0;

    if(gap>1){
      gap = (gap.toDouble / 1.3).toInt;
      if ((gap==9)||(gap==10)){gap=11;}
    }

    while (i+gap < x){

      if (isSmaller(i+gap, i)){
        echange=true;
        swap(i, i+gap);
      }
      i+=1;
    } 

  }while((gap>1)||(echange==true))
}