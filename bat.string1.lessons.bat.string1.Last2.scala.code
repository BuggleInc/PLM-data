def last2(str:String):Int = {
  if (str.length<3){
    return 0
  }
  var T = str.charAt(str.length-2)+str.charAt(str.length-1)
  var c=0
  for (i<-0 to str.length-3){
    if ((str.charAt(i)==str.charAt(str.length-2)) && (str.charAt(i+1)==str.charAt(str.length-1))){
      c+=1
    }
  }
  return c
}