def cocktailSort() {
  var Tri=1
  var nbValeurs=getNombreValeurs()
  while(Tri!=0) {
    Tri=0
  for (i<-0 to nbValeurs-2) {
    if (!plusPetit(i,i+1)) {
      echange (i, i+1)
      Tri=Tri+1
    }
  }
  for (i<-0 to nbValeurs-2)
  if (plusPetit(nbValeurs-2-i,nbValeurs-2-i+1)) {
    echange (nbValeurs-2-i,nbValeurs-2-i+1)
    Tri=Tri+1
  }
}
}