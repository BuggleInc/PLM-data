def cocktailSort() {
var a=0
	do {
		for (i<- 0 to getNombreValeurs()-2)
		{
			if (!(plusPetit(i:Int, i+1:Int)))
			{
				echange(i+1:Int, i:Int)
				a=a+1
			}
			
		}
		for (i<- getNombreValeurs()-2 to 0  by -1)
		if (!(plusPetit(i:Int, i+1:Int)))
			 {
			 	echange(i+1:Int, i:Int)
			 	a=a+1
			 }
	}
	while (a>0)
	
}
