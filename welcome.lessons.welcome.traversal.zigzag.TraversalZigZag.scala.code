override def run() {
var cpt = -1
  for (h <- 0 to getMondeLargeur()-1) {
    if (h%2 == 0) {
    for (l <- 0 to getMondeHauteur()-1) {
      setPos(l,h)
      cpt = cpt +1
      ecritMessage(cpt)
    } 
	}
    else {
      for (l <- getMondeHauteur()-1 to 0 by -1) {
      setPos(l,h)
      cpt = cpt +1
     
    }
  }
  
}