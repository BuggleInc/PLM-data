def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
(a,b,negative) match {
	case (x,y,z) if {negative==false && a<0 && b>0} => return true
	case (x,y,z) if {negative==true && a<0 && b<0} => return true
	case (x,y,z) if {negative==false && a<0 && b<0} => return false
	case (x,y,z) if {negative==true && a<0 && b>0} => return false
	case (x,y,z) if {negative==false && a>0 && b<0} => return true
	case (_,_,_) => return false
}
}
