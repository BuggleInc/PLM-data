def bubbleSort()  {
  var longueur:Int = getNombreValeurs() //longueur du tableau
  var estTrie:Boolean = false
  while (!estTrie) {
    estTrie = true
    for (i <- 0 to longueur-2) {
      if (plusPetitQue(getValeur(i),getValeur(i+1))) {
        echange(i,i+1)
        estTrie = false
      }
    }
  }
}