var diag = 0;
override def run() {
  var cpt = 0
  var x = 0
  var y = 0
  var z = 0
  var finale = 0
  while (finale==0) {
		for (i<-0 to diag) {
      setPos(x,z)
      x = x + 1
      y = y + 1
      ecritMessage(cpt)
      cpt = cpt + 1
    }
    if (cpt<48) {
      y = y + 1
      setPos(y,0)
    } else {
      finale=1
    }
  }
}
