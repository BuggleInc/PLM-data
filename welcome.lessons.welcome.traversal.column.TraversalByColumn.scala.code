override def run() {
  var x=getX()
  var y=getY()
  var pas=0
  while (!posFinale(x,y)) {
    ecritMessage(pas.toString)
    if (y==0) {
      setPos(x+1, 0)
    } else setPos(x, y-1)   
    x=getX()
    y=getY() 
    pas=pas+1  
  }
}

def posFinale(x:Int,y:Int):Boolean = {
  var yf=getMondeHauteur()-1
  var xf=getMondeLargeur()-1
  if (y==yf & x==xf) {
    return true
  } else return false
}
