def cocktailSort() {
	var fin=false
	do {
		fin=false
		for (i<-0 to getNombreValeurs()-2) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				fin=true
			}
		}

		for (i<-getNombreValeurs()-2 to 0 by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				fin=true
			}
		}
		
	} while (fin==false)
}
