def combSort() {
  val lgr=getNombreValeurs()
  var ecart=lgr
  var inverse=false
  do{
    inverse=false
    if(ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i=0
    while(i+ecart<lgr){
      if (plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        inverse=true
      }
      i+=1
      
    }
  }while (ecart>=1 || inverse==true)
}