override def run() {
  var hauteur : Int = getMondeHauteur()
  var largeur : Int = getMondeLargeur()
  
  //---
  def trouverSortie() {
  	for (h<-0 to hauteur-1 ; l <- 0 to largeur-1)
    		{if (aBiscuit(l,h)) {setIndication(l,h-1,0)}}
  }
  
  //---
  
  trouverSortie()
}
