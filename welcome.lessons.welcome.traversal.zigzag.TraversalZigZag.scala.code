def positionFinale():Boolean = {
	if ((getX() == getMondeLargeur()-1) && (getY() == getMondeHauteur()-1)){
		return true
	}
	return false
}
override def run() {
	var cpt=0
	for (i <- 1 to getMondeHauteur()){
		for (i <- 1 to getMondeLargeur()){
			ecritMessage(cpt)
			cpt=cpt+1
		
		if (getX() == 0){
			setPos(getX()+1, getY())
		}
		if ((getX() == 6) && (!positionFinale())){
			setPos(getX()-1, getY())
		}	
		}
		setPos(getX(), getY()-1)
	}
	
}

