override def run() {


var steps:Int = 0;



	while(!estArrive())
	{

		ecritMessage(steps);
		avance();
		steps = steps + 1;
		if(steps % 6 == 0)
		{
			setPos(steps / 6 - 1,0);
		}

	
	}
	
		

def estArrive() : Boolean = {

	var result = false;

	if(getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur() - 1)
	{
		result = true;
	}

		return result;

	
}


}


