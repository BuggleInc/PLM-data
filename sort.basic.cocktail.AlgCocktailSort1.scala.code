def cocktailSort() {
  var a = true
  do {
    a = true
  for (i <- getValueCount()-2 to 0) {
    if (isSmaller(i+1,i)){
      swap(i,i+1)}
		if (isSmaller(getValueCount()-1-i,getValueCount()-2-i)){
      swap(getValueCount()-1-i,getValueCount()-2-i)}
    a = false
  }


    
  
} while (a == false)
}