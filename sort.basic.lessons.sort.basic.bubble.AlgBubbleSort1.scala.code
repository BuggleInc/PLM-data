def bubbleSort()  {
	var inverser=true
	do {
		inverser=false
		for (i<-0 to getNombreValeurs()-2){
			if (plusPetit(i, i+1)==false){
				echange(i, i+1)
				inverser=true	
			}
		}
	} while (inverser==false);
		
}


