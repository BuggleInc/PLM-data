def golomb(num:Int): Int = {
  var T = new Array [Int](num)
  var i = 1
  var avancement = 1
  //T(0)=1
  T(1)=2
  while (T(num-1)==null){
    for (go <- 1 to T(i)){
      avancement = avancement + 1
      T(avancement)=T(i)
    }
  }
  return T(num-1)
}
  
  
  
  
