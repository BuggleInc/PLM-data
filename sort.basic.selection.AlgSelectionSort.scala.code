def selectionSort() {
  var lgr = getNombreValeurs()
  var j = 0
  var min = getValeur(0)
  var ind = 0
  for (i <- 0 to lgr-2) {
  	min=getValeur(i)
  	ind = i
  	j=i+1
  	while (j<lgr) {
      if (getValeur(j)<min) {
      	min=getValeur(j)
        ind = j
      }
      j+=1
    }
  echange(i,ind)
  }
}