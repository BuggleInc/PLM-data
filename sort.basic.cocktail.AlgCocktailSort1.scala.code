def cocktailSort() {
  
  var i:Int=0
  var cpt:Int=0
  var cpt2:Int=0
  var test:Boolean=false
  
	do{
		cpt=0
    for(i<-0 to getNombreValeurs()-2){
      if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt+=1
      }
    }
    cpt2=0
    for(i<- getNombreValeurs()-2 to 0 by -1){
       if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt2+=1
    	}
    }
    
    if(cpt==0 && cpt2==0){
      test=false
    }

  }while(test==true)
  
  
}