override def run() {
  var w = getWorldWidth();
  var h = getWorldHeight();
  
  var count:Int = 0;
  var direction = 1;
  
  var x = -1;
  var y = -1;
  
  
  do {
    
    count += 1;
    x += direction;
    
    if (x < 0) {
      y += 1;
      x = 0;
      direction = 1;
    } else if (x >= w) {
      y += 1;
      x = w - 1;
      direction = -1;
    }
    
    
    setPos(x,y);
    writeMessage(count);
    
  } while (!isOnFinalDestination());
}

def isOnFinalDestination() : Boolean = {
  return getX() == getWorldWidth()-1 && getY() == getWorldHeight()-1;
}