def shellSort()  {
  var gap:Int = getValueCount() / 2
  var position:Int = 0
  var v:Int = 0
  while(gap >= 1){
    v = getValue(0)
    position = 0
    while(position + gap < getValueCount() && getValue(position + gap) < v){
    	copy(position + gap, position)
      position += gap
    }
    setValue(position, v)
    gap /= 2
  }
}