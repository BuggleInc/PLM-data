def keepHandOnSideWall()
{
  // s'il y a un mur face au buggle, il tourne ? droite pour avancer au prochain tour
  // sinon il avance, et tourne ? gauche s'il n'y a pas de mur ? gauche 
  
  if (!estFaceMur())
  {
    avance()
    
    gauche()
    if (estFaceMur())
    	droite()
  }
  else
    droite()
}

override def run()
{
  while(!estSurBiscuit())
    keepHandOnSideWall()
  
  prendBiscuit()
}