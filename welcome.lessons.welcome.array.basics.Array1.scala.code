override def run() {
	var T = new Array[Color](getMondeHauteur());
	for (i <- 1 to getMondeHauteur()) {
		T(i) = getCouleurSol() ;
		avance();
	}
	for (i <- 1 to getMondeHauteur()-1) {
		gauche(); avance(); droite();
		for (tmp <- 1 to getMondeHauteur()) {
			avance();
			setCouleurBrosse(T(tmp));
			baisseBrosse();
			leveBrosse();
		}
	}
	
}
