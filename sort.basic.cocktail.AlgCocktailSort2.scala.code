def cocktailSort2() {
  var n=getNombreValeurs()
var t=0
  var ec=false
  do{
    ec=false
    for (i<- t to n){
      if (plusPetit(i+1,i)){
        echange(i,i+1);n-=1;ec=true}}
    for (i<- n to t by -1){
      if (plusPetit(i+1,i)){
        echange(i+1,i);ec=true;t+=1
      }
    }
    
  }while (ec)
}