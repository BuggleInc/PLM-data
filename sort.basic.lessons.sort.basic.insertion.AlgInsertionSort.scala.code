def insertionSort() {
  
  val lgr = getNombreValeurs()
  var continue = true
  var a = 1
  
  for (i <- 1 to lgr-1){
    var stockeur = getValeur(i)
    a = 1
    
    while(stockeur < getValeur(i-a) && continue){
      
 			continue = true
      copie(i-a,i-a+1)
      a+=1
      
      if (i-a <0){
        continue = false
        a -=1
      }
      
    }
    setValeur(i-a, stockeur)
  }
}