def cocktailSort() {
	val taille = getNombreValeurs();
     var échange = true;
     while ( échange == true ) {
     échange = false;
	for (i <- 0 to taille-2) {
		if ( plusPetit(i,i+1) == false) {
			echange(i,i+1);
			échange = true;
		} } 
	for (i <- taille-2 to 0 by -1) {
		if ( plusPetit(i,i+1) == false) {
			echange(i,i+1);
			échange = true;
		}
	}
}
} 
