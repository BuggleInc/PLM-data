def cocktailSort3() {
	var debut = 0
	var fin = getNombreValeurs - 2
	var swap = true
	while (swap){
		swap = false
		for (i <- debut to fin){
			if (plusPetit(i+1,i)){
				echange(i,i+1)
				swap=true
			}
		}
		fin = fin - 1
		if (!swap){
			return
		}
		for (i <- fin to debut by -1){
			if (plusPetit(i+1,i)){
				echange(i,i+1)
				swap = true
			}
		}
		debut = debut + 1
	}
	if (!swap){
		return
	}
	
}
