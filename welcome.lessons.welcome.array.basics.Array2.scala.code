override def run() {
  var T = new Array[Color](getMondeHauteur())
  for (i <- 0 to getMondeHauteur()-2){
    T(i)=getCouleurSol()
    avance()
  }
  for (j <- 0 to getMondeLargeur()){
    var decalage = litMessage().toInt
    
    for (k <- 0 to getMondeHauteur-3){
      setCouleurBrosse(T((k+decalage)%getMondeHauteur()))
      baisseBrosse()
      droite()
      gauche()
      leveBrosse()
      avance()
    }
  }
  
}