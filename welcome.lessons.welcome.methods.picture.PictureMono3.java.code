public void run(){
	for (int st=0;st<7;st++){
		for(int step=0;step<9;step++){
			for(int stepper=0;stepper<4;stepper++){
				forward(2);
				brushV();
				forward();
				left();
				forward();
				brushV();
				backward();
				right();
				forward();
				brushV();
				forward(2);
				left();
			}
			forward(7);
		}
	left();
	forward(7);
	right();
	}
}

void brushV(){
	brushDown();
	brushUp();
}