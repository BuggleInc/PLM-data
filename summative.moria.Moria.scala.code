while (!(getGroundColor() == Color.green)) {
  if (getDirection() == 1) {
    if (isFacingWall()) {
      right();
    } else if (isOverBaggle()){
      pickupBaggle();
      right();
      right();
    } else if (isCarryingBaggle()) {
      dropBaggle();
      forward();
    } 
    else {
      forward();
    }
  } else if (getDirection() == 2) {
    if (isFacingWall()) {
      left();
      forward();
    } else {
      forward();
    }
  } else if (getDirection() == 3) {
    if (isOverBaggle()) {
      right();
      right();
      forward();
    } else {
      forward();
    }
  }
}