while (!porteBiscuit()) { //tant qu'il ne porte pas de biscuit
	while (!estSurBiscuit()) { //s'il n'est pas sur un biscuit
		droite(); //il tourne vers la droite et avance
		avance();
	} else {
		prendBiscuit(); //s'il se trouve sur un biscuit, il le prend
	}
} else { //s'il porte déjà un biscuit
	gauche(); 
	while (estSurBiscuit()) { //s'il se trouve sur un biscuit
		recule(); //il recule et pose son biscuit
		poseBiscuit();
	} else {
		avance();
	}
}
