def eq(a:Int, b:Int, c:Int):Int= {
	var x:Int=0;
	var T=Array(a,b,c);

	for (y<-0 to 2) {
		for (z<-0 to 2) {
			if (T(z)==T(y) && z<=3) {
				x=x+1
			}
		}
	}
	return x
}

def inOrderEqual(a:Int, b:Int, c:Int, equalOk:Boolean):Boolean = {
	var x:Boolean=false;

	if (equalOk==false&&a<b&&b<c) {
		x=true
	}
	else if (equalOk==true&&(eq(a,b,c)==2&&eq(a,b,c)==3)) {
		x==true
	}
	return x
}
