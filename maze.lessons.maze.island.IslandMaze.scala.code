override def run() {
  while(!estSurBiscuit()){
    while(getY()!=0 || getY() = getHauteurMonde()-1){
    	while(!estFaceMur){
      	avance()
    	}
      var n = 0
    	while(estFaceMur){
      	gauche()
        n = n+1
    	}
      avance()
      while(n!=0){
        gauche()
        n = n-1
      }
    }
  	GaucheMur()
  }
  prendBiscuit()
}

def GaucheMur(){
  gauche()
  if(estFaceMur){
    droite()
  	while(estFaceMur()){
    	droite()
  	}
    avance()
  }
  else{
      avance()
  }
}