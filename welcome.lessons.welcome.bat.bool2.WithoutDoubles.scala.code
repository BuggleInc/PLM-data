def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
  var de1:Int = die1
  var de2:Int = die2
  if (noDoubles == true && de1 == de2) {
    if (de1 == 6) {
      de1 = 1
      return de1+de2
    }
    else {if (de2 == 6) {
      de2 = 1
    return de1+de2}}
    return de1+de2+1
  }
  return de1 + de2
}