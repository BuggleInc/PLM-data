def array123(nums:Array[Int]): Boolean = {
  var a=false
  var b=false
	var c=false
	for (l<- 0 to nums.length-1){
    if (1==nums(l)){
      var a=true
    }
    if (2==nums(l)){
      var b=true
    }
    if (3==nums(l)){
      var c=true
    }
  }
  if (a==false || b==false ||c==false){
    return false
  }
  else{
    return true
  }
}
  