def inOrderEqual(a:Int, b:Int, c:Int, equalOk:Boolean):Boolean = {
  if (equalOk) {
    if (a <= b) {
      if (b <= c)
      	return true
      return false
    }
    return false
  }
  if (a < b) {
    if (b < c)
     return true
    return false
  }
  return false
}

