override def run() {
  var n :Int= getMondeHauteur()
  var T =new Array [Color](n)
  for (i<-0 to n-1){
    avance(i)
    T(i)=getCouleurSol()
    recule(i)
  }
  def reproduireMotif(){
    var decal : Int= litMessage().toInt
    setCouleurBrosse(T(decal))
    baisseBrosse()
    leveBrosse()
    for (k<-decal+1 to n-2){
      setCouleurBrosse(T(k))
      baisseBrosse()
      leveBrosse()
      avance()
    }
  }
  gauche()
  avance()
  droite()
  reproduireMotif()
}