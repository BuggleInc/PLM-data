def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  var anniv:Int = 0
  if (isBirthday){
    anniv = 5;
  }
  var limite1 = 61 + anniv
  var limite2 = 81 + anniv
  if (speed<limite1){
    return 0;
  }
  else{
    if (speed < limite2){
      return 1;
    }
    else{
      return 2;
    }
  }
}