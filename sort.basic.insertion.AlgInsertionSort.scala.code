def insertionSort() {
  var k:Int =1
  var a:Int =0
  var e:Int =1
  var j:Int = 1
  for(i<- 1 to getNombreValeurs()-1){
    a=getValeur(i)
    k=1
    while(e==1 && j<i){

      if(getValeur(i-j)>a){
        k=i-j   
        copie(i-j,i-j+1)  
      }
      else{
        e=0
      }
			j+=1


    }
    if(k!=1 && i!=1){
      setValeur(k,a)}
		j=1
    e=1
  }
}