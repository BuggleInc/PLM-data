def gnomeSort() {
  var nbOps = 0;
  var i = 0;
  do {
    if (i == 0) {
      i += 1;
    } else if (i == getNombreValeurs() -1) {
      return
    } else {
      if (plusPetit(i+1, i)) {
        echange(i, i+1);
        nbOps += 1
        i -= 1;
      } else {
        i += 1;
      }
    }
  } while (nbOps != 0 && i >= 0)
}