override def run() {
  var T = new Array[Color](getMondeHauteur())
  for (i <- 0 to getMondeHauteur()-1){
    T(i)=getCouleurSol()
    avance()
  }
  for (j <- 0 to getMondeLargeur()){
    droite()
    avance()
    gauche()
    avance()
    for (k <- 0 to getMondeHauteur()-1){
      baisseBrosse()
      setCouleurBrosse(T(k))
      avance()
      leveBrosse()
    }
  }
}