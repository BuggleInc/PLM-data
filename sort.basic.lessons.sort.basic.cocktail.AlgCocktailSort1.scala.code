def cocktailSort() {
  var taille=getNombreValeurs()
  var trier=false
  while(trier=false){
    trier=true
    for(j<- 0 to taille-2){
      if(plusPetit(j+1,j)==true){
        echange(j+1,j)
        trier=false
      }
      
    }
    for (i<-taille-2 to 0){
      if ( plusPetit(i+1,i)==true){
        echange(i+1,i)
        trier=false
      }
    }
  }
}
