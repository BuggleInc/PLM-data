def isFacingTrail():
    forward()
    getGroundColor()
    backward()
    if getGroundColor != Color.green:
        return False
    else:
        return True

while not isOverBaggle():
    if isFacingWall():
        left()
    elif isFacingTrail():
        forward()
    else:
        isFacingTrail()
        
    
    
