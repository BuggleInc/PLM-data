def cocktailSort3() {
	var b=true;
	var a=true;
	val l=getNombreValeurs() ;
	var debut=0;
	var fin=l-2;
	while (a&&b) {
		b=false;
		a=false;
		for (i<-debut to fin) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				b=true;
			}
		}
		fin -=1;
		if (b)
		for (i<-debut to fin) {
			if (plusPetit(debut+fin-i+1,debut+fin-i)) {
				echange(debut+fin-i,debut+fin-i+1);
				a=true;
			}
		}
		debut+=1;
	}
}