override def run() {
	var compteur:Int = 0;
	while (!estSurBiscuit()) {
		if (compteur == 0) {
			while (!estFaceMur()) {
				avance();
			}
			droite();
			compteur = compteur - 1;
			ecritMessage(compteur.toString);
		} else if (compteur !=0) {
				while (estFaceMur()) {
					gauche();
				}
				compteur = compteur + 1;
				ecritMessage(compteur.toString);
				avance();
			} else {
				avance();
			}
	}
}