#tant qu'on n'est pas sur le biscuit on avance

pasavances = 0
while not (estSurBiscuit() or porteBiscuit()) :
    avance()
    pasavances += 1
#si on est arriv? l? c'est qu'on est sur le biscuit
if estSurBiscuit():
    prendBiscuit()

while pasavances > 0:
    recule()
    pasavances -= 1

poseBiscuit()
