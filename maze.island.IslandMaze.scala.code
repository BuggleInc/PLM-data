override def run() {
  var test=false
  while(!estSurBiscuit()){
    while(getY()>0 && test==false){
      nord()
    }
    test=true
    droite()
    keeHandOnSideWall()
  }
  prendBiscuit()
}

def keeHandOnSideWall(){
  gauche()
  while(estFaceMur()==true){
    droite()
  }
  avance()
}

def nord(){
  if(!estFaceMur()){
    avance()
  }else{
    while(estFaceMur()){
      droite()
    }
    avance()
  }
  setDirection(Direction.NORTH)
}