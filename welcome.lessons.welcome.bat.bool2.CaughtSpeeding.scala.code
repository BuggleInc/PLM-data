def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if ((speed <= 60 && !(isBirthday)) || (speed <= 65 && (isBirthday))) {
		return 0;
	}
	else if (((speed >= 61 && speed <= 80) && !(isBirthday)) || ((speed >= 66 && speed <= 85) && (isBirthday))) {
		return 1;
	}
	else if ((speed >= 81 && !(isBirthday)) || (speed >= 86 && (isBirthday))) {
		return 2;
	}
}
