def cocktailSort2() {
  var beg:Int=0;
  var end:Int=getValueCount()-2;
  var sp:Boolean=false;
  do {
    sp=false
    for (i <- beg to end) {
      if (!isSmaller(i,i+1)) {
        swap(i,i+1)
        sp=true
      }
    }
    end-=1
    for (i <- beg to end) {
      if (!isSmaller(end-i,end-i+1)) {
        swap(end-i,end-i+1)
        sp=true
      }
    }
    beg+=1
  } while (sp==true)
}
