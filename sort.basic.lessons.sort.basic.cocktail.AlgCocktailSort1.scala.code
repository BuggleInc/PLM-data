def cocktailSort() {
  var n = getNombreValeurs();
  var parcours = true;
  while (parcours) {
    parcours=false
  	for (i<-0 to n-2) {
    	if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
        parcours=true
      }
      
    
    
      if (getValeur(n-i)<getValeur(n+1-i)) {
      	echange(n-i,n+1-i)
        parcours=true
      
    }
  }
}

}