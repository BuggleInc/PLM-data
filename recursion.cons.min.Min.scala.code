def min(l:List[Int]): Int = {
  
  def min_helper(l:List[Int], v:Int):Int = {
    
    if(l.tail == Nil){
      
      return v;
      
    } else if(l.head < v){
      
      				return min_helper(l.tail, l.head);
      
    			 } else{
      
      				return min_helper(l.tail, v);
      
    			 }
    
  }
  
  return min_helper(l, l.head);
  
}