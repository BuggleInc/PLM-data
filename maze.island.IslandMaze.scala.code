override def run() {
  setDirection(Direction.NORTH)
  while(!estSurBiscuit()){
    while(!estFaceMur() && getDirection()==Direction.NORTH){
      avance()
    }
    while(estFaceMur()){
      keepHandOnSideWall()
    }
  }
}

def keepHandOnSideWall(){
  if (estFaceMur()){
    droite()
  }
  else {
    avance();gauche()
    while (!estFaceMur()){
      avance();gauche()
    }
    droite()
  }
}