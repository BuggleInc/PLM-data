public void cocktailSort() {
	boolean swapped;
	do {
		swapped = false;
		for(int i = 0; i < getCountSize() - 1; i++)
			if(isSmaller(i+1, i)){
				swap(i, i+1);
				swapped = true;
			}
		for(int i = getCountSize() - 1; i > 0 ; i++)
			if(isSmaller(i+1, i)){
				swap(i, i+1);
				swapped = true;
			}
	} while(swapped);
}	
