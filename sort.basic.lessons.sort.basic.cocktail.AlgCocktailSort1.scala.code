def cocktailSort() {
  var chgt1 = false
  var chgt2 = false
  do{
    chgt1 = false
    for (i <- 0 to getNombreValeurs()-2){
      if (!plusPetit(i,i+1)){
        echange(i,i+1)
        chgt1 = true
      }
      chgt2 = false
    for (i <- getNombreValeurs()-2 to 0 by -1){
      if (!plusPetit(i+1,i)){
        echange(i+1,i)
        chgt2 = true
      }
    }
    }
  }
  while (chgt1 || chgt2)
}
  