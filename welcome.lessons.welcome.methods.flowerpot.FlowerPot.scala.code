def marquer()
{
	baisseBrosse();
	leveBrosse();
}

def cacahouete(c: Color)
{
setCouleurBrosse(Color.YELLOW);
marquer();
setCouleurBrosse(c);
avance();
marquer();
recule();
gauche();
avance();
marquer();
recule(2);
marquer();
avance();
gauche();
avance();
marquer();
}
def growFlowers(){
	avance();
	cacahouete(Color.RED);
}
