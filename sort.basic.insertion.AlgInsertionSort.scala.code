def insertionSort() {
  var len = getNombreValeurs()
  var rgpositif:Boolean = true
  var v = 0
  var rg = 0
  for (i <- 1 to len-1) {
    rg = i-1
    v = getValeur(i)
    while (rgpositif) {
    	if (plusPetitQue(v,rg))
    		copie(rg,rg+1)
      else
        rgpositif = false
      if (rg>0)
      	rg-=1
    }
    copie(v,rg+1)
  }
}
    	