def max2Value(nums:Array[Int]): Int = {
  var Nb =0
  var L = nums.length
  var max1 = Integer .MIN_VALUE
  var max2 = Integer .MIN_VALUE
  var med = 0
	for (i <- 0 to L-1){
    if (max1<nums(i)){
      med = max1
      max1 = nums(i)
      max2 = med}
    if (max2<nums(i) && max1>nums(i)){
      max2 = nums(i)
    }
    if (max1==nums(i)){
      Nb=Nb+1
    }
    if(Nb>0){
      max2=max1
    }
   }
 return max2
}