
while estSurMessage()==True:
    litMessage();
    if litMessage()=="A":
        avance();
    elif litMessage()=="L":
        gauche();
        avance();
    elif litMessage()=="Z":
        recule();
    elif litMessage()=="B":
        avance(2);
    elif litMessage()=="R":
        recule();
    elif litMessage()=="I":
        retourne();
        avance();
    elif litMessage()=="X":
        recule(3);
