def combSort() {
  var ecart:Int=getNombreValeurs()
  var compteur:Int=0
  var i:Int=0
  do {
    if (ecart>1) {
      ecart=ecart/1.3}
    i=0
    do{
      compteur=0
      if(plusPetit(i+ecart,i)) {
        echange(i+ecart,i)
        i+=1
        compteur+=1}}
    while(i+ecart < getNombreValeurs())
  }
  while( ecart>1 || compteur!=0)
  
}