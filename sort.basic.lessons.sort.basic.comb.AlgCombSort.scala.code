def combSort() {
  val n = getNombreValeurs()
  var eca = n
  var e = false
  var i = 0
  do {e=false
      if (eca > 1) {eca = (eca.toDouble / 1.3).toInt}
      i = 0
      while (i + eca < n) {
        if (plusPetit(i+eca,i)) {echange(i+eca,i);e = true}
        i+=1
      }
     } while (eca > 1 ||  !e)
    for (i<- 0 to n-2) {if (plusPetit(i+1,i)) {echange(i+1,i)}}

}