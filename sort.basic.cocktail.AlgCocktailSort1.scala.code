def cocktailSort() {
      var i =0
			var echanger:Boolean=true
  		var j =0
  
  while(echanger){
  
		echanger=false
    
    for (j<- getNombreValeurs-2 to 0){
        if (plusPetit(j+1:Int,j:Int):Boolean){
          echange(j:Int,j+1:Int)
        }}
    
 	  for (i<- 0 to getNombreValeurs-(2) ){
    	
      if(plusPetit(i+1:Int,i:Int):Boolean){
        echange(i:Int,i+1:Int)}}
    
        
        echanger=true;
    } 
}