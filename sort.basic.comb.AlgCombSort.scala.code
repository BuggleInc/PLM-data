def combSort() {
  var ecart:Int = getNombreValeurs()
  var echanges:Boolean = true
  while ((echanges)||(ecart>1)){
    echanges = false
  	if (ecart>1){
      ecart = (ecart.toDouble/1.3).toInt
    }
    var i:Int = 0
    while (i+ecart<getNombreValeurs()-1){
      if (getValeur(i)>getValeur(i+ecart)){
        echange(i,(i+ecart))
        echanges = true
      }
      i = i+1
    }
  }
}