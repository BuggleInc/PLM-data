override def run() {
  var i = 0
  var x = 0
  var y = 0
  while (x != getMondeLargeur()-1) {
    ecritMessage(i)
    while (y != getMondeHauteur()-1) {
      i += 1
      y += 1
      setPos(x,y)
      ecritMessage(i)      
    }
    x += 1
    i += 1
    y = 0
    setPos(x,y)
  }
  ecritMessage(i)
  while (y != getMondeHauteur()-1) {
    i += 1
    y += 1
    setPos(x,y)
    ecritMessage(i) 
  }
}