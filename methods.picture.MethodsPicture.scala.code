override def run() {
  for (i <- 1 to 3) {
    for (i <- 1 to 3) {
      makeV(Color.YELLOW, Color.RED, Color.BLUE, Color.GREEN);
  		fiveSteps();
    }
  	left();
  	fiveSteps();
  	right();
  }
}

