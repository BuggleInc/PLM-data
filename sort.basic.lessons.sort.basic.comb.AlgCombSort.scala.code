def combSort() {
var ecart==getNombreValeurs()
var echange==false
while (ecart > 1 || echange == true) {
	echange = false
	if (ecart > 1) {
		ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
	}
	i = 0
	while (i+ecart<getNombreValeurs()){
		if (plusPetit(i+1,i)==true) {
			echange(i,i+1)
			echange=true
	}
	i=i+1
}
}
}
