def cocktailSort3() {
  var tri:Boolean=true
  var fin=getNombreValeurs()-2
  var debut=0
  while(tri){
    tri=false
    var test=0
    for (i<-debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        tri=true
        test=test+1
      }
    }
    fin=fin-1
    if (test==0){
      for (j<-fin+1 to debut+1 by -1){
        if (plusPetit(j,j-1)){
          echange(j-1,j)
          tri=true
        }
      }
      debut=debut+1
    }
  }
}