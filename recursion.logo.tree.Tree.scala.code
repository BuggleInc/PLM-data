def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if(steps!=0){
  	baisseCrayon();
  	avance(length);
  	gauche(angle);
  	avance(length);
  	leveCrayon();
  	recule(length);
  	baisseCrayon();
  	droite(2*angle);
  	avance(length);
  	leveCrayon();
  	tree(steps-1,length*shrink,angle,shrink) 
  }
}