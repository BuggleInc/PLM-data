def combSort() {
   var i=0
   var test = false
   var ecart=getNombreValeurs()
   var lgr =getNombreValeurs()
 
  do{ 
    if (ecart>1) {
      ecart = (ecart.toDouble /1.3).toInt     
    }
    i = 0
    
    while (i+ecart < lgr ){
      test = plusPetit(i , i+ecart)
      if (!test){
         echange (i,i+ecart)
         i=i+1
      }
    }
  } while (ecart >1 || i!=0)
}