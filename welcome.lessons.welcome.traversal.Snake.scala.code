def positionFinale():Boolean = {
	if(estFaceMur()) {
		if(getDirection() == Direction.EST) {
			gauche();
				if(estFaceMur())
					return true;
			droite();
		}
		if(getDirection() == Direction.OUEST) {
			droite();
				if(estFaceMur())
					return true;
			gauche();
		}
	}
	return false;
}

override def run() {
	baisseBrosse();
	while (!positionFinale()) {
		if(!estFaceMur())
			avance();
		
	}
}
