def shellSort()  {
  var i=0
  val l=getValueCount()
  var v=0
  var k=0
  var m=1
  var gap:Int=l/2
  for(i<- 1 to l-1){
    k=0
    m=i
    v=getValue(i)
    while ((gap>0)&&(i-(k+1)*gap)>0){
   		if (isSmaller((i-(k+1)*gap),(i-k*gap))==false){
       	copy(i-(k+1)*gap,i-k*gap)
       	m=i-(k+1)*gap
      }
    k=k+1
    setValue(m,v)
  	}
  gap=gap/2
	}  	
}