def pentaKoch(levels:Int, length:Double) {
if (levels!=0) {
		pentaKoch(levels-1,length/(3))
		left(108)
		pentaKoch(levels-1,length/(3))
		right(72)
		pentaKoch(levels-1,length/(3))
		right(72)
		pentaKoch(levels-1,length/(3))
		right(72)
		pentaKoch(levels-1,length/(3))
		left(108)
		pentaKoch(levels-1,length/(3))
	}
	else {
		avance(length)
	}
}
