def shellSort()  {
  var longueur = getNombreValeurs()
  var ecart = getNombreValeurs()
  var i = 0
  var j = 0
  var compteur = 1
  var valeur = 0
  while (ecart > 0) {
    ecart = (ecart.toDouble / 2).toInt
    j = i
    compteur = 0
    valeur = getValeur(j)
    while (j - (compteur+1)*ecart > 0 && getValeur(j-((compteur+1)*ecart)) > valeur) {
      copie(j-((compteur+1)*ecart),j-(compteur*ecart))
      j -= ecart
      compteur += 1
    }
    setValeur(j,valeur)
    i += 1
  }
}