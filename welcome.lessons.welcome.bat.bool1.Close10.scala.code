def close10(a:Int, b:Int): Int = {
  var x=0
  var y=0
  var a1=a
  var b1=b
  if (a<=10){
    while (!(a1==10)){
      x=x+1
      a1=a1+1
    }
  }
  else {
    while (!(a1==10)){
      x=x+1
      a1=a1-1
    }
  }
  if (b1<=10){
    while (!(b1==10)){
      y=y+1
      b1=b1+1
    }
  }
  else {
    while (!(b1==10)){
      y=y+1
      b1=b1-1
    }
  }
  if (x>=y)
  {
    return b}
  return a
}