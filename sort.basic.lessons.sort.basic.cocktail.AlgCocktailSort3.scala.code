def cocktailSort3() {
var debut=0
var fin=getNombreValeurs()-2
var x=1
var y=1
while((x!=0) && (y!=0)){
	for (i<- debut to fin){
			x=0
		if(!plusPetit(i, i+1)){
			echange(i,i+1)
			x=1
		}
	}
	fin-=1
	for (j<- fin to debut by -1){
		y=0
		if(!plusPetit(j, j+1)){
			echange(j,j+1)
			y=1
		}
	}
		debut+=1
}
}
