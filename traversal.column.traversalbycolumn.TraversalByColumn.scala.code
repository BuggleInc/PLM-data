override def run() {
  var cpt=0
  ecritMessage(0)
  while(!testFin()){
    go()
    cpt+=1
    ecritMessage(cpt)
  }
}

def testFin():Boolean= {
  if(getX()+1==getMondeLargeur && getY()+1==getMondeHauteur){
    return true
  }else{
    return false
  }
}

def go(){
  if(getY()+1<=getMondeHauteur){
    setPos(getX(),getY+1)
  }else{
    setPos(getX()+1,0)
  }
}