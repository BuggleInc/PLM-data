def cocktailSort3() {
	var debut=0
	var fin=getNombreValeurs()-2
	var tri=false
	do{
		tri=false
		for(i<- debut to fin){
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				tri=true
			}
		}
		fin -=1
		}while(tri)
		do{
		for(i<- fin to debut by -1){
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				tri=true
			}
		}
		debut +=1
		
	}while(tri)

}
