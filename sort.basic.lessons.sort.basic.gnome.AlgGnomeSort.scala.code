def gnomeSort() {
  var pos=0;
  var ech=false;
  while(pos<getNombreValeurs()-1){
    while(ech==false){
      if(!plusPetit(pos,pos+1)){
        echange(pos,pos+1)
        ech=true
      }
      else pos=pos+1
    }
    for(i<-pos to 1 by -1){
      if(!plusPetit(i-1,i)){
        echange(i-1,i)
    	}
  	}
    pos=0
	}
}