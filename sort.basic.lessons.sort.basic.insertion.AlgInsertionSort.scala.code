def insertionSort() {
  var n = getNombreValeurs()
  var v = 8
  var marqueur = 0
  var j=0
  for (i <- 1 to (n-1)) {
    v = getValeur(i)
    marqueur = 0
    j=1
    while (j<i) {
       if (getValeur(i-j)>v) {
         copie(i-j,i-j+1)
         setValeur(marqueur,v)
         j=i+1
       } else {
         j=j+1
       }    
    }
  }
}