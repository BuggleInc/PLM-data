def insertionSort() {
  var n : Int =getValueCount()
  
  for(it<-1 to n-1){
    
    var elementId : Int = it
    var elementVal : Int = getValue(it)
    while(elementId >0 && !isSmallerThan(elementId-1,elementVal)){
      copy(elementId-1,elementId)
      elementId-=1
    }
    if(elementId>=0)
	    setValue(elementId,elementVal)
    else
  		setValue(0,elementVal)
  }
}