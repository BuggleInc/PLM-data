def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  
  if (isBirthday==false){
    if (speed<=60){
      return 0
    }
    if (speed>60 & speed<81){
      return 1
    }
    if (speed>=81){
      return 2
    }
  }
  if (isBirthday==true) {
    if (speed<=65){
      return 0
    }
    if (speed>65 & speed<86){
      return 1
    }
    if (speed>=86){
      return 2
    }
    
  }
}