def cocktailSort2() {
	var hasSwap:Boolean = false;

	do{
		hasSwap = false;
		for(i <-1 to getNombreValeurs()-1 by 1)
		{
			if (plusPetit(i, i-1))
				{
					echange(i,i-1);
					hasSwap = true;
				}
		}
		for( i<- getNombreValeurs()-1 to 1 by -1)
		{
			if (plusPetit(i, i-1))
				{
					echange(i-1,i);
					hasSwap = true;
				}
		}
	}while(hasSwap)
	
}
	
}
