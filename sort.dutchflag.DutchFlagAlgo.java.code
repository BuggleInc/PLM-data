void solve() {
  
  int bleu = 0;
  int rouge  = getTaille();
  int blanc = getTaille();
  
  while(bleu<=blanc)
  {
  	if(getCouleur(bleu)==BLEU)
      {
      bleu++;
    }
    else if(getCouleur(bleu)==ROUGE)
      {
      echange(bleu,blanc);
      blanc--;
    }
    else{
     echange(bleu,blanc);
     echange(blanc,rouge);
   	 rouge--;
     blanc--;
  
    }

    
	}
}