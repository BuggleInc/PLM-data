def combSort() {
  var ecart = getNombreValeurs()
  var aInverse = true
  var i = 0
  val n = getNombreValeurs
  while ((ecart > 1) || aInverse) {
    if (ecart > 1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i = 0
    aInverse = false
    while (i+ecart < n) {
      if (getValeur(i+ecart) < getValeur(i)) {
        echange(i+ecart,i)
        aInverse = true
        i += 1
      }
    }
  }
}