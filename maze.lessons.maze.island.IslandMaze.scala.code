def checkMurLeft() :Boolean = {
	var murleft = false
	droite()
	if(estFaceMur == true){
		murleft = true
	}
	gauche()
	return murleft;
}


override def run() {
	var nord = 0
	while(!estFaceMur && nord%4 == 0){
		avance()
	}
	while(!estSurBiscuit){
		if(checkMurLeft == true && estFaceMur == false){
			avance()
		} else if(checkMurLeft == true && estFaceMur == true){
			gauche() ; nord = nord - 1
		}
		else {droite() ; nord = nord + 1 ; avance()}
	}
	prendBiscuit()	
}


