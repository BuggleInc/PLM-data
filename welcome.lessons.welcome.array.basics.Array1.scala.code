override def run() {
  var T = new Array[Color](getMondeHauteur())
  var nbr:Int=1
  col1(T)
  for (m<-0 to getMondeLargeur()){
    col(T,m)
    nbr=nbr+1
  }
}

def col1(T:Array[Color]){
  for (i<-1 to getMondeHauteur()){
    T(i)=getCouleurSol()
    avance()
    left()
    avance()
    right()
  }
}

def col(T:Array[Color],nbr:Int){
  for (l<-1 to nbr){
    avance()
  }
  for (l<-1 to getMondeHauteur()){
    setCouleurBrosse(T(l))
    baisseBrosse()
    leveBrosse()
    avance()
  }
  
}