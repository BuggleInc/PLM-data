def shellSort()  {
  var ecart:Int = getNombreValeurs() / 2
  while (ecart > 0) {
    for (i <- ecart - 1 to getNombreValeurs() - 1) {
      var v:Int = getValeur(i)
      var j:Int = i

      while (j >= ecart && getValeur(j - ecart) > v) {
        copie(j - ecart, j)
        j -= ecart
      }
      setValeur(j, v)
    }
    ecart = ecart / 2
  }
}