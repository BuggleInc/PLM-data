def shellSort()  {
  var longueur = getNombreValeurs()
  var ecart = longueur
  var i = 0
  var j = 0
  var compteur = 1
  var valeur = 0
  while (ecart > 0) {
    ecart = (ecart.toDouble / 2).toInt
    j = i
    compteur = 1
    valeur = getValeur(j)
    while (j - (compteur+1)*ecart > 0 && getValeur(j-((compteur+1)*ecart)) > valeur) {
      copie(j-((compteur+1)*ecart),j-(compteur*ecart))
      j -= ecart
    }
    setValeur(j,valeur)
  }
}