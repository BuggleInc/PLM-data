def inOrderEqual(a:Int, b:Int, c:Int, equalOk:Boolean):Boolean = {
	if(!equalOk){
		if(order(a,b)== true && order(b,c)==true){
			return true
		}else{
			return false;
		}
	}else{
		if(orderS(a,b)== true && orderS(b,c)==true){
			return true
		}else{
			return false;
		}
	}
}







def order(a:Int , b:Int):Boolean = {
	if ((b - a) > 0 ){
		return true;
	}else{
		return false;
	}
}

def orderS(a:Int , b:Int):Boolean = {
	if ((b - a) > 0 || a==b ){
		return true;
	}else{
		return false;
	}
}

