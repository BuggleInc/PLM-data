var diag = 0;
override def run() {
  var y=0
  var yy=0
  var x=0
  var cpt=0
  while (getX()!=getMondeLargeur()-1){
    while (y!=0){
      cpt+=1
      ecritMessage(cpt)
      x+=1
      y-=1
      setPos(x,y)
    }
    cpt+=1
    ecritMessage(cpt)
    if (y!=getMondeHauteur()-1){
      yy+=1
      x=0
      setPos(x,yy)
      y=yy
    }
  }
}