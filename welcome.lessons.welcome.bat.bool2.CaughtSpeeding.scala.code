def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if(isBirthday==false){
    if(speed<=60){
      return 0
    }
    else if(60<speed && speed<81){
      return 1
    }
    else return 2
  }
  else{
    if(speed<=65){
      return 0
    }
    else if(65<speed && speed<86){
      return 1
    }
    else return 2
  }
}