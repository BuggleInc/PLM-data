def combSort() {
	var ecart = getNombreValeurs();
	var invers = true;
	while(ecart > 1 || invers){
		invers=false;
		if(ecart > 1){
			ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int];
		}
		var i = 0;
		while(i+ecart < getNombreValeurs()){
			if(plusPetit(i+ecart,i)){
				echange(i+ecart,i);
				i+=1;
				invers = true;
			}
		}
	}
}
