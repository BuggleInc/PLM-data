def solve() {
  var n = getSize();
  
  var nextBlue = 0;
  var nextRed = n - 1;
  var i = 0;
  
  while (nextRed >= i) {
    getColor(i) match {
      case RED =>
      	swap(i, nextRed);
      	nextRed -= 1;
      case BLUE =>
      	if (i != nextBlue) {
       	 swap(i, nextBlue);
     	 }
      	nextBlue += 1;
      	i += 1;
      case WHITE =>
      	i += 1;
    }
  }
}