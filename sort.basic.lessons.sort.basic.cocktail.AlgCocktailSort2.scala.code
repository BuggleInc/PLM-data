def cocktailSort2() 
{
  var taille = getNombreValeurs()
  var permute = true
  var debut = 0
  var fin = taille-1
  do 
  {
    permute = false
    for (i<-debut to fin)
    {
      if (!plusPetit(i,i+1)) 
      {
        echange(i,i+1)
        permute = true
      }
    } 
    fin-=1
    for (i<-fin to debut by -1)
    {
      if (!plusPetit(i-1,i)) 
      {
        echange(i,i-1)
        permute = true
      }
    }
    debut+=1
  } while (permute)
}