def isFacingTrail():Boolean = {
    while(!isCarryingBaggle()){
      if(!isFacingWall()){
        forward();
      }else{
        right();
      }
      var x= getGroundColor()
      if(x!=Color.green){
        backward();
        left();
        
        
      }
      if(x==Color.green){
        brushDown();
        brushUp();
      }
    
  if(isOverBaggle()){
    pickupBaggle();
  }}
  return isCarryingBaggle()
}