def cocktailSort3() {
	var x:Boolean=true
	var b=0
	var e=getNombreValeurs()-2
		for(i<-b to e){x=false
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				x=true
			}
			if(x==false) return
		}
		e-=1

		for(i<-e to b by-1){x=false
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				x=true
			}
			if(x==false) return
		}
		b+=1
	
}
