override def run() {
  retourne()
  var compteur:Int=0
  while (!(getX() ==(getMondeLargeur()-1) && getY() == getMondeHauteur()-1)) {
    ecritMessage(compteur)
    setPos(getX(),getY()+1)
    compteur+=1
    if (estFaceMur() && !(getX()==6) ){
      ecritMessage(compteur)
      setPos(getX()+1,0)
    }   
  }
  compteur+=1
  ecritMessage(compteur)
}