def bubbleSort3()  {
  var swapped = false;
  
    for (k <- 0 to getValueCount()-2) {
      swapped = false;
      var i = getValueCount()-2 - k
      print(i + " " + getValueCount() + " ## ")
      
      for (j <- 0 to i) {
        if (isSmaller(j+1, j)) {
          swap(j, j+1);
          swapped = true;
        }
      }
      
      if (!swapped) {
        return
      }
    }
}