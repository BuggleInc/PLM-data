def bubbleSort()  
{
  for(i<-0 to (getNombreValeurs()-2)) 
  //on prend chaque élément de la liste pour le remonter
  	{
      for (j<-i to (getNombreValeurs()-1))
      //chaque élément va être comparé lgr-i fois
      	{
      		if (plusPetit(getValeur(j),getValeur(i)))
      			{
          		echange(i,j)
        		}
    		}
		}
}