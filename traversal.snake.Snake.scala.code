override def run() {
  while(!arrivee) {
    avance()
    if (estFaceMur && getDirection==Direction.EST) {
      gauche(); avance(); gauche()
    }
    if (estFaceMur && getDirection==Direction.OUEST) {
      droite(); avance(); droite()
  }
}

def arrivee():Boolean = {
  if (getDirection()==Direction.EST) gauche()
  if (getDirection()==Direction.OUEST) droite()
  if (estFaceMur) return true
  return false
}
