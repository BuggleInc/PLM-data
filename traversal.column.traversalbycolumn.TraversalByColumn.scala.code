override def run() {
  var abscisse : Int = 0
  var ordonnee : Int = 0
  var compteur : Int = 0
  var hauteur : Int = getMondeHauteur()
  var largeur : Int = getMondeLargeur()
  ecritMessage(compteur)
  for (colonne <- 1 to largeur) {
    ordonnee = 0
    while (getY() != hauteur) {
      ordonnee = ordonnee + 1
      setPos(abscisse, ordonnee)
      compteur = compteur + 1
      ecritMessage(compteur)      
    }
    abscisse = abscisse + 1
    setPos(abscisse,0)
  }
}

