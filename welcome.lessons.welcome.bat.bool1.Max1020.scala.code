def max1020(a:Int, b:Int):Int = {
  if (int1020(a)==true && int1020(b)==true){
    return Math.max(a,b)
  }
  else if (int1020(a)==true && int1020(b)==false){
    return(a)
  }
  else if (int1020(a)==false && int1020(b)==true){
    return(b)
  }
  else return 0
}


def int1020(a:Int):Boolean ={
  if (Math.abs(15-a)<6){
    return true
  }
  else return false
}