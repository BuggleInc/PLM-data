def combSort() {
  var len = getValueCount()
  var gap = getValueCount()
  var swapped = true
  
  do{
    swapped = false
    if (gap > 1){
      gap = (gap.toDouble / 1.3).toInt
    }
    var i = 0
    while (i+gap < len){
      if (isSmaller(i+gap, i)){
        swap(i+gap, i)
        i+=1
        swapped = true
      }
    }
    
  }while (gap > 1 || swapped)
}