override def run() {reproMotif()}



def reproMotif(){
  var i = getMondeLargeur()
  for (j <- 0 to i-1){
    gauche()
    avance()
    droite()
    Motif()
    
  }}
  

def Motif(){
  var i = getMondeHauteur()
  var T = new Array[Color](i)
  for (j <- 0 to i-1){
    T(j)=getCouleurSol()
    avance()
  for (j <- 0 to i-1){
    setCouleurBrosse(T(j))
    brosse()
    avance()
  }
  }}
  

def brosse(){
  baisseBrosse()
  leveBrosse()
}