def shellSort()  {
  var ecart=(getNombreValeurs())/2
  while(ecart>0){
    for (i<- ecart-1 to getNombreValeurs()-1){
    	val v=getValeur(i)
    	var j =i
    	while(j>=ecart && (getValeur(j-ecart)>v)){
      	setValeur(j-ecart, j);
     		j-=ecart;
    	}
    setValeur(j, v);
  }
  }
   ecart=ecart/2;}
