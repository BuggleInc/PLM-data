override def run() {
	var x = 0
	var y = 0
	var mess = 0
	
	ecritMessage(mess)
	
	
	while (getY() != getMondeHauteur() - 1 && getX() != getMondeLargeur() - 1)
	{
		if (getY() == getMondeHauteur() - 1)
		{
			x = x + 1 
			setPos(x , 0 )
			mess = mess + 1  
			ecritMessage(mess)		  
		}

		y = y + 1
		setPos(x , y)
		mess = mess + 1
		ecritMessage(mess)	
		
	}

	setPos(getMondeLargeur - 1 , getMondeHauteur -1 )
	ecritMessage(48)

}
