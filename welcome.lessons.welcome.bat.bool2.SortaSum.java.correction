$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class sortaSum extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( sortaSum((Integer)t.getParameter(0), (Integer)t.getParameter(1)) ); }
	int sortaSum(int a, int b) {
		int sum = a+b;
		if (sum >= 10 && sum <= 19)
			return 20;
		else
			return sum;
	}
 }