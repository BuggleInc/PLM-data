public void combSort() {
  int ecart = getNombreValeurs(); 
  int i = 0;
  boolean swap = true;
  do{
    if(ecart >1){
      (int)ecart = ecart / 1.3;
    }
    i = 0 ;
    swap = false;
    while(i+ecart < getNombreValeurs()){
      if(plusPetit(i+ecart,i)){
        i++;
        echange(i+ecart,i);
        swap = true;
      }
        
    }
    
    
  }while((ecart > 1)|| (swap == true));
}