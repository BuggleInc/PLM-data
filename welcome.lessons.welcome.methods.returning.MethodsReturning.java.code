boolean haveBaggle() {
boolean seenBaggle = false;
int steps = 6;

		for(int i = 0; i < steps; i++){
			forward();
		}

		if(isOverBaggle()){
			seenBaggle = true;
		}
		
		for(int i = 0; i < steps; i++){
			backward();
		}
		
	if(seenBaggle == true){
		return true;
	}else
		return false;
	}
}//end function
