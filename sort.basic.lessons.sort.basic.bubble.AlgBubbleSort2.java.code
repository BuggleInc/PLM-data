public void bubbleSort2() {

	int compteurBoucle =0;
	boolean aucunEchange = false;
	int tab = getNombreValeurs();

	while(!aucunEchange)
	{
		aucunEchange = true;
		compteurBoucle++;
		for(int i=0 ; i < tab -1 - compteurBoucle ; i++)
		{
			if(getValeur(i)>getValeur(i+1))
			{
				echange(i,i+1);
				aucunEchange = false;
								
			}
			
		}
	}
	


}
