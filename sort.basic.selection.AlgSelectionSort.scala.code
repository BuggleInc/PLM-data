def selectionSort() {
  for(N<-0 to getNombreValeurs()-2)
  	{
      var pluspetit = getValeur(N)
      var indice=N
      for(i<-N to getNombreValeurs()-1)
      	{
          if(getValeur(i)<pluspetit)
          	{
              indice= i
              pluspetit = getValeur(i)
            }
        }
      echange(N,indice)
    }
}