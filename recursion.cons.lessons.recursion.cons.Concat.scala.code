def concat(l1:List[Int],  l2:List[Int]): List[Int] = {
  def last(l:List[Int]): Int = {
  	if(l.tail==Nil){
   	 return l.head
  	}else{
    	return(last(l.tail))
  	}
	}
  def butLast(l:List[Int]): List[Int] = {
  	if(l.tail==Nil){
  	  return Nil
 	 }
  	else{
   	 return (l.head)::butLast(l.tail)
  	}
	}
  if(l1==Nil){
    return l2
  }
  else{
    return concat(butLast(l1),last(l1)::l2)
  }
}