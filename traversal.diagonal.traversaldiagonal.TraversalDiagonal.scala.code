var diag = 0;
override def run() {
  val largeur : Int = getMondeLargeur()
  val hauteur : Int = getMondeHauteur()
  var cpt : Int = 0 
  var fini : Boolean = false
  while (!fini) {
  (getX(),getY()) match {
    case (6,6) => ecritMessage(cpt); fini = true
    case (_,0) => ecritMessage(cpt); cpt= cpt + 1; diag = diag+1; setPos(0,diag)
   
    case (_,_)=> ecritMessage(cpt); cpt=cpt+1; setPos(getX()+1,getY()-1)
  }
  }}