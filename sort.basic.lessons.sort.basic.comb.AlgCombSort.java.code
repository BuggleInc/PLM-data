public void combSort() {
  double ecart = getValueCount(); 
  boolean swap = true;
  do{
    if(ecart >1){
      ecart = (ecart/1.398);
    }
    int gap = (int)ecart;
    int i = 0 ;
    swap = false;
    while(i+gap < getValueCount()){
      if(isSmaller(i+gap,i)){
        swap(i+gap,i);
        swap = true;
      }
      i++;
    }
  }while((ecart > 1)||swap);
}