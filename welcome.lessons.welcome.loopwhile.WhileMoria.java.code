// isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle(). 
public void run() { 
	// Turn around!
	right();
	right();

	// Find first baggle
	while (!isOverBaggle()) {
		forward();
	}

	// Move baggle
	pickupBaggle();
	while (!isOverBaggle()) {
		backward();
	}
	
	// Move forward one cell (so in a free space)
	forward();
	dropBaggle();
}
