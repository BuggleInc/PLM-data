def Moria(){
  while(getCouleurSol()!=Color.green){
    if (estFaceMur==true && estSurBiscuit==false){
    droite()
    avance()
    gauche()
    avance()
  }
    else rameneBiscuit()
  }
}

def rameneBiscuit(){
  retourne
  do {
    avance()
  }
  while (!estSurBiscuit() && estFaceMur==false)
  prendBiscuit()
  retourne()
  while (!estSurBiscuit()){
    avance
  }
  recule()
  poseBiscuit()
}

Moria()