def cocktailSort() {
  var mouv:Boolean=true
  var N=getNombreValeurs()
  while(mouv){mouv=false
             for(x<-0 to N-2){if(plusPetit(x+1,x)){echange(x,x+1);mouv=true}}
             for(x<-0 to N-2){
               var j=N-2-x
               if(plusPetit(j+1,j)){echange(j,j+1);mouv=true}}
             }
}