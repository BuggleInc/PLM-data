def cocktailSort() {
	var sorted = false
	while (!sorted){
		sorted = true
		for (i <- 0 to getNombreValeurs()-2){
			if (plusPetit(i,i+1) == false){
				echange(i,i+1)
				sorted = false
			}
		}
		for (i <- getNombreValeurs()-2 to 0 by -1){
			if (plusPetit(i,i+1) == false){
				echange(i,i+1)
				sorted = false
			}
		}
	}	
}
