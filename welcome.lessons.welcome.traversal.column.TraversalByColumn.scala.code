override def run() {
	def positionFinale():Boolean = {
		if ((getX() == getMondeLargeur) && (getY() == getMondeHauteur)){
			return true
		}
		return false
	}
	def cpt = 0
	while (positionFinale == false){
		for (i <- 1 to getMondeLargeur){
			for (i <- 1 to getMondeHauteur)
			ecritMessage()=cpt
			cpt=cpt+1
			setPos(getX(), getY()+1)
		}
	}
}
