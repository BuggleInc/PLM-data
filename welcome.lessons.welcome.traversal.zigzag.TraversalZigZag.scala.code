override def run() {
  var ord=getY()
  var abs=getX()
  var l=getMondeLargeur()
  var m=0
  var d=0
  while (arrive()==false){
    ecritMessage(m)
    m=m+1
    if (d==l-1){
      ord=ord+1
      setPos(abs,ord)
      d=0
    }
    else {
    abs=abs+1-2*(ord%2)
    setPos(abs,ord)
    d=d+1
  }
 }
  ecritMessage(m)
}

def arrive():Boolean={
  var h=getMondeHauteur()
  var l=getMondeLargeur()
  if (getX()==l-1 && getY()==h-1){
    return true
  }
  else return false
}
