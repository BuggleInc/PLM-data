def gnomeSort() {
	var length = getNombreValeurs();
	var i = length / 2;
	var swaped = true;
	do{
		if(i==0){
			i += 1;
		}else if(i == length){
			i -= 1;
		}
		swaped = false;
		if(plusPetit(i + 1, i)){
			i += 1;
			echange(i+1, i);
			swaped = true;
		}else if(plusPetit(i, i-1)){
			i -= 1;
			echange(i, i-1);
			swaped = true;
		}
	}while(swaped);
}
