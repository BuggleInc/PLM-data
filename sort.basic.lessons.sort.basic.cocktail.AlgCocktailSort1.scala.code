def cocktailSort() {
  var trie: Boolean=false
  while(trie==false){
    trie = true
    for (i<-0 to getNombreValeurs()-2){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)

      }
    }
    for (i<-getNombreValeurs()-1 to 1 by -1){
      if(!plusPetit(i-1,i)){
        echange(i+1,i)
        trie = false
      }
    }
  }
}

