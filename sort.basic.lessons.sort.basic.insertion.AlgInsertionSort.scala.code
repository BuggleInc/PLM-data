def insertionSort() {
	val longueur:Int = getNombreValeurs();
	var tmp:Int = 0;
	var lastCase:Int = 0;
	for (i <- 1 to longueur -1) {
		tmp = getValeur(i);
		for (j<- 0 to i) {
			if (!plusPetit(getValeur(i-j),tmp)){
				setValeur(i-(j-1),getValeur(i-(j-2)));
				lastCase = i-(j-2);
			}
		}
		 setValeur(lastCase,tmp);
	}
	
}
