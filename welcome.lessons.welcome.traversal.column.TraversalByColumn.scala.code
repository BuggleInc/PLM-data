override def run() {
  var cpt=0
  var x=0
  var y=0
  ecritMessage(cpt)
  setPos(x,y+1)
  while (fin()==false){
    if (getY()==getMondeHauteur()){
      x=x+1
      y=0
      setPos(x,y)
      cpt+=1
      ecritMessage(cpt)
    } else {
      y=y+1
      setPos(x,y)
      cpt+=1
      ecritMessage(cpt)
    }
  }
}

def fin(){
  if (getX()==getMondeLargeur() && getY()==getMondeHauteur()){
    return true
  } else return false
}

