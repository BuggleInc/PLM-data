def cocktailSort2() {
  var d:Int=0
  var c:Int=1
  var f:Int=getNombreValeurs()-2
  while( c==1){
    c=0
  for (i <-d to f){
    if(plusPetit(i+1,i)){
      echange(i,i+1)
      
      c=1
    }
  }
    
  for (i <-d+1 to f){
    if(plusPetit(f -i+1,f -i)){
      echange(f-i+1,f-i)
      c=1
      
}
       }

    f-=1
    d+=1
       }
}