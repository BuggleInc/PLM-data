def solve() {
	val l=getTaillePile();
	var p=0;
	for (i<-0 to l-2) {
		if ((!estTriee())&&(getRayonCrepe(l-1-i)!=(l-i))) {
			for (j<-0 to l-i-1) {
				if (getRayonCrepe(j)==l-i) {
					p=j;
				}
			}
			if (p!=0) {
				retourne(p+1);
			}
			if (!estCrepeRetournee(0)) {
				retourne(1);
			}
			retourne(l-i);
		}
	}
	for (i<-0 to l-2) {
		if (estCrepeRetournee(i)) {
			retourne(i+1);
			retourne(1);
			retourne(i+1);
		}
	}
	if (estCrepeRetournee(0)) {
		retourne(1);
	}
}