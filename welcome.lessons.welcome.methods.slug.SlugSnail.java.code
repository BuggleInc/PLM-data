public void hunt(Color c) {
  while(!estSurBiscuit()){
    if(isFacingTrail(c)){
      move(1,true);
    }else{
      gauche();
    }
  }
 }

boolean isFacingTrail(Color c){
  return true;
}

void move(int nbPas,boolean versLAvant){
  int i=0;
  while(i!=nbPas){
    i++;
    if(versLAvant==true){
    	avance();
    }else{
    	recule();
    }
  }
}