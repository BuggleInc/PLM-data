override def run() {
  var T =Array[Color](getCouleurSol)
  avance()
  while ( T(0)!=getCouleurSol ){
    T=T:::(getCouleurSol)
    avance()
  }
  for (i<-0 to getMondeHauteur()-1){
    gauche()
    avance()
    droite()
    baisseBrosse()
    for (j<- 0 to getMondeHauteur()-1){
      avance()
      setCouleurBrosse(T(j))
    }
    leveBrosse()
    avance()
  }
}