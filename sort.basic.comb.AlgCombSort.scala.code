def combSort() {
  var ecart : Int = getNombreValeurs()
  var ech : Int = 0
  var i : Int = 0
  do{
    ech = 0
    if (ecart >1){
      ecart = ecart / 1.3
    }
    i = 0
    while (i+ecart < getNombreValeurs()) {
      if (plusPetit(i+ecart, i )) {
        echange(i+ecart,i)
      }
      i=i+1
    }
    }
  while (ecart > 1 || ech != 0 )
}