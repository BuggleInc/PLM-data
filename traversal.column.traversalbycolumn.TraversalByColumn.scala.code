override def run() {
  ecritMessage("0")
  var cpt = 0
  while (!posFinale()) {
    	posSuivante()
    	cpt += 1
    	ecritMessage(cpt)
    	
  }
}


def posFinale() : Boolean = {
  return ((getMondeLargeur() - 1 == getX()) && (getY() == getMondeHauteur() - 1))
}



def posSuivante() {
  if (getY() == getMondeHauteur() - 1) {
    	setPos((getX()+1), 0)
  }
  else {
  		setPos(getX(), (getY()+ 1))
  }
}