public void bubbleSort2() {
	boolean sorted = false;
	 for(int i = 0; i < getValueCount() ; i++){ 
	 	sorted = false;
	 	int stepLast = getValueCount() - i - 1;
	 	for(int j = 0; j < stepLast ; j++)
	 		if(isSmaller(j + 1,j)){
	 			swap(j,j+1);
	 			sorted = true;
	 		}
	 	if(!sorted){
	 		break;
	 	}
	 }
}
