def estAdo(a:Int): Boolean =
{
  return (a >= 13 && a <= 19)
}

def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var compteur:Int = 0
  
  if (estAdo(a))
  	compteur = compteur + 1
  if (estAdo(b))
  	compteur = compteur + 1
  if (estAdo(c))
  	compteur = compteur + 1
  if (estAdo(d))
  	compteur = compteur + 1
  
  return compteur
}