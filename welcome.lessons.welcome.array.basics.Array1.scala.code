override def run() {
  var a = Color.blue
  var i = 0
  var j = 0
  var h = getMondeHauteur()
  var l = getMondeLargeur()
  var T = new Array[Color](h)
  for (i<- 0 to T.length-1){
    a = getCouleurSol()
    T(i) = a
    avance()
  }
  for (i<- 1 to l){
    gauche()
    avance()
    droite()
    avance()
    for(j<-1 to h){
    	tampon(T,h)
    	avance() 
    }
  }
}

def tampon(t:Array[Color],H:Int){
  setCouleurBrosse(t(H-1)
  baisseBrosse()
  leveBrosse()
}