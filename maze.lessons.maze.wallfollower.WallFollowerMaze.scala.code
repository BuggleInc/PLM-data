override def run() {
  
  while(estSurBiscuit==false){
    ouAvance() match{
      case 0 => gauche();avance()
      case 1 => avance()
      case 2 => droite();avance()
      case 3 => retourne();avance()
    }
  }
  
  prendBiscuit
}

def ouAvance():Int ={
  gauche()
  if (estFaceMur==false){ return 0 }
  else{
    droite()
    if(estFaceMur==false){ return 1 }
    else {
      droite()
      if (estFaceMur==false){ return 2 }
      else{return 3}
    }
  }
}