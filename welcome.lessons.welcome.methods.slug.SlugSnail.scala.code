def hunt(c:Color) 
{
    while(!estSurBiscuit())
    {
	    	if(isFacingTrail(c) == false)
	    	{
	    		droite()
	     }
	     else if(isFacingTrail(c) == true)
	     {
	    		avance()
	    		baisseBrosse()
	    		leveBrosse()
	     }
    }
    if(estSurBiscuit())
    {
    		prendBiscuit()
    }
}

def isFacingTrail(c:Color):Boolean = 
{
	if(estFaceMur())
	{
		return false
	}
     avance()
     if(getCouleurSol() == c)
     {
    		recule()
    		return true
     }
     else
     {
    		recule()
	    	return false
     }
}
