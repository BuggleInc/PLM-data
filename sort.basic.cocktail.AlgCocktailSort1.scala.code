def cocktailSort() {
  var a = false
  do {
    a = true
  for (i <- 0 to getValueCount()-2) {
    if (isSmaller(i+1,i)){
      a = false
      swap(i,i+1)}
		if (isSmaller(getValueCount()-1-i,getValueCount()-2-i)){
      a = false
      swap(getValueCount()-1-i,getValueCount()-2-i)}
    
  }


    
  
} while (a == false)
}