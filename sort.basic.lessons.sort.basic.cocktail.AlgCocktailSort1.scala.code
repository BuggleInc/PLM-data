def cocktailSort() {
	
	var n : Int =getNombreValeurs()
	var i :Int =n-2	
	var c1:Int=0
	var c2:Int=0

	do{
		for(i <- 0 to n-2 ){
			c1=0
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				c1=c1+1		
						}
				}
		
		for(i <- n-2 to 0){
			c2=0
			if(!plusPetit(i,i+1)){
				echange(i+1,i)
				c2=c2+1		
						}
			
				}
			
		



	} while(c1!=0 )
}
