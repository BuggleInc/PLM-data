def bubbleSort()
{
  var n = getNombreValeurs()-1;
  var isEchange = true;
  
  while (isEchange)
  {
    isEchange = false;
    
    for (i <- 0 to n)
    {
      if (plusPetit(i+1,i))
      {
        echange(i+1,i);
        isEchange = true;
      }
    }
  }
}




