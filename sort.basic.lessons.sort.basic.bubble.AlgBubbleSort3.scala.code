def bubbleSort3(){	
	var i:Int = getNombreValeurs() ;
	var hasSwap:Boolean = false ;
	var nBoucle:Int = 0;
	
	while(nBoucle != i)
	{
		for (j <- i-1 to nBoucle+1 by -1)
			{
				if (plusPetit(j,j-1))
				{
					echange(j-1,j);
					hasSwap = true;
				}
			}
		nBoucle = nBoucle + 1;
		if (!hasSwap)
			return;
		hasSwap = false;
		
	}
	
}