def faireV(c:Color) {
	setCouleurBrosse(c);
	avance();
	baisseBrosse();
	leveBrosse();
}

override def run() {
	for (x<-0 to 1) {
		for (x<-0 to 1) {
			faireV(Color.red);
			faireV(Color.blue);
			gauche();
			faireV(Color.blue);
			gauche();
			faireV(Color.green);
			gauche();

			avance(5);
		}
		gauche();
		avance(5);
		droite();
	}
}
