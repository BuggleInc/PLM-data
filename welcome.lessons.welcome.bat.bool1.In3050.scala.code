def in3050(a:Int, b:Int):Boolean = {
  if (int3040(a,b)==true || int4050(a,b)==true){
    return true
  }
  else return false
}
  
  
def int3040(a:Int, b:Int):Boolean = {
  if (Math.abs(35-a)<6 && Math.abs(35-b)<6){
    return true
  }
  else return false
}
 
def int4050(a:Int, b:Int):Boolean = {
  if (Math.abs(45-a)<6 && Math.abs(45-b)<6){
    return true
  }
  else return false
}