def cocktailSort2() {
  var debut=0;
  var fin= getNombreValeurs()-2
  var c = 1
  do {
    c = 0
    for (i <- debut to fin){
      if (plusPetit( i+1, i)){
        echange( i+1, i)
        c +=1
      }
    }
    fin -= 1
    for (j <- debut to fin){
      if (plusPetit(fin-j, fin-j+1)){
        echange(fin-j+1, fin-j)
        c += 1
      }
      }
    debut +=1
    } while (c != 0)
  }