override def run() {
  var i = 0
  var cpt=0
    while(i<=6){
      if (getX()==0){
        for (j<-0 to 6){
          setPos(j,i)
          ecritMessage(cpt)
    			cpt=cpt+1
        }
        i=i+1
      }else if(getX()==6){
        for (k<-6 to 0){
          setPos(k,i)
          ecritMessage(cpt)
    			cpt=cpt+1
      	}
        i=i+1
    	}
  	}
}