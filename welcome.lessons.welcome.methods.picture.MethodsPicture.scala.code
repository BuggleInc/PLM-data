val colors = Iterator(Color.YELLOW, Color.RED, Color.BLUE, Color.GREEN)

override def run() {

  for (color <- colors){
  	makeV(color)
  }
 
  forward(5)
}



def makeV(c: Color){
  setBrushColor(c)
  forward()
  brushDown()
  brushUp()
  forward()
  left()
  forward()
  brushDown()
  brushUp()
  backward()
  right()
  forward()
  brushDown()
  brushUp()
  forward()
  left()
}