override def run() {
	var j:Int = 0;
	var flag:Boolean = true;
	while (!(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1)) {
		ecritMessage(j);
		if (getY() == getMondeHauteur()-1) {
			setPos(getX() +1, getY);
			flag = false
		}
		else if (flag == true) {
			setPos(getX(), getY() + 1);
		}
		else setPos(getX(), getY() - 1);
	j = j + 1;
	}
	ecritMessage("48");
}

