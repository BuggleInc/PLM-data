def combSort() {
  var ecart = getNombreValeurs();
  var n = getNombreValeurs();
  var i = 0
	var  parcours = true
  while (ecart>1 || parcours) {
 		
   	ecart=(ecart.toDouble/1.27).toInt 
    
 		i=0
  	
 		while (i+ecart<n) {
     	 parcours = false
 			 if (getValeur(i)>getValeur(i+ecart)) {
 	  		 echange(i,i+ecart)
         parcours = true
 	  	 }
     i=i+1 
    }
  }
}
