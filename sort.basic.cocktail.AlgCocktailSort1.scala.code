def cocktailSort() {
  var i=0
  var n=getNombreValeurs()
  var m=0
  var o=0
  if (m!=(n-1)){
    m=0
    for (i <- 0 to n-2){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
      }
      else {
        m+=1
      }
    }
  }
  if (o!=(n-1)){
    o=0
    for (i <- n-2 to 0 by -1){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
      }
      else {
        o+=1
      }
    }
  }
}




