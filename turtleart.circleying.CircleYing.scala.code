override def run()
{
	cercleTroue2();
  zigZag();
}

def cercleTroue2()
{  
  for (i <- 1 to 181)
  {
    avance(2);
    droite(1);
	}
  
  leveCrayon();
  for (i <- 1 to 3)
  {
    avance(2);
    droite(1);
	}
  baisseCrayon();
  
  for (i <- 1 to 176)
  {
    avance(2);
    droite(1);
	}
}

def zigZag()
{  
  for (i <- 1 to 180)
  {
    avance(1);
    droite(1);
	}
  avance(1);
  for (i <- 1 to 180)
  {
    avance(1);
    gauche(1);
	}
}