override def run() {
  nord()
  while(!estSurBiscuit()) {
    keepHandOnSideWall()
  }
  def keepHandOnSideWall() {
    while(estFaceMur()) {droite()}
    avance()
    gauche()
  }
  def nord() {
    while (getY()!=0) {
      var posa=getY()
      while (getY()<=posa) {
        setDirection(Direction.NORTH)
    		while (!estFaceMur()) {avance()}
        keepHandOnSideWall()
      }
    }
  }
  

  prendBiscuit()

}