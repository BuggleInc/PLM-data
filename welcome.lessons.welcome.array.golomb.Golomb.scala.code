

def golomb(num:Int): Int = {
	
var suite = new Array[Int](num + 1);
var sol = 0;

	suite(1) = 1; 

		for(i <- 1 to num)
		{

			if(suite(i-1) < nbOccurs(suite, i-1))
			{
				suite(i) = suite(i-1);
			}
			else if(suite(i-1) == nbOccurs(suite, i-1))
			{
				suite(i) = suite(i-1) + 1;
			}
			
		}

	sol = suite(num);
}
def nbOccurs(table : Array[Int], n:Int):Int ={

	var count = 0;
	for(i <- 0 to table.length - 1){

		if(table(i)==n){
			count = count + 1;
		}
		
	}
	return count ;
}























