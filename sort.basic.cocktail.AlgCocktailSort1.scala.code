def cocktailSort() {
  var k = true;
  var j = 0;
  val len = getValueCount();

  while(k){
    k = false ;
    for(i<- 0 to len-2){
      if(!isSmaller(i, i+1)){
        swap(i,i+1);
        k = true ;
      } 
    }
    for(i<- 0 to len-2){
      j = len-2-i ;
      if(!isSmaller(j, j+1)){
        swap(j,j+1);
        k = true ;
      } 
    }
    
  }
}