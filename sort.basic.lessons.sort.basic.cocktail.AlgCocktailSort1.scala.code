def cocktailSort() {
  
  var swapped= false
  var c:Int=getValueCount()-2
  do {
    swapped=false;
    for (i<-0 to c){
      if (!isSmaller(i,i+1)) {
        swap(i,i+1);
        swapped= true      
      }
    }
    for (i<-c to 0 by (-1)){
       if (!isSmaller(i,i+1)) {
      swap(i+1,i)
      swapped= true      
      }
    }
    
    
    
  }while (swapped);
  
}
