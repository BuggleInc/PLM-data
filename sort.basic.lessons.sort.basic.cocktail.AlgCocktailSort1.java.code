public void cocktailSort() {
	boolean swapped = false;
	int passes = 1;
	do {
		//swapped = false;
		System.out.println("Iterations next round: " + (getValueCount() - passes));
		for (int i = 0; i < getValueCount() - 2; i++) {
			if (!isSmaller(i, i+1)) {
				swap(i, i+1);
				swapped = true;
			}
		}
		for (int i = getValueCount() - 2; i > 0; i--) {
			if (!isSmaller(i, i + 1)) {
				swap(i, i + 1);
				swapped = true;
			}
		}
		passes++;
	} while(swapped&&passes < 10);
}

