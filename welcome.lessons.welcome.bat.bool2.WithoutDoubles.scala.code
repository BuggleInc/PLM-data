def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
  if (die1!=die2 & noDoubles==false) {
    return (die1+die2)
  } else {
    if (die1==die2 & noDoubles==true & die1!=6) {
      return die1=die1+1
    } else {
      if (die1==die2 & noDoubles==true & die1=6)
    } return die1=1
  }
}
