var finished : Boolean = false
var m:String =""
while (!finished){
  m=readMessage()
  if (m=="R") {right();forward()}
  else if (m=="L"){left();forward()}
  else if (m=="I"){
    right();right();forward();
  }
  else if (m=="A")forward();
  else if (m=="B"){forward();forward();}
	else if (m=="C"){forward();forward();forward()}
	else if (m=="Z")backward();
  else if (m=="Y"){backward();backward();}
	else if (m=="X"){backward();backward();backward();} 
else finished = true}
