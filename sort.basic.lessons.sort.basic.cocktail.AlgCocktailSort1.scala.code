def cocktailSort()
{
  var cpt =0
  var chgt = true
  while (chgt == true)
  {
  if (cpt%2==0 && chgt == true)
  {
  	chgt = false
  	cpt +=1
  	for (k <-0 to getNombreValeurs()-2)
  	{
  		if (!plusPetit(k,k+1))
  		{
  			echange(k,k+1)
  			chgt = true
  		}
  	}
  }
  if (cpt%2 == 1 && chgt == true)
  {
  	chgt = false
  	for (k <-getNombreValeurs()-2 to 0 by -1)
  	{
  		if (!plusPetit(k,k+1))
  		{
  			echange(k,k+1)
  			chgt = true
  		}
  	}
  	cpt+=1
  }
  }
  return
}
