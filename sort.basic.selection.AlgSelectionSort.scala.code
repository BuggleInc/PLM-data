def selectionSort() {
  var lgr=getNombreValeurs
  var indice=0
  
  var min=0
  var posMin=0
  var posAvant=0
  
  while (indice!=lgr-1){
    min=getValeur(indice)
    posMin=indice
    for (i<- (indice+1) to lgr-1){
      if (plusPetitQue(i+1,min)){
        posAvant=posMin
        posMin=indice+1
        min=getValeur(indice-1)
      }
    }
    echange(posAvant,posMin)
    indice+=1
  }
}