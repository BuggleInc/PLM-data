public void run() {
	keepHandOnSideWall();
}


void keepHandOnSideWall(){
	while(!isOverBaggle()){
		if(wallOnLeft()){
			stepForward();
			left();
		} else {
			forward();
		}
	}
	pickupBaggle();
}


void stepForward(){
	if(!isFacingWall())
		forward();
}

void faceEmptyAndLeftWall(){
	while(isFacingWall() && !wallOnLeft())
		left();
}

boolean wallOnLeft(){
	left();
	boolean a = isFacingWall();
	right();
	return a;
}
