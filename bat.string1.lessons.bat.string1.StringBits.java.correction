$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class stringBits extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( stringBits((String)t.getParameter(0)) ); }
	String stringBits(String str) {
		String result = "";
		
		for (int i=0; i<str.length(); i+=2) {
			result = result + str.substring(i, i+1);
			
		}
		return result;
	}
 }