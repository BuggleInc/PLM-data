def carre() {
  faireF(Color.red);avance();avance();gauche();avance();retourne();faireF(Color.blue);recule();recule();gauche();avance();retourne();faireF(Color.green);avance();avance();avance();gauche();faireF(Color.yellow);avance();avance();avance();gauche();
}

def prochaincarred() { for (i <- 1 to 4) {avance()}
  
}

