public void bubbleSort()  {
    int length = getValueCount();
    boolean swap = false;
    do {
        swap = false;
        for(int i = 0; i<length-1; i++) {
            if(!isSmaller(i,i+1)) {
                swap(i,i+1);
                swap = true;
            }
        }
    } while(swap);
}
