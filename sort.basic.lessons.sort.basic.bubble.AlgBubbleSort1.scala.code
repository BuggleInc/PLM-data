def bubbleSort()  {
  var temp:Int =0;
  var incr:Int =0;
  do{
    incr=0;
  	for(i <- 0 to getNombreValeurs()-1){
  	  	if(getValeur(i)>getValeur(i+1)){
        	temp=getValeur(i);
      	  setValeur(i,getValeur(i+1));
      	  setValeur(i+1,temp);
      	  incr=incr+1;
    	  }
  	}
  }while(incr!=0)
}