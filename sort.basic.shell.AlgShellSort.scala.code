def shellSort()  {
  var n:Int=getNombreValeurs()
  var ecart:Int=n
  var compteur:Boolean=true
  while (ecart>0){
    ecart=(ecart.toDouble/2.0).toInt
    insertionSort()
  }
  
}

def insertionSort() {
  var n:Int=getNombreValeurs()
  for ( k <- 1 to (n-1)){
    var stock:Int=getValeur(k)
    var l:Int=k-1
    while ((l>=0)&&(stock<getValeur(l))){
      setValeur(l+1,getValeur(l))
      l=l-1
    }
    setValeur(l+1,stock)
  }
}