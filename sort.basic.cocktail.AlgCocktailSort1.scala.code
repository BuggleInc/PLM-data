def cocktailSort() {
  var n = getNombreValeurs()-1
  var b = true
  while(b)
  {
    b = false
    for( i <- 0 to n-1)
    {
      if(plusPetit(i+1,i))
      {
        echange(i,i+1)
        b = true
      }
    }
    for(i <- n-1 to 0 by -1)
    {
      if(plusPetit(i+1,i))
      {
        echange(i,i+1)
        b = true
      }
    }
  }
}