override def run() {
  while (!testMurGauche() && !estFaceMur)
  {
    avance()
  }
  droite()
  while (!estSurBiscuit())
  {
    keepHandOnSideWall()
  }
  prendBiscuit()
}


def keepHandOnSideWall()
{  
  var murGauche = testMurGauche()
  if (estFaceMur())
  {
    droite()
  }
  if (!murGauche)
  {
    gauche()
    avance()
  }
  if (murGauche && estFaceMur())
  {
    droite()
  }
  if (murGauche && !estFaceMur())
  {
    avance()
  }

}


def testMurGauche() : Boolean =
{
  gauche()
  if (estFaceMur())
  {
    droite()
    return true
  }
  droite()
  return false
}