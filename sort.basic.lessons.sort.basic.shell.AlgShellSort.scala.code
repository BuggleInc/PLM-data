def shellSort() {
	val l=getNombreValeurs();
	var v=0;
	var w=0;
	var j=0;
	var b=true;
	var ecart=l/2;
	while (ecart>0) {
		for (i<-1 to l-1) {
			v=getValeur(i);
			b=true;
			j=i;
			while (b) {
				b=false;
				if (j==0) {
					setValeur(0,v);
				} else if (plusPetitQue(j-1,v)) {
					w=getValeur(j);
					setValeur(j,v);
					v=w;
					b=true;
				} else {
					copie(j-1,j);
					j-=1;
					b=true;
				}
			}
		}
		ecart/=2;
	}
}
