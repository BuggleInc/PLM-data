override def run() {
  val xMax=getMondeLargeur()-1
  val yMax=getMondeHauteur()-1
  var posX = 0
  var posY = 0
  while(!((posX == xMax) && (posY == yMax))) {
    ecritMessage(posX+(yMax+1)*(posY))
    if(posX<xMax) {
      posX += 1
    }
    else {
      posX = 0
      posY += 1
    }
    setPos(posX,posY)
  }
}

