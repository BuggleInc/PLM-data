def pentaKoch(levels:Int, length:Double) {
	if (levels==0) {avance(length); return}
	pentaKoch(levels-1, length*0.4)
	gauche(120)
	pentaKoch(levels-1, length*0.4)
	droite(120)
	pentaKoch(levels-1, length*0.4)
	droite(120)
	pentaKoch(levels-1, length*0.4)
	droite(120)
	pentaKoch(levels-1, length*0.4)
	gauche(90)
	pentaKoch(levels-1, length*0.4)
}
