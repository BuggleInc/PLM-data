def golomb(num:Int): Int = {
 	var aMettre = 0
  var i = 2
  var indiceGauche :Int  = 2
  
  var T = new Array[Int](2 * num);
for (i <- 0 to T.length-1) {
  T(i) = 0
 
}
  T(0 )= 1
  T(1) =2
  T(2)=2
  while (T(num) == 0)
  		{
        aMettre = T(i)
        for (i <- indiceGauche + 1 to indiceGauche + aMettre )
        {T(i) = i + 1}
        indiceGauche = indiceGauche + aMettre
        i = i + 1 
				        
      }
  
  return T(num)
  }