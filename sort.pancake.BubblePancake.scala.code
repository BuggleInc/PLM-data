def solve() {
	var n =getTaillePile()
  while (!estTriee()) {
    	for (i<- 0 to n-2) {
        if (plusGrand(i,i+1)) {
      		retourne(i)
  				retourne(2)
    			retourne(i)
        }
      
    }
  }

  
  
  
}



def plusGrand(i,j) : Int = {
	var x = getRayonCrepe(i)
  var y = getRayonCrepe(j)
  return (x>y)
}

