def combSort() 
{
	var i = 0
	var fini = false
	var ecart = getNombreValeurs()

	do
	{
		if(ecart>1)
			ecart = (ecart.toDouble / 1.3).toInt
		i = 0
		

		while(i+ecart < getNombreValeurs())
		{
			fini = true
			if(plusPetit(i+1,i))
			{
				echange(i,i+1)
				fini = false
			}
			i += 1
		}
	}while(ecart>1 || !fini)
}

