def combSort() {
  var ecart:Int = getNombreValeurs()
  var fini:Boolean=false
  var nop:Int=0
  
  while (ecart>1 || !fini){
    ecart = (ecart.toDouble / 1.3).toInt
    var i:Int=0
    while(i+ecart<getNombreValeurs()){
      if(!plusPetit(i, i+ecart)){
        echange(i,i+ecart)
        nop=1
      }
      i+=1
    }
    if(nop==0){
      return true
    }
    else{
      nop=0
    }
  }
}