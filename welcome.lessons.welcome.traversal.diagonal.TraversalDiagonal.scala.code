var diag = 0;
override def run() {
  var diag=0
  var y=0
  var x=0
  var compteur=0
  ecritMessage(compteur)
  compteur+=1
  while (diag!=getMondeHauteur()-1){
    for(i<-1 to diag){
      for(j<-0 to i){
        setPos(i,j)
        ecritMessage(compteur)
        compteur+=1
      }
    }
    diag+=1
  }
}