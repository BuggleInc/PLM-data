def golomb(num:Int):Int = {
  if (num==1) {
    return 1
  }
  if (num==2) {
    return 2
  }
  var y:Int = golomb(num-1)
  var w:Int = golomb(y)
  var z:Int = chercheGolomb(y-1)
  if ((num-1-z)<w) {
    return y
  }
  else {
    return y+1
  }
}

def chercheGolomb(y:Int):Int = {
  var cpt:Int = 1
  var rep:Int = golomb(cpt)
  while (rep!=w) {
    cpt+=1
    rep=golomb(cpt)
  }
  return cpt
}