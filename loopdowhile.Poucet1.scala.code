var s=0
do{
  for (stepper<-1 to 5){
    if (isOverBaggle()){
      s=s+1
      if (s <= 2){
        right()
        for (stepper1 <- 1 to 5){
          forward()
        }
      }else {
        left()
        for (stepper2 <- 1 to 5){
          forward()
        }
      }
    }
  }
  
}while (!exitReached())