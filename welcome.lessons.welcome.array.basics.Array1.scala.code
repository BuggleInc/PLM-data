override def run() {
  var x:Int = getMondeHauteur()
  var T = new Array[Color](x+1)
  for(k<- 1 to x){
  	T(0)=getCouleurSol()
    for(i<- 1 to T.length-1){
    	avance()
      T(i)=getCouleurSol()
  	}
  	T(x)=T(0)
  	gauche()
  	avance()
  	droite()
  	for(j<- 0 to T.length-1){
    	setCouleurBrosse(T(j))
    	baisseBrosse()
    	leveBrosse()
    	avance()
  	}
	}
}