$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class nearHundred extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( nearHundred((Integer)t.getParameter(0)) );		 }
	boolean nearHundred(int n) {

		return (90<=n && n<=110)||(190<=n&&n<=210);
	}
 }