override def run() {
	def finale():Boolean = {
		var NORD:Int = 0
		if(estFaceMur && getDirection() == Direction.NORTH) {
			return true
		}
		return false	
	}
	def snake() {
		while(!estFaceMur) {
			avance()
		}
		if(getDirection() == Direction.EST) {
			gauche()
			avance()
			gauche()
		}
		else {
			droite()
			avance()
			droite()
		}
	}
	while(!finale()) {
		snake()
	}
}

	
		
