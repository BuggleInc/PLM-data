def cocktailSort3() {
	
var c=true
var d=true
var debut=0
var fin=0
	var a=getNombreValeurs()
	
	while (b&&c) {
		
		c=false
		d=false	
		
		for (i<-debut to a-2-fin) {if (plusPetit(i+1,i)) 
		{echange(i+1,i); d=true }}
