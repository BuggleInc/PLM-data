override def run(){
  for (i <- 1 to 4){
    faireMotif()
  	avance(4)
		if (i%2==0){
      gauche()
      avance(4)
      droite()
    }
  }
}


def faireV(c:Color){
  avance(2)
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  gauche()
  recule()
}

def faireMotif(){
  	faireV(Color.BLUE)
    faireV(Color.GREEN)
    faireV(Color.YELLOW)
    faireV(Color.RED)
}