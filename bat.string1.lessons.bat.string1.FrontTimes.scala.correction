$package
import plm.universe.bat.{BatEntity,BatWorld,BatTest}; 
import plm.universe.World; 
class frontTimes extends BatEntity { 
   override def run(t: BatTest) {
   t.setResult( frontTimes( t.getParameter(0).asInstanceOf[String], t.getParameter(1).asInstanceOf[Int]) )
   }
def frontTimes(str:String, n:Int):String = {
  var frontLen = 3
  if (frontLen > str.length)
    frontLen = str.length
  var front = ""
  if (str.length >0)
    front = str.substring(0,frontLen)
  return front * n
} }