override def run() {
  var H=getMondeHauteur()
  var L=getMondeLargeur()
  var count = 0
  while (PositionFinale==false){
    ecritMessage(count)
    count=count+1
    if (getX()+1==L){
      setY(getY()+1)
    }
    else if (getY()%2==1){
      setX(getX()+1)
    }
    else if (getY()%2==0){
      setX(getX()-1)
    }
  }
  ecritMessage(count)
}


def PositionFinale() : Boolean = {
  var H=getMondeHauteur()
  var L=getMondeLargeur()
  if (getX()+1==L && getY()+1==H){
    return true
  }
  return false
}