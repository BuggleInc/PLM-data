def array123(nums:Array[Int]): Boolean = {
  var a=false
  var b=false
  var c=false
	for(i<-0 to nums.length-1){
    if (nums(i)==1){
 			a=true
    }
    else if (nums(i)==2){
 			b=true
    }
    else if (nums(i)==3){
 			c=true
    }
  }
  if (a==true && b==true && c==true){
    return true
  }
  else return false
}