def estAdo(n:Int) : Boolean = {
  if (n>=13 && n<=19){
    return true
  }else return false
}

def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var nbAdo : Int = 0
  if (estAdo(a)){
    nbAdo=nbAdo+1
  }
  if (estAdo(b)){
    nbAdo=nbAdo+1
  }
  if (estAdo(c)){
    nbAdo=nbAdo+1
  }
  if (estAdo(d)){
    nbAdo=nbAdo+1
  }
  return nbAdo
}