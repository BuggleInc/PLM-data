override def run() {
  baisseBrosse()
  var x = true
  while(x){
  	while(!estFaceMur()){
    	avance()
  	}
  	if (estFaceMur()){
      if(getDirection() == Direction.EST){
        gauche()
        if(!estFaceMur()){
          avance()
        	gauche()
        }
      }else{
        droite()
        if(!estFaceMur()){
          avance()
        	droite()
        }
      }
     	
    }else if(getCouleurSol() == Color.red){
      x = false
    }
  }
 droite() 
}