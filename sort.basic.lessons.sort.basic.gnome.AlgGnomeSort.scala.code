def gnomeSort() {
  var nain=0
  var lgr=getNombreValeurs()
  var mvt:Boolean=false
  while(nain!=lgr){
    mvt=false
    if (nain==0){
      nain+=1
   	  mvt=true}
    if (nain==lgr){return}
    if (plusPetit(nain,nain-1)){
      echange(nain,nain-1)
      nain-=1
    }else if(!mvt){nain+=1}

  }
}