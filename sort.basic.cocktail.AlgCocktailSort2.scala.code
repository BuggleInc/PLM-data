def cocktailSort2() {
  var echangefait:Boolean = true
  var debut:Int= 0
  var fin:Int=getNombreValeurs()
  do {
    echangefait= false
    for (i <- debut to fin) {
      if (plusPetit(i+1, i)){
        echange(i, i+1)
        echangefait=true
        fin=fin-1
      }
    }
    for (i <- 0 to fin) {
      if (plusPetit(fin-1-i, fin-2-i)){
        echange(getNombreValeurs()-2-i, getNombreValeurs()-1-i)
        echangefait=true
        debut=debut+1
      }
  	}
  } while (echangefait)
}