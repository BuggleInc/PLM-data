def cocktailSort2() {
  var n = getNombreValeurs()
  var end = true
  var tour = 0
  
  while(end)
  {
    end = false
    for(i<-0+tour to n-2-tour)
    {
     	if (plusPetit(i,i+1)==false)
      {
        echange(i,i+1)
        end = true
      }
    }
    for(i<-2+tour to n-tour)
    {
      var j = n - i 
      if (plusPetit(j,j+1)==false)
      {
        echange(j,j+1)
        end = true
      }
    }
    tour = tour + 1
  }
}