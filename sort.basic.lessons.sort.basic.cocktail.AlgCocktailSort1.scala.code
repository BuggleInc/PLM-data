def cocktailSort() {
var swap:Boolean =true;
while(swap){
	swap=false
	for(i<-0 to getNombreValeurs()-2){
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap=true
		}
	}
	for(i<-getNombreValeurs()-2 to 0 by -1){
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap=true
		}
	}
	if(swap==false){
		return
	}
}
}
