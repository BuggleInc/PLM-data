def Flower(c:Color){
  setCouleurBrosse(c)
  baisseBrosse();avance(2);recule();gauche();avance();recule(2);avance()
  setCouleurBrosse(Color.yellow);droite()
  leveBrosse()
}
def growFlowers(){
  Flower(Color.red);avance(3);Flower(Color.cyan);recule(3);droite();avance(3)
  gauche();Flower(Color.orange)
}