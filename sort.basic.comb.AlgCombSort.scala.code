def combSort() {
  var ecart = lgr
  var cpt=0
  var i = 0
  do{cpt=0
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i=0
    do{
      if(plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        i+=1
        cpt=1
      }
    }while(i+ecart<lgr)
  }while(ecart >1 || cpt==1)
}