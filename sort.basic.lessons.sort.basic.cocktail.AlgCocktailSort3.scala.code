def cocktailSort3() {
  var swapped= false
  var j=getValueCount()-2
  var k=0
  do {
    swapped=false
    for (i <- k to j)
      if (! isSmaller(i,i+1)){
      swap(i,i+1)
        swapped=true;
        
    }
    j=j-1
    if (swapped==false){
      return}
    for (i<- j to k by -1)
      if (! isSmaller(i,i+1)){
        swap(i,i+1)
        swapped=true;
       
      }
     k=k+1
    if (k==j){
      return
    }
  }  while (swapped);
}
