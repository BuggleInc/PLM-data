def shellSort() {
  var ecart = getNombreValeurs()
  var v = 0
  while (ecart>0){
    ecart=(ecart.toDouble / 2).toInt
    for (i<-1 to getNombreValeurs()-1){
    	v = getValeur(i)
    	var j = i-1
    	while(j>=0 && !plusPetitQue(j,v)){
      	copie(j,j+1)
      	j-=1
    	}
    	j+=1
    	setValeur(j,v)
  	}
  }
}