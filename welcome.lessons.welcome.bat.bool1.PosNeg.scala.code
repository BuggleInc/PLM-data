def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
  negative match {
    case true => if ((a < 0) && (b < 0)) {return true} else {return false} ;
    case false => {if (a < 0) {if (b < 0) {return false} 
                                     else {return true } ; } ;
                         
    										 if (a > 0) {if (b > 0) {return false} 
                                     else {return true} ; }
                        }}}