def stringX(str:String):String =
{
  var s=""
  var  suppression=false;
 	var i=0;
  var j=str.length();
  
  if(str.drop(i).take(1)=="x")i+=1
  if(str.drop(j).take(1)=="x")j-=1;
  
 for(k <- i to j-1)
  {
    
    if(str.drop(k).take(1)!="x" )
    {
      s+=str.drop(k).take(1)
    }
    
  }

  return s
}