  var comptr = 0
  
  for (i <- 0 to getNombreValeurs()-2 by -1) {
    
    for (j <- 0 to i) {
      
    	if (!plusPetit(j,j+1)) {
          
      	echange(j,j+1)
          
      }
        
      else {
          
      	comptr = comptr + 1
          
      }
      
    	}
      
      if (comptr == getNombreValeurs - 1) {
        
        return
        
      }
      
		}
  
}