def cocktailSort3() {
	var debut=0
	var fin=getNombreValeurs()-2
	var inverse=true
	var table=false
do{
	for (i<-debut to fin){
		table=false
		if (!plusPetit(i,i+1)){
			echange(i,i+1)
			inverse=true
			table=true
		}
	}
	if (table==false){
			return
		}
	fin=fin-1
	for (i<-fin to debut by -1){
		if (!plusPetit(i,i+1)){
			echange(i,i+1)
			inverse=true
		}
	}
	debut=debut+1
	
}while(inverse)
}
