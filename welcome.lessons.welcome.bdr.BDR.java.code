char con = a;
while(con == a)
{
	char action = getIndication();
	switch(action)
	{
		case 'R' : right();break;
		case 'L' : left();break;
		case 'I' : left();left();forward();break;
		case 'A' : forward();break;
		case 'B' : forward(2);break;
		case 'C' : forward(3);break;
		case 'Z' : backward();break;
		case 'Y' : backward(2);break;
		case 'x' : backward(3);break;
		//default : con = b ; break;
	}
}
