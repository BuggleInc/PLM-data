def combSort11() {
  var len = getValueCount();
  var gap = len;
  var swapped = false;;
  
  do {
    swapped = false;
    
    if (gap > 1) {
      gap = (gap.asInstanceOf[Double] / 1.3).asInstanceOf[Int];
      if (gap == 9 || gap == 10) {
        gap = 11;
      }
    }
    
    var i = 0;
    while (i+gap < len) {
      if (isSmaller(i+gap, i)) {
        swap(i, i+gap);
        swapped = true;
      }
      i += 1;
    }
  } while (swapped || gap > 1);
}