public void run() {
	boolean loop = true;
	while (loop) {
		while(!estSurBiscuit() && !estDosMur()) {
			recule();
		}
		if (estSurBiscuit()) {
			prendBiscuit();
			while(!estSurBiscuit()) {
				avance();
			}
			recule();
			poseBiscuit();
			recule();
		} else {
			droite();
			recule();
			gauche();
			recule();
			loop = false;
		}
	}
}
