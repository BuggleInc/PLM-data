def surMurG() :
  
  '''
  Verifie que mur ? gauche
  '''
  
  ok = False
  left()
  if estFaceMur():
    ok = True  
  right()
  
  return ok
  
    
def cherche() :
  '''
  Positionne en face d'un couloir
  '''
  
  while estFaceMur() :
    right()


def keepHandOnSideWall() :
  '''
  Avance d'une case en gardant patte sur mur de gauche
  '''
  if surMurG() :
    if estFaceMur() :
      cherche()
      
    avance()

    
    
keepHandOnSideWall()
    
  