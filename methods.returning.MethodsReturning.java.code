boolean haveBaggle() {
  boolean vuBiscuit = false;
  for (int i = 0 ; i<6; ++i){
    for (int j = 0 ; j<6 ; ++j){
      if (estSurBiscuit()){
        vuBiscuit = true ;
      }
      avance();
    }
    for (int k = 0 ; k<6 ; k++){
      recule();
    }
    if (vuBiscuit){
      return vuBiscuit ;
    }
    droite();
    avance();
    gauche();
  }
  return vuBiscuit ;
}

