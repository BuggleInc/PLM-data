def gnomeSort() {
  var i=0

  var sorted=false
  while(!sorted){
    sorted=true

    if (i==getNombreValeurs()){
      sorted=true
    }
    else {
      if (i==0){
        i=i+1
      }
      else if (plusPetit(i+1,i+2)){
        i=i+1
      }
      else{
        echange(i+1,1+2)
        i=i-1
      }
      sorted=false
    }
  }

}