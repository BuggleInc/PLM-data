override def run() {
  var dir:Direction=Direction.NORTH;
  var angle=1;
  while (!isOverBaggle()) {
    while (!isFacingWall()) {
      setDirection(Direction.NORTH)
      forward()
    }
    do {
      if (!isFacingWall()) {
        forward();left();angle = angle -1
        if (isFacingWall()) {
          right; angle=angle+1;
        }
        else {
          forward()
        }
      }
      else {
        right(); angle=angle+1
      }
    } while (angle!=0)
  }
}