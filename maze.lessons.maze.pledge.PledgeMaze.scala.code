override def run() {
	def murAGauche():Boolean={
		gauche()
		if(estFaceMur()){
			droite()
			return true
		}else{
			droite()
			return false
		}
	}
	def keepHandOnSideWall(){
		if (murAGauche()){
			if(!estFaceMur()){
				avance()
			}else{
				droite()
				sommeAngle = sommeAngle - 1
			}
		}else{
			gauche()
			sommeAngle = sommeAngle + 1
			if(!estFaceMur()){
				avance()
			}
		}
	}

	
}
