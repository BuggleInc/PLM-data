def combSort() {
  var ecart=getNombreValeurs()
  var i=0
  var a=false
  do{
    i=0
  if (ecart>1){
    ecart = (ecart.toDouble / 1.25).toInt
  }
    a=false
  for (i <- 0 to getNombreValeurs()-ecart-1){
    if (!plusPetit(i,i+ecart)){
      echange(i,i+ecart)
      a=true
    }
    i=i+1
    }
} while (ecart>1 || a==false)
}