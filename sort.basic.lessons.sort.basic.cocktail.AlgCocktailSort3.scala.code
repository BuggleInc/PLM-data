def cocktailSort3() {
var ok:Int=0;
var debut:Int=0;
var fin:Int=getNombreValeurs()-2;
do{
	ok=1;
	for(i<-debut to fin){
		if(plusPetit(i+1, i)){
			echange(i+1,i)
			ok=0;
		}
	}
	return 0;
	fin-=1;
	for(i<-fin to debut by -1){
		if(plusPetit(i+1, i)){
			echange(i+1,i)
			ok=0;
		}
	}
	debut+=1;
}while(ok==0)
}
