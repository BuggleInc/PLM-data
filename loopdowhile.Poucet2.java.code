public void run() { 
  int compteurBiscuit = 0;
  while (!sortieTrouvee()){
    do {
      avance();
      if (estSurBiscuit()){
        compteurBiscuit++;
      }
    }
    while (!crossing());
      if (compteurBiscuit < 3){
        droite();
      }
      else {
        gauche();
     }
    compteurBiscuit = 0;
    if (sortieTrouvee()){
      avance();
    }
   }
}