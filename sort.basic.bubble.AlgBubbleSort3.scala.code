def bubbleSort3()  {
  var length = getNombreValeurs()
  var nb_echange = 0
  var i=2
do 
  {
    	nb_echange = 0
     for(k<-0 to (length-i))
      {
          
        if (!plusPetitQue(k,getValeur(k+1)))
          {
            echange(k,k+1)
            nb_echange = nb_echange+1
          }
      }
    i=i+1

   
 } while (nb_echange!=0)
  	
}