override def run() {

def fonctionTest():Boolean = {
	(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1)
}

var i = 0
var j = 0
var cpt = 0


while (!fonctionTest) {
	setPos(Math.abs(i),j)
	ecritMessage(cpt)
	i += 1
	cpt += 1
	if (i == getMondeLargeur()-1) {
		i = -getMondeLargeur()
		j += 1
	}
}

}

