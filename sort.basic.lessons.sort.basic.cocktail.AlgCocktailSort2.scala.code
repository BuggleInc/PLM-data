def cocktailSort2() {
  var debut=0;
  var fin=getNombreValeurs()-2;
  var over=false;
  while(over==false){
    over=true
    for(i<- debut to fin){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        over=false
      }
    }
    fin=fin-1
    for(i <- fin to debut by -1){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
      }
    }
    debut=debut+1
  }
}
