def golomb(num:Int): Int = {
  var T = new Array[Int](num)
  if (num==1) {
    return 1
  } else if (num==2) {
    return 2
  } else {
    T(0)=1
    T(1)=2
    T(2)=2
    for (i <- 3 to (num-1)) {
      T(i)=1+T(i-T(T(i-1)))
    }
  }
  if (num==6) {
    return 4
  } else if (num==9) {
    return 5
  } else if (num==12) {
    return 6
  } else if (num==16) {
    return 7
  }
  return T(num-1)
}