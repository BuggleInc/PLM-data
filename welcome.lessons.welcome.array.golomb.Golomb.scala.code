def golomb(num:Int): Int = {
  num match{
  	case num if num == 1 => 1
  	case num if num == 2 => 2
    case num if num == 3 => 2
  	case _ => terme(num)
	}
}

def terme(num:Int):Int={
  var T = new Array [Int] (num)
  T(0)=1
  T(1)=2
  T(2)=2
  var element:Int=3
  var cas:Int=3
  while (T(num-1)==0){
    for (i<-1 to T(element-1)){
      if (cas<=T.length-1){
        T(cas)=element
        cas=cas+1
      }
    }
  }
  return element
}
  
