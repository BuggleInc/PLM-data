def cocktailSort() {
  var n = getNombreValeurs()
  var end = true
  var tour = 0
  
  while(end)
  {
    end = false
    for(i<-0 to n-2-tour)
    {
     	if (plusPetit(i,i+1)==false)
      {
        echange(i,i+1)
        end = true
      }
    }
    for(i<-n-2-tour to 0)
    {
      if (plusPetit(i,i+1)==false)
      {
        echange(i,i+1)
        end = true
      }
    }
    tour = tour +1
  }
}
