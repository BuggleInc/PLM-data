override def run() {
  while (posFinale() == false) {
    	posSuivante()
  }
}


def posFinale() {
  return ((getMondeLargeur() == getX()) && (getY() == getMondeHauteur()))
}

def posSuivante() {
  val x = getX()
  val y = getY()
  if (getY() == getMondeHauteur()) {
    	setPos((x+1), 0)
  }
  else
  		setPos(getX(), (y+ 1))
}