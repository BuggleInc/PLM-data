def cocktailSort2() {
	var d=0;
	var fin=getNombreValeurs()-2;
	var verif=false;
	do{
		verif=false;
		for (i <- d to fin by 1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				verif=true;
			}
		}
		fin = fin-1;
		for (i <- fin to d by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				verif=true;
			}
		}
		d=d+1;
	} while (verif)
}
