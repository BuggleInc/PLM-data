def estAdo(x):
    if x<=19 and x>=13:
        return True
    else:
        return False    
def countTeen(a, b, c, d):
    compteur=0
    if estAdo(a)==True:
        compteur=compteur+1
    if estAdo(b)==True:
        compteur=compteur+1
    if estAdo(c)==True:
