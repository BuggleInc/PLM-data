def shellSort()  {
      var gap = a.length/2
    while (gap >=1) {
      for (i <- gap until a.length){
        var j= i -gap
        var tmp = a(i)
        while(j>=0 && tmp < a(j)) {
          a(j+gap) = a(j)
          j-=gap
        }
        a(j+gap)=tmp
      }
      gap = (gap/2.2).round.toInt
    }
  }
}