def monkeyTrouble(aSmile:Boolean, bSmile:Boolean): Boolean = {

  if(aSmile){
    if(bSmile){
      return true
    }
    else{
      return false
    }
    
  }
  else if (!aSmile){
  	if(!bSmile){
      return true
    } 
    else {
      return false
    }
  }  
  
  else{
    if(!bSmile){
      return true
    }
    else {return false
    }
  
  }
}