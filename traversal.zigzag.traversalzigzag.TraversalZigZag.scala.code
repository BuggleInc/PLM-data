override def run() {
  var cpt = 0
  var y = 0
  var finale = 0
  var pair = 0
  while (finale==0) {
		for (i<-0 to 6) {
      if (pair == 0) {
 	     setPos(i,y)
      } else {
        setPos(6-i,y)
      }
      ecritMessage(cpt)
      cpt = cpt + 1
    }
    if (pair==0) {
      pair = 1
    } else {
      pair = 0
    }
    if (cpt<48) {
      y = y + 1
      setPos(0,y)
    } else {
      finale=1
    }
  }
}
