def combSort() {
	var ecart=getNombreValeurs();
	var verif=false;
	do {
		verif=false;
		if (ecart>1) {
			ecart= (ecart.toDouble / 1.3).toInt;
		}
		var i=0;
		while (plusPetit(ecart+i,getNombreValeurs())) {
			if (plusPetit(i+ecart,i)){
				echange(i,i+ecart);
				verif=true;
			}
			i=i+1;
		}
	} while (ecart>1 | verif)
}
