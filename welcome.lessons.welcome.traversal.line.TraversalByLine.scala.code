override def run() {
  var xf:Int = getWorldWidth() - 1
  var yf:Int = getWorldHeight() - 1
  var counter:Int = 0
  
  while (true){
      
   	writeMessage(counter)
    
    if (xf == getX() && yf == getY()){
      return
    }
    
  	if (getX() + 1 <= xf){
      setPos(getX() + 1, getY())
      
    }else{
      setPos(0, getY() + 1)
    }
    
    counter += 1
	}
  
}
