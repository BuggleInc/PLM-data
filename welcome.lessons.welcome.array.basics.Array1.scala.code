override def run() {
  var tailleTableau=0
  tailleTableau = getMondeHauteur()
  tableau = Array[Color](tailleTableau)
  for (pas <-0 to tailleTableau -1){
    tableau(pas)=getCouleurSol()
    avance()
  }
  for(colonne<-1 to getMondeLargeur()-1){
    gauche()
    avance()
    droite()
    colonneSuivante()
  }
}

def colonneSuivante(){
  avance()
  for(pas <-0 to tailleTableau -1){
    setCouleurBrosse(tableau(pas))
    baisseBrosse()
    leveBrosse()
    avance()
  }
}