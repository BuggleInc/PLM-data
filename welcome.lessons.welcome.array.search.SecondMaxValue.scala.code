def max2Value(nums:Array[Int]): Int = {
  
  for (i<-0 to nums.length -2){
    if (nums(i+1)<nums(i)){
      nums(i+1)=nums(i)
    }
  }
  var compteur=0
  while (compteur<nums.length -1){
    if (nums(compteur)!= nums(compteur+1)){
    	return nums(compteur+1)
    }else{
      compteur=compteur+1
    }
  }
  return nums(0)
}