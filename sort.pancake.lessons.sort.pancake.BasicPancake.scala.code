def solve() {
	def chercheRayonMax(rang:Int):Int = {
		var max = rang

		for (i <- rang + 1 to getTaillePile() - 1) {
			if (getRayonCrepe(i) > getRayonCrepe(max)) max = i
		}

		return max
	}

	var rangInit = 0
	
	while (!estTriee()) {
		retourne(getTaillePile() - chercheRayonMax(rangInit) - 1)
		retourne(getTaillePile() - rangInit)
		rangInit = rangInit + 1
	}
}
