def carre() {
  for (i<-0 to 3) {
    avance(100)
    droite(90)
  }
}




override def run() {
  for (i<-0 to 1) {
    carre() ; droite(90) ; avance(100) ; droite(90)
  }
  droite(180) ; avance(100) ; droite(90) ; avance(100) ;droite(90) ; avance(200)
  droite(90) ; avance(100) ; droite(90) ; avance(100) ; droite(90) ; avance(100)
  droite(180) ; avance(100) ; droite(90) , droite(180)
}