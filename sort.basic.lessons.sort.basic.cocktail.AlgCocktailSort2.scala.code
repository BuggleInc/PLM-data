def cocktailSort2() {
	var etape:Int=0
	var compteur:Int=1
	while (compteur!=0) {
		compteur=0
		for (x<-0 to getNombreValeurs()-2) {
			if (plusPetit(x,x+1)) {}
			else {
				echange(x,x+1)
				compteur=compteur+1
			}
		}
		for (x<-getNombreValeurs()-2 to 0 by -1) {
			if (plusPetit(x,x+1)) {}
			else {
				echange(x,x+1)
				compteur=compteur+1
			}
		}	
	}
}
