def bubbleSort3()  {
  var compt = getValueCount()
  var swapped=false;
  for (i<- 0 to getValueCount()-1) {
    
    for (i<-0 to compt-2) {
      
      if (!isSmaller(i,i+1))  		  
          {
            swap(i,i+1);swapped=true;
          }
      
    }
    if (!swapped) {return}
    compt=compt-1
  } 
}