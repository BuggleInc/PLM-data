override def run() {
  
  
  
  def posFinal() : Boolean =  {
    
    var H = getMondeHauteur();
  	var L = getMondeLargeur();
        
    if ((getX() == L) && (getY() == H)) {
      return true
    }
    return false
    
  }
  
  var cpt = 0
  
  while(!posFinal()) {
    
    ecritMessage(cpt);
    
    if (getY() == getMondeHauteur()) {
      
      setPos(getX()+1, 0)
      
    } else {
  
      setY(getY()+1)
    }
    cpt += 1
    
    
    
    
  }
  
}