public void run() {
	 for (int i = 0; i < 4; i++) {
	 	makeV();
	 }
}

void makeV(){
	for (int i = 0; i < 6; i++){
		forward();
		if (i == 1 || i == 3) setMark();
		if (i == 2){
			left();
			forward();
			setMark();
			backward();
			right();
		}
	}
	left();
}

void setMark(){
	brushDown();	
	brushUp();
}
