def cocktailSort2() {
  var lgr=getNombreValeurs()
  var debut=0
  var fin=lgr-2
  var parcours:Boolean = true
  do{
    parcours=false
    for (i<-debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        parcours=true
        fin=fin-1
      }
    }
    for (k<-fin to debut by -1){
      if (plusPetit(k+1,k)){
        echange(k+1,k)
        parcours=true
        debut=debut+1
      }
    }
  }while (parcours)
}