public void run() { 
  int compteurBiscuit = 0;
  while (!sortieTrouvee()){
    for (int pas = 0; pas<5; pas++){
    	avance();
    	if (estSurBiscuit()){
      	compteurBiscuit++;
    	}
    }
    if (compteurBiscuit < 3){
      droite();
    }
    else{
      gauche();
    }
     compteurBiscuit = 0;
    if (sortieTrouve()){
      avance()
    }
	}
}