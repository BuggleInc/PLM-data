def shellSort()  {
  var i=getNombreValeurs()-1
  var v=0
  var ecart=getNombreValeurs()/2
  while(ecart>0){
    for(i <- getNombreValeurs()-1 to ecart+1 by -1){
      if(plusPetit(i,i-ecart)==true){
        v=getValeur(i)
        for(j <- i-ecart to i){
          copie(j,j+1)
        }
        setValeur(i-ecart,v)
      }
    }
    ecart=ecart/2
  }
}