def tree(steps:Int, length:Double, angle:Double, shrink:Double) {
  
  if(steps > 0) {
    
   	current(steps)
 		forward(length)
  
 		right(angle)
	
 		tree(steps-1, shrink*length, angle,shrink)

   	left(2*angle)

		tree(steps-1, shrink*length, angle,shrink)
  	
		right(angle)
		current(steps)
		backward(length)
    
  }
  
}