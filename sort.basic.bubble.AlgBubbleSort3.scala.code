def bubbleSort3() {
  var aInverse = false
  val n = getNombreValeurs()
  for (i <- n - 1 to 1 by -1) {
    aInverse = false
    for (j <- 0 to i - 1) {
      if (getValeur(j+1) < getValeur(j)) {
        echange(j+1,j)
        aInverse = true
      }
    }
    if (!aInverse) {
      return
    }
  }
}