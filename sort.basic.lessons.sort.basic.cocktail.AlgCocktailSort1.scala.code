def cocktailSort() {
var a=0
var b=0
var fin=8
var x=false
do{
	x=false
	
	for (n<-0 to fin){
	a= getValeur(n)
	b= getValeur(n+1)
	if(a>b){echange(n,n+1); x=true}
	}
	
	fin=fin-1
	
	for (n<-fin to 0){
	a= getValeur(n)
	b= getValeur(n+1)
	if(a>b){echange(n,n+1); x=true}
	}
	
}while(x==true && fin>0)

}
