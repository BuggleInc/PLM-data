def testPos() : Boolean ={
  var h: Int= getMondeHauteur()-1
  var l: Int = getMondeLargeur()-1
  if (getX == l & getY == h) {
    return true
  }
  return false
}



def deplace(){
  var x : Int=0
  var y : Int=0
  if (getY < getMondeHauteur-1){
    y= y+1
    setPos(x, y)
  }
  else{
    y=0
    x=x+1
    setPos(x, y)
  }
}



override def run() {
  ecritMessage(0)
  var c : Int=1
  do {
    deplace();    
    ecritMessage(c)
    c = c+1
  }
  while (!testPos)
}