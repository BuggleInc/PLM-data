def combSort() {
  var ecart = getNombreValeurs();
  var t=false
  do  {
    t=false
    if (ecart>1) {
    	ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
  	}
  	var i = 0
  	while (i+ecart<getNombreValeurs()) {
      if (!plusPetit(i:Int,i+ecart:Int)) {
      	echange(i:Int,i+ecart:Int)
        t=true
    	}
    	i=i+1;
  	}
  }
  while (ecart>1 || t==true  ) 
}