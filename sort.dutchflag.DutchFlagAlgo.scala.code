def solve() {
  var taille = getTaille()
  var apresBleu = 0
  var blancRouge = taille - 1
  var avantBlanc = taille - 1
  while (apresBleu <= avantBlanc) {
    if (getCouleur(apresBleu) == BLEU) {
      apresBleu += 1
    } else if (getCouleur(apresBleu) == ROUGE) {
      echange(apresBleu,blancRouge)
      blancRouge -= 1
    } else {
      echange(apresBleu,avantBlanc)
      avantBlanc -= 1
    }
  }
}