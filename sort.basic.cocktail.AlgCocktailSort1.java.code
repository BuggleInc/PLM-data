public void cocktailSort() {
  var verif:Boolean = false;
  var n:Int = getValueCount() ;
  do {
    verif = false;
    for(i<-0 to n-2) {
      if isSmaller(i,i+1) {
        swap(i,i+1);
        verif = true;
      }
    }
    
    for (i<- n-2 to 0) {
      if isSmaller(i,i+1) {
        swap(i,i+1);
        verif = true;
      }
    }
  } while (!verif)
}