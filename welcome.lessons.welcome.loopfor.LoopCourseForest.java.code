public void run() {
int laps = 7;
int steps = 16;
int sides = 4;

	//loop for laps
	for(int x = 1; x <= laps; x++){
		//loop all four sides
		for(int y = 1; y <= sides; y++){
			//loop for one side
			for(int z = 1; z <= steps; z++){
				forward();
				if(z == 4){left();}
				if(z == 6){right();}
				if(z == 10){right();}
				if(z == 12){left();}
				if(z == 16){left();}
			}
		}

	}
}
