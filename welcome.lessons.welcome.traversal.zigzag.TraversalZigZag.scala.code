def testPose() : Boolean = {
  var h: Int= getMondeHauteur()-1
  var l: Int = getMondeLargeur()-1
  if (getX == l & getY == h) {
    return true
  }
  return false
  
}


def position(){
  if (getX < getMondeLargeur-1){
    setPos(getX+1, getY)
  }
  else{
    setPos(0, getY+1)
  }
}


override def run() {
  ecritMessage(0)
  var c : Int=1
  
  do{
     position()
     ecritMessage(c)
     c = c+1
    
  } while ( !testPose)
}