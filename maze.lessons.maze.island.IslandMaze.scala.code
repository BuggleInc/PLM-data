override def run() {

	def keepHandOnSideWall() {
		gauche()
		if (!estFaceMur()) {avance()}
		else {
			droite()
			if (!estFaceMur()) {avance()}
			else {droite()}
		}
	}

	while (!estSurBiscuit()) {
		while(getY!=0) {
			while(!estFaceMur()) {avance()}
			droite()
			while(getDirection()!=Direction.NORTH) {keepHandOnSideWall()}
		}
	}

	prendBiscuit()

}
