override def run() {
  while (getY()!=0 && !estSurBiscuit()){
    if (!estFaceMur()){
      setDirection(Direction.NORTH)
      avance()
    }else{
      while(estFaceMur()){
        KeepHandOnSideWall()
      }
     avance()
  }
  }
  while(!estSurBiscuit()){
    KeepHandOnSideWall()
  }
  prendBiscuit() 
  gauche()
}

def KeepHandOnSideWall(){
  if (!estFaceMur()){
    avance()
    gauche()
  } else{
    droite()
  }
}