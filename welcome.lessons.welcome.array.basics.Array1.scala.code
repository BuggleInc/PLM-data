}
def tableaucouleur(){
  var T=Array[Color](6)
  for (i <- 0 to 4){
    T(i)= getCouleurSol()
    avance()
  }
  T(5)=getColeurBrosse()
  for (i <- 0 to 5){
    recule()
  }
}

def peindre(){
  for (i <- 0 to 4){
    baisseBrosse()
    setCouleurBrosse(T(i))
    leveBrosse()
    avance()
  }
  baisseBrosse()
  setCouleurBrosse(T(5))
  leveBrosse()
  for (i <- 0 to 5){
    recule()
  }
}