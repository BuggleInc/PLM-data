def cocktailSort2() {
var a=true
var b=true
    if (a==false && b==false){
      return
    }
a=false
b=false
  for (j <- 1 to getValueCount()-1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true
		}
}
	for (j <- getValueCount()-1 to 1 by -1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      b=true
		}
}      
}