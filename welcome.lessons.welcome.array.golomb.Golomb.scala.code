def golomb(num:Int): Int = {
  var tab = new Array[Int](num)
  var repere = 0
  tab(0)=1
  for (k <- 1 to 1) {
    tab(k) = 2
  }
  for (k <- 0 to num-1) {
    var occ = tab(k)
    for (i <- repere to repere+occ) {
      if (i <= num-1) {
      	tab(i) = k
    }
    }
    repere = k
  }
  return tab(num-1)
}