override def run() {
def finale():Boolean = {
		if(getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur - 1) {
			return true
		}
		else return false
	}
	var c:Int = 0
	while(!finale) {
		ecritMessage(c)
		c = c + 1
		setPos(getX() + 1,getY())
		if(getX() == getMondeHauteur - 1) {
			ecritMessage(c)
			c = c + 1
			if(getY() < getMondeHauteur - 1) {
			setPos(0,getY() + 1)
			}
		}
		
	}
}
