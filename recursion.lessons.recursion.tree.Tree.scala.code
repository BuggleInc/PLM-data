def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {

if(steps != 0){
	current(steps)
	avance(length)
	droite(angle)
	tree(steps-1,length*shrink,angle,shrink)
	gauche(angle)
	leveCrayon()
	recule(length)
	baisseCrayon()
	gauche(2*angle)
	tree(steps-1,length*shrink,angle,shrink)
	avance(length)
	
	recule(length)
	droite(angle)	
	recule(legnth)
	
}
}
