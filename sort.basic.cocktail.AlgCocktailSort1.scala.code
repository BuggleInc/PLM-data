def cocktailSort() {
  do {
    var sp=false
    for (i <- 0 to getValueCount()) {
      if (!isSmaller(i,i+1)) {
        swap(i,i+1)
        sp=true
      }
    }
    for ( i <- 0 to getValueCount()) {
      if (!isSmaller(getValueCount()-i-1,getValueCount()-i) {
        swap(getValueCount()-i-1,getValueCount()-i)
        sp=true
      }
    }
  } while (sp == true)
}