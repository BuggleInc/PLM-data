public void run() {
	faireCarre(false);
  droite(90);
  faireCarre(false);
  gauche(180);
  faireCarre(true);
  faireCarre(false);
  droite(180);
}


public void faireCarre(boolean gauche)
{
  baisseCrayon();
  for(int i = 0; i <= 3; i++)
  {
    avance(100);
    if (!gauche)
    	droite(90);
    else
      gauche(90);
  }
}