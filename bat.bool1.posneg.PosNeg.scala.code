def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
  if ((a>=0 && b<=0) || (b>=0 && a<=0))
  	{
      if (negative)
      	{
          return false
        }
      else{
        return true
      }
    }
  else if (negative && a<=0 && b<=0)
  	{
      return true
    }
  else{
    return false
  }
}