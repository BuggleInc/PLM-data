def cocktailSort3() {
  var debut = 0;
  var fin = getNombreValeurs()-2;
  var trie = false;
  
  do
  {    
    trie = false;
  	for(i <- debut to fin)
  	{
			if(!plusPetit(i,i+1))
   	 	{
   			echange(i,i+1);
        trie = true;
     	}
      if(!trie)
      return;
   	}
		fin-=1; 
    
    
  	for(i <- fin to debut by -1)
  	{
    	if(!plusPetit(i,i+1))
    	{
      	echange(i,i+1);
        trie = true;
    	}
      if(!trie)
      return;
  	}
		debut+=1;
    
  }while(trie)
}