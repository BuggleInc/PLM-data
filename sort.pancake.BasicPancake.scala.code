def solve() {
  while (isSorted()==false){
    var i =1
    var max=0
    var n =1
    for (i <- n to getStackSize()-1) {
      if (getPancakeRadius(i)>max){
      max=i
      }
    }
    if (i==0){
      flip(i+1)
      flip(i+1)
    }
    else if (i==getStackSize()-1){
      return 0
    }
    else {
    flip(i)
    flip(i+1)
    }
  }
}
      
    