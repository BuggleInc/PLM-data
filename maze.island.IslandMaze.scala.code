var etat = 0
override def run() {
  while (!estSurBiscuit){
  	etat match {
    	case 0 => auNordChef()
    	case 1 => aGaucheToute()
  	}
  }
  prendBiscuit()
}

def aGaucheToute(){
    droite()
  while (getDirection==Direction.NORD && !estFaceMur()){
    gauche()
    while(estFaceMur()){
      droite()
    }
   	forward()
  }
}

def auNordChef(){
  while (!estFaceMur){
    forward()
  }
  etat=1
}