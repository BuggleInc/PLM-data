def combSort() { 
  var ecart:Int = getNombreValeurs()
  var i:Int = 1 
  while(ecart > 1|| i >0 )  {
     if (ecart > 1){ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]}
    i = 0
    while(i+ecart < getNombreValeurs()){
      if (!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        i +=1
      }
    }
  }
   
}