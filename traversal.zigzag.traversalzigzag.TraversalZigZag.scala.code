def posTest():Boolean = { 
  if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {
  	return true
  }
  return false
}



def colonne() {
	var j=0
  var cpt=0
  
  while (!posTest()) {
    for(i<-0 to 6) {
      setPos(i,j)
      
      ecritMessage(cpt:Int)
    	cpt=cpt+1
    }
    
    j=j+1
  }
}
      











override def run() {
  colonne()
  
}