def max2Value(nums:Array[Int]): Int = {
  var longueurT = nums.length
  var mUn = 0
  var mDeux = 0
  
  if (nums(0)>=nums(1)){
    mUn = nums(0)
    mDeux = nums(1)
  }else{
    mUn = nums(1)
    mDeux= nums(0)
  }
  
  
  //On parcoure tout le tableau
  for (i <- 0 to longueurT-1){
    if (nums(i)>mUn){
      mDeux = mUn
      mUn = nums(i)
    }
  }
  return mDeux
}