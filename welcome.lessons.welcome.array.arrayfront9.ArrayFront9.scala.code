def arrayFront9(nums:Array[Int]): Boolean = {
  if (nums == null)
  	return false
  if (nums.length > 3) {
    for (i <- 0 to 3) {
      if (nums(i) == 9)
      	return true
    }
    return false
  } else {
    for (i <- 0 to nums.length-1) {
      if (nums(i) == 9)
      	return true
    }
    return false
  }
}

