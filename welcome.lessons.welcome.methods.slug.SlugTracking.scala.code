def isFacingTrail():Boolean = {

  if (estSurBiscuit()){
    prendBiscuit()
    return true}
  else{
    avance()
    if(getCouleurSol()==Color.green){
      recule()
      avance()
      baisseBrosse()
      leveBrosse()
      return true}
    else {
      recule()
      gauche()
      return true}
    if (estFaceMur()){
      return false}
    else{
      return true}
  }
}



