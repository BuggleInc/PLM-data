def max2Value(nums:Array[Int]): Int = {
  var max:Int=nums(0)
  var max2:Int=nums(1)
  for (a<-2 to nums.length-1){
    if (nums(a)>max2){
      if (nums(a)<=max){
        max2=nums(a)
      }
      if (nums(a)>max){
        max2 = max
        max = nums(a)
      }
    }
  }
  return max2
                        }