public void run() { 
	while (true) {
		back();
		while (!isOverBaggle() && !isFacingWall())
			forward();
		if (isOverBaggle()) {
			pickupBaggle();
			back();
			while (!isOverBaggle())
				forward();
			backward();
			dropBaggle();
			backward();
		}
		else
			break;
	}
	right();
	forward();
	left();
	forward();
}
