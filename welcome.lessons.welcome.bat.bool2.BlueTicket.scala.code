def blueTicket(a:Int, b:Int, c:Int):Int = {
  if (dix(a,b) || dix(b,c) || dix(a,c)) { return 10}
  else if (dixplus(a+b,b+c) || dixplus(a+b,a+c)) {return 5}
  else {return 0}
}

def dix (a:Int,b:Int): Boolean= {
  if (a+b==10) {return true}
  else {return false}
}

def dixplus (a:Int,b:Int):Boolean={
  if (a+10==b || b+10==a) {return true}
  else {return false}
}