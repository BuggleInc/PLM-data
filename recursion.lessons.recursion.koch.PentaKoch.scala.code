def pentaKoch(levels:Int, length:Double) {
	if(levels==0){
		avance(length)
	}
	else{
		pentaKoch(levels-1,length*0.4)
		gauche(108)
		pentaKoch(levels-1,length*0.4)
		droite(72)
		pentaKoch(levels-1,length*0.4)
		droite(72)
		pentaKoch(levels-1,length*0.4)
		droite(72)
		pentaKoch(levels-1,length*0.4)
		gauche(108)
		pentaKoch(levels-1,length*0.4)
	}
	
}
