var message:String="";
var fini:Boolean=false
while (fini==false) {
	message=litMessage
	if (message== "Z")
		recule()
	if (message=="Y")
		recule(2)
	if (message=="X")
		recule(3)
	if (message=="A")
		avance()
	if (message=="B")
		avance(2)
	if (message=="C")
		avance(3)
	if (message=="R") {
		droite()
		avance()
	}
	if (message=="L") {
		gauche()
		avance()
	}
	if (message=="I") {
		droite()
		droite()
		avance()
	}
	if (message=="")
		fini=true
}

