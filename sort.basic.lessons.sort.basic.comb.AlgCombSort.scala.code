def combSort() {
  var ecart = getValueCount()
  var swapped = false
  do {
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i = 0
    swapped=false
    while (i+ecart<getValueCount()-1) {
      if (getValue(i)>getValue(i+ecart)) {
        swap(i,i+ecart)
        swapped = true
      }
      i+=1
    }
  } while (ecart>=1 || !swapped)
}