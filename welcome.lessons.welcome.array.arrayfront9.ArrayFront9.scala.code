def arrayFront9(nums:Array[Int]): Boolean = {
  
  var longueurTableau:Int = nums.length
  
  if ((longueurTableau >= 4) & ((nums(0) == 9) | (nums(1) == 9) | (nums(2) == 9) | (nums(3) == 9))) {return true}
  
  else if (longueurTableau <=3) {
    
    var compteur:Int = 0
   
    for (i <- 0 to longueurTableau -1) {
      if (nums(i) == 9) {compteur = compteur + 1}
    }
   
    if (compteur != 0) {return true}
   
    else {return false}
  }
  
  else {return false}
  
}