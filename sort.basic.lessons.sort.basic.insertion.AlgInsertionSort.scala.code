def insertionSort() {




  var nbVal = getNombreValeurs()

  for(i<-0 to nbVal-1 by 1){
    var copier = true
    
    var tmp = getValeur(i)
    
 		var n=1
    
    while((copier)&&(i-n>0)){
    //  println("i-n-1 : "+(i-(n+1))+"	i:"+i+"	n:"+n)
      if(plusPetitQue(i-n,tmp)){
        copie(i-n,i)
        n+=1
      }else{
        setValeur(i,tmp)
        copier=false
      }
    }
    
    
  }

}