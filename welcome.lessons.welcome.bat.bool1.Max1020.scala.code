def max1020(a:Int, b:Int):Int = {
  if (estDansInt(10,20,a)){
    if (estDansInt(10,20,b)){
      return Math.max(a,b);
    }
    else{
      return a;
    }
  }
  else{
        if (estDansInt(10,20,b)){
      return b;
    }
    else{
      return 0;
    }
  }
}

def estDansInt(a:Int, b:Int, x:Int):Boolean ={
  return x <= b && x >= a
}
