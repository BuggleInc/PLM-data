def shellSort()  {

  var lgr = getNombreValeurs();
  var ecart = lgr/2;
  var v = 0 ;
  var j = 1 ;
  while(ecart>0){
	for(i<- 1 to lgr-1){
    v = getValeur(i);
    while((j+ecart)<=i && v<getValeur(i-(j+ecart))){
      copie(i-(j+ecart),i-(j+ecart)+1);
      j += 1;
    }
    setValeur(i-(j+ecart)+1,v)
  }
    ecart -= 1;
  }
  
}