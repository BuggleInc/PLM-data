def combSort()
{
  var ecart = getNombreValeurs()
  var inversion = true
  
  while (ecart > 1 && inversion)
  {
    if (ecart > 1)
    	ecart = (ecart.toDouble / 1.3).toInt
    
    var i = 0
    inversion = false
    while (i + ecart < getNombreValeurs())
    {
    	if (plusPetit(getValeur(i + ecart), getValeur(i)))
      { 
        inversion = true
        echange(i, i + ecart)
    		i += 1;
      }
    }
  }
}