override def run() {
  baisseBrosse()
  while(!arrivee) {
    avance()
    if (estFaceMur && getDirection==Direction.EST) {
      gauche(); avance(); gauche()
    }
    if (estFaceMur && getDirection==Direction.OUEST) {
      droite(); avance(); droite()
    }
  }
  droite()
}

def arrivee():Boolean = {
  if (estFaceMur && getDirection()==Direction.EST) gauche()
  if (estFaceMur && getDirection()==Direction.OUEST) droite()
  if (estFaceMur) return true
  return false
}
