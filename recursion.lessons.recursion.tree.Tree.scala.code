def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	if (steps == 0)
	return
	else {
		avance(length);
		droite(angle);
		current(steps);
		tree(steps-1,shrink*length,angle,shrink);
		
		gauche(angle);
		recule(length);
		avance(length);
		gauche(angle);
		tree(steps-1,shrink*length,angle,shrink);
		current(steps);
		
		
	}
}
