def combSort() {
  var echanges:Boolean=true;
  var ecart:Int=getValueCount();
  var i:Int=0

  while((echanges==true) || ecart>1){
    echanges=false
    ecart = (ecart.toDouble / 1.3).toInt
    if(ecart<1){
      ecart=1;
    }
    while(i+ecart<getValueCount()){
    	if (isSmaller(i+ecart,i)){
        echanges=true;
      	swap(i,i+ecart);
    	}
      i+=1;
  	}
	}
}
