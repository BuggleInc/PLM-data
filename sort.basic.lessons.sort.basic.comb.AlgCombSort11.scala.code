def combSort11() {

  var ecart = getNombreValeurs()
  var sorted=false

  while(ecart>1 || !sorted ){
    sorted=true
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
      if (ecart==9 || ecart==10){
        ecart=11
      }
    }

    var i=0
    while (i+ecart<getNombreValeurs()){
      if (!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        sorted=false
      }
      i=i+1
    }

  }
}
