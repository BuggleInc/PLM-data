def interleavedHanoi(height, src1,src2, other, dst):
  hanoi(height-1,src1,other,dst)
  deplace(src1,dst)
  hanoi(height-1,src2,dst,src1)
  deplace(src2,dst)
  interleavedHanoi(height-1,other,src1,src2,dst)
