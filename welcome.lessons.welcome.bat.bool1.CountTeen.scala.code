def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var nbAdos:Int = 0
  if (estAdo(a)) {
    nbAdos = nbAdos + 1
  }; if (estAdo(b)) {
    nbAdos = nbAdos + 1
  }; if (estAdo(c)) {
    nbAdos = nbAdos + 1
  }; if (estAdo(d)) {
    nbAdos = nbAdos + 1
  };
  return nbAdos
}

def estAdo(n:Int):Boolean = {
  if (n >= 13 && n<= 19) {
    return true
  } else {
    return false
  }
}
