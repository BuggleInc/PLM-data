override def run() {
  while (!isOverBaggle()) {
    keepHandOnSideWall()
  }
  
  pickupBaggle();
}

def keepHandOnSideWall() {
  if (!isFacingWall()) {
    
    right();
    if (isFacingWall()) {
      
      left();
      forward();
    }
    else {
      left();
      forward();
      right();
      forward();
    }   
  }
  
  else {
    right();
  }
  
}