override def run() {
  var i=0
	while(posFinale()==false){
    ecritMessage(i+"")
    nextPos()
    i=i+1
  }
  ecritMessage(i+"")
}

def posFinale(): Boolean={
  if (getX()==getMondeLargeur-1 & getY()==getMondeHauteur()-1)
  	return true
  return false
}

def nextPos(){
  if (getY()==getMondeHauteur()-1){
    setPos(getX()+1,0)
  }
  else{
    setY(getY()+1)
  }
}
