def combSort() {
  var ecart = getNombreValeurs()
  val n = getNombreValeurs()
  var inverse = true
  var i = 0
  while (ecart >= 1 || inverse) {
    if (ecart > 1) {
      print ecart
      ecart = (ecart.toDouble / 1.3).toInt
    }
    inverse = false
    i = 0
    while (i + ecart < n) {
      if (getValeur(i) < getValeur(i+ecart)) {
        echange(i+ecart,i)
        inverse = true
        i += 1
      }
    }
  }
}
