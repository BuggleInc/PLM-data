def shellSort()  {
  var eca = getNombreValeurs()/2
  while (eca>0) {
    
    
     for (i<- eca-1 to getNombreValeurs() -1){
        
        
    var v = getValeur(i)
		var k = i
    
    
   while (k>= eca && (getValeur(k-eca)>v)){
      
      copy(k-eca,k)
      k-=eca
   }
		setValeur(k,v)
        
        
        
        
      
        
     }
    
        eca = eca/2
  
  
  }
}