override def run() {
  faireV(Color.YELLOW)
  faireV(Color.RED)
  faireV(Color.BLUE)
  faireV(Color.GREEN)
}

def faireMotif(c:Color){
  for (i<-1 to 4){
    faireV(c)
  }
  avance(7)
}

def faireV(c:Color){
  avance(2)
  marquer(c)
  gauche()
  avance()
  droite()
  avance()
  marquer(c)
  avance()
  droite()
  avance()
  marquer(c)
  gauche()
  avance(2)
  gauche()
}

def marquer(c:Color){
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
 
}