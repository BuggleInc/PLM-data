def array123(nums:Array[Int]): Boolean = {
  
  var Un = 0
  var Deux = 0
  var Trois = 0
  
  for (i <- 0 to nums.length-1){
    if (nums(i) == 1){
      Un = Un + 1
    } else if (nums(i) == 2){
      Deux = Deux + 1
    } else if (nums(i) == 3){
      Trois = Trois + 1
    }
  }
  if ((Un != 0)&&(Deux != 0)&&(Trois != 0)){
    return true
  }
  return false
}