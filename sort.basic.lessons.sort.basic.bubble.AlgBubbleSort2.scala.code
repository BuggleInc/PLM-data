def bubbleSort2() {
  var temp:Int =0;
  var incr:Int =0;
  var nbDeTour:Int =0;
  do{
    incr=0;
    nbDeTour=nbDeTour+1;
  	for(i <- 0 to (getNombreValeurs()-(1+nbDeTour))){
  	  	if(getValeur(i)>getValeur(i+1)){
        	temp=getValeur(i);
      	  setValeur(i,getValeur(i+1));
      	  setValeur(i+1,temp);
      	  incr=incr+1;
    	  }
  	}
  }while(incr!=0);
}