override def run() {
  var n :Int= getMondeHauteur()
  var T =new Array [Color](n)
  for (i<-0 to n-1){
    T(i)=getCouleurSol()
    avance()
  }
  
  def reproduireMotif(){
    for (k<-0 to n-1){
      avance()
      setCouleurBrosse(T(k))
      baisseBrosse()
      leveBrosse()
    }
  }
  gauche()
  avance()
  droite()
  for (compt<-1 to n-1){
    reproduireMotif()

  }
}