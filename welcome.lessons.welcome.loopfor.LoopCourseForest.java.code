public void run() { 
	int lap = 0;
	int step;
	while(lap < 7)
	{
		for(step =0; step <4;step++)
		forward();

		left();
		for(step=0;step<2;step++)
		forward();

		right();
		for(step=0;step<4;step++)
		forward();

		right();
		for(step=0;step <2;step++)
		forward();

		left();
		for(step=0;step<4;step++)
		forward();

		left();
		lap++;
	}
}
