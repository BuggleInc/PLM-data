public void run() { 
	while(!isOverBaggle() && !isFacingWall()){
		backward();
			if (isOverBaggle()){
				pickupBaggle();
				while(isCarryingBaggle() && !isOverBaggle()){
					forward();
				}
				if(isOverBaggle()){
					backward();
					dropBaggle();
					if (!isFacingWall()){
					backward();
					}

					if (isFacingWall()){
						right();
						forward();
						left();
						forward();
					}
				}
			}

	}
}
