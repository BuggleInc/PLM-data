def shellSort()  {
  var ecart=getValueCount()
  while (ecart>0){
    var n=getValueCount()
    var v=0
    var k=1
    var indice=0
    for (i<- ecart to n-1){
      k=1
      v=getValue(i)
      indice=i
      while (i-k*ecart>=0 && getValue(i-k*ecart)>v){
        setValue(i-k*ecart+ecart,getValue(i-k*ecart))
        indice=i-k*ecart
        k=k+1
      }
      setValue(indice,v)
    }
    ecart=ecart/2
  }
}