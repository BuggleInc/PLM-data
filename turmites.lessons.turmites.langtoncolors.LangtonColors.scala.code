def step(rule:Array[Char], colors:Array[Color]) {
	var I=0;
	for (i <- 0 to i) {
		if (getCouleurSol()==colors(i)) {
			I=i;
		}
	}
	if (rule(I)=="R") {
		setCouleurBrosse(colors(I));
		baisseBrosse();
		leveBrosse();
		droite();
		avance();
	}
	else if (rule(I)=="L") {
		setCouleurBrosse(colors(I));
		baisseBrosse();
		leveBrosse();
		gauche();
		avance();
	}	
}
