override def run() {
	rassemblage(); var y = getMondeLargeur(); var z = getMondeHauteur();
	for( j <- 1 to y){
		droite();avance();gauche();avance();
		for ( n <- 1 to z){
			pint();
		}
	}
}
def rassemblage(){
	var x = getMondeHauteur();var T = new Array[Color](x);
	for (i <- 0 to x-1){
		T(i) = getCouleurSol();avance();
	}
}
def pint(){
	var x = getMondeHauteur();val T = rassemblage();
	for ( k <- 0 to x-1){
		setCouleurBrosse(T(k):Color);baisseBrosse();leveBrosse();avance()
	}
}
	


