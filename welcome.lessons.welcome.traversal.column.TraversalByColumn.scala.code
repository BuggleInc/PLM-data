override def run() {
	var cpt : Int =0
	ecritMessage(cpt);

	while(!posF){
			
			if(estDosMur){
				setPos(getX()+1, 0);
				}else{
					setPos(getX(),getY()+1);
					}
			cpt = cpt + 1
			ecritMessage(cpt);
			
	}
}



def posF():Boolean ={
	if( getX()== (getMondeLargeur()-1)  && getY()==(getMondeHauteur()-1)){
		return true
	}else{
		return false
	}
}
