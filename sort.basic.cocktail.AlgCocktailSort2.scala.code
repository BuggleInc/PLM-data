def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var na=1
  var nb=1
  var tour=0
  do{
    na=0
    nb=0
    for(i<-debut to fin){
      if(getValeur(i)>getValeur(i+1)){
        echange(i,i+1)
        na=na+1
      }
      
    }
    fin=fin-1
  
  	for(i<- debut to fin){
      if (getValeur(fin-i)<getValeur(fin-i-1)){
        echange(fin-i,fin-i-1)
        nb=nb+1
      	} 
    	}
  	debut=debut+1
		} while(na!=0 && nb!=0)
}
