def cocktailSort2() {
  var a=true
  for (i <- getValueCount()-1 to 0 by -2){
    if (a==false){
      return
    }
    a=false
    for (j <- i-1 to 0 by -1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true
		}
}  
  for (j <- 0 to i-1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true
		}
}  
}
}