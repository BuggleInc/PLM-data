var diag = 0;
var cpt:Int=0;
var x:Int=0
var y:Int=0
x=getX()
y=getY()
override def run() {
 	ecritMessage(cpt)
  setPos(x,y+1)
  cpt+=1

  
  //parcours
  diag+=1
  ecritMessage(cpt)
  cpt+=1
  setPos(x+diag,y)
  ecritMessage(cpt)
  
  
  x=getX()
	y=getY()
  diag+=1
  
  ecritMessage(cpt)
  cpt+=1
  setPos(x+diag,y)
  ecritMessage(cpt)
  
  
  
}


