def cocktailSort2() {
  var a=true
    if (a==false){
      return
    }
    a=false
  var d=0
  var f=getNombreValeurs-2
  for (j <- d to f){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      f=f-1
      a=true
    }
}
for (j <- f to d by -1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      d=d+1
      a=true
		}
}   
}