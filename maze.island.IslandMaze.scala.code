override def run() {
  var murs=0
  
  def keepHandOnSideWall(){
    gauche()
    while(estFaceMur){
      droite()
    }
    avance()
      }
	while(murs<3){
    murs=0
    if(!estFaceMur){
      avance()
    }else{
      murs=murs +1
      droite()
      if(estFaceMur){
        murs=murs +1
        retourne()
        if(estFaceMur){
          murs=murs +1
        }else{
          avance()
          droite()
        }
      }else{
        avance()
        gauche()
      }
  
      
    }
    
  }
  
    
  while(!estSurBiscuit){
