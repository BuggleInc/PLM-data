getColonne() {
	Color[] Tab=new Color[getMondeHauteur()];
	for (int nbCase=0; nbCase<getMondeHauteur()-1;nbCase++) {
		Tab[nbCase]=getCouleurSol();
		avance();
	}
}

doColonne() {
	for (int nbCasesFaites=0; nbCasesFaites<getMondeHauteur()-1; nbCasesFaites++) {
		setCouleurBrosse(Tab[nbCasesFaites]);
		baisseBrosse();
		leveBrosse();
		avance();
	}
}


