def bubbleSort()  {
  var i: Int = 0
  var j: Int = 0
  var lastEchange: Boolean = false
  do {
    lastEchange = false
    for (compteur <- 0 to getNombreValeurs()-2) {
      i = compteur
      j = compteur + 1
      if (plusPetit(j,i)) {
        echange(i,j)
        lastEchange = true
      }
    }
  } while (lastEchange==true);
}