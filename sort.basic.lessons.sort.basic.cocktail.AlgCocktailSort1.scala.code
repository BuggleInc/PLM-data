def cocktailSort() {
	var inverse = false
	do{
		for(i<-0 to getNombreValeurs-2){
			if(!(plusPetit(i,i+1))){
				echange(i,i+1)
				inverse = true
			}
		}
		for(i<-getNombreValeurs-2 to 0 by (-1)){
			if(!(plusPetit(i,i+1))){
				echange(i,i+1)
				inverse = true
			}
		}
		
		
	}while(!inverse)
}
