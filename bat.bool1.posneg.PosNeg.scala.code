def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
  var c = a*b
  if(c<0 && negative){
    return false
  } else if (c>0 && negative){
    if(a>0 && b>0){return false
    }
    else {
      return true
    }
  }
    else if (c>0 && !negative){
    if(a>0  b>0){return false
    }
    else {
      return true
    }
  }else {
    return true
  }
}
