
while estSurMessage():
        if "A":
            avance()
        if "R":
            droite()
            avance()
        if "B":
              avance(2)    
        if "I":
            retourne()
            avance()
        if "Z":
              recule()
        if "L":
            gauche()
            avance(1)        
        if "C":
              avance(3)
        if "Y":
              recule(2)
        if "X":
            recule(3)