def selectionSort() {

	var currentMin = getValeur(0);
	var rank = 0;
	
	for ( i <- 0 to getNombreValeurs - 2)
	{
		
		for ( j <- i  to getNombreValeurs -2)
		{
			if (plusPetitQue(j, currentMin))
			{
				currentMin = getValeur(j);
				rank = j;
			}						
		}

		echange(i, rank);
		currentMin = getValeur(i+1);
		
	}
}
