def cocktailSort2() {
  var lgr = getNombreValeurs()
  var debut = 0
  var fin = lgr-2
  for(k<-0 to lgr){
    for(i<- debut to fin){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
      }
      fin = fin -1
    }
    for(j<- fin to debut){
      if(plusPetit(j,j+1)){
        echange(j,j+1)
      }
      debut = debut +1
    }
  }
}