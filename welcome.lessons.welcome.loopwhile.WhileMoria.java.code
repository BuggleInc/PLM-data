public void run() {
	while (!isFacingWall()){
		right();
		right();
		forward();
		while(!isOverBaggle() && !isFacingWall()){
			forward();
		}
		if (!isFacingWall() && !isOverBaggle()) {
		pickupBaggle();
		left();
		left();
		while(!isOverBaggle()){
			forward();
		}
		backward();
		dropBaggle();} else {
			right();
			forward();
			left();
			forward();
		}
	}
	
}
