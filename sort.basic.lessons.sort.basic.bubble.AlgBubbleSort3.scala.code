def bubbleSort3() {
	var break = true;
	while(break) {
		for (i <- getNombreValeurs()-2 to 0 by -1) {
			var inverse = true;
			if (inverse) {
				inverse = false;
				for (j <- 0 to i) {
					if (plusPetit(j+1,j)) {
						echange(j+1,j);
						inverse = true;
					}
				}	
			}
			break = false;
		}
	}
}
