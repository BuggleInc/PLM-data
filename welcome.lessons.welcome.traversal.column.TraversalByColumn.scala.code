override def run() {
  var avancement = 0
  ecritMessage(avancement)
  while(getY()!=getMondeHauteur()-1 && getX!=getMondeLargeur()-1)
  {
    go()
    avancement = avancement + 1
    ecritMessage(avancement)
  }
}

def go(){
  var Hauteur = getMondeHauteur()
  var Largeur = getMondeLargeur()
  var X = getX()
  var Y = getY()
  if (Y==Hauteur)
  {
    setPos(X+1,0)
  }
  else
  {
    setPos(X,Y+1)
  }
  }

