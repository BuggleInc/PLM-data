def combSort() {
	var ecart = getNombreValeurs()
	var n = getNombreValeurs()
	var x = false
	do {
		x = true
		if ( ecart > 1) {
			ecart = (ecart.toDouble / 1.3).toInt
			var i = 0
			while ( (i+ ecart) < n) {
				if (plusPetit(i+ecart,i)) {
				echange(i,i+ecart)
				x = false
				}
				i += 1
			}
		}
	} while ( ecart>1 || x == false)
}
