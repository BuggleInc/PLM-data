def shellSort() {
  var temp = 0
  var ecart = getNombreValeurs()/2
  var i =getNombreValeurs()-1
  while (ecart>0 && i>0) {
    temp = getValeur(i)
    var j=i
    while((j-ecart>=0&&getValeur(j-ecart)>temp)){
      setValeur(j,getValeur(j-ecart))
      j=j-ecart
    }
    setValeur(j,temp)
    i=i-1
  }
}