override def run() {
  while(!posFin()){
    if(!estFaceMur()){
      avance()
    }else if(getDirection() == Direction.EST){
      gauche()
      avance()
      gauche()
    }else if(getDirection() == Direction.OUEST){
      droite()
      avance()
      droite()
    }
  }
}

def posFin() : Boolean = {
  if(estFaceMur()){
    return true
  }else{
    return false
  }
}