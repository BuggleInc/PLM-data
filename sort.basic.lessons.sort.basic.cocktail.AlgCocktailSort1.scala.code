def cocktailSort(){
  var a=0
  var b=0
  do{
    a=0
    b=0
  	for (i <- 0 to getNombreValeurs()/2){
    	if(plusPetit(i+1,i)){
       		echange(i,i+1)
        	a=a+1
    }
}
    for (i <- getNombreValeurs()/2 to 0){
    	if(plusPetit(i+1,i)){
       		echange(i,i+1)
        	b=b+1
    }
}
    while (a>0|b>0)
}
}