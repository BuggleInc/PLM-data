def bubbleSort3() {
  var swapped:Boolean=true
  var len:Int=getValueCount();
  for (i <- len-2 to 0 by -1) {
  while (swapped) {
    for(i <- len-2 to 0 by -1) {
      swapped=false
      for (j <- 0 to i) {
        if (!isSmaller(j,j+1)) {
          swap(j,j+1)
          swapped=true
        }
      }
    }
  }
}