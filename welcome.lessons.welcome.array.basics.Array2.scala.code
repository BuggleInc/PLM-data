override def run() {
  var T : Array[Color] = new Array[Color] (getMondeHauteur())
  for (i<-0 to getMondeHauteur()-2){
    T(i)=getCouleurSol();
    avance();
  }
  T(getMondeHauteur())=getCouleurSol()
  for(k<-0 to getMondeLargeur()-2) {
    gauche();
    avance();
    droite();
    avance();
    for (l<-0 to getMondeHauteur()-1) {
      setCouleurBrosse(T(l));
      baisseBrosse();
      leveBrosse();
      avance();
    }
  }
}