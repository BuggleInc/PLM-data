def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
if(steps!=0){
	current(steps)
	avance(length)
	droite(angle)
	tree(steps-1, shrink*length, angle, shrink)
	current(steps)
	gauche(angle*2)
	recule(length)
	avance(length)
	tree(steps-1, shrink*length, angle, shrink)
	recule(length)
}
}
