def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if (!isBirthday) {
		if (speed<=60) {
			return 0;
		}
		else if (speed<=80) {
			return 1;
		}
		else if (speed>=81) {
			return 2;
		}
	}
	else if (isBirthday) {
		if (speed<=65) {
			return 0;
		}
		else if (speed<=85) {
			return 1;
		}
		else return 2;
	}
}
