def cocktailSort() {
  var tri : Boolean = true
  var taille = getNombreValeurs()
  while (tri)
  {
    tri= false 
    for (i <- 0 to taille-2)
    {
      if (!plusPetit(i,i+1))
      {
        echange(i,i+1)
        tri=true
      }
    }
    for (i <- 0 until taille-2)
    {
      if(plusPetit(i,i+1))
      {
        echange(i,i+1)
        tri=true
      }
    }
  }
}