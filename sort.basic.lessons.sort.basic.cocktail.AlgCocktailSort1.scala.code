def cocktailSort() {
  var inversement=false
  do {
    for (i<-0 to getNombreValeurs()-2){
    	if (plusPetit(i+1,i)){
        echange(i,i+1)
        inversement=true
      }
    }
    for (i<- getNombreValeurs()-2 to 1 by -1){
      if(plusPetit(getNombreValeurs()-i,getNombreValeurs()-i-1)){
        echange(getNombreValeurs()-i,getNombreValeurs()-i-1)
        inversement=true
      }
    }
  }while(inversement)
}