def cocktailSort() {

var trie : Boolean = false;
var nb : Int = 0;

	while (!trie) {
		for (i <- 0 to getNombreValeurs() - 2) {
			nb = 0;
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				nb = nb + 1;
			}
		}


		for (j <- 0 to getNombreValeurs() - 2) {
			nb = 0;
			if (!plusPetit(j,j+1)) {
				echange(j,j+1);
				nb = nb + 1;
			}
		}
		
		
	}
	
		if (nb == 0) {
			trie = true;
		}
}