def cocktailSort() {
  var changement=true
  
  while (changement==true){
    changement=false
    for (compteur<-0 to getNombreValeurs()-2 ){
      if (plusPetit(compteur+1,compteur)==true){
        echange(compteur+1,compteur)
        changement=true
      } 
    }
    for (compteur<-getNombreValeurs()-2 to 0 by -1){
      if (plusPetit(compteur+1,compteur)==true){
        echange(compteur+1,compteur)
        changement=true
      }
    }
  }
}