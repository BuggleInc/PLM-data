def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	if(steps>0){
  baisseCrayon()
  current(steps)
  avance(length)
  droite(angle)
  tree(steps-1,shrink*length,angle,shrink)
  gauche(2*angle)
  leveCrayon()
  tree(steps-1,shrink*length,angle,shrink)
    droite(angle)
   recule(length)
  }
}