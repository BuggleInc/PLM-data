  override def initialize():Array[Point] ={
  	val Liste:List[Point]= getSol()
  	var plat:Array[Point]= new Array[Point] (2)
  	for (i <- 0 to Liste.length-1) {
  		if (Liste(i).y==Liste(i+1).y) {
  			Array(0)=Liste(i)
  			Array(1)=Liste(i+1)
  		}
  	}
  	return plat
  }

