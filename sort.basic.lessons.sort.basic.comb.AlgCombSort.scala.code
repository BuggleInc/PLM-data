def combSort() {

	var ecart= getNombreValeurs();
	var lrg = getNombreValeurs();
	var hasSwap=true;
	var i = 0;

	while (ecart>1 || hasSwap)
	{
		if(ecart > 1)
		{
			ecart = (ecart.toDouble / 1.3).toInt
		}
		hasSwap=false;
		i=0
		while(i+ecart < lrg)
		{
	 		if (plusPetit(i+ecart,i))
	 		{
	 			echange(i,i+ecart);
	 			hasSwap=true;
	 		}
	 		i+=1;
		}
	}
  }


}