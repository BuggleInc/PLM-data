override def run() {
	def murAGauche():Boolean={
		gauche()
		if(estFaceMur()){
			droite()
			return true
		}else{
			droite()
			return false
		}
	}
	def keepHandOnSideWall(){
		if (murAGauche() && !estFaceMur()){
			avance()
		}else{
			gauche()
			if(!estFaceMur()){
				avance()
			}
		}
	}
	while(!estSurBiscuit()){
		keepHandOnSideWall()
	}
	prendBiscuit()
}
