def insertionSort() {

  var v = 0
  var mem = 0

  for (i<- 1 to getValueCount()-1){

    v = getValue(i)

    for (j<- i to i){
      if(!isSmallerThan(i-j-1,v)){
        mem=i-j-1;
        copy(i-j-1,i-j);
      }
    }
		setValue(mem,v);
  }
}



