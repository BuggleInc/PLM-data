def shellSort()  {
  var x:Int=getNombreValeurs()
  var v=0
  var y=0
  var ecart=x/2
  var t=0
  while (ecart>0){
  for (i<- 0 to ecart) {
    t=i
    while (t+ecart<x-1) {
      if (plusPetitQue(t,getValeur(t+ecart))==false) {
        echange(t+ecart,t)
      }
          t=t+ecart
          }
  }
    ecart=ecart/2
  }
}