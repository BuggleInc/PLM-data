var Largeur = getMondeLargeur()
var Hauteur = getMondeHauteur()

def trouverSortie():Tuple2[Int,Int] = {
  
  var xS = 0
  var yS = 0
  
  for (x <- 0 to Largeur-1; y <- 0 to Hauteur-1) {
    
    if (aBiscuit(x,y)) {
      
      xS = x
      yS = y
      
    }
    
  }
  
  return (xS,yS)
  
}

override def run() {
    
  var sortie = trouverSortie()
  
}