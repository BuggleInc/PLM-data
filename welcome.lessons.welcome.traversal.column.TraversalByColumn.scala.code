override def run() {
  var colonne = 0;
  var ligne = 0;
  var compteur = 0;
  ecritMessage(compteur);
  do {if(getY==getMondeHauteur-1){ligne=0;colonne=colonne+1;setPos(colonne,ligne)}
      else {ligne=ligne+1;setPos(colonne,ligne)};compteur=compteur+1;ecritMessage(compteur)}
  while (!(getX==getMondeLargeur-1 && getY==getMondeHauteur-1))
}
