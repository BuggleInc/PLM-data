override def run() {
  var hauteur=getMondeHauteur()
  var largeur=getMondeLargeur()
  var compteur=0
  for (i<-0 to hauteur-1){
    for (j<-0 to largeur-1){
      if (j==hauteur-1){
        setPos(largeur-1-j,i)
      	ecritMessage(compteur)
      	compteur=compteur+1
      }else{
        setPos(j,i)
      	ecritMessage(compteur)
      	compteur=compteur+1
      }
    }
  }
}