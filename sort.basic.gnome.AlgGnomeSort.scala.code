def gnomeSort() {
  var length:Int = getNombreValeurs()
  var continuer:Boolean = true
  var i:Int = 0
  while (continuer){
    if (length == 10){print(i + " ")}
    if (i == length - 1){
      continuer = false
    } else if (plusPetit(i,i+1)){
      i += 1
    } else {
      echange(i,i+1)
      if (i > 0){
      	i -= 1
      } else if (i == 0){
        i += 1
      }
    }
  }
}