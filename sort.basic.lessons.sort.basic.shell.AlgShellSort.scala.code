def shellSort()  {

	var ecart = (getNombreValeurs().toDouble / 2).toInt
	var v = 0
	var cpt = 1

	while (ecart>0) {
		//algo du tri par insertion
		for (i<-1 to getNombreValeurs()-1) {
		v=getValeur(i)
		cpt=i
			while (cpt>0 && !plusPetitQue(cpt-1, v)) {
			copie(cpt-1, cpt)
			cpt=cpt-ecart
			}
		if (cpt>=0) {setValeur(cpt, v)}
		else {setValeur(cpt+ecart, v)}
		//fin
		}
		ecart = (ecart.toDouble / 2).toInt
	}

}
