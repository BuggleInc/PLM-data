override def run() {
  var cpt = 0
  while(!fin()){
    if(getY()<getMondeHauteur()){
      setPos(getX(),getY()+1)
      cpt+=1
    }
    else if(getX()<getMondeLargeur()){
      setPos(getX()+1,0)
      cpt+=1
    }
    ecritMessage(cpt)
  }
}

def fin():Boolean={
  var posf = false
  if((getX(),getY())==(getMondeLargeur(),getMondeHauteur())){
    posf=true
  }
  return posf
}