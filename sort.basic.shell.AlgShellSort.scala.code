def shellSort()  {
  var lgr = getNombreValeurs()
  var ecart = lgr/2
  var casei = 0
  var j =0
  while(ecart>0){
    for(i<-ecart to lgr-1){
    j = i - ecart
      casei = getValeur(i)
    while(j>0){
      if(getValeur(j-ecart)>casei){
        echange(j-ecart,j)
      }
      j = j-ecart    
    }
      ecart = ecart/2
  	}
  } 
}
