def array123(nums:Array[Int]): Boolean = {
  var rep=false
  var rep1=false
  var rep2=false
  var rep3=false
  for (i <- 0 to nums.length-1){
  	if(nums(i)==1){
     	rep1=true
   	}
    if(nums(i)==2){
     	rep2=true
   	}
    if(nums(i)==3){
     	rep3=true
   	}    
  }
  if((rep1==true) && (rep2==true) && (rep3==true)){
    rep=true    
  }
  return rep
}