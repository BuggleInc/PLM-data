override def run() {
  var dir:Direction=Direction.NORTH
  while(porteBiscuit()==false) {
    if (estFaceMur()==false&&getDirection()==Direction.NORTH) {
      avance()
    }
    else {
      keepHandOnSideWall()
    }  
    if (estSurBiscuit()==true) {
      prendBiscuit()
    }
    dir = getDirection()
    setDirection(Direction.NORTH)
    if (estFaceMur()==false) {
      avance()
    }
    else {
      setDirection(dir)
    }
  }
}  

def keepHandOnSideWall() {
  gauche()
  if (estFaceMur()==true) {
    droite()
    if (estFaceMur()==false) {
    avance()
    }
    else {
      droite()
    }
  }
  else {
    avance()
  }  
}