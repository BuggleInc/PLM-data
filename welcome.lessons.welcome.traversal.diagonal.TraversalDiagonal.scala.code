var diag = 0;
override def run() {
  var cpt = 0
  ecritMessage(cpt)
  while (getY()!= getMondeHauteur()-1)
  {
    if (getY()==0)
    {
      diag+=1
      setPos(0,diag)
      cpt+=1
      ecritMessage(cpt)
    }
    else
    {
      setPos(getX()+1,getY()-1)
      cpt+=1
      ecritMessage(cpt)
    }
  }
}

