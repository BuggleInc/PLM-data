def in3050(a:Int, b:Int):Boolean = {
  var ecarta35:Int = Math.abs(a-35)
  var ecartb35:Int = Math.abs(b-35)
  var ecarta45:Int = Math.abs(a-45)
  var ecartb45:Int = Math.abs(b-45)
  if (ecarta35 <= 5 && ecartb35 <= 5) {
    return true
  } else if (ecarta45 <= 5 && ecartb45 <= 5) {
    return true
  } else {
    return false
  }
}