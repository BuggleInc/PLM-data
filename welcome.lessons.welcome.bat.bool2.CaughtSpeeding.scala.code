def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (!isBirthday){
    speed match{
      case i if i<=60=>0
      case i if 61<=i<=80=>1
      case i if i>=81=>2
    }   
  }
  else{
    speed match{
      case i if (i<=65)=>0
      case i if (66<=i<=85)=>1
      case i if (i>=86)=>2
    }   
    
  }
}