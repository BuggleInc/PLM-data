  T(0) = 1
  if (num>1) 
  {
    T(1)=2
    for (i <- 1 to num-1) 
    {
      pointeur1 = i +1
      for (j <- 1 to pointeur1-1) 
      {
        T(pointeur2) = 2
        pointeur2 = pointeur2 + 1
      }
    } 
