var b:Int = 0;

def lastDigit(a:Int, b:Int, c:Int):Boolean = {
  if ( dernierChiffre(a) == dernierChiffre(b) || dernierChiffre(b) == dernierChiffre(c) || dernierChiffre(a) == dernierChiffre(c)){
    return true;
  } else {
    return false;
  }
}
  
def dernierChiffre(a:Int) : Int = {
  b = a;
  while ( b%10 != b ){
    b = b%10;
  }
  return b;
}
