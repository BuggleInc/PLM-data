def cocktailSort3() {
  var debut: Int = 0
  var lgr= getNombreValeurs() 
  var fin = lgr-2
  var cpt =0
  
  do
  {
    cpt=0
    for (i<- debut to fin)
    {
      if (plusPetit(i+1,i))
      {
        echange(i+1,i)
        cpt+=1
      }
    }
    
    fin = fin -1
    for (i<- fin to debut by -1)
    {
      if (plusPetit(i+1,i))
      {
        echange(i+1,i)
        cpt+=1
      }
      
    }
    
    debut+=1
    
  }
  
  while(cpt!=0)
}