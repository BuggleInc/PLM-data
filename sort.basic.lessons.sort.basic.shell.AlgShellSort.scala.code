def shellSort() {
  var v = 0
  val lgr = getNombreValeurs()
  var ecart = (lgr/2).toInt
  while (ecart>0){
    for (i<-(lgr/2).toInt to getNombreValeurs()-1){
  		v = getValeur(i)
    	var j = i-ecart
    	while(j>=0 && !plusPetitQue(j,v)){
      	copie(j,j+1)
      	j-=ecart
    	}
    	j+=ecart
    	setValeur(j,v)
  	}
  }	
}