override def run() {
  for(n <- 1 to 2) {
    for(m <- 1 to 2) {
      motif(Color.red)
      motif(Color.blue)
      motif(Color.green)
      motif(Color.yellow)
      avance(4)
    }
    gauche()
    avance(4)
    droite()
  }
  recule(2)
}

def motif(c:Color) {
  setCouleurBrosse(c:Color)
  gauche()
  avance()
  baisseBrosse()
  leveBrosse()
  recule()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  avance(2)
  gauche() }