def insertionSort() {
	val longueur:Int = getNombreValeurs();
	var tmp:Int = 0;
	var lastCase:Int = 0;
	for (i <- 0 to longueur -1) {
		for (j<- 0 to i) {
			tmp = getValeur(i);
			if (plusPetit(getValeur(i-(j-1)),tmp)){
				setValeur(i-j,getValeur(i-(j-1)));
				lastCase = (i-(j-1));
			}
			
		}
		 setValeur(lastCase,tmp);
	}
	
}
