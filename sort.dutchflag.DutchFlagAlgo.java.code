void solve() {
  
  int bleu = getTaille()-1;
  int rouge  = 1;
  int blanc = 1;
  int i = 0;
  
  while(blanc<=bleu)
  {
  	if(getCouleur(blanc)==BLEU)
      {
      bleu--;
    }
    else if(getCouleur(blanc)==ROUGE)
      {
      echange(rouge,blanc);
      rouge++;blanc++;
    }
    else{
     echange(blanc,rouge);
   	 rouge++;
  
    }

    
	}
}