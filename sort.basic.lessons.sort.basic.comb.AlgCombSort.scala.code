def combSort()
{
  ecart = getNombreValeurs()
  inversion = true
  
  while (ecart > 1 && inversion)
  {
    if (ecart > 1)
    	ecart = (ecart.toDouble / 1.3).toInt
    
    i = 0
    while (i + ecart < getNombreValeurs())
    {
    	if(plusPetit(getValeur(i + ecart), getValeur(i))
      { 
        echange(i, i + ecart)
    		i += 1;
      }
    }
  }
}