def cocktailSort2() {
	var swapped: Boolean = true
	while (swapped) {
		swapped = false
		for (i <- 0 to getNombreValeurs()-2) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				swapped = true
			}
			fin -= 1
		}
		for (j <- getNombreValeurs()-2 to 0 by -1) {
			if (plusPetit(j+1,j)) {
				echange(j,j+1)
				swapped = true
			}
		}
	}
}

