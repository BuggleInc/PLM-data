def bubbleSort()
{
  var n = getNombreValeurs()-1;
  var isEchange = true;
  var i = n;
  
  while (isEchange)
  {
    isEchange = false;
    
    for (j <- i-1 to 0)
    {
      if (plusPetit(i,j))
      {
        echange(i,j);
        isEchange = true;
      }
    }
    i-=1;
  }
}




