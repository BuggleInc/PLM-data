def cocktailSort() {
  var lgr = getNombreValeurs() 
  var cpt=0
  do
  {
    cpt=0
    for (i <- 0 to lgr-2)
    {
      if (plusPetit(i,i+1))
      {
        echange(i,i+1)
        cpt+=1
      }

    }
    for (i <- lgr-2 to 0 by -1)
    {
      if (plusPetit(i+1,i))
      {
        echange(i+1,i)
      }

    }
  }
    while(cpt!=0)

  
}