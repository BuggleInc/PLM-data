def combSort() {
  var ecart:Int = getNombreValeur()
  var x:Boolean = false
  while(i > 1 || !x){
    x = true
    if(ecart > 1){
      ecart = ecart/1.3
    }
    i=0
    while(i+ecart < getNombrezValeurs()){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        i += 1
        x = false
      }
    }
  }
}