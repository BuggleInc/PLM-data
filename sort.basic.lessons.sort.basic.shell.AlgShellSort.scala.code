def shellSort()  {
  var ecart:Int = getNombreValeurs()/2
  while (ecart >0){
    var v = 0
    for (i<-ecart to getNombreValeurs()-1){
      v=getValeur(i)
      var j=i
      while(j>ecart-1 && !plusPetitQue(j-ecart,v)){
          copie(j-ecart,j)
          j=j-ecart
      }
      setValeur(j,v)
    }
   ecart = ecart/2
 }
}