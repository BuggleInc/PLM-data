def nearTen(num:Int):Boolean = {
  (num) match {
    case (num) if num%10==0 =>true
    case (num) if num%10==1 =>true
    case (num) if num%10==2 =>true
    case (num) if num%10==3 =>false
    case (num) if num%10==4 =>false
    case (num) if num%10==5 =>false
    case (num) if num%10==6 =>false
    case (num) if num%10==7 =>false
    case (num) if num%10==8 =>true
    case (_) =>true
  }
}