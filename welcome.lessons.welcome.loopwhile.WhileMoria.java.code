public void run() { 
	while(!isFacingWall()) {
		left();
		left();
		forward();
		wall();
		while(!isOverBaggle()) {
			forward();
			wall();
		}
		pickUpBaggle();
		left();
		left();
		while(!isOverBaggle()) {
			forward();
		}
		backward();
		dropBaggle();
	}
}

public void wall() {
	if(isFacingWall()) {
		right();
		forward();
		left();
		forward();
	}
}
