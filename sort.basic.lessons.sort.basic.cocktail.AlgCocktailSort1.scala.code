def cocktailSort() {
  var m=getNombreValeurs()
  var n= false
  do{
    n= false
    for (i<- 0 to (m-2)){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          n= true     
     }
    }
    for (j<- m-2 to 0 by -1){
      if (!plusPetit(j,j+1 )){
        	echange(j, j+1)
          n= true      
     }
    }
  } while(n)
}
