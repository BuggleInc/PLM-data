def combSort() {
  var ecart:Double=getNombreValeurs()
  var swapped: Boolean = true
  do {
    swapped=false
    if (ecart>1){
      ecart=ecart/1.3
    }
    var i:Int=0
    while (i+ecart<getNombreValeurs()){
      if (plusPetit(i,i+1)==true){
        echange(i,i+1)
        swapped=true
        i=i+1
      }
    }
  } while (ecart > 1 || swapped = true)
}