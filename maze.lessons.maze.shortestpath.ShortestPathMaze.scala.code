def coordonneesBiscuit():Array[Int] =
{
  for (x <- 0 to getMondeLargeur)
  {
    for (y <- 0 to getMondeHauteur)
  	{
      if (aBiscuit(x, y))
      	return Array[Int](x, y)
  	}
  }
  
  return Array[Int](-1, -1)
}

override def run() 
{
  var biscuitX = coordonneesBiscuit()(0)
  var biscuitY = coordonneesBiscuit()(1)
}