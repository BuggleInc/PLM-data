def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
current(steps)
if(steps != 0){
	avance(length)
	droite(angle)
	tree(steps-1,length*shrink,angle,shrink)}
	gauche(angle)
	leveCrayon()
	recule(length)
	baisseCrayon()
	gauche(2*angle)
	tree(steps-1,length*shrink,angle,shrink)
	
	

}
