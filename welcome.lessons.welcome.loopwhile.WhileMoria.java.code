public void run() {
	while (getGroundColor != Color.green){
		right();
		right();
		forward();
		while(!isOverBaggle() && !isFacingWall()){
			forward();
		}
		if (isFacingWall()) {
		pickupBaggle();
		left();
		left();
		while(!isOverBaggle()){
			forward();
		}
		backward();
		dropBaggle();} else {
			right();
			forward();
			left();
			forward();
		}
	}
	
}
