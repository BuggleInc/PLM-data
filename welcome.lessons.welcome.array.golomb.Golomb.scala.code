def golomb(lum:Int): Int = {
  var gol = new Array[Int] (lum+1)
  var occ = new Array[Int] (lum+1)
 	gol(1)= 1
  for (i <- 0 to lum) {
    occ(i) = 0
  }
 	occ(1) = 1
  var c = 1
  for (i <- 2 to lum) {
    if (occ(c) == gol(c)) {
      c=c+1
    };
    gol(i)=c;
    occ(c)=occ(c)+1
  }
  return gol(lum)
}