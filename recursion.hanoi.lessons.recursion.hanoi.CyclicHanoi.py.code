def clockwise(height, src,mid,dst):
    print("clockwise")
    if height != 0:
        anti(height-1,src,dst,mid);
        move(src,dst);
        anti(height-1,mid,src,dst);
def anti(height, src,mid,dst):
    if height != 0:
        anti(height-1,src,mid,dst);
        move(src,mid);
        clockwise(height-1,dst,mid,src);            
        move(mid,dst);
        anti(height-1,src,mid,dst);

