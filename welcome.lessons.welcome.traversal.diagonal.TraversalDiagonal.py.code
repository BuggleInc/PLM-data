nDiag=1
n=0
while getX() != getWorldWidth()-1 or getY() != getWorldHeight()-1:
    if nDiag <getWorldHeight():
        for i in range(nDiag):
            writeMessage(n)
            setPos(getX()+i-1,getY()-i-1)
            n+=1
        setPos(getX()-nDiag-1,getY()+nDiag)
        nDiag +=1
            