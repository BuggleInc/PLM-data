boolean haveBaggle() {
	boolean seenBaggle = false;
	//right();
	while (!isFacingWall()){
		//left();
		int progressY = 0;
		while (!isFacingWall()){
			if (isOverBaggle()){
				seenBaggle = true;
			}
			forward();
			progressY++;
		}
		while (progressY != 0) {
			backward();
			progressY--;
		}
		//right();
		//forward();
	}
	return seenBaggle;
}
