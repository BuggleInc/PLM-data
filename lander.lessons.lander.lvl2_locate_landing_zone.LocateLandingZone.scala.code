def getLandingZone():Segment = {

var sol=getSol()
var pos:Double= 0
var plat = 0
var premier = Point(300,200)
var deuxieme = Point(0,0)

	for(i<-0 to getSol.length-1){
		if((pos==sol(i).y)&&plat == 0){
		 	plat = 1 
		 	premier = sol(i-1)
		}
		else if((pos==sol(i).y)&&plat == 1){
			deuxieme = sol(i-1)
		}
		else if((pos!=sol(i).y)&&plat == 1){
			return Segment(premier,deuxieme)
		}
		else{
			pos = (sol(i).y)
		}
	}
	return Segment(premier,premier)
}
