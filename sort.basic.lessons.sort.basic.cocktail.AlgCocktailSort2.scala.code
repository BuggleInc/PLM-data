def cocktailSort2() {
  var a=true;
  var c=getValueCount()-2
  var cmin = 0
  while(c-cmin>2){
  	a=false;
  	for (i<- 0 to c){
    	if (isSmaller(i+1, i)){
      	swap(i, i+1);
        a=true;
    	}
    }
    c-=1
    for (i<- 0 to c-cmin-1){
      if (isSmaller(c-i+1, c-i)){
        swap(c-i, c-i+1);
        a=true
      }
    }
    cmin+=1
  }
}