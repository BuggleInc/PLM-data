def estPositionFinale() : Boolean = {
  if (getX() == getMondeHauteur()-1 && getY() == getMondeLargeur()-1) {
    return true;
  } else {
    return false;
  }
}

def positionSuivante() {
  if (getX() == 6) {
    setPos(0, getY() + 1);
  } else {
    setPos(getX() + 1, get Y());
  }
}

override def run() {
  var cpt : Int = 0;
  ecritMessage("cpt");
  while (!estPositionFinale) {
    positionSuivante();
    cpt += 1;
    ecritMessage("cpt");
  }
}