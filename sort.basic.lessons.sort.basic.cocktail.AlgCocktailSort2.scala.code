def cocktailSort2() 
{
	var hasSwap:Boolean = false;
	var end: Int = getNombreValeurs()-1;
	var beginning: Int = 1;

	do{
		hasSwap = false;
		for(i <-beginning to end by 1)
		{
			if (plusPetit(i, i-1))
				{
					echange(i,i-1);
					hasSwap = true;
				}
		}
		end-=1;
		for( i<-end to beginning by -1)
		{
			if (plusPetit(i, i-1))
				{
					echange(i-1,i);
					hasSwap = true;
				}
		}
		beginning +=1;
	}while(hasSwap)
	
}
	

