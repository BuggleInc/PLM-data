void faireCarre() {
	setCouleurBrosse(Color.YELLOW);
	faireV();
	setCouleurBrosse(Color.RED);
	faireV();
	setCouleurBrosse(Color.BLUE);
	faireV();
	setCouleurBrosse(Color.GREEN);
	faireV();
	for (int pasAFaire=0; pasAFaire<5; pasAFaire = pasAFaire + 1) {
		avance();
	}
}

void faireLigne(int x) {
	for (int carresAFaire=0; carresAFaire<x; carresAFaire = carresAFaire + 1) {
		faireCarre();
	} gauche();
	for (int pasAFaire=0; pasAFaire<5; pasAFaire = pasAFaire + 1) {
		avance();
	} droite();
}

void faireMotif(int y, int x) {
	for (int lignesAFaire=0; lignesAFaire<y; lignesAFaire = lignesAFaire + 1) {
		for (int carresAFaire=0; carresAFaire<x; carresAFaire = carresAFaire + 1) {
		faireCarre();
	} gauche();
	for (int pasAFaire=0; pasAFaire<5; pasAFaire = pasAFaire + 1) {
		avance();
	} droite();
	}
}

public void run() {
faireMotif(9, 9);
}
