def cocktailSort() {
	var test:Boolean = true;
	do{
		test=false;
		for(i<-0 to getNombreValeurs()-2 by 1){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				test=true;
			}
		}
		for(i<-getNombreValeurs() to 1 by -1){
			if(plusPetit(i,i-1)){
				echange(i-1,i);
				test=true;
			}
		}
	}while(test)
}
