x=a=getX()
y=b=getY()
n=0
while(n!=10):
    while(b-8!=y):
        y=y-1
        setPos(x,y)
    gauche()
    while(a-8!=x):
        x=x-1
        setPos(x,y)
    gauche()
    while(b!=y):
        y=y+1
        setPos(x,y)
    gauche()
    while(a!=x):
        x=x+1
        setPos(x,y)
    gauche()
    n=n+1