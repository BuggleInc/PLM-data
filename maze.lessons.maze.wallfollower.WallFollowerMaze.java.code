public void run() {
	keepHandOnsidewall();
}

public void keepHandOnsidewall(){
int spins = 4;

	do{
		for(int i = 0; i < spins; i++){
			if(isFacingWall() && getDirection() == Direction.EAST){
				right();
			}
		}
		if(!isFacingWall()){
			forward();
		}/*else{
			right();
		}*/
		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());
}
