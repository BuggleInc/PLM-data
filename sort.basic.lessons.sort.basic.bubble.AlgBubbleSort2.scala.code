def bubbleSort2()  {
  var estTrie = false
  var compteur = 0
  while ((!estTrie)&&(compteur<getNombreValeurs())){
    compteur+=1
    var test = 0
    for (i <- 0 to getNombreValeurs()-2){
    	if (!plusPetit(i,i+1)){
        echange(i,i+1)
        test += 1
      }  

    }
    if (test == 0){
    	estTrie = true
    }
    
  }
}