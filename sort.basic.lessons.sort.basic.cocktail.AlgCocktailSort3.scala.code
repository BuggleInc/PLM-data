def cocktailSort3() {
	var tri=false
	var debut=0
	var fin=getNombreValeurs-2
	while (!tri) {
		tri=true
		for (i<- debut to fin) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				tri=false
			}
		}
		if (tri) {
			return
		}
		fin-=1
		for (i<- fin to debut by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				tri=false
			}
		}
		if (tri) {
			return 
		}
		debut+=1
	}
}
