def insertionSort() {
  val n = getNombreValeurs()
  var v = 0
  var k = 0
  var m = 0
  for (i<- 1 to n-1) {
    k = i
    v = getValeur(i)
    var b = false
    var j = i-1
    while (j>= 0 && !b){if (getValeur(j) > v) {setValeur(j+1,getValeur(j));k = j} else {b = true};j-=1
  }
   setValeur(k,v)
  }}
