override def run() {
  
  
  while (!isFacingWall()){
    forward()
  }
  
  while (!isOverBaggle()){
    keepHandOnSideWall()
		forward()
  }
  
  
  pickupBaggle()
}



def keepHandOnSideWall(){

  if (getDirection == Direction.SOUTH){
    left()
  	for (i <- 1 to 3){
  	if (isFacingWall()){
   	 right()
  	}
  }
  }else{
  
	right()
  for (i <- 1 to 3){
  	if (isFacingWall()){
   	 left()
  	}
  }
    
  }
}