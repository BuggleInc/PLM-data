override def run() {
  var compteur = 1
  ecritMessage(0)
  while(!fin()) {
    parcoursDouble(getX(),getY(),true)
    ecritMessage(compteur)
    compteur =compteur+1
  }
}

def fin() :Boolean={
  if (getX()==getMondeLargeur()-1 && getY==getMondeHauteur()-1) return true
  else return false
}


