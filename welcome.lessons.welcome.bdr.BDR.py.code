fini = False
while fini is False:
    c = litMessage()
    if c=='R':
        droite()
        avance()
    elif c=='I':
        gauche()
        avance()
    elif c=='A':
        avance()
    elif c=='B':
        avance(2)
    elif c=='C':
        avance(3)
    elif c=='Z':
        recule()
    elif c=='Y':
        recule(2)
    elif c=='X':
        recule(3)
    else:
        done = True
    
