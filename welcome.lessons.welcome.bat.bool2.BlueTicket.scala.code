def blueTicket(a:Int, b:Int, c:Int):Int = {
  
  var somme_ab: Int = a + b
  var somme_bc: Int = b + c
  var somme_ac: Int = a + c
  if ((somme_ab == 10) | (somme_bc == 10) | (somme_ac == 10)) {return 10}
  else if ((somme_ab == 10 + somme_bc) | (somme_ab == 10 + somme_ac)) {return 5}
  else {return 0}
}