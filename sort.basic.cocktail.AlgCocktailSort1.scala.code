def cocktailSort() {
  var a=getNombreValeurs()
  var b=false
  do{
    b=false
    for (i<-0 to (a-2)){
    	if(plusPetit(i,i+1)==false){
      	echange(i,i+1)
        b=true
    	}
  	}
    var j=a-1
    while (j>=1){
      if(plusPetit(j,j-1)==true){
      	echange(j,j-1)
        b=true
    	}
      j-=1
    }
  } while (b==true)
}