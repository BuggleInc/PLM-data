override def run() {
  var i : Int = getMondeHauteur();
  var T = new Array[Color](i);
  for (j <- 0 to T.length - 1) {
    T(j) = getCouleurSol();
    avance();
  }
  for (j <- 2 to getMondeLargeur()) {
    gauche();
  	avance();
  	droite();
  	avance();
    for (k <- 0 to T.length - 1) {
      setCouleurBrosse(T(k));
      baisseBrosse();
      leveBrosse();
      avance();
    }
  }
}