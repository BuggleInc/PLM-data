override def run() {
		val dec = litMessage().toInt
	var T = new Array[Color](getMondeHauteur)
	for (i <- 0 to T.length - 2) {
		T(i)=getCouleurSol()
		avance()
	}
	for(i<-2 to getMondeLargeur()) {

		recule (getMondeHauteur()-dec)
		gauche()
		avance()
		droite()
		avance()

		for (j <- dec to getMondeHauteur()) {
			setCouleurBrosse(T(j))
			baisseBrosse()
			leveBrosse()
			avance()
		}
	}
}
