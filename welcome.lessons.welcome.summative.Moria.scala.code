var fin:Boolean = false;
while ( !fin ){
  retourne();
  while ( !estSurBiscuit() && !estFaceMur() ){
    avance();
  }
  if ( estSurBiscuit() ) {
    prendBiscuit();
    retourne();
  }
  while ( !estSurBiscuit() && !estFaceMur() ){
    avance();
  }
  if ( porteBiscuit() ){
    recule();
    poseBiscuit();
    recule();
  }
  if ( estFaceMur() && !estSurBiscuit() ){
    droite();
    avance();
    gauche();
    avance();
    fin = true;
  }
}
