def shellSort()  {
  var ecart = getNombreValeurs()
  while(ecart>0.5){
    ecart = ecart/2
    var v = 0
  	for(i<- 1 to getNombreValeurs()-1){
    	v = getValeur(i)
      var j = i
    	while(j-ecart >=0 && getValeur(j-ecart)>v){
        copie(j-ecart,j)
        j=j-ecart
    		}
  		setValeur(j, v)
  
    }
    
  }
  
}