def arrayFront9(nums:Array[Int]): Boolean = {
  var compteur = 0
  if (nums.length < 4)
  {
    for (i <- nums)
    {
      if (i==9)
      	compteur +=1
    }
  }
  else
  {
    for (i <- 0 to 3)
    {
      if (i==9)
      	compteur +=1
    }
  }
	if (compteur!=0)
  	return true
  else
  	return false
}