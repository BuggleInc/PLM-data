override def run() {
	val NbB=getNombreBases()
  do{
    var trouB=(getTrouBase()+1)%NbB
    var dMax=0
    var (bas,posi)=(getTrouBase(),getTrouPosition())
      for (position<-0 to 1){
        var d=(getCouleurJoueur(trouB,position)-trouB)%4
        if(d>dMax){bas=trouB
                   posi=position}
      
    }
    deplace(bas,posi)
  }while(!estTrie())
}