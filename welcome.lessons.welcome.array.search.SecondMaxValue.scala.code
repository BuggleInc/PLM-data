def max2Value(nums:Array[Int]): Int = {
  var rep = nums(0)
  var maxi=maxValue(nums)
  if {maxi == rep}(rep=nums(1))
  var last = nums.length -1
  
  
  for (i <- 1 to last){
    if (nums(i)> rep && rep < maxi){
       rep = nums(i)
    }
  }
   
  return rep
}



def maxValue(nums:Array[Int]): Int = {
 	var rep = nums(0)
  var last = nums.length -1
  for (i <- 1 to last){
    if (nums(i)> rep){
       rep = nums(i)
    }
  }
   
  return rep
}