def golomb(num):
    a=[0,1]
    for b in range(num-1):
        a.append(0)
    for n in range(2,len(a)):
       nValeurs=0
       for i in a:
           if i == n-1:
               nValeurs +=1
       if nValeurs < a[n-1]:
           a[n]=n-1
       elif nValeurs == a[n-1]:
           a[n] =n
    return a[num]