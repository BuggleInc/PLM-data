def array123(nums:Array[Int]): Boolean = {
	var nb1=0
	var nb2=0
	var nb3=0
	for(i<-0 to nums.length-1) {
		if (nums(i)==1) {nb1=nb1+1}}
	for(i<-0 to nums.length-1) {
		if (nums(i)==2) {nb2=nb2+1}}
	for(i<-0 to nums.length-1) {
		if (nums(i)==3) {nb3=nb3+1}}
	if (nb1>0 && nb2>0 && nb3>0) {return true}
	return false
}
