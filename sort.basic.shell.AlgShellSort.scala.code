def shellSort()  {
  var lgr=getNombreValeurs
  var ecart=lgr/2
  while (ecart>2) {
    insertionSort
    ecart=ecart/2
  }
}



def insertionSort() {
  var lgr=getNombreValeurs
  for (i <- 1 to lgr-1) {
    var v=getValeur(i)
    var j=i
    while ((j>0) && (!plusPetitQue(j-1,v))) {
      copie(j-1,j)
      j-=1
    }
    setValeur(j,v)
    }
  }