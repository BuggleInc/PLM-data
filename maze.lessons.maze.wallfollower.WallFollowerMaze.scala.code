override def run() {
  while(!estSurBiscuit())
  {
		keepHandOnSideWall()
  }
  prendBiscuit()
  repose()
}

def keepHandOnSideWall() {
  while(testGauche() && !estFaceMur())
  {
    avance()
  }
  gauche()
  if(!estFaceMur())
  	avance()
  else
  {
    retourne()
    if(!estFaceMur())
    	avance()
  }
}

def testGauche():Boolean = {
  var test = false
  gauche()
  test = estFaceMur()
  droite()
  return test
}

def repose() {
  if(getDirection()==Direction.EAST)
  	gauche()
}