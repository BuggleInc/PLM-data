def cocktailSort() {

  var swapped:Boolean =true
  
  
  do {
    swapped=false
    for (i <- 0 to getValueCount()-2) {
    	if ( ! isSmaller(i,i+1)) {
      	swap(i,i+1)
      	swapped=true
      }
    }
    
    for (i <- (getValueCount()-2) to 0) {
    	if ( ! isSmaller(i,i-1)) {
      	swap(i,i-1)
      	swapped=true
      }
    }
    
  } while (swapped)
}
