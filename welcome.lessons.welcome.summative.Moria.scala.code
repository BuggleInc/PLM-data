def trouverBiscuit(){
  setDirection(Direction.EST)
  while (!estSurBiscuit()){
    avance()
  }
  prendbiscuit()
  setDirection(Direction.OUEST)
  while (!estSurBiscuit()){
    avance()
  }
  poseBiscuit()
  setDirection(Direction.EST)
  avance()
}

def sortie(): Boolean={
