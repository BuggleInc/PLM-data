def cocktailSort2() {
	var swap = true;
	var fin = getNombreValeurs()-2;
	var debut = 0;
	do
	{
		swap = false;
		
		for (i <- debut to getNombreValeurs()-2)
		  if (plusPetit(i+1,i))
		 {
			echange(i,i+1);
			swap = true
		 }
		fin = fin -1; 
         for (j <- fin to 0 by -1)
          if (plusPetit(j+1,j))
		 {
			echange(j,j+1);
			swap = true
		 }
		debut = debut+1;

          	} while(swap)		
}
