def pentaKoch(levels:Int, length:Double) {
if (levels !=0)
{
	pentaKoch(levels-1, length/3)
	gauche(90)
	pentaKoch(levels-1, length/3)
	droite(90)
	squareSide(levels-1, length/3)
	droite(90)
	squareSide(levels-1, length/3)
	gauche(90)
	squareSide(levels-1, length/3)
	
	
}
else
{
	avance(length)
}
}






