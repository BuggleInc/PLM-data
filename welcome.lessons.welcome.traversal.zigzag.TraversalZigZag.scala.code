override def run() {
  var cpt=0
  var i=0
  while(cpt<=48){
    if (pair(i)){
      setPos(0,i)
      while(!isWallR()){
        ecritMessage(cpt)
        setPos((cpt+1)%getMondeHauteur(),i)
        cpt=cpt+1
      }
    }
    else {
      setPos(6,i)
      while(!isWallL()){
        ecritMessage(cpt)
        setPos((getMondeLargeur()-(cpt+1)%getMondeLargeur()),i)
        cpt=cpt+1
      }
    }
    i=i+1
  }
}



