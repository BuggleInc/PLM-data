override def run() {
  var cpt=0

  do {

    for(y <- 0 to getMondeHauteur()-1){
      if (y%2==0){
        for (x <- 0 to getMondeLargeur()-1)
        {	setPos(x,y)

         ecritMessage(cpt)
         cpt = cpt + 1
        }



      }
      else
      {
        for (x <- getMondeLargeur()-1 to 0)
        {	setPos(x,y)

         ecritMessage(cpt)
         cpt = cpt + 1
        }
      }



    }

  } while (cpt==48)

}