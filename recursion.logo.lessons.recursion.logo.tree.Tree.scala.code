def tree(steps:Int, length:Double, angle:Double, shrink:Double)    
{
  steps match { case 0 => return
                case steps => current(steps); avance(length); droite(angle); tree(steps-1,shrink*length,angle,shrink);gauche(angle);gauche(angle);tree(steps-1,shrink*length,angle,shrink);current(steps);recule(length);
  
  
              }  
  
}