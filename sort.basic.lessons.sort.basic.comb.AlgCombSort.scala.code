def combSort() {
	var ecart = getNombreValeurs();
     var swap = false;
     var i = 0;
		do
       	{
			if (ecart > 1)
				ecart = (ecart.toDouble / 1.3).toInt
			if (ecart < 1)
				ecart = 1;
			i = 0;
			swap = false;
			do
			{
        		  if (plusPetit(i+ecart,i))
		       {
			    echange(i,i+ecart);
            	    swap = true
		       }
		       i+=1;

			} while (i+ecart < getNombreValeurs())
       		
       	} while (ecart > 1 || swap)
}
