def solve() {
  var apresbleu=0
  var blancrouge=getTaille()-1
  var avantBlanc=getTaille()-1
  var c:Int =0
  while (c<getTaille()){
    avantBlanc=blancrouge
    if (getCouleur(c)==BLEU){
      echange(c,apresbleu)
      print(apresbleu + avantBlanc)
      apresbleu+=1
    }
    if (getCouleur(c)==ROUGE){
      echange(c,blancrouge)
      blancrouge-=1
    }
    if (getCouleur(c)==BLANC){
      echange(c,avantBlanc)
      avantBlanc-=1
    }
    c+=1
  }
}