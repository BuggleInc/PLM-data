var diag = 0;
var a : Int = 0
var counter : Int = 0;
var finalpos : Boolean = true;
override def run() {
    if( diag <= 7){
      for(y <- 0 to 6){
        for (a <- 0 to y){
          setPos(y-a,y)
          writeMessage(counter)
          counter = counter + 1
        }
        diag = diag + 1
      }
 
    }
  if (diag > 7){
    for ( x <- 0 to 6){
      for (y <- 0 to 6){
        setPos(x,6-y)
        writeMessage(diag)
        counter=counter+1
      }
      diag= diag + 1
    }
  }
}