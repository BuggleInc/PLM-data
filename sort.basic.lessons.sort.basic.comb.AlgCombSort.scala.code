def combSort() {
var lg = getNombreValeurs()
var ecart = getNombreValeurs()
var cond= true 

do {if ( ecart>1) {
	ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
	}
	cond=true
	for ( i <- 0 to lg-ecart-1)
 {if (plusPetit(i+ecart,i)) {
 	cond= false 
 	echange(i+ecart, i)	
}
}
}
while (ecart>1 || cond==true)
}