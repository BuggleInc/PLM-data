def solve() {
  var afterBlue:Int = 0
  var beforeWhite:Int = getSize()-1
  var afterWhite:Int = getSize()-1
  while (afterBlue<beforeWhite) {
    for (x <- 0 to getSize()-1) {
      if (getColor(x)==BLUE) {
        swap(x,afterBlue)
        afterBlue+=1
      } else { 
        if (getColor(x)==RED) {
          swap(x,afterWhite)
          afterWhite-=1
        } else {
          swap(x,beforeWhite)
          beforeWhite-=1
        }
      }
    }
  }
}
