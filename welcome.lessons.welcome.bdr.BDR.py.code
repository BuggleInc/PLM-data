while estSurMessage():
       if litMessage("A"):
            avance(1)
        elif litMessage("L"):
            gauche()
            avance(1)    
        elif "R":
            droite(1)
            avance(1)
        elif "B":
            avance(2)    
        elif "I":
            retourne()
            avance()
        elif "Z":
            recule()
        elif "C":
            avance(3)
        elif "Y":
            recule(2)
        elif "X":
            recule(3)

          