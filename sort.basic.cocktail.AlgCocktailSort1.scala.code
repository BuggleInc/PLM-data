def cocktailSort() {
  var len=getValueCount()
  var bool=false
  for(j<-0 to len-2){
		bool=true
    if(j%2==0){
  	for(i<-0 to len-2-j){
      if(!isSmaller(i,i+1)){
        swap(i,i+1)
        bool=false}
      }
      }
    else{
      for(i<-0 to len-2-j){
      if(isSmaller(i,i+1)){
        swap(i,i+1)
        bool=false}
    }
    if (bool==true){
      return
    }
  }
}
}