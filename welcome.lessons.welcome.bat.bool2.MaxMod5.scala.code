def maxMod5(a:Int, b:Int):Int = { 
  var r1:Int=a%5
  var r2:Int=b%5
  
if (a==b){
  return 0
}
  else if (r1==r2){
    return min(a,b)
  }
  else{
    return max(a,b)
  }}

def min(a:Int,b:Int):Int={
  if (a>b){
    return b
  }
  else {return a}
}

def max(a:Int,b:Int):Int={
  if (a>b){
    return a
  }
  else {return b}
}
