def solve() {
  var lgr = getTaille();
	var bleu = 0;
  var blanc = 0;
  var rouge = 0;
  while((bleu+rouge+blanc)!=lgr){
    for(i<-0 to lgr-1){
      getCouleur(i) match {
        case BLEU => echange(i,bleu+1); bleu += 1;
        case BLANC => blanc += 1;
        case ROUGE => echange(i,lgr-rouge-1); rouge += 1;
      }
    }
  }
}