public void run() {
	keepHandOnSideWall();
}


void keepHandOnSideWall(){
	while(!overBaggle()){
		if(!wallOnLeft()){
			left();
			forward();
		} else {
			stepForward();
		}
	}
	pickupBaggle();
}


void stepForward(){
	if(!isFacingWall())
		forward();
}

void faceEmptyAndLeftWall(){
	while(isFacingWall() && !wallOnLeft())
		left();
}

boolean wallOnLeft(){
	left();
	boolean a = isFacingWall();
	right();
	return a;
}
