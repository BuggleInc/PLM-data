def lastDigit(a:Int, b:Int, c:Int):Boolean = {
	if (Digit(a,b)){
		return true;
	}
	if (Digit(b,c)){
		return true;
	}
	if (Digit(a,c)){
		return true;
	}else{
		return false;
	}
	
}





def Digit(a:Int, b:Int):Boolean = {
          if ( Math.abs( ( dec(a) - dec (b) )) == 0 ){
          	return true;
          }else{
          	return false;
          }
}



 def  dec(a:Int):Int = {
 	return  (a % 10);
 	
 }
