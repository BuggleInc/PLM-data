override def run() {
  for ( i <- 0 to 3) {
    drawHouse();right(90);forward(50);left(90)
  }
}

def drawHouse() {
  penDown()
  for (i <- 0 to 3) {
    forward(30);right(90)
  }
  forward(30);right(30);forward(30);right(120);forward(30);right(30)
  forward(30);right(90);forward(30);right(90); penUp()
}