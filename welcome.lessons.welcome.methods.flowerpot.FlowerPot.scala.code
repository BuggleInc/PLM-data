def flower(c:Color) {
	setCouleurBrosse(c);
	baisseBrosse();
	for (petale <- 1 to 4) {
		avance();
		recule();
		gauche();
	}
	setCouleurBrosse(Color.YELLOW);
	leveBrosse();
	move(4,true);
	droite();
}
def move(nbPas : Int, versLAvant : Boolean) {
	for (pas <- 1 to nbPas) {
		if (versLAvant) {
			avance();
		} else { 
			recule();
		}
	}
}
def growFlowers() {
	avance();
	flower(Color.RED);
	flower(Color.CYAN);
	flower(Color.GREEN);
	flower(Color.PINK);
	
}
	
