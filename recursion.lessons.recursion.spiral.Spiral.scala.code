def spiral(steps:Int, angle:Int, length:Int, increment:Int)    {

var longueur=length
var nbPas=steps

	avance(length)
	gauche(angle)
	nbPas-=1
	longueur=longueur+increment

		if(steps=0)
		{
		return
		}
		else
		{
		spiral()
		}

}

























