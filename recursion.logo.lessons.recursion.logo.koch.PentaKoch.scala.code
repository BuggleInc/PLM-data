def pentaKoch(levels:Int, length:Double) {}
  if (levels==0) {
    avance(length)
  }
  else {
  	pentaKoch(levels-1,length/3)
  	gauche(72)
  	pentaKoch(levels-1,length/3)
  	droite(72)
  	pentaKoch(levels-1,length/3)
  	droite(72)
  	pentaKoch(levels-1,length/3)
    droite(72)
  	pentaKoch(levels-1,length/3)
    gauche(72)
  }
}