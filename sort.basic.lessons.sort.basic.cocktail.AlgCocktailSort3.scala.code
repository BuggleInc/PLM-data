def cocktailSort3() {
	var swap=false
	var debut=0
	var fin=getNombreValeurs()-2
	do{
		swap=false
		for(i<-debut to fin){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swap=true
			}
		}
		if(swap==false) return
		fin-=1
		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swap=true
			}
		}
		debut+=1
	}while(swap)
}
