override def run() {
  while (!fonctionTest()) {
    baisseBrosse();
    avance();
  }
}

def fonctionTest() : Boolean = {
  if (estFaceMur()) {
    if (getDirection() == Direction.OUEST)
    { retourne(); }
    while (estFaceMur()) {
      droite();
      avance();
      droite();
    } 
    if (getDirection() == Direction.EST)
    { retourne(); }
    while (estFaceMur()) {
      gauche();
      avance();
      gauche();
    }


    return true
  }
  else {return false}
}