def combSort() {
  var ecart=getNombreValeurs()
  var estTriee=false
  while(ecart>1 || !estTriee){
    estTriee=true
    if (ecart>1){
      ecart= (ecart.toDouble / 1.3).toInt
    }
    var i=0
    while(i+ecart<getNombreValeurs()){
      if (!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        estTriee=false
      }
      i=i+1
    }
  }
  
}