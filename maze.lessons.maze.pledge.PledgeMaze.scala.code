override def run() {
  avance()
  while(!estSurBiscuit()){
    while (getDirection!=Direction.NORTH){
      keepHandOnWall()
    }
    direction()
  }
  prendBiscuit()
}
    

def keepHandOnWall(){
  gauche()
  if (estFaceMur()){
    droite()
    if(estFaceMur()){
      droite()
    }else{
      avance()
    }
  }else{
    avance()
  }
}

def direction(){
  if (getDirection()==Direction.NORTH && !estFaceMur()){
    while(!estFaceMur()){
      avance()
    }
  }
}