def bubbleSort3() 
{
  var echangeUseless=false
  for (i<- getNombreValeurs()-2 to 0 by -1)
  	{
      for (j<-0 to i)
      	{
      		if (plusPetit(j+1,j))
      			{
          		echange(j+1,j)
              echangeUseless=false
        		}
          else
          	{
              echangeUseless=true
            }
    		}
      if (echangeUseless){return}
      
    }
}