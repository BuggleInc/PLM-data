def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if(!isBirthday) {
		if(speed > 60 && speed <= 80) {
			return 1
		}
		if(speed > 80) {
			return 2
		}
		else {
			return 0
		}
	}
	else {
		if(speed > 65 && speed <= 85) {
			return 1
		}
		if(speed > 85) {
			return 2
		}
		else {
			return 0
		}
	}
}
