def Moria(){
  while(getCouleurSol()!=Color.green){
     rameneBiscuit()
  }
}

def rameneBiscuit(){
  retourne
  avance
  while (!estSurBiscuit() && estFaceMur==false){
    avance()
  }
  if (estFaceMur){
    droite()
    avance()
    gauche()
    avance()
  }
  else {
  prendBiscuit()
  retourne()
  while (!estSurBiscuit()){
    avance
  }
  recule()
  poseBiscuit()
  }
}

Moria()