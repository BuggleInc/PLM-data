var diag = 0;
var cpt = 0;
override def run() {
  while(cpt<=20)//!finalPos())
  {
    setY(diag)
    doDiag()
    diag = diag+1
  }
  
}

def doDiag()
{
  do
  {
    if(getY()>0)
    	setPos(getX()+1,getY()-1)
    ecritMessage(cpt)
    cpt=cpt+1
  }while(getY()!=0);
}

def finalPos():Boolean = 
{
  return (getMondeHauteur()==getY()+1)&&(getMondeLargeur()==getX()+1)
}