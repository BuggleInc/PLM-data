def solve() {
  var afterBlue:Int = 0
  var beforeWhite:Int = getSize()-1
  var afterWhite:Int = getSize()-1
  var x:Int = 0
  while (afterBlue<beforeWhite) {
    
      if (getColor(x)==BLUE) {
        swap(x,afterBlue)
        afterBlue+=1
      } else { 
        if (getColor(x)==RED) {
          swap(x,afterWhite)
          afterWhite-=1
        } else {
          swap(x,beforeWhite)
          beforeWhite-=1
        }
      }
    x+=1
  }
}
