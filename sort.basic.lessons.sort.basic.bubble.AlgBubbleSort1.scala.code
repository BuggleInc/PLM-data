def bubbleSort()  {
  var nbValue = getValueCount();
  var fin = false;
  while(!fin) {
    var echange = false;
    
    var i = 0; 
    while(i < nbValue-1) {
      var x = getValue(i);
      var y = getValue(i+1);
      
      if(!isSmaller(x,y)){
        swap(i,i+1);
        echange = true;
      }
      
      i+=1;
    }
    
    
    if(echange) {
      fin = false;
    }
  }
}
