def cocktailSort() {
  var estTrie:Boolean = false
  do {
    estTrie = true
    for (i <- 0 to getNombreValeurs()-2) {
    	if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
        estTrie = false
    	}
  	}
  	for (j <- getNombreValeurs()-2 to 0 by -1) {
    	if (getValeur(j)>getValeur(j+1)) {
      	echange(j,j+1)
        estTrie = false
    	}
  	}
  } while (!estTrie)
}
