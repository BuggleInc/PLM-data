def combSort() {
  var ecart:Int = getNombreValeurs()
  var a:Boolean = true
  do {
    a = true
    if (ecart > 1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i:Int = 0
    while (i+ecart < getNombreValeurs()) {
      if (getValeur(i) > getValeur(i+ecart)) {
        echange(i,i+ecart)
        a = false
      }
      i += 1
    }
  } while (ecart >= 1 || a == false)
}