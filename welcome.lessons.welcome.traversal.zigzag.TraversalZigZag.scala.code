def positionFinale():Boolean = {
	if ((getX() == getMondeLargeur()-1) && (getY() == getMondeHauteur()-1)){
		return true
	}
	return false
}
override def run() {
	var cpt=0
	for (i <- 1 to getMondeHauteur()){
		if (getX() == 0){
			for (i <- 1 to getMondeLargeur()){
				ecritMessage(cpt)
				cpt=cpt+1
				setPos(getX()+1, getY())
			}
		}
		if ((getX() == 6) && (!positionFinale())){
			setPos(getX(), getY()-1)
			for (i <- 1 to getMondeLargeur()-1){
				ecritMessage(cpt)
				cpt=cpt+1
				setPos(getX()-1, getY())
			}
			
		}	
		}
		
	}
	


