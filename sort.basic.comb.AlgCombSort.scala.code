def combSort() {
  var ecart =  getNombreValeurs()
    if (ecart>1){
      ecart = ecart/1.3
    }
  var i=0
  var m=1
  while (m!=0 && ecart >1){
  	m=0
    while (i+ecart < getNombreValeurs()){
      if (plusPetit(i+ecart,i)==true){
        echange(i,i+ecart)
        i=i+1
        m=m+1
      }
    }
  }
}