override def run() {
  var positionFinal=false
  while (!positionFinal){
    if(!estFaceMur()){
      avance()
    }else{
      if(getDirection() == Direction.EST){
       gauche()
       avance()
       gauche()
      }else{
        droite()
        avance()
        droite()
      }
      positionFinal=positionFinale()
    }
  }
}

def positionFinale():Boolean{
  while (getDirection()!=Direction.north){
    gauche()
  }
  if (estFaceMur){
    return true
  }else{
    return false
  }
}