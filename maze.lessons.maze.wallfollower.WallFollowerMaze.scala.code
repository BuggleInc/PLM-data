def keepHandOnSideWall()
{
  // s'il y a un mur face au buggle, il tourne ? droite
  // sinon il avance et 
  
  if (!estFaceMur())
  {
    avance()
    
    gauche()
    if (estFaceMur())
    	droite()
  }
  else
    droite()
}

override def run()
{
  while(!estSurBiscuit())
  {
    keepHandOnSideWall()
  }
  
  prendBiscuit()
}