def step() {
  if (getCouleurSol()==Color.black){
    setCouleurBrosse(Color.white)
    baisseBrosse()
    leveBrosse()
    gauche()
    droite()
  } else if (getCouleurSol()==Color.black){
      setCouleurBrosse(Color.white)
      baisseBrosse()
      leveBrosse()
      gauche()
      droite()
  }
}