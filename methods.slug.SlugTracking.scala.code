def isFacingTrail():Boolean = {
  var go = true
  while (go==true){
    avance()
    if (getCouleurSol()==Color.green){
      go==true
      baisseBrosse()
      leveBrosse()
      if (estSurBiscuit()){
        return false
      }
    }else{
      recule()
      go=false
    }
  }
  return go
}



