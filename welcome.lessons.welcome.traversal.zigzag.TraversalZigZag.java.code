def run() {
  
  var xf:Int = getWorldWidth() - 1
  var yf:Int = getWorldHeight() - 1
  var counter:Int = 0
  var toggle:Boolean = false
  
  while (true){
      
    var x = getX()
    var y = getY()
    
   	writeMessage(counter)
    
    if (xf == x && yf == y){
      return
    }  
    
    if ((x == xf || x == 0) && counter != 0) {
      setPos(x, y + 1)
      toggle = !toggle
    }
    
    if (toggle){
      setPos(x - 1, y)
    }else{
      setPos(x + 1, y)
    }
    
  	counter += 1
  }
}