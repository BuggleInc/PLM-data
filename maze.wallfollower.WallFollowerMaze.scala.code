def isWallOnTheLeft () : Boolean = {
  left();
  var answer = isFacingWall();
  // back to the initial position :
  left();
  return answer;

}

def makeMoving() {
  while (isWallOnTheLeft() && !isFacingWall()) {
    forward();
  }
  left();
}

override def run() {
	while (!isOverBaggle()) {
    makeMoving();
  }
  pickupBaggle();
}