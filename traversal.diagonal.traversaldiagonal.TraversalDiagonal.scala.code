override def run() {
  var diag=0
  var x=0
  var y=0
  var compteur=0
  while (x==getMondeLargeur && y==getMondeHauteur){
    while (y!=0){
      ecritMessage(compteur)
      y=y-1
      x=x+1
      compteur = compteur+1
      setPos(x,y)
    }
    diag = diag +1
    y=diag
    x=0
    compteur = compteur+1
    setPos(x,y)
  }
}