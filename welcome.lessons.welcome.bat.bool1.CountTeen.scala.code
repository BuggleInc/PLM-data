
def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
	var compteur:Int=0
	if ( ado(a)){
		compteur=compteur+1 
	} 
	if ( ado(b)) {
		compteur=compteur+1 
	} 
	if ( ado(c)) {
		compteur=compteur+1 
	}	
	if ( ado(d)){
		compteur=compteur+1 
	}
	return compteur
}
def ado(n:Int):Boolean=
{
	return 13<=n && n<=19
}