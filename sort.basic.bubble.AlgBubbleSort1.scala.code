def bubbleSort()
{
  var n = getNombreValeurs()-1;
  var i = 0;
  
  while (i < n)
  {
    for (j <- 0 to i-1)
    {
      if (plusPetit(i,j)) echange(i,j);
    }
    i+=1;
  }
}




