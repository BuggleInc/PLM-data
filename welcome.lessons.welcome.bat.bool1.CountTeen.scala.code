def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var count:Int = 0
  if(isTeen(a))
  	count += 1
  if(isTeen(b))
  	count += 1
  if(isTeen(c))
  	count += 1
  if(isTeen(d))
  	count += 1
  return count
}
  
def isTeen(a:Int) : Boolean = {
  if(a >= 13 && a <= 19)
  	return true
  else
  	return false
}