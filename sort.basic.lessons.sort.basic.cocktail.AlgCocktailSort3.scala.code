def cocktailSort3() {
	var debut=0
	var fin=getNombreValeurs()-2
	var fini1=false
	var fini2=false
	
	do {
		fini=true
		for (i<-debut to fin) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				fini=false
			}
		}
		fin-=1

		for (i<-fin to debut by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				fini=false
			}
		}
		debut+=1
		if (fini1!=fini2 || (fini1==false&&fini2==false)) return()
	} while (fini==false&&fini2==false)
		
}

	


	
