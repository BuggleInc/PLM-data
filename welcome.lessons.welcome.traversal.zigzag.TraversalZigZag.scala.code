override def run() {
var c:Int=0
	for(x<-0 to getMondeHauteur()-1) {
		setPos(x,0)
		ecritMessage(c)
		c=c+1
	}
	for (y<-1 to 6)
	{
		for (x<-(0 to getMondeHauteur()-1).reverse) 
	{  
		setPos(x,y)
		ecritMessage(c)
		
		c=c+1
	}
		for(x<-0 to getMondeHauteur()-1)
	{
		setPos(x,y+1)
		ecritMessage(c)
		c=c+1
	}	
	}
setPos(6,6)
}
