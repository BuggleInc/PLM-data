def cocktailSort3() {

var debut : Int = 0;

var fin: Int = getNombreValeurs()-2;

var trie: Int = 0;

while (trie == 0) {
   trie = 1;
   
 for(i <- debut to fin)
   	{
  		 if(getValeur(i+1) < getValeur(i))
    	{  
  echange(i+1,i);
  trie = 0;
    	}
     else {trie=1;}
    	
  	}
 fin = fin-1;
 for(i <- fin to debut by -1) {
	 
  if(getValeur(i+1) < getValeur(i)) {
   echange(i+1,i);
   trie = 0;
  }
  
  
 }
 
  debut = debut +1;
}
 







}
