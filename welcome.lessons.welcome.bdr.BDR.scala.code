var Msg ="A";
while (estSurMessage) {	
	Msg=litMessage();
	if (Msg=="R") {droite();avance();}
	if (Msg=="L") {gauche();avance();}
	if (Msg=="I") {droite();droite();avance();}
	if (Msg=="A") avance();
	if (Msg=="B") avance(2);
	if (Msg=="C") avance(3);
	if (Msg=="Z") recule();
	if (Msg=="Y") recule(2);
	if (Msg=="X") recule(3);	
}
