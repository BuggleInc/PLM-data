def hunt() {
  while (porteBiscuit==false) {
    if (estSurBiscuit==true){
      prendBiscuit()
    }
    else if (isFacingTrail==true) {
      avance()
      baisseBrosse()
      leveBrosse()
    }
    else {
      gauche()
    }
  }
  
}




