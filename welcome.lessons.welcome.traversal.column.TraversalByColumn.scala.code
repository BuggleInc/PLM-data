override def run() {
  var comp = 0;
  var col = 0;
  var lign = 0;
  while(!(positionFinale())) {
  	if (lign==7) {
      col=col+1;
      setPos(lign,col);
      lign=0;
    } else {
      setPos(col,lign);
      lign = lign + 1;
    }
  }
}

def positionFinale():Boolean = {
  var res = false;
  if (getX()==6  && getY()==6) {
    res = true;
  }
  return res
}