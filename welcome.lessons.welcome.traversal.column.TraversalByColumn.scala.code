override def run() {
  var i=0
  do{
    ecritMessage(i+"")
    nextPos()
  }while(posFinale()==false)
}

def posFinale(): Boolean={
  if (getX()==getMondeLargeur & getY()==getMondeHauteur())
  	return true
  return false
}

def nextPos(){
  if (getY()==getMondeHauteur()){
    setPos(getX()+1,0)
  }
}