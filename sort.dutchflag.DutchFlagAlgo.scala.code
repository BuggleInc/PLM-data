def solve() {
  var afterBlue =0
  var beforeRed =getSize()-1
  while (getColor(beforeRed)==RED){
    beforeRed=beforeRed-1
  }
  
  while (getColor(afterBlue)==BLUE){
    afterBlue=afterBlue+1
  }
  for (i <- afterBlue to beforeRed){
    if (getColor(i)==BLUE){
      swap(afterBlue,i)
      afterBlue=afterBlue+1
    }
    else if (getColor(i)==RED){
      swap(beforeRed,i)
      beforeRed=beforeRed-1
    }
  }
}