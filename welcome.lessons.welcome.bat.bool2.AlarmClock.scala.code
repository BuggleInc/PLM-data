def alarmClock(day:Int, vacation:Boolean): String = {
	if ( (day == 1 || day == 2 || day == 3 || day == 4 || day == 5) && vacation == false ) {
		return "7:00" 
	} else if (( (day == 0 || day == 6) && !vacation) ||
	( (day == 1 ||day == 2 || day == 3 || day == 4 || day == 5) && vacation == true )) {
		return "10:00"
	} else if ((day == 0 || day == 6) && vacation) {
		return "off"
	}
}
