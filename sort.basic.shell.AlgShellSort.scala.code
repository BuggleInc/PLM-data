def shellSort()  {
  var gap=getValueCount()/2
  while (gap>0) {
    for (i <- 1 to getValueCount() -1) {
      var j=1
      var v=getValue(i)
      while (i-j*gap>=0 && ! isSmallerThan(i-j*gap,v)) {
        copy(i-j*gap,i-j*gap+1)
        j=j+1
      }
      setValue(i-j*gap+1,v)
    }
    gap=gap/2
  }
}