def golomb(num:Int): Int = {
  if (num == 1) {return 1}
  else if (num == 2) {return 2}
  else if (num == 3) {return 2}
  else {
    var a = new Array[Int](num+1)
    a(1)=1
    a(2)=2
    a(3)=2
    a(num)=0
    var ecrire = 4
    var c = 3
    while (a(num)==0) {
      for (i <- 1 to a(c)) {
        if (ecrire < a.length) {
          a(ecrire) = c
        	ecrire = ecrire + 1
        }
      }
      c = c + 1
    } 
    return a(num)
  }
}