def solve() {
  var apresBleu=0
  var blancRouge=getTaille()-1
  var apresBlanc=getTaille()-1
  while(!estTrie){
    getCouleur(apresBleu) match {
    case BLUE => apresBleu=apresBleu +1
    case RED => echange(apresBleu,blancRouge);blancRouge=blancRouge +1
      case WHITE => echange(apresBleu,apresBlanc);apresBlanc=apresBlanc-1;if (apresBlanc>blancRouge) {apresBlanc=blancRouge
  	}
	}
}}