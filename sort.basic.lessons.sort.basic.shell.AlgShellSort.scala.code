def shellSort()  {
  var ecart = getNombreValeurs()/2
  var v = 0
  var j = 0
  while (ecart > 0) {
    for (i <- 1 to getNombreValeurs()-1) {
      v=getValeur(i)
      j=i
      while (j!=0 && !plusPetitQue(j-1,v)) {
        copie(j-ecart,j)
        j-=ecart
      }
      setValeur(j,v)
    }
  }
}