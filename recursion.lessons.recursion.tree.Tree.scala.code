def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {

	current(steps)
	avance(length)
	
	if(steps != 1) {
		droite(angle)
		tree(steps - 1,length*shrink,angle,shrink)
		gauche(2*angle)
		tree(steps-1,length*shrink,angle,shrink)
		droite(angle) 
		recule(length)
		
	}

	else {
		recule(length)
	}
	
	
			
}
