def haveBaggle():Boolean = 
{
  var vuBiscuit = false
  var nbPas = 0
  do
  {
    if (estSurBiscuit)
    {
      vuBiscuit = true
      for (i <- 1 to nbPas)
      {
        recule()
      }
      return true
    }
    else
    {
      avance()
      nbPas += 1
    }
  } while (!estSurBiscuit || nbPas < 6)
  for (i<- 1 to nbPas)
  {
    recule()
  }
  droite()
  avance()
  gauche()
  return false
}