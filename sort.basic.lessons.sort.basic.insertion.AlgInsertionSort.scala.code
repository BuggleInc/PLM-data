def insertionSort() {
	for (i <- 1 to getNombreValeurs() - 1) {
		var valeur = getValeur(i)
		var nouvelleCase = i
		var j = i - 1
		
		while (j != -1) {
			if (getValeur(j) > valeur) {
				copie(j,j+1)
				nouvelleCase = j
				j = j - 1
			} else j = -1
		}
		
		setValeur(nouvelleCase,valeur)
	}
}
