$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; import lessons.recursion.cons.universe.*;public class length extends BatEntity { public IntSequence cons(int head, IntSequence tail){  return new IntSequence(head, tail);} public void run(BatTest t) {
		t.setResult( length(IntSequence.fromIntArray((int[])t.getParameter(0))) ); }
	int length(IntSequence seq) {
		if (seq == null)
			return 0;
		return 1+length(seq.tail);
	}
 }