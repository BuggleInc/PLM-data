var etat = 0
override def run() {
  while(!estSurBiscuit()){
    etat match  {
      case 0 => while(!estFaceMur()){avance()};etat = 1
      case 1 => gauche();if(!estFaceMur()){avance()};else{keepHandOnSideWall()};etat = 0
    }
    gauche()
    if(!estFaceMur()){
      avance()
    }else{
      keepHandOnSideWall()
    }
   
  }
  prendBiscuit()
}

def keepHandOnSideWall(){
  while(!estFaceMur()){
    gauche()
  }
  while(estFaceMur()){
    droite()
  }
  avance()
}