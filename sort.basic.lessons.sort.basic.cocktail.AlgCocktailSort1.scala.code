def cocktailSort() {
  var n = getNombreValeurs()
  var end = true
  
  while(end)
  {
    end = false
    for(i<-0 to n-2)
    {
     	if (plusPetit(i,i+1)==false)
      {
        echange(i,i+1)
        end = true
      }
    }
    for(i<-n-2 to 0)
    {
      if (plusPetit(i,i+1)==false)
      {
        echange(i,i+1)
        end = true
      }
    }
  }
}