while sortieTrouvee() == False:
    premiereFois = True
    nbBiscuit = 0
    while premiereFois or not croisement():
        premiereFois = False
        if estSurBicuit() == True:
            nbBiscuit = nbBiscuit + 1
        if nbBiscuit >= 3:
            gauche()
        else:
            droite()

