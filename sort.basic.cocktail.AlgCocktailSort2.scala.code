def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var permutation=false
  do {
    permutation=false
    for (i<- debut to fin){
      if (plusPetit(i,i+1)==false){
        echange(i,i+1);
        permutation=true;
        fin=fin-1;
      }
    }
   for (i<-fin to debut){
     if (plusPetit(i,i+1)==false){
       echange(i,i+1);
       permutation=true;
       debut+=1;
     }
   }
  } while (permutation==true)
}