def insertionSort() {var l=0
  for (i<- 1 to getNombreValeurs()-1)
                     {var v=getValeur(i)
                     for (k<- i to 1 by -1)
                     {if (plusPetit(v,getValeur(k-1)))
                      {copie(k-1, k)
                       l=k}
                       copie(v,l) }
                     }
}