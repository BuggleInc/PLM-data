def shellSort()  {
  var lgr=getNombreValeurs
  var ecart=lgr/2
  while (ecart>0) {
    insertionSort(ecart)
    ecart=ecart/2
  }
  insertionSort(ecart)
}



def insertionSort(x:Int) {
  var lgr=x
  for (i <- 1 to lgr-1) {
    var v=getValeur(i)
    var j=i
    while ((j>0) && (!plusPetitQue(j-1,v))) {
      copie(j-1,j)
      j-=1
    }
    setValeur(j,v)
    }
  }