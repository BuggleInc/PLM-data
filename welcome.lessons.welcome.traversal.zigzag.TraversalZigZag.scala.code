def fini():Boolean = {
  if(getX()==6 && getY()==6){
    return true
  }
  return false
}

override def run() {
  
  var nb:Int = 0;
  var x:Int = 0;
  var y:Int = 0;
    
  while(!fini()){
    
    y = (nb-nb%7)/7;
    x = nb%7;
    
    if(y%2==1){
      x=6-x;
    }
    
    setPos(x,y);
    
    ecritMessage(nb);
    
    nb += 1;
    
  }
  
}