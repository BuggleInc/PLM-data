void getColor() {
	Color[] T=new Color[getMondeHauteur()];
	for (int nbCase=0; nbCase<getMondeHauteur()-1;nbCase++) {
		T[nbCase]=getCouleurSol();
		avance();
		recule(getMondeHauteur());
	}
}

void doColonne() {
	for (int nbCasesFaites; nbCasesFaites<getMondeHauteur()-1; nbCasesFaites++) {
		setCouleurBrosse()=T[nbCasesFaites];
		baisseBrosse();
		leveBrosse();
		avance();
	}
}

public void run() {
	getColor();
}
