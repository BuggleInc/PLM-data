var diag=0
override def run() {
  var cpt = 0
  ecritMessage(cpt)
  diag = diag+1
  for(i<-0 to 12){
    cpt = diago(cpt)
  }
  
}

def diago(cptV:Int):Int = {
  var cpt = cptV
  for(i<- 0 to (diag%getMondeHauteur())-1){
    ecritMessage(cpt)
    cpt = cpt+1
    setPos(getX()+1,getY()-1)
  }
  if(diag<getMondeHauteur()-1){
    diag = diag+1
    setPos(0,diag)
  }
  else{
    diag=diag+1
    setPos(diag%getMondeHauteur(),getMondeHauteur()-1)
  }
  return cpt
}