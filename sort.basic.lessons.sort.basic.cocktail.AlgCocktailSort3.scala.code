def cocktailSort3() {
	var debut:Int =0
	var fin:Int = getNombreValeurs()-2
	var isechange_c = false
	var isechange_d = false
	do {
		isechange_c = false
			for (i<-debut to fin) {
				if (plusPetit(i+1,i)) {
					echange(i+1,i)
					isechange_c = true
				}
			}
		fin -= 1
		isechange_d = false
		for (i<-fin to debut by -1) {
				if (plusPetit(i+1,i)) {
					echange(i,i+1)
					isechange_d = true
				}
			}
		 debut += 1
	} while(isechange_c && isechange_d)
}
