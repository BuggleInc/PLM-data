def array123(nums:Array[Int]): Boolean = {
  var un:Boolean = false
  var deux:Boolean = false
  var trois:Boolean = false
  for (i <- nums)
  {
    if (i==1)
    	un = true
    if (i==2)
    	deux = true
    if (i==3)
    	trois = true
  }
  if (un && deux && trois)
  	return true
  else
  	return false
}