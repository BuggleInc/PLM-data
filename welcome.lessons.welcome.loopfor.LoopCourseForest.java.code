public void run() {
int laps = 7;
int steps = 64;

	for(int x = 1; x <= laps; x++){
		
		for(int y = 1; y <= steps; y++){
			forward();
			if(y == 4){
				left();
				forward();
				forward();
				right();
			}
			if(y == 8){
				right();
				forward();
				forward();
				left();
			}
			if(y == 12){
				left();
				forward();
				forward();
				forward();
				forward();
				left();
			}
		}

	}
}
