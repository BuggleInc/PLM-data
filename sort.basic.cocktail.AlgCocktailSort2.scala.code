def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs() - 2
  var aInverse = true
  while (aInverse) {
    aInverse = false
    for (i <- debut to fin) {
      if (getValeur(i+1) < getValeur(i)) {
        echange(i+1,i)
        aInverse = true
      }
    	fin -= 1
    }
    for (j <- fin to debut by -1) {
      if (getValeur(j+1) < getValeur(j)) {
        echange(j+1,j)
        aInverse = true
      }
      debut += 1
    }
  }
}