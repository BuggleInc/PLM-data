def cocktailSort3() {
	var lgr : Int = getNombreValeurs();
	var debut : Int = 0;
	var fin : Int = lgr -2;
	var eff : Int = 1;
	
	while(eff == 1){
		eff = 0;
		var i : Int = debut;
		
		while(i <= fin) {
			if(plusPetit(i+1, i)){
				echange(i, i+1);
				eff = 1;
			}
			i = i+1;
		}
		fin -= 1;

		if(eff == 0)
			return;
		
		i = fin;
		while(i >= debut){
			if(plusPetit(i+1, i)){
				echange(i, i+1);
				eff = 1;
			}
			i = i-1;
		}
		debut += 1;
	}
}
