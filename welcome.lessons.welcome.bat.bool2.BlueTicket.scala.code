def blueTicket(a:Int, b:Int, c:Int):Int = {
		var d = a+b ;
		var e = b+c;
		var f = a+c;
		var m = e + 10;
		var n = f + 10;
		(d,e,f) match {
			case (10,_,_) => return 10 
			case (_,_,10) => return 10 
			case (_,10,_) => return 10
			}
			if( d == m || d == n){
				return 5
			}else{
				return 0
			}
}