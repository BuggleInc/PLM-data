def cocktailSort3() {
	var chgtFait : Boolean = true
	var debut : Int = 0
	var fin : Int = getNombreValeurs()-2
		for (i <- debut to fin) {
			chgtFait = false
			if (plusPetit(i+1, i)) {
				echange(i, i+1)
				chgtFait = true
			}
		}
		fin -= 1
		if (chgtFait == false) {
				return
			}
		for (i <- fin to debut by -1) {
			chgtFait = false
			if (plusPetit(i+1, i)) {
				echange(i, i+1)
				chgtFait = true
			}
		}
		debut += 1
		if (chgtFait == false) {
				return
		}
}
