def combSort() {
  var lgr = getNombreValeurs()
  var ecart = lgr
  var NbEch = 1
  var i = 0
  do{
    if (ecart>1){
      ecart=(ecart.toDouble / 1.3).toInt
    }
    NbEch=0
    if (plusPetit(i+ecart,i)){      
      echange(i,i+ecart)
      i=i+1
      NbEch=1
  }
  }
  while (i+ecart < lgr || NbEch!=0)
}
    