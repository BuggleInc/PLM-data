def blueTicket(a:Int, b:Int, c:Int):Int = {
  var sum1:Int = a + b
  var sum2:Int = a + c
  var sum3:Int = b + c
  if(sum1 == 10 || sum2 == 10 || sum3 == 10)
    return 10
  else if(Math.abs(sum1 - sum2) == 10 || Math.abs(sum1 - sum3) == 10 || Math.abs(sum2 - sum3) == 10)
  	return 5
  else
  	return 0
}