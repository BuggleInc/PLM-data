var compteur:Int = 0;

def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  compteur = 0;
  if (verif(a)){
    compteur += 1;
  }
  if (verif(b)){
    compteur += 1;
  }
  if (verif(c)){
    compteur += 1;
  }
  if (verif(d)){
    compteur += 1;
  }
  return compteur;
}

def verif(a:Int):Boolean = {
  if ( a<=19 && a>=13 ){
    return true;
  } else {
    return false;
  }
}
