def golomb(num:Int): Int = {
	var tab = new Array[Int](num)
	var avancee = 1
	var progres = 1
	tab(0) = 1
	while (avancee != num -1){
		var x = progres + 1
		while (x > 0){
			tab(avancee) = x
			x = x-1
			avancee = avancee + 1
			if(avancee == num - 1){
				return avancee
			}
		}
		
	}
	return avancee
}
