def selectionSort() {
  var finale = 0
  var temp = 10000
  var j = -1
  while (finale==0) {
    j=j+1
    finale=1
    for (i <- 1 to getNombreValeurs-2) {
      if (plusPetitQue(i,temp)) {
        temp = i
        finale=0
      }
    }
    if (finale==0) {
    	echange(temp,j)
      temp = temp + 1
    }
  }
}