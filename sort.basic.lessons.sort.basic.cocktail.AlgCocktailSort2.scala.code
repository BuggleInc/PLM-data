def cocktailSort2() {
var x= true
while (x==true){
		x=false
	var debut =0
	var fin = getNombreValeurs()-2
	for (i<- debut to fin) {
		if (plusPetit(i+1,i)){
			echange(i,i+1)
			x=true
		}
		fin -=1
	}
	for (i<- fin to debut by -1){	
		if  (plusPetit(i+1,i)) {
			echange(i+1,i)
			x=true		
		}
		debut = debut + 1
	}	
}
}