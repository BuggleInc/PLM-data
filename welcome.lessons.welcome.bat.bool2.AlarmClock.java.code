String alarmClock(int day, boolean vacation) {
boolean weekday=weekDay(day);
if (weekday && !vacation){
	return "7:00";
}
if ((!weekday && !vacation) || (weekday && vacation) ){
	return "10:00";
}
else {
	return "off";
}
}


boolean weekDay(int day){
	if (day==0 || day==6){
		return false;
	}
	else {
		return true;
	}
}
