def combSort() {
	var ecart:Int = getNombreValeurs()
	var i:Int = 0
	var inverse:Boolean = true 
	
	do { 
		inverse = false
		if ( ecart > 1) { 
			ecart = (ecart.toDouble / 1.3).toInt
		}
		
		while ( i + ecart < getNombreValeurs() ) {
			if ( plusPetit(i+ecart,i) ) {
				echange(i,i+ecart)
				inverse = true 
			}
			i = i + 1
		}
	}
	while ( ecart>1 || inverse )
}