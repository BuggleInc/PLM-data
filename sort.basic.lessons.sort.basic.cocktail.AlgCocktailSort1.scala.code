def cocktailSort() {
  var testFini = 1
  while (testFini == 1) {
    testFini = 0;
    for (i <- 0 to getNombreValeurs()-2) {if (plusPetit(i, i+1) == false) {echange(i, i+1); testFini = 1}};
    for (i <- getNombreValeurs()-2 to 0 by -1) {if (plusPetit(i, i+1) == false) {echange(i, i+1); testFini = 1}}
  }
}