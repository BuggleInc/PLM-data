def cocktailSort() {
  var cpt = 0
  var j = 0
  do{
    for (i<- 0 to getNombreValeurs-2){
      cpt=0
      if (plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt=1
      }
    }
    for (i<- 0 to getNombreValeurs-2){
      j=getNombreValeurs-i-1
      cpt=0
      if (plusPetit(j,j+1)==false){
        echange(j,j+1)
        cpt=1
      }
    }
  } while (cpt==1)
}