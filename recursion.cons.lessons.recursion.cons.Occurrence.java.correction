$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; import lessons.recursion.cons.universe.*;import java.util.Vector;public class Occurence extends BatEntity { public RecList cons(int head, RecList tail){  return new RecList(head, tail);} @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( occurences( (RecList)t.getParameter(0), (int)t.getParameter(1) ) ); }
	int occurences(RecList seq, int val) {
		if (seq == null)
			return 0;
		if (seq.head == val)
			return 1 + occurences(seq.tail, val);
		return occurences(seq.tail,val);
	}
 }