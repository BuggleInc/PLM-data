override def run() {
  carre
}

def cote(c: Color){
  setCouleurBrosse(c)
  avance()
  baisseBrosse()
  leveBrosse()
  recule()
  gauche()
  avance()
  baisseBrosse()
  leveBrosse()
  recule()
  droite()
  avance(3)
  gauche()
}

def carre(){
  cote(Color.red)
  cote(Color.blue)
  cote(Color.green)
  cote(Color.yellow)
}