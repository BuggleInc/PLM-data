def cocktailSort2() {
	var m=0
  var n=2
  var sorted=false
  while (m!=getNombreValeurs()){ 
    m=m+1
    sorted = true
    for (i<-m to getNombreValeurs()-n){

      if (!plusPetit(i,i+1)){
        sorted=false
        echange(i,i+1)
      }

    }
    for (i<- getNombreValeurs()-n to 0 by -1){

      if (!plusPetit(i,i+1)){
        sorted=false
        echange(i,i+1)
      }

    }
    n=n+1
  }


}