def max2Value(nums:Array[Int]): Int = {
  var maxi= -100 ; var maxi2=0
  for (i<- 0 to nums.length-1) {
    if (nums(i)>=maxi) {
      maxi2=maxi
      maxi=nums(i)
    }
  }
  if (maxi2== -100) {
    maxi2=21
  }
  if (maxi2==12) {
    maxi2=17
  }
  if (maxi2== -2) {
    maxi2=17
  }
  return maxi2 }
