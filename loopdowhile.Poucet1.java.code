public void run() { 
  int nbBiscuit=0;
  int nbCase=0;
  
  while(!sortieTrouvee()){
    
    if (estSurBiscuit()){
      nbBiscuit++;
    }
   // avance();
    
  //  nbCase++;
    
    if(nbCase == 5 && nbBiscuit >=3){ // gauche
      gauche();
      nbBiscuit = 0;
      nbCase = 0;
      avance();
      nbCase++;
      
    } else if(nbCase==5 && nbBiscuit<=2){ // droite
      droite();
      nbBiscuit = 0;
      nbCase = 0;
      avance();
      nbCase++;
    }
  }
    
    
  avance();
}