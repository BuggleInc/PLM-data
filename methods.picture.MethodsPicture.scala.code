override def run() {
  for(i<- 1 to 3){
    faireMotifPrep()
    faireMotifPrep()
    faireMotifL()
  }
}

def faireMotifPrep() {
  faireV(Color.YELLOW)
  faireV(Color.RED)
  faireV(Color.BLUE)
  faireV(Color.GREEN)
  
  for(o<-1 to 5){
    avance()
  }
}

def faireV(c:Color){
  setCouleurBrosse(c)
  avance()
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  avance()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  gauche()
}

def faireMotifL(){
	faireV(Color.YELLOW)
  faireV(Color.RED)
  faireV(Color.BLUE)
  faireV(Color.GREEN)
  
  gauche()
  for(o<-1 to 5){
    avance()
  }
  gauche()
  for(o<-1 to 10){
    avance()
  }
  gauche()
  gauche()
}