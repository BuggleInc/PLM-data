def cocktailSort2() {
  var a = 0
  var b = getValueCount() - 2
  var t = false
  do {
  t = true
  for (i <- a to b) {
    if (isSmaller(i+1,i)){
      
      t = false
      swap(i,i+1)}
    
  for (i <- b to a)	
		if (isSmaller(i,i+1))){
      t = false
      swap(i+1,i)}
    
  }
    


    
  
} while (t == false)
}