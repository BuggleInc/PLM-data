def cocktailSort() {
  var nb : Int = getNombreValeurs()
  var bool : Boolean = false
  
  while (bool == true) {
    for (i<-0 to nb-2) {
     	if (plusPetit(i+1,i)) {
        echange(i,i+1)
        bool = true
      }
    }
    for (i<-0 to nb-2 by -1) {
     	if (plusPetit(i+1,i)) {
        echange(i,i+1)
        bool = true
      }
    }
  }
}
  