public void run() {
int baggleCount = 0;
int loopCount = 0;

	do{
		forward();
		
		if(isOverBaggle()){
			baggleCount++;
		}
		
		loopCount++;

		if(crossing()){
			if(baggleCount >= 3){
				left();
			}else{
				right();
			}
			baggleCount = 0;
			loopCount = 0;
		}

		if(exitReached()){
			forward();
		}
		
	}while(loopCount < 5 && !exitReached()); 
}
