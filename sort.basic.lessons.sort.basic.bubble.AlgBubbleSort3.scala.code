def bubbleSort3()  {
  var trier = true ;
  var longueur = getNombreValeurs()
  var n = 0
  
  while(trier) {
    trier = false
    
    for (i <- 0 to longueur-(2+n) ){
      
      //si c'est mal trie
      if (plusPetit(i+1,i)){
        //on echange et on continue jusqu'a une traverse sans echange
        echange(i,i+1)
        trier = true
      }
      n = n+1
    }
 
  }
}