def cocktailSort2() {
	var d:Int=0;
	var f:Int=getNombreValeurs-2;
	var te, t:Boolean=false;

	while (te== true || t=true) {
		te=false;
		t=false;
		for (i<-d to f) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				f=f-1;
				t=true;
			}
		}
		for (i<-f to d by -1) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				d=d+1;
				te=true;
			}
		}
	}
}
