def cocktailSort3() {
  var c:Boolean = true
  var n:Int = getNombreValeurs()-2
  var f:Int = 0
  while (c){
    c=false
    for (i<- 0 to n-f){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        c = true
      }
    }
    if(c){
    for (i <- n-f to 1 by -1){
      if (!plusPetit(i-1,i)){
        echange(i-1,i)
        c = true
    	}
     }
    }
   }
  
}
