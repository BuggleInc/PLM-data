override def run() {
  var c = 0
  ecritMessage(c)
  while (!arrive()) {
    avance()
    c = c + 1
    ecritMessage(c)
  }
}

def arrive():Boolean = {
  var x = getMondeHauteur()
  var y = getMondeLargeur()
  return ((getX()==x) && (getY()==y))
}

def avance() = {
  var y = getMondeHauteur()
  if (getY() == y) {
    setPos(getX()+1, 0)
  } else {
    setPos(getX(), getY()+1)
  }
}