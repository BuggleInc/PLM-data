public void run() {
	lineByLine();
}

void lineByLine(){
int width = getWorldWidth();
int height = getWorldHeight();
int gx = getX();
int gy = getY();
int count = 0;

	for(int x = gx; x < width; x++){
		for(int y = gy; y < height; y++){
			
			if(!finalPos()){
				setPos(y, x);
				writeMessage(count);
				count++;
			}			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth() && getY() == getWorldHeight()){
		return true;
	}

	return false;
}//end finalPos

