public void cocktailSort() {
  int nb = getValueCount();
  boolean swap = true;
  do{
    for(int i = 0; i<nb-1; i ++ ){
      if(isSmaller(i+1,i)){
        swap(i+1,i);
      }
    }
    
    for(int j = nb-1; j>0;j++){
      if(isSmaller(i+1,i)){
        swap(i+1,i);
      }
      
    }
  }while(swap);
}