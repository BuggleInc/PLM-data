def cocktailSort() {
  var ok:Boolean = false
  var n:Int = getNombreValeurs()-2
  while(ok == false){
    ok=true
    for(i<-0 to n){
      if( !plusPetit(i,i+1)){echange(i,i+1);ok=false; }      
    }
    for(i<-n to 0){
      if( !plusPetit(i,i+1)){echange(i,i+1);ok=false; }      
    }
    n+= -1
    
  }

}