def combSort() {
  var ecart:Int =getNombreValeurs()
  var fini:Boolean =true
  var i:Int=0
  
  while (ecart>1 || fini==false){
    
    fini=true
    if (ecart>1){
      ecart=(ecart.toDouble/1.3).toInt
    }
    i=0
    
    while (i+ecart<getNombreValeurs()){
      if (plusPetit(i+1,i)==true){
        echange(i+1,i)
        i+=1
      }
    }
  }
}