def posFinale():Boolean = {
  if (getX() == (getMondeLargeur() - 1) && getY() == (getMondeHauteur() - 1)){
    return true
  }
  return false
}
      
def colone(cpt:Int) {
  ecritMessage(cpt)
  if (getY() != (getMondeHauteur() - 1)){
    setPos(getX(), getY()+1)
  } else {
  	setPos((getX() + 1),0)
  }
}

override def run() {
  var cpt:Int = 0
  while(!posFinale()){
    colone(cpt)
    cpt += 1
  }
}