override def run() {
	var hauteur=getMondeHauteur();
	var largeur=getMondeLargeur();
	var num =0 ;
	var x=0 ;
	var y=0 ;
	while ( !(getX()==largeur-1) && !(getY()==hauteur))
{

	if (x ==0 )
	{
		for ( i <- 0 to 6 ) 
		{
	ecritMessage(num)
	setPos(x+i,y)
	num=num+1
		}
		ecritMessage(num)
		y=y+1
		setPos(6,1)
		for( i<-0 to 6 ) 
		{
			ecritMessage(num)
			setPos(x-i,y)	
		}
	}
			

}

}
