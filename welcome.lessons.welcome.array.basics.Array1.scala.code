override def run() {
  var taille=getMondeHauteur()
  var tab = new Array[Color] (taille)
  for (i <- 0 to taille-1) {
    tab(i) = getCouleurSol
    avance()
  }
  for (k <- 0 to taille-2) {
  	gauche();
  	avance();
  	droite();
  	for (j <- 0 to taille-1) {
      avance();
    	setCouleurBrosse(tab(j));
    	baisseBrosse();
  	}
    leveBrosse();
    avance();
  }
}
