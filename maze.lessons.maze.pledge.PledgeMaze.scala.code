override def run() {
  while(!estSurBiscuit()){
    keepHandOnWall()
  }
  prendBiscuit()
}

def keepHandOnWall(){
  gauche()
  if (estFaceMur()){
    droite()
    if(estFaceMur()){
      droite()
    }else{
      avance()
    }
  }else{
    avance()
  }
}

def ext(){
  if (!estFaceMur()){
    avance()
  }else{
    droite()
    if(!estFaceMur()){
      avance()
      gauche()
    }else{
      gauche()
      gauche()
      avance()
      droite()
    }
  }
}