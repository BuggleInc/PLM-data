def max1020(a:Int, b:Int):Int = {
var result = 0;
	if(is1020(a))
	{
		result = a;
		
		if(is1020(b))
		{
			result = Math.max(a,b);
		}
		
	}
	else if(is1020(b))
	{
		result = b;
		
		if(is1020(a))
		{
			result = Math.max(a,b);
		}
	}
	return result;

}

def is1020(a : Int) : Boolean = {

	var is1020Bool = false;
	if(a >= 10 && a <= 20)
	{

		is1020Bool = true;
		
	}

	return is1020Bool;
	
}

