def cocktailSort()  {
  var n = getNombreValeurs()
  var c = 1
  while (c!=0) {
    c = 0
    for (i<-0 to (n-2)) {
      if ((getValeur(i))>getValeur(i+1)) {
        echange(i,(i+1))
        c=c+1
      }
    }
    for (i<-(n-2) to 0 by -1) {
      if ((getValeur(i))>getValeur(i+1)) {
        echange(i,(i+1))
        c=c+1
      }
    }
  }
}