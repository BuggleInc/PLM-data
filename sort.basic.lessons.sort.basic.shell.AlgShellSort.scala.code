def shellSort()  {

	var ecart = (getNombreValeurs().toDouble / 2).toInt
	var v = 0
	var cpt = 1

	while (ecart>0) {
		//algo du tri par insertion
		for (i<-1 to getNombreValeurs()-1) {
		v=getValeur(i)
		cpt=i-ecart
			while (cpt>=ecart && !plusPetitQue(cpt, v)) {
			copie(cpt, cpt+ecart)
			cpt=cpt-ecart
			}
		setValeur(cpt, v)
		//fin
		}
		//ecart divis√© par 2
		ecart = (ecart.toDouble / 2).toInt
	}

}
