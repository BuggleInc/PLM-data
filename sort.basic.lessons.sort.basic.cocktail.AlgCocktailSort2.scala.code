def cocktailSort2() {
var sorted = false
	while (!sorted){
		sorted = true
		for (i <- 0 to getNombreValeurs()-2){
			for (j <- 0 to i){
				if (plusPetit(i,i+1) == false){
					echange(i,i+1)
					sorted = false
				}
			}	
			for (j <- i to 0 by -1){
				if (plusPetit(i,i-1) == false){
					echange(i,i-1)
					sorted = false
				}
			}
		}
	}
}
