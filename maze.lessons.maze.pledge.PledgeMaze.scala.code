var sommeAngle:Int=0
override def run() {
  while (!estSurBiscuit()){
    ecritMessage(sommeAngle)
    if (getDirection==Direction.NORTH && !estFaceMur()&&sommeAngle==0){
      avance()
    }else{
      if (getDirection==Direction.NORTH){
      	droite()
        sommeAngle=sommeAngle-1
      }
      keepHandOnSideWall()
    }
  }  
  prendBiscuit()
}

def keepHandOnSideWall():Unit={
  gauche()
  sommeAngle=sommeAngle+1
  if(!estFaceMur()){
    avance()
  }else{
    droite()
    if(!estFaceMur()){
      avance()
      sommeAngle=sommeAngle-1
    }else{
      droite()
      if(!estFaceMur()){
      	avance()
        sommeAngle=sommeAngle-1
      }else{
        droite()
        sommeAngle=sommeAngle-1
        avance()
      }
    }
  }
}