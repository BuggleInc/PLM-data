def step(rule:Array[Char], colors:Array[Color]) {
	val sol:Color = getCouleurSol()
	val l:Int = colors.length -1
	var inter:Int = 0
	while ( colors(inter) != getCouleurSol() ) {
			inter = inter + 1
		}
	if (rule(inter) == 'R') {
		droite()
	} else {gauche()}
	setCouleurBrosse(colors((inter+1) % l))
	baisseBrosse()
	leveBrosse()
	avance()
	
}
