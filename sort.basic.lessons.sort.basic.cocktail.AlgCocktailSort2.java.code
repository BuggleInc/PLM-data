public void cocktailSort2() {

	boolean fini=false;
	int inf=0;
	int sup=getNombreValeurs()-2;
	while(!fini){
		fini=true;
		
		for(int i=inf; i<=sup; i++){
			if(plusPetit(i+1, i)){
				fini=false;
				int temp = getValeur(i);
				copie(i+1, i);
				setValeur(i+1, temp);
			}
		}

		inf++; 

		for(int i=sup; i>=inf; i--){
			if(plusPetit(i+1, i)){
				fini=false;
				int temp = getValeur(i);
				copie(i+1, i);
				setValeur(i+1, temp);
			}
		}

		sup--;
	}
}

