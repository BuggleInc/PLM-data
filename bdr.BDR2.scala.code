var c:String = ""
while (isOverMessage()){
  c =readMessage()
  c match {
  case "R" => right();forward();
  case "L" => left();forward();
  case "I" => left();left();forward();
  case "A" => forward();
  case "B" => forward();forward();
  case "C" => forward();forward();forward();
  case "Z" => backward();
  case "Y" => backward();backward();
  case "X" => backward();backward();backward(); 
  }
}