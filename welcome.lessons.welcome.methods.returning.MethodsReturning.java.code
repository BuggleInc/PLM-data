boolean haveBaggle() {
	boolean vuBiscuit=false;
	for (int casesParcourues=0; casesParcourues<7; casesParcourues++) {
		avance();
		if (estSurBiscuit()==true) {
			vuBiscuit=true;
		}
	} for (int casesDeRetour=0; casesDeRetour<7; casesDeRetour++) {
		recule();
	} if (vuBiscuit==true) {
		return true;
	} else {
		return false;
	}
}
