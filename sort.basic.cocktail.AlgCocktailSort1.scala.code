def cocktailSort() {

  var swapped:Int =1
  var done:Int=0
  while (swapped!=0) {
    swapped=0
    for (i <- 0 to getValueCount()-2-done) {
    	if ( ! isSmaller(i,i+1)) {
      	swap(i,i+1)
      	swapped+=1
      }
    }
    for (i <- (getValueCount()-2-done) to 0) {
    	if ( ! isSmaller(i,i+1)) {
      	swap(i,i+1)
      	swapped+=1
      }
    }
    done+=2
  }
}
