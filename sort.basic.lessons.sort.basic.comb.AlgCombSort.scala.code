def combSort() {
  var tri:Boolean=true
  var ecart=getNombreValeurs()-1
  while (ecart>1|| tri){
    tri=false
    if (ecart>1){
      ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
    }
    var i=0
    while (i+ecart<getNombreValeurs()-1){
      if (plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        i=i+1
        tri=true
      }
    }
  }
}