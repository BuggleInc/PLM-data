override def run() {
  while (!positionFinale){
    Fairecolonne()
  }
}

def positionFinale():Boolean={
  if (getMondeHauteur()==getY()+1 && getMondeLargeur==getX()+1){
    return true
  } else {
    return false
  }
}

def Fairecolonne(){
  var x=0
  var y=0
  var c=0
  x=getX()
  y=getY()
  while (y<=getMondeHauteur-5){
    ecritMessage(c)
    y=y+1
    setPos(x,y)
  }
  x=x+1
  setPos(x,0)
}