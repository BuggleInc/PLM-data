var diag = 0;
override def run() {
  ecritMessage(0)
  var x=0
  var y=0
  var n=0
  var p=1
  while (x!=getMondeLargeur() && y!=getMondeHauteur()){
    if (x==getMondeLargeur()-1){
      y=getMondeHauteur()-1
      x=p
      p+=1
    }
    else if (y==0){
      diag+=1
      x=0
      y=diag
    }
    else{
      x+=1
      y-=1
    }
    
    if (x!=getMondeLargeur() && y!=getMondeHauteur()){
      setPos(x,y)
      n+=1
      ecritMessage(n)
    }
  }
}