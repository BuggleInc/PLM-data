public void combSort() {
  double ecart = getNombreValeurs(); 
  int gap = 0 ;
  int i = 0;
  boolean swap = true;
  do{
    if(ecart >1){
      ecart = ecart / 1.3;
    }
    i = 0 ;
    gap = ecart;
    swap = false;
    while(i+gap < getNombreValeurs()){
      if(plusPetit(i+gap,i)){
        i++;
        echange(i+gap,i);
        swap = true;
      }
        
    }
    
    
  }while((ecart > 1)|| (swap == true));
}