def max1020(a:Int, b:Int):Int = {
  def in1020(a:Int):Boolean={
    return (a>=10 && a<=20)
  }
  if (!in1020(a) && !in1020(b)){
    return 0
  }else{
    if (!in1020(a) && in1020(b)){
      return b
    }else{
    	if (in1020(a) && !in1020(b)){
      	return a
      }else{
        if (a>b){
          return a
        }else{
          return b
        }
      }
    }
  }
}
