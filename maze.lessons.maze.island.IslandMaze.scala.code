override def run() {
  
  while (!isOverBaggle()){
    keepHandOnSideWall()
  }
  
  
  pickupBaggle()
}



def keepHandOnSideWall(){
  
  if (getDirection() == Direction.NORTH){
		while (!isFacingWall()){
      forward()
    }
    
  }else{
    
  for (i <- 1 to 3){
  	if (isFacingWall()){
   	 right()
  	}
  }
  }
  
}