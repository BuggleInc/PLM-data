override def run() {
  var cpt:Int = 0
  var colonne:Int = 0
  ecritMessage(cpt)
  while (!positionfinale) {
    cpt+=1
  	if (!estDosMur())
    	recule()
    else {
      colonne+=1
    	setPos(colonne,0)
    }
    ecritMessage(cpt)
  }
}

def positionfinale():Boolean = {
  if (estDosMur()) {
    gauche()
    if (estDosMur()) {
    	droite()
    	return true
    }
    else {
      droite()
    	return false   
  	}
  }
  return false
}
