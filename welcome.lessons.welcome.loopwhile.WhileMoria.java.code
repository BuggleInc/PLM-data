public void run() { 
	while(!isBackingWall()){
		while!!isOverBaggle()){
			backward();
		} 
		if(isOverBaggle()){
			pickupBaggle();
		}
		while(!isOverBaggle()){
			forward();	
		}
		backward();
		dropBaggle();
		backward();
	}
	right();
	while(!isBackingWall()){
		backward();
	}
	right();
	while(getGroundColor() != Color.green){
		forward();
	}
}
