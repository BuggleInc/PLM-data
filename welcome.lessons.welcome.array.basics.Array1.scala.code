override def run() {
  var world_size:Int = getWorldHeight()
  var array = new Array[Color](world_size)
  
  
  for (i <- 1 to world_size){
    
    if (i == 1){
    for (j <- 0 to world_size - 1){
    	array(j) = getGroundColor()
    	forward()
  	}  
    }else {
    for (j <- 1 to world_size + 1){
      brushUp()
    	forward()
      setBrushColor(array(j-1))
      brushDown()
  	}    
    }
    
    if (i != world_size){
    left()
  	forward()
  	right()
    }
  	} 
}