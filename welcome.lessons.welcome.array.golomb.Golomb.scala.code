def golomb(num:Int): Int = {
	 var n1: Int = 0;
	 var table = new Array[Int](num);
	 var compteur: Int = 0;
	 table(0) = 1;

	 for(n1 <- 1 to num - 1){
		if(compteur == 0){
			table(n1) = table(n1 - 1) + 1;
			compteur = table(n1) - 1;
		}
		else{
			table(n1) = table(n1 - 1);
			compteur = compteur - 1;
		}
	 }
	 return table(n1);
}

