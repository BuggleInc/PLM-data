def cocktailSort3() {
  var swap = true
  var fin=getNombreValeurs()-2
  var debut=0
  do{
    swap=false
    for (i <- debut to fin) {
      if (!plusPetit(i,i+1)) {
        echange(i,i+1)
        swap=true
      }
    }
    fin-=1
    if (!swap){
           return
    }
    swap = false
    for (i <- fin to debut by -1) {
      if (!plusPetit(i,i+1)) {
        echange(i, i+1)
        swap=true
      }
    }
    debut+=1
  } while (swap)
} 
