

ecart = getNombreValeurs()
fini = False
while ecart > 1 or (not fini):
    if ecart > 1:
        ecart = int(ecart/1.3)
    i = 0
    action  = False
    while i + ecart < getNombreValeurs():
        
        if plusPetit(i+ecart,i):
            echange(i,i+ecart)
            action = True
        i = i+1
    if action == False:
        fini = True
        


