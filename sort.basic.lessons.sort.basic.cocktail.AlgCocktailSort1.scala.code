def cocktailSort() {
  var m=getNombreValeurs()
  var n= false
  do{
    n= false
    for (i<- 0 to (m-2)){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          n= true     
     }
    }
    for (j<- 1 to m-1){
      if (!plusPetit(m-j, m-j-1)){
        	echange(m-j-1, m-j)
          n= true      
     }
    }
  } while(n)
}