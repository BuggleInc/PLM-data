override def run() {
  var a = Color.blue
  var i = 0
  var j = 0
  var h = getMondeHauteur()-1
  var l = getMondeLargeur()
  var T = new Array[Color](h)
  for (i<- 0 to T.length-1){
    a = getCouleurSol()
    T(i) = a
    avance()
  }
  recule(h)
  for (i<- 1 to l-1){
    gauche()
    avance()
    droite()
    avance()
    for(j<-1 to h){
    	tampon(T,(j+5)%h)
    	avance() 
    }
  }
}

def tampon(t:Array[Color],H:Int){
  setCouleurBrosse(t(H-1))
  baisseBrosse()
  leveBrosse()
}