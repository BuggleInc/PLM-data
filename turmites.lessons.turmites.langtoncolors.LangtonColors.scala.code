var n:Int=0


def step(rule:Array[Char], colors:Array[Color]) {


while(getCouleurSol()==colors(n)){
	n=n+1
}
n=n-1


if (rule(n)=='R'){
	right();
}else{	
	left();
}


if(n<colors.length-1){
	setCouleurBrosse(colors(n+1))
}else{
	setCouleurBrosse(colors(0))
}
baisseBrosse();
leveBrosse();
avance();
}
