def combSort11() {
	var ecart: Int = getNombreValeurs();
	var lgr: Int = getNombreValeurs();
	var inverse: Boolean = false;
	do{
		inverse = false;
		if(ecart > 1){
     		ecart = (ecart.toDouble / 1.3).toInt;
		}
		if(ecart == 9 | ecart == 10){
			ecart = 11;
		}
   		var i: Int = 0;
   		while(i + ecart < lgr){
     		if(!plusPetit(i, i + ecart)){
				echange(i, i + ecart);
				inverse = true;
     		}
     		i = i + 1;
		}
	}while(ecart > 1 || inverse)
}
