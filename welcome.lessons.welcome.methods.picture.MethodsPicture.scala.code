override def run() {
  
  def marquerSol(c:Color){
    setCouleurBrosse(c)
    baisseBrosse()
    leveBrosse()
  }
  
  def faireV(c:Color){
    avance(2)
    marquerSol(c)
    avance()
    gauche()
    avance()
    marquerSol(c)
    recule()
    droite()
    avance()
    marquerSol(c)
    avance(2)
    gauche()
  }
  
  def faireMotif(){
    faireV(Color.yellow)
    faireV(Color.red)
    faireV(Color.blue)
    faireV(Color.green)
  }

}