def bubbleSort2() {
  var echanger =true
  var len = getNombreValeurs()
  while (echanger == true) {
    var not =0
    var j = 0
    for (i <- 0 to len -j) {
      j += 1
    	if (plusPetit(i+1, i)) {
      	echange(i, i+1)
      };else { 
        not +=1
      }
      };if (not == len-j) {
      		echanger = false
    	}	
}
}