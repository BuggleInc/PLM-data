def tag(i:Int){
  ecritMessage(i)
}

override def run() {
  var nb=0
  var bin=0
  for(i<-0 to getMondeHauteur()-1){
    if(bin==0){
      for(j<-0 to getMondeLargeur()-1){
        setPos(j,i)
        tag(nb)
        nb=nb+1
      }
      bin=1
    }
    else{
      for(j<-0 to getMondeLargeur()-1){
        setPos(getMondeLargeur()-1-j,i)
        tag(nb)
        nb=nb+1
      }
      bin=0
    }
  }
}