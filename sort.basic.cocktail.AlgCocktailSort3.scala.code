def cocktailSort3() {
  var fini = false;
  var n = getNombreValeurs();
  do {
    fini = true ;
    for (i<- getNombreValeurs()-n-1 to n-2){
      if (!plusPetit(i,i+1)){
        echange(i,i+1);
        fini = false
      }

    }
    
    if (fini){
      return 
    }
    
    for (i<- getNombreValeurs()-n to n-2){
      if (!plusPetit(n - i -2,n - i - 1)){
        echange(n - i -2 ,n - i - 1);
        fini = false
      }
    }

   		n -= 1;
  } while(!fini)

}