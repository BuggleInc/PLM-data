def gnomeSort() {
	var debut=0
	var fin=getNombreValeurs()-1
	for (i<-1 to (getNombreValeurs()-2)){
		if (plusPetit(i,i+1) & plusPetit(i-1,i)){
			if (i!=fin){
			i=i+1
			}
			else return
		}
		else { echange(i-1,i+1)
		       if (i!=fin){
		       i=i-1	
		       }
		       else i=i+1
		       
	     }
	}
}
