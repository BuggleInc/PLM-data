int dateFashion(int you, int date) {
	if(eitherLess(2,you, date)){
		return 0;
	} else if (eitherMore(8, you, date)){
		return 2;
	} else {
		return 1;
	}
}

boolean eitherMore(int n, int a, int b){
	return (a >= n || b >= n);
}
boolean eitherLess(int n, int a, int b){
	return (a <= n || b <= n);
}
