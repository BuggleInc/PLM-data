var diag = 0;
var cpt:Int=0;
override def run() {
  for (i <- 0 to 6) {
    for (j <- 0 to 6) {
        writeMessage(cpt)
        cpt+=1
      if (getY==0 && getX!=6) {
        diag+=1
        setPos(0,diag)
      }
      else if (getX==6) {
        diag=1
        setPos(diag,6)
      }
      else {
        setPos(getX+1,getY-1)
      }
    }
  }
}