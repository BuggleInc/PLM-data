def combSort11() {
	def combSort()
{
	var ecart:Int = getNombreValeurs();
	var i:Int = 0;
	var hasSwap:Boolean = false;
	do 
	{		
		hasSwap = false;
		if (ecart > 1)
		{
			ecart = (ecart.toDouble /1.3).toInt;
		}
		i = 0;
		while(i+ecart < getNombreValeurs())
		{
			if (plusPetit(i+ecart,i))
			
				{
					echange(i+ecart,i);
					hasSwap = true;
					
				}
			i= i+1;
		}
 	}while( ecart > 1 || hasSwap)
}

}
