override def run() {
  var posfinale = false
  var cpt=0
  var raw=0
  while((getX() != getMondeLargeur()-1)&&(getY() != getMondeHauteur()-1)){
  	for (i <- 0 to getMondeLargeur()-1){
      ecritMessage(cpt)
     	if (raw!=0){
       	setPos(getX()+1, getY()-6)
      	ecritMessage(cpt)}
    	for(j <- 0 to getMondeHauteur()-2){
      	setPos(getX(), getY()+1)
      	raw=raw+1
        cpt +=1
      	
      
    	}
  	}
  }
}
