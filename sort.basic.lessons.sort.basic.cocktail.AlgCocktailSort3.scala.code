def cocktailSort3() {

	
	var debut=0; var fin=getNombreValeurs()-2
	var swapped = true
	while(swapped)
	{
		swapped=false
		for (i <- debut to fin)
		{
			if (plusPetit(i+1:Int, i:Int) )
			{
				echange(i:Int, i+1:Int)
				swapped=true
			}
			else
			{
				for (i <- fin to debut by -1 )
				{
					if (plusPetit(i+1:Int, i:Int) )
					{
						echange(i:Int, i+1:Int)
						swapped=true
					}
				}
			}
		debut=debut+1
		fin = fin -1
	
}
	
}
}
