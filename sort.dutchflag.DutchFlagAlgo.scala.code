def solve() {
  var i=0
  var apresBleu=0
  var blancRouge=getTaille()-1
  var avantBlanc=getTaille()-1
  while(estTrie()==false) {
    if (getCouleur(i)==0) {
      apresBleu=apresBleu+1
    }
    else if (getCouleur(i)==2) {
      echange(i,blancRouge)
      blancRouge=blancRouge-1
      avantBlanc=avantBlanc-1
    }
    else {
      echange(i,avantBlanc)
      avantBlanc=avantBlanc-1
    }
    
    i=i+1
  }
  
    
  }
