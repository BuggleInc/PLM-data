def cocktailSort()  {
  var sorted:Boolean = false
  while(!sorted){
    sorted = true
  	for(j <- 0 to getValueCount() - 2)
      if(getValue(j) > getValue(j + 1)){
        swap(j, j + 1)
        sorted = false
      }
  	for(j <- getValueCount() - 2 to 0 by -1)
      if(getValue(j) > getValue(j + 1)){
        swap(j, j + 1)
        sorted = false
      }
  }
}