def shellSort()  {
  var v:Int = 0
  var h:Int = 1
  var lastCopied:Int = 1
  var copied = true
  var gap:Int = 0
  
  for (i <- 1 to getValueCount()-1) {
    v = getValue(i)
    lastCopied = i
    copied = true
    h = 1
    gap = (getValueCount()/2).toInt
    
    while (i-h >= 0 && copied) {
      if (getValue(i-h) > v) {
      	copy(i-h, i-h+1);
      	lastCopied = i-h
    	} else {
        copied = false
      }
      
      h+=1;
    }
    setValue(lastCopied, v)
  }
}