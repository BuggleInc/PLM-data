def teaParty(tea:Int, candy:Int): Int = {
  if (tea>=5 & candy>=5) {
    return 1
  } else {
    if (tea>= 2* candy) {
      return 2
    } else {
      if (candy>= 2*tea) {
        return 2
      } else {
        if (candy <= 5) {
          return 0
        } else {
          if (tea<=5) {
            return 0
          }
        }
      }
    }
  }
}