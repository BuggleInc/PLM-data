void solve() {
  
  int apresBleu = 0;
  int blancRouge  = -1;
  int avantBlanc = -1;
  int i = 0;
  
  while(apresBleu<=avantBlanc)
  {
  	if(getCouleur(i)==BLEU)
      {
      echange(apresBleu,avantBlanc);
      apresBleu++;
    }
    else if(getCouleur(i)==BLANC)
      {
      avantBlanc++;
    }
    else
      {
     echange(avantBlanc,blancRouge);
    
   	 blancRouge++;
      
    }

    i++;
	}
}