var diag = 0;
var finalpos : Boolean = true;
override def run() {
  while (finalpos==false){
    if (diag==0){
      setPos(diag,diag)
      writeMessage(diag)
      diag=diag+1
    }
    for ( a <- 0 to diag -1){
      if (diag == 6) {
        setPos(diag,diag)
        writeMessage(diag)
        finalpos=true
      }
      setPos(diag-a,diag-a)
      writeMessage(diag)
      diag = diag + 1
    }
  }
  
}