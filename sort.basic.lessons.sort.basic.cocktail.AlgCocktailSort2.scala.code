def cocktailSort2() {
var v=0
var u=0
do {
	v=0
	for (i<- 0 to getNombreValeurs()-2-u) {
		if (plusPetit(i,i+1)==false) {
				echange(i, i+1)
				v=v+1
			}
	}
	u=u+1

	for (j<- getNombreValeurs()-1 to 1+u by -1) {
		if (plusPetit(j, j-1)==true) {
			echange (j,j-1)
			v=v+1
		}
	}
	u=u+1
}
 while (u<getNombreValeurs()-1)
}
