def solve() {
  var l=0
  var apresbleu=0
  var blancrouge=getTaille()-1
  var avantblanc=getTaille()-1
while(!estTrie()) {
  var c=getCouleur(l)
  if (c==Color.blue) {
    apresbleu+=1
    echange(l,apresbleu)
  }
  if (c==Color.red) {
    blancrouge-=1
    echange(l,blancrouge)
  }
  if (c==Color.white) {
    avantblanc-=1
    echange(l,avantblanc)
  }
  l=l+1
}
}