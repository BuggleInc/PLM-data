def shellSort()  {
  def triInsertion(gap:Int , debut:Int){
    var len=getNombreValeurs()
    for (i<-debut to len ; if i % gap ==0 ){
      var v=getValeur(i)
      var M=1
      while (M-gap>0 && !plusPetitQue(M-gap,v)){
        copie(M-gap,M)
        M-=gap
      }
      setValeur(M,v)
      
    }
 }
  var ecart=(getNombreValeurs()/2).toInt
  while (ecart>1){
    for (debut<- 0 to ecart-1){
      triInsertion(ecart,debut)
    }
    ecart=(ecart/2).toInt
  }
}