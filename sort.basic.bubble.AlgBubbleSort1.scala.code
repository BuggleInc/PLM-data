def bubbleSort()  {
  val n = getNombreValeurs()
  while(!estTrie())
  {
    var i = 0
    while(plusPetit(i+1, i) && i < n-2)
    {
      echange(i, i+1)
      i += 1
    }
    i+=1
  }
}

def estTrie() : Boolean =
{
  for (i<- 0 to getNombreValeurs()-2)
  {
    if (!plusPetit(i, i+1))
    	return false
  }
  return true
}