}
def faireCarre() {
	faireV(Color.BLUE);
	faireV(Color.GREEN);
	faireV(Color.YELLOW)
	faireV(Color.RED)
}

def move(nbPas : Int, versLAvant : Boolean) {
	for (pas <- 1 to nbPas) {
		if (versLAvant) {
			avance();
		} else { 
			recule();
		}
	}
}
