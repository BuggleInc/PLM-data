def arrayFront9(nums:Array[Int]): Boolean = {
  if (nums.length<=4){
    for (i<-0 to nums.length -1){
      if (nums(i)==4)
    		return true 
    } else{
        return false
      }
  }else{
      for (i<-0 to 3){
        if (nums(i)==4){
    			return true
      }else{
        return false
      }
      
    }
  }
}