def combSort() {
  var:Double ecart = getNombreValeurs()
  var inv = true
  while ( ecart>=1 || inv ) {
    inv = false
    if (ecart>1) {
      ecart = ecart/(1.3)
    }
    i = 0
    while ((i+ecart)<lgr) {
      if (!(plusPetit(i,(i+ecart)))) {
        echange(i,(i+ecart))
        i = i+1
        inv = true
      }
    }
  }
}