def island(num:Array[Int]): Int = {
  val res : Int = 0
  for (i <- 1 to num.length) {
    val aux : Boolean = true
    for (j <-1 to num.length) {
      if (j>i){
        if (num(i)>num(j)){
          aux==false
        }
        if ((num(i)==num(j)) && aux) {
          res == res + 1
        }
      }
    }
  }
}