def bubbleSort()  {
  var length = getNombreValeurs()
  var nb_echange = 1
while (nb_echange!=0)
  {
    	nb_echange = 0
     for(k<-0 to (length-1))
      {
          
        if (!plusPetitQue(k,getValeur(k+1)))
          {
            echange(k,k+1)
            nb_echange = nb_echange+1
          }
      }

   
 } 
  	
}