def cocktailSort2() {
  var Change1: Boolean=true
  var F =getNombreValeurs()-2
  var D = 0
  
  while (Change1==true){
    Change1=false

 	  for (k<- F+1 to D by -1 ){
      if (getValeur(k) < getValeur(k-1)){
        echange(k,k-1)
       
      	Change1=true
  	}
    F=F-1
  	}     
  	for (l<-D to F){
      if (getValeur(l) > getValeur(l+1)){
        echange(l,l+1)

       	Change1=true
          
    		}
  	}
    D=D+1
  
  
}
}