def max1020(a:Int, b:Int):Int = {
	var m = 0;
	if ((a >= 10 && a <= 20) && (b >= 10 && b <= 20)) {
		m = Math.max(a,b);
		return m;
	} else if ((a >= 10 && a <= 20) && (b < 10 && b > 20)) {
		return a;
	} else if ((a < 10 && a > 20) && (b >= 10 && b <= 20)) {
		return b;
	} else  if ((a < 10 && a > 20) && (b < 10 && b > 20)) {
		return 0;
	}
}
