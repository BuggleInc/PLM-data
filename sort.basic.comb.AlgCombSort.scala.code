def combSort() {
   var ecart=0
   var lgr = 0
   var i=0
   var test = false
   var ptdrsicestca = false
   ecart=getNombreValeurs()
   lgr =getNombreValeurs()
 
  do{ 
    if (ecart>1) {
      ecart = (ecart.toDouble /1.3).toInt     
    }
    i = 0
    
    while (i+ecart < lgr ){
      test = plusPetit(i , i+ecart)
      if (!test){
         echange (i,i+ecart)
         i=i+1
      }
    }
    ptdrsicestca = (ecart >1 || i!=0)
  } while ( ptdrsicestca)
}