def solve() {
  insertionSort()
}



def insertionSort() {
  var n:Int=getTaille()
  for ( k <- 1 to (n-1)){
    var stock:Int=getCouleur(k)
    var l:Int=k-1
    if (stock==BLEU){
      while(getCouleur(l)!=BLEU && l>0){
        l-=1
        echange(l,l-1)
      }
     
    } else {
      if(stock==BLANC){
        while(getCouleur(l)==ROUGE && l>=0){
          l-=1
        }
        echange(k,l-1)
      }
    }
  }
}