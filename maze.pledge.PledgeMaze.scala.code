var c = 0;

override def run() {
  
 while(!estSurBiscuit()){
  while ((c == 0) && (getDirection() == Direction.NORTH) && !estFaceMur()){
    avance();
  }
  droite();
  c += 1;
  run2();
 }
  
}

def run2(){
  while(!estSurBiscuit() && (c != 0)){
    fun();
  }
}

def fun(){
  gauche(); 
  c -= 1 ;
  while(estFaceMur())
  {
    droite();
    c += 1;
  }
  avance();
}