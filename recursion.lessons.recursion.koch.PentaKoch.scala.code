def pentaKoch(levels:Int, length:Double) {
	if(levels==0){
		avance(length)
	}
	else{
		avance(length/3)
		gauche(108.0)
		avance(length/3)
		droite(72.0)
		avance(length/3)
		droite(72.0)
		avance(length/3)
		droite(72.0)
		avance(length/3)
		gauche(108.0)
		avance(length/3)
		pentaKoch(levels-1, length)
	}
}
