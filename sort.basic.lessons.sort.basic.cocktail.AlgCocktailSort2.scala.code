def cocktailSort2() {
	var fin:Int = getNombreValeurs - 2
	var debut:Int = 0
	do {
		for(i <- 0 to getNombreValeurs - 2) {
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				fin = fin - 1
			}
		}
		for(i <- getNombreValeurs - 2 to 0 by -1) {
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				debut = debut + 1
			}
		}
	}
	while(debut <= fin)
}
