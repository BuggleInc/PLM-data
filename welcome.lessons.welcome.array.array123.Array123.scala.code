def array123(nums:Array[Int]): Boolean = {
	var c = 0
	var r = 0
	
	while(nums.length > c)
	{
		
		if(nums(c) == 1 && c < nums.length - 1)
		{
			 if (nums(c+1) == 2 && c +2 < nums.length)
			 {
			 	if (nums(c+2) == 3)
			 	{
			 		r = 1;
			 	}
			 }
		}
		c = c+1
	}

	if (r == 1 )
	{
		true
	}
	else false
}
