def marquerSol(){baisseBrosse();leveBrosse()}

def faireV(){setCouleurBrosse();avance();marquerSol();avance();gauche();avance();marquerSol();recule();droite();avance();marquerSol();avance();gauche()}

def faireMotif(){
  for(i<- 0 to 3){faireV()}
}

def reposition(){gauche();avance();avance();avance();avance();avance();droite()}