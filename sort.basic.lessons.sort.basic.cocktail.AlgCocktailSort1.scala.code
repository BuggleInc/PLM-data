def cocktailSort() {
  var nb = getValueCount();
  var fin = false;
  
  while(!fin) {
    fin = true;
    for(i <- 0 to nb-1){
			if(getValue(i) > getValue(i+1)){
        swap(i,i+1);
        fin = false;
      }
    }
    for(i <- nb to 1){
      if(getValue(i) < getValue(i-1)){
        swap(i,i+1);
        fin = false;
      }
    }
  }
  
}