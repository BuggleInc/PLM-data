def close10(a:Int, b:Int): Int = {
  var nb0a=Math.abs(a)
  var nb0b=Math.abs(b)
  var nb10a=Math.abs(10-a)
  var nb10b=Math.abs(10-b)
  var absa=Math.abs(a)
  var absb=Math.abs(b)
  if (nb0a<nb10a){
    absa=nb0a
  }else{
    absa=nb10a
  }
  if (nb0b<nb10b){
    absb=nb0b
  }else{
    absb=nb10b
  }
  if (absa<absb){
    return a
  }else{
    return b
  }
}