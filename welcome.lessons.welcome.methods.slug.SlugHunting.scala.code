def hunt() {
  while (estSurBiscuit()==false){
    if (isFacingTrail()==true){
      avance()
      baisseBrosse();leveBrosse()
    } else{ do{
      gauche()
    }while (isFacingTrail()==false)}
  }
  prendBiscuit()
}

