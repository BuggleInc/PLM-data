def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (speed <= 60 && !isBirthday )
  	{return 0}
  if  (speed >= 61 && speed <= 80 && !isBirthday )
  	{return 1}
  if (speed >= 81 && !isBirthday)
  	{return 2}
  if (speed <= 65 && isBirthday )
  	{return 0}
  if  (speed >= 66 && speed <= 85 && isBirthday )
  	{return 1}
  if (speed >= 86 && isBirthday)
  	{return 2}
  return 1565871
}
