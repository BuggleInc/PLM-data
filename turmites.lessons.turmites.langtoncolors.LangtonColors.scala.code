def ChangeColor(c:Color){
	setCouleurBrosse(c);
	baisseBrosse();
	leveBrosse();
	
}
var x=getCouleurSol()
var i=0;
def step(rule:Array[Char], colors:Array[Color]) {
x=getCouleurSol();
	while(colors(i)!=x){
		i=i+1
		
	}
	
	if (rule(i)=='R'){
		droite()
	}
	else {
		gauche();
	}
	if (i==colors.length-1){
		ChangeColor(colors(0))
	}
	else {
	ChangeColor(colors(i+1))
	
	}
	avance();
	i=0;
	


}

