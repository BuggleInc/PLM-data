override def run() {
  var cpt=0
  for (y<-0 to (getMondeLargeur()-1)/2){
    for (x<-0 to getMondeHauteur()-1){
      setPos(x,2*y)
      ecritMessage(cpt)
      cpt+=1
    } 
    cpt+=7
  }
  for (y<-1 to (getMondeLargeur()-1)/2){
    for (x<-0 to getMondeHauteur()-1){
      setPos(getMondeHauteur()-1-x,2*y-1)
      ecritMessage(cpt)
      cpt+=1
    }
    cpt+=7
  }
  setPos(getMondeLargeur()-1,getMondeHauteur()-1)
}

