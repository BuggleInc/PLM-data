def solve() {
  var longueur:Int = getTaillePile() - 1
  for(i <- 0 to getTaillePile() - 1) { 
  	var maxRayon:Int = 0
  	var posMaxRayon = i
  
    for(j <- i + 1 to getTaillePile()) {
      if(getRayonCrepe(j) > maxRayon) {
        maxRayon = getRayonCrepe(j)
        posMaxRayon = j
      }
    }
  	retourne(posMaxRayon - 1)
  }
}