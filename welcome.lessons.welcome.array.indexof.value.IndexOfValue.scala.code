def indexOfValue(nums:Array[Int] ,value:Int): Int = {
	var V=false
	var sol=""
	if (nums!=0) {
		for (i <- nums) {
			while (value!=nums(i)) {
				V=false
			}
			if(value==nums(i)) {
				sol=i
				V=true
				return sol
				} 
				
		} 
		if (V==false) {
		return -1
			} else {
				return sol
			}
	}
	else {
		return -1
	}
}
