

override def run() {
  var cpt = 0
  var diag = 0
  while(cpt != (getMondeLargeur()+getMondeHauteur()-1)){
    var x = diag
    var y = 0
    setPos(y,x);
      ecritMessage(cpt);
    while(y != diag){
      setPos(y,x);
      ecritMessage(cpt);
      x -= 1;
      y += 1;
      cpt += 1;
    }
    diag += 1;
  }
}