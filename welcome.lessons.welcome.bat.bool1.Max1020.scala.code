def max1020(a:Int, b:Int):Int = {
  if (estIntervalle(a) && estIntervalle(b)) {
    return Math.max(a,b):Int
  } else if (estIntervalle(a) && !estIntervalle(b)) {
    return a
  } else if (!estIntervalle(a) && estIntervalle(b)) {
    return b
  } else {
    return 0
  }
}

def estIntervalle(n:Int):Boolean = {
  if (n>=10 && n<=20) {
    return true
  } else {
    return false
  }
}