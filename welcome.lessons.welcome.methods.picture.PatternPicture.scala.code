override def run() {
  
  def marquerSol(c:Color){
    setCouleurBrosse(c)
    baisseBrosse()
    leveBrosse()
  }
  
  def faireDessin(c:Color){
    avance()
    marquerSol(c)
    gauche()
    avance()
    gauche()
    avance()
    marquerSol(c)
    gauche()
    avance()
    gauche()
    avance(3)
    gauche()

  }
  faireDessin(Color.red)
  faireDessin(Color.blue)
  faireDessin(Color.yellow)
  faireDessin(Color.green)
  

}