override def run() {
  var dir:Int=0;
  while (!isOverBaggle()) {
    left();dir+=1
    if (!isFacingWall()) {
      forward()
    }
    else {
      right();dir-=1
      if (isFacingWall()) {
        right();dir-=1
        if (isFacingWall()) {
          right();dir-=1
          forward()
        }
        else {
          forward()
        }
      }
      else {
        forward()
      }
    }
  }
  pickupBaggle()
}