def cocktailSort() {

  var n=0
  var k=0
  var c=0
  var l=0
  n= getNombreValeurs()
  
 	 while(true){
     
   	 c=0
   	 for(k<- 0 to n-2){
   	   if(plusPetit(k+1,k)){
    	    echange(k,k+1)
    	  }
     	 }
       
     for(k<- n-2 to 0 by -1){
   	   if(plusPetit(k,k+1)){
    	    echange(k,k+1)
    	  }else{
     	   c=c+1
     	 }
       if(c==n-2){
         return
       }
     	 }
   	 
 	 }
}

