def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var count=0
  if (ado(a)==true) {
    count=count+1
  }
  if (ado(b)==true) {
    count=count+1
  }
  if (ado(c)==true) {
    count=count+1
  }
  if (ado(d)==true) {
    count=count+1
  }
  return count
}


def ado(a:Int):Boolean = {
  if (a>=13 && a<=19) {
    return true
  } else {
    return false
  }
}