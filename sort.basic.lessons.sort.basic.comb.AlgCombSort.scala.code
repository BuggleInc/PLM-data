def combSort() {
  var n:Int = getNombreValeurs()
  var ecart:Int = n
  var i:Int = 0
  var inverser:Boolean = false
  do{
    inverser = false
    if (ecart > 1){ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]}
    i = 0
    while (i + ecart < n){
      if (plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        i = i + 1
        inverser = true
      }
    }
  }while(ecart>1 || inverser)
}