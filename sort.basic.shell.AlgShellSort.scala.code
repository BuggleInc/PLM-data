def shellSort()  {
  
  var ecart = getNombreValeurs/2
  while (ecart>0) {
    
    for (i <- 1 to getNombreValeurs()/ecart-1) {
    
      var c = 1
      var v = getValeur(i-(c-1)*ecart)
      var last = 0


      while (i-c*ecart >= 0 && !plusPetitQue(i-c*ecart,v)) {

        copie(i-(c+1)*ecart,i-c*ecart)
        last = c*ecart
        c = c + 1

      }

      setValeur(i-last,v)
    
  	}
    
  }
  
}