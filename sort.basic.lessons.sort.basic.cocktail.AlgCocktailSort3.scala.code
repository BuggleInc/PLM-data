def cocktailSort3() {
	val taille = getNombreValeurs()
	var inv = true
	var debut = 0 
	var fin = getNombreValeurs()
while (inv){
	inv = false
	for (i<- debut to fin-2){
		if(plusPetit(i,i+1) == false){
			echange(i,i+1)
			inv = true
			}
		}
		if (inv == false){
			return
		}
		fin -= 1
	for (i<- fin-2 to debut by -1){
		if(plusPetit(i,i+1) == false){
			echange(i,i+1)
			inv = true
			}
		}
		if (inv == false){
			return
		}
		
	debut += 1
	}
}
