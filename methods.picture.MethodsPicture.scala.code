def makePattern(c:Color) {
  setBrushColor(c)
  
  forward()
	colorize()
  left()
  forward()
  right()
  forward()
  colorize()
  forward()
  right()
  forward()
  colorize()
  left()
  forward()
}

def makeSquare(c:Color) {
  for(i <- 1 to 4) {
    makePattern(c)
    left()
  }
}

def colorize() {
  brushDown()
  brushUp()
}

override def run() {
  makeSquare(Color.yellow)
}