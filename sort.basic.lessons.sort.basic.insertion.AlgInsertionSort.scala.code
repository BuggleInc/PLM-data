def insertionSort() {
  val taille=getNombreValeurs
  var echange_pos=false
  while (echange_pos==true){
    echange_pos=false
    for ( i <- 1 to taille-1 ){
      var j=i
      while (j!=0){
      var v=getValeur(j)
      if (getValeur(j-1)>v){
        echange(j-1, j)
        echange_pos=true
      }
     if (j!=0){
       j=j-1
     } 
    }
  }
}