def goAndGet() {
  var stepAmount:Int = 0
  var stepDone:Int = 0
  while (!isOverBaggle()) {
    forward();
    stepAmount = stepAmount+1
  }
  pickupBaggle()
  for (stepDone <- 0 to stepAmount) {
    backward()
  }
  dropBaggle
  stepAmount=0
  stepDone = 0
}

for (i <- 1 to 7) {
    goAndGet();
    right();
    forward();
    left();
}