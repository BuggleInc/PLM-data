def trouveBiscuit():(Int,Int)={
  val xmax=getMondeLargeur()
  val ymax=getMondeHauteur()
  for (i<-0 to xmax-1){
    for (j<-0 to ymax-1){
      if (aBiscuit(i,j)){return (i,j)}
    }
  }
  return (9999,9999) //cas oÃ¹ il n'y a pas de biscuit
}

def indiqueBiscuit():Unit={
  var (x,y)=trouveBiscuit()
  setIndication(x,y,0)
}

