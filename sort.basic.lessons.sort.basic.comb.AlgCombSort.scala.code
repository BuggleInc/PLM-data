def combSort() {
	var lgr = getNombreValeurs(); var ecart = lgr; var ech = false; var i = 0;
	do {
		ech = false;
		if(ecart > 1) {
			ecart = (ecart.toDouble / 1.3).toInt;
		}
		i = 0;
		while(i+ecart < lgr) {
			if(plusPetit(i+ecart, i)) {
				echange(i, i+ecart);
				ech = true;
				i+=1;
			}
		}
		
	}while(ecart >= 1 || ech)
}