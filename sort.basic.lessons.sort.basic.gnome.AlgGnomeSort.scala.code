def gnomeSort() 
{
	var curseur:Int = getNombreValeurs()-2;

	do
	{
		
		if(!plusPetit(curseur-1,curseur+1) )
			{
				echange(curseur+1,curseur-1);

			curseur=curseur-1;
				
			}else{
				curseur=curseur+1;
			}
		if (curseur == 0)
			{curseur = curseur+1}
	
	}while(curseur == getNombreValeurs())
	
} 
