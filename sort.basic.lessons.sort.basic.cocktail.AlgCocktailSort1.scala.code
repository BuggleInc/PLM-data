def cocktailSort() {
var swap : Boolean = true
 while ( swap == false ) {
 	

	for (i <- 0 to getNombreValeurs()-2 ) {
		swap = false
		if ( plusPetit(i , i+1) == false ) {
			echange (i , i+1 ) 
			swap = true
		}
		
	}
	for (i <- getNombreValeurs()-2 to 0 by -1) {
		swap = false 
		if ( plusPetit(i , i+1) == false ) {
			echange (i , i+1 ) 
			swap = true
		}
		
	}
}
}
