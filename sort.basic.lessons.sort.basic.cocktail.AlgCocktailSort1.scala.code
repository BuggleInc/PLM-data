def cocktailSort() {
  var len = getValueCount()
  var swapped = false
  
  while(!swapped){
    
  for (i <- 0 to len - 2){
    if (isSmaller(i+1, i)){
      swap(i+1, i)
      swapped = true
    }
  }
  for (i <- len-2 to 0 by -1){
    if (isSmaller(i+1, i)){
      swap(i+1, i)
      swapped = true
    }
  }
    
  }
}