def insertionSort() {
                     for (i<- 1 to getNombreValeurs()-1)
                     {var v=getValeur(i)
                      var l=0
                      for (k<- i to 1 by -1)
                      {if (plusPetit(v,getValeur(k-1)))
                       { l=k-1
                         copie(k-1, k)
                       }
                      }
                      copie(v,l) 
                     }
                    }