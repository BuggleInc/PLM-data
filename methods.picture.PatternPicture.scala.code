override def run() {faire2carré();gauche();for (i<-1 to 4){avance();};droite();faire2carré()
}
def faireN(c:Color) {setCouleurBrosse(c);avance();baisseBrosse();leveBrosse();gauche();avance();gauche();avance();baisseBrosse();leveBrosse();gauche();avance();gauche();}
def faireZ(c:Color)
{setCouleurBrosse(c);baisseBrosse();leveBrosse();avance();gauche();avance();baisseBrosse();leveBrosse();gauche();avance();gauche();avance();gauche();}

def fairecarré(){faireN(Color.red);avance();avance();faireZ(Color.blue);recule();recule();gauche();avance();avance();droite();faireZ(Color.yellow);avance();avance();faireN(Color.green);recule();recule();gauche();recule();recule();droite();}

def faire2carré(){fairecarré();for (i<-1 to 4){avance();}; fairecarré();for (i<-1 to 4){recule();};}