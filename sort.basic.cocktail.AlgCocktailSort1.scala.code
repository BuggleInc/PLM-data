def cocktailSort() {
  var test=false
  do{
    for(i <- 0 to getNombreValeurs()-2){
      if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        test=true
      }
    }
    for(j <- 0 to getNombreValeurs()-2){
      if(plusPetit(getNombreValeurs()-2-j,getNombreValeurs()-3-j)==true){
        echange(getNombreValeurs()-2-j,getNombreValeurs()-3-j)
        test=true
      }
    }
  }
  while(test==true)
}