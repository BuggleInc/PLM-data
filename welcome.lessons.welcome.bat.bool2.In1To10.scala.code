def in1To10(n:Int, outsideMode:Boolean):Boolean = {
  if ((n<=10)&&(n>=1)) {
    var in = true
  } else {
    var in = false
  }
  if ((outsideMode)&&(in)) {
    return false
  } else if ((outsideMode)&&(in==false)) {
    return true
  } else if ((outsideMode==false)&&(in==false)) {
    return false
  } else {
    return true
  }
}