override def run() {
def fonctionTest():Boolean={
	if (getX()==getMondeLargeur && getY==getMondeHauteur){return true}
	else{return false}
}
var n:Int = 0
while (fonctionTest()==false){
	ecritMessage(n.toString);
	n=n+1
	getX(); getY();
	var x:Int = getX()
	var y:Int = getY()
	if (getY()==getMondeHauteur()-1){setPos(x+1, 0)}
	else{setPos(x, y+1)}
}
}
