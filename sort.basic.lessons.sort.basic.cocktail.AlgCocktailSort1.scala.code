def cocktailSort() {
  var c:Int=1
  while (c!=0) {
    c=0
    for (i <- 0 to getNombreValeurs()-2) {
      if (getValeur(i:Int)>getValeur((i+1):Int)){
        c=1
        echange(i:Int, (i+1):Int)
      }
    }
    for (i <- 0 to getNombreValeurs()-2) {
      if (getValeur(getNombreValeurs()-2-i:Int)>getValeur((getNombreValeurs()-2-i-1):Int)){
        c=1
        echange(i:Int, (i+1):Int)
  }
}