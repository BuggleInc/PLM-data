override def run() {
var H =  getMondeHauteur()
var L =  getMondeLargeur()
var T = new Array[Color](H)
for( i <- 0 to H-1 )
{
	T(i) = getCouleurSol()
	avance()
}
for( j <- 0 to L-2 )
{
gauche()
avance()
droite()
for( i <- 0 to H-1 )
{
	if(i == 0)
	{
	setCouleurBrosse(T(H-(j+1)))
	baisseBrosse()
	leveBrosse()
	avance()		
	}
	else
	{
	setCouleurBrosse(T(H-(j+i+1)))
	baisseBrosse()
	leveBrosse()
	avance()
	}
}
}
}
