def shellSort() {
  var n = getNombreValeurs()
  var v = 8
  var j=0
  var ecart = n/2
  var k=1
  while (ecart>0) {
      for (i <- ecart-1 to (n-1)) {
        v = getValeur(i)
        j=i-1
        while ((j>=0)&&((getValeur(j)>v))) {
           setValeur(j+ecart,getValeur(j))
           j=j-1
        }
        setValeur(j+ecart,v)
      }
    ecart = ecart/2
    }
}