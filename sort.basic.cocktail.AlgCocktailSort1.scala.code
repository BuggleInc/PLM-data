def cocktailSort() {
  var n : Int =getValueCount()
  
  var i : Int = n-1
  
  while(i >= 0 ){
    var triee : Boolean = true
    var j : Int = 0
    
    for(j<-0 to i-1){
      
      if( isSmaller(j+1,j)){
	    	swap(j,j+1)
        triee=false
      }
    }
    
    j=n-1
    while(j>0){
      
      if( isSmaller(j,j-1)){
	    	swap(j,j-1)
        triee=false
      }
      j-=1
    }
    if(triee)
    	return;
    i=i-1
  }

}