def max2Value(nums:Array[Int]): Int = {
  var t : Int = nums.length
  var a : Int = nums(0)
  var p : Int = 0
  var d : Int =0
  for ( i <- 0 to t-1){
      if (nums(i)>a) {
        a = nums(i)
      if (nums(i) == a){
        p=p+1
       }
      }
      
    }
  for ( k <- 0 to t-1){
    if (p == 1 || p==0 ){
      if (nums(k)<a & d<nums(k) ){
        d= nums(k)
      }
    }
    else{
      if (nums(k)<=a & d<nums(k) ){
        d= nums(k)
      
    }
  }
  }
  return d
 
}
