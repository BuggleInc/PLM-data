
while (estSurMessage()){
	litMessage();
	if (litMessage()=="Z"){
	recule();
	}
	else if (litMessage()=="L"){
		gauche();
		avance();
	}
	else if (litMessage()=="B"){
		avance();
		avance();
	}
	else if (litMessage()=="X"){
		recule();
		recule();
		recule();
	}
