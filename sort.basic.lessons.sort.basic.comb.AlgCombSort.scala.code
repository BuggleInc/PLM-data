def combSort() {
  var ecart = getValueCount()
  var swapped = false
  var i =0
  do {
    swapped=false
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i = 0
    while (i+ecart<getValueCount()) {
      if (!isSmaller(i,i+ecart)) {
        swap(i,i+ecart)
        swapped = true
      }
      i+=1
    }
  } while (ecart>1 || !swapped)
}