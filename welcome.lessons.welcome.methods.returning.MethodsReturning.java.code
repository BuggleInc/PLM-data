boolean haveBaggle() {
	boolean seenBaggle = false;
	right();
	int progressX = 0;
	while (progressX < 6 && !seenBaggle){
		left();
		int progressY = 0;
		while (progressY < 6){
			if (isOverBaggle()){
				seenBaggle = true;
			}
			forward();
			progressY++;
		}
		while (progressY != 0) {
			backward();
			progressY--;
		}
		right();
		forward();
	}
	left();
	left();
	forward();
	right();
	return seenBaggle;
}
