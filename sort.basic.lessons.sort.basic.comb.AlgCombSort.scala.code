def combSort() {
val n = getNombreValeurs()
var ecart = n
var swapped = true
	while ((ecart > 1) || (swapped))
		{swapped = false
		if (ecart > 1)
			{ecart = (ecart.toDouble / 1.3).toInt}
		var i = 0
		while (i+ecart < n)
			{if (!plusPetit(i,i+ecart))
				{echange(i,i+ecart)
				i += 1
				swapped = true}
			}
		}
}