def shellSort()  {
  var ecart:Int = getNombreValeurs()/2
  var v = 0
  var j = 0
  while (ecart >0){
    for (i<-ecart -1 to getNombreValeurs()-1){
      v=getValeur(i)
      j=i
      while(j>ecart-1 && !plusPetitQue(j-ecart,v)){
          copie(j-ecart,j)
          j=j-ecart
      }
      setValeur(j,v)
    }
   ecart = ecart/2
 }
}