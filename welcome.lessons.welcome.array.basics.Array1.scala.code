override def run() {
  var tailleY:Int = getMondeHauteur()
  var tailleX:Int = getMondeLargeur()
  var tabColor = new Array[Color](tailleY)
  for (i <- 0 to tailleY - 1){
    tabColor(i) = getCouleurSol()
    avance()
  }
  for (i <- 0 to tailleX-1){
  	gauche()
  	avance()
    droite()
    avance()
    ligne(tabColor)
  }
  
  
}
def ligne(tabColor:Array[Color]){
  for (c <- tabColor ){
    setCouleurBrosse(c)
    baisseBrosse()
    leveBrosse()
    avance()
  }
}