override def run() {
  var cpt: Int = 0
  var x: Int = 0
  ecritMessage(cpt);
  while (getMondeHauteur() != getY() && getMondeLargeur() != getX()) {
    cpt += 1;
    if (getY() == getMondeHauteur()) {
      setPos(x+1,0); ecritMessage(cpt); x +=1
    } else {
      setPos(getX(),getY+1); ecritMessage(cpt)
    }
  }
}