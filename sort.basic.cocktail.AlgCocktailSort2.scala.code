def cocktailSort2() {
  var debut = 0
	var fin = getNombreValeurs()-2
	var mem = 0
  do {
    mem=1
    for (i<-debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
        mem=0
      }
    }
    fin-=1
    for (i<-debut to fin) {
      if (plusPetit(fin-i+2,fin-i+1)) {
        echange(fin-i+2,fin-i+1)
        mem=0
      }
    }
    debut+=1
  } while (mem==0)
}