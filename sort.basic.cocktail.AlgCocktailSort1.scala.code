def cocktailSort() {
  var lastEchange: Boolean = false
  do {
    lastEchange = false
    for (i <- 0 to getNombreValeurs()-2) {
      if (plusPetit(i+1, i)) {
        echange(i,i+1)
        lastEchange = true
      }
    }
    for (i <- getNombreValeurs()-2 to 0 by -1) {
      if (plusPetit(i+1, i)) {
        echange(i, i+1)
        lastEchange = true
      }
    }
  } while (lastEchange == true)
}