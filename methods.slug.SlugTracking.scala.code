def isFacingTrail():Boolean = {
  	var couleur = Color.black
    while (!estSurBiscuit) {
      if (estFaceMur()) {
        droite()
      }
      avance()
      couleur = getCouleurSol()
      recule()
      if (couleur == Color.green) {
        avance()
      } else {
        droite()
      }
    }
  return estSurBiscuit
}