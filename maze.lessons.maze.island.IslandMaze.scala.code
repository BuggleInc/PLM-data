override def run() {
def keepHandOnSideWall() = {
	gauche();
	while (estFaceMur()) {
	droite();
	}
	avance();
}

def avoirMurGauche(): Boolean = {
	gauche();
	if (estFaceMur()) {
		return true;
		droite();
	}
	droite();
	return false;
}

def chercheMur() = {
	if (estFaceMur()) {
		droite();
	} else {
		avance();
	}
}

def deplacelelong() = {
	while (!avoirMurGauche()) {
		chercheMur();
	}
	keepHandOnSideWall();
}
	

var etat=0;
while (!(estSurBiscuit())) {
etat match  {
  case 0 => setDirection(Direction.NORTH); while (!(estFaceMur())) {avance()}; etat = 1;
  case 1 => while (getDirection != Direction.NORTH) {deplacelelong()}; etat = 0;
