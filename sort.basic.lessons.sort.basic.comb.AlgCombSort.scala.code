def combSort() {
	var ecart=getNombreValeurs()
	var nb=getNombreValeurs()
	var y=1
	var i=0
	
	do{
		y=0
		if (ecart>1){
			ecart = (ecart.toDouble / 1.3).toInt
		}
		i=0
		while(i+ecart<nb){
			if(getValeur(i)>getValeur(i+ecart)){
				echange(i,i+ecart)
				y=y+1
			}
			i=i+1
		}
	}while(y!=0 || ecart>1)

}
