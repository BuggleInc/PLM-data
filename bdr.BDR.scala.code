while (isOverMessage()==true){
  if (readMessage()=='R'){
    right();
    forward();
  }
  if (readMessage()=='L'){
    left();
    forward();
  }
  if (readMessage()=='I'){
    left();
    left();
    forward();
  }
  if (readMessage()=='A'){
    forward();
  }
  if (readMessage()=='B'){
    forward();
    forward();
  }
  if (readMessage()=='C'){
    forward();
    forward();
    forward();
  }
  if (readMessage()=='Z'){
    backward();
  }
  if (readMessage()=='Y'){
    backward();
    backward();
  }
  if (readMessage()=='X'){
    backward();
    backward();
    backward();
  }
}