def cocktailSort() {
  var nb=false
  var n=getNombreValeurs-2
  
  do{
    for( i<- 0 to getNombreValeurs()-2){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        nb=true
      }
    }
    for(i<- 0 to n){
      
      if(plusPetit(n+1,n)){
        echange(n,n+1)
        nb=true
      }
    }
  }
  while(nb)
}