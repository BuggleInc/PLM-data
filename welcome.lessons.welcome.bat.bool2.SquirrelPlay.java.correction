$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class squirrelPlay extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( squirrelPlay((Integer)t.getParameter(0), (Boolean)t.getParameter(1)) ); }
	boolean squirrelPlay(int temp, boolean isSummer) {
		return (temp >= 60 && ((isSummer && temp <= 100) || temp <= 90));
	}
 }