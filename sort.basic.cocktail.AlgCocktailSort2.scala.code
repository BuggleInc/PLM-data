def cocktailSort2() {
  var n=getNombreValeurs()
  var debut=0
  var fin=n-2
  var m=0
  var o=0
  
  while (m!=fin-debut || o!=fin-debut){
    m=0
    o=0
    for (i <- debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
      }
      else {
        m+=1
      }
    }
    fin-=1
    for (i <- fin to debut by -1){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
      }
      else {
        o+=1
      }
    }
    debut+=1
  }
}