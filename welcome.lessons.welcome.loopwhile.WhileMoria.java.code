public void run() { 
	if (!isOverBaggle()){
		while(!isCarryingBaggle()) {
			backward();
		}
		while(isCarryingBaggle()) {
			forward();
		}
	}else if (isOverBaggle()){
		pickupBaggle();
	}else if (isCarryingBaggle(){
		if (isOverBaggle()){
			dropBaggle();
		}else{
			forward();
		}
	}else{
		backward();
	}
}


