def combSort() {
	val taille = getNombreValeurs();
     var échange = true;
     var écart = taille;
     while ( {écart > 1}||{échange == true} ) {
       échange = false;
       if (écart > 1) { écart = (écart.toDouble / 1.3).toInt; }
       var i = 0;
	  while (i+écart < taille) {
		if ( plusPetit(i,i+écart) == false) {
			echange(i,i+écart);
			échange = true;
		
		}
		i += 1;
		} 
	
	}
}

