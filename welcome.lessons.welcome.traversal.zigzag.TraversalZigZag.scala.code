override def run() {
  var x = 0
  var y = 0
  var compteur = 0
 
   while (!(getX() == 6) || !(getY() == 6) )
  {		
  if ( y % 2 == 0 && x !=6)
  			{
          x = x + 1
        }
  		if (y % 2 == 1 && x != 0 )
  			{
          x = x - 1
        }
  		if (y % 2 == 0 && x ==6)
  			{
          y = y + 1 
         
        }
  	if (y % 2 == 1 && x == 0 )
  			{
          y = y + 1
        }
    
    ecritMessage(compteur)
    compteur = compteur + 1
		
    setPos(x,y)
    
  	
  }
  
ecritMessage(compteur)}