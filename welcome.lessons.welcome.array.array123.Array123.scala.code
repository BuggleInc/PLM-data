def array123(nums:Array[Int]): Boolean = {
  var pres1:Int = 0
  var pres2:Int = 0
  var pres3:Int = 0
  for (i <- 0 to (nums.length - 1) ) {
    if (nums(i)==1) {
      pres1 = 1
    } else if (nums(i)==2) {
      pres2=1
    } else if (nums(i)==3) {
      pres3=1
    }
  }
  if ((pres1==1) && (pres2==1) && (pres3==1)) {
    return true
  } else {
    return false
  }
}
