def insertionSort() {
	var length = getNombreValeurs();
	var i = 1;
	var j = 0;
	for(i <- 1 to length - 1){
		var v = getValeur(i);
		for(j <- 0 to i){
			if(plusPetit(j, v)){
				echange(j, j+1);
			}
		}
		setValeur(i, v);
	}
}

