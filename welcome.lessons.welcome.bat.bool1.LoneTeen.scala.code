def loneTeen(a:Int, b:Int):Boolean = {
  if ( (in1319Duo(a,b)) && ( (!in1319(a)) || (!in1319(b)) )) return true
  else return false
      
}

def in1319Duo(a:Int, b:Int):Boolean = {
  if ((a>=13 && a<=19) || (b>=13 && b<=19)) return true
  else return false
}

def in1319(a:Int):Boolean = {
  if (a>=13 && a<=19) return true
  else return false
}    
