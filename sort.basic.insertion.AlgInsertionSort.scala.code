def insertionSort() {
  for (i <- 1 to getNombreValeurs() -1){
    var v : Int = getValeur(i)
    var indice : Int = i-1
    while(indice >=0 && !(plusPetitQue(indice,v))){
			copie(indice, indice+1)
      indice = indice - 1
     
      
    }
    indice = indice +1
    if (indice != i) {setValeur(indice,v)}
 
    
  }
}