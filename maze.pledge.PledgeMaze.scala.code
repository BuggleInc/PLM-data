override def run() {
  while(!estSurBiscuit){
    if (isDirectionFree(Direction.EAST)==true){
    	setDirection(Direction.EAST)
      while(!estFaceMur){
        avance()
    	}
    }else{
      keepHandOnSideWall()
    }
  }
}

def keepHandOnSideWall(){
  var cpt=0
  cpt=0
 	if(estFaceMur()){
    droite()
    cpt=cpt+1
  }  
  if(estFaceMur()){
    droite()
    cpt=cpt+1
  }
  if(estFaceMur()){
    droite()
    cpt=cpt+1
  }
  avance()
  gauche()
  cpt=cpt-1
}

def isDirectionFree(dir:Direction){
  var c=0
  while(getDirection!=dir){
    c+=1
    droite()
  }
  if(estFaceMur){
    while(c!=0){
      gauche()
      c-=1
    }
    return false
  }
  else{
    while(c!=0){
      gauche()
      c-=1
    }
    return true
  }
}