def isFacingTrail():Boolean = {
	var solVert:Boolean = false
	if(estFaceMur) {
		return solVert
	}
	else {
		avance()
		getCouleurSol()
		if(getCouleurSol == Color.green) {
			solVert = true
		}
		recule()
		if(solVert == true) {
			()
		}
		else {
			gauche()
			if(!estFaceMur) {
				avance()
			}
		}
	}
	return solVert
}