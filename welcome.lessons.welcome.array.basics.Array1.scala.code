override def run() {
  var T = new Array[Color](getMondeHauteur())
  var nbr:Int=1
  col1(T)
  for (m<-1 to getMondeLargeur()-1){
    col(T,nbr)
    nbr=nbr+1
  }
}

def col1(T:Array[Color]){
  for (i<-1 to getMondeHauteur()-1){
    T(i)=getCouleurSol()
    avance()
    left()
    avance()
    right()
  }
}

def col(T:Array[Color],nbr:Int){
  for (l<-1 to nbr){
    avance()
  }
  for (l<-1 to getMondeHauteur()-1){
    setCouleurBrosse(T(l))
    baisseBrosse()
    leveBrosse()
    avance()
  }
  
}