def gnomeSort() {
  var taille = getNombreValeurs() 
  var i = 0
  

  while(i != taille - 2)
  {
  	if (plusPetit(i,i+1)&&plusPetit(i+1,i+2))
  		{
        i = i +1
      }
     else
  		{
        if(!plusPetit(i+1,i+2))
        {
          echange(i+1,i+2)
        }
           
        if(i != 0)
        {
          i = i -1 
        }
        else
        {
          i = i+1
        }
      }
    	
      
  
  }
}
