def alarmClock(day:Int, vacation:Boolean): String = {
  if (vacation == true && day>=1 && day<=5)
  return "10:00"
  else if (vacation == true && day<1 && day>5)
  return "off"
  else if (vacation == false && day>=1 && day<=5)
  return "7:00"
  else if (vacation == false && day<=1 && day>=5)
  return "10:00"
}
  
  
  
  