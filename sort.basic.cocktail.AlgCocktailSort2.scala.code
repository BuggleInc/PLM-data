def cocktailSort2() {
  var nb= false
  var debut=0
  var fin=getNombreValeurs()-2
  do{
    nb=false
    for(i<-debut to fin){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        fin=fin-1
        nb=true
      }
    }
    for(i<-debut to fin){
      if(plusPetit(fin-i+1,fin-i)){
        echange(fin-i,fin-i+1)
        debut=debut+1
        nb=true
      }
    }
  }
  while(nb)
}