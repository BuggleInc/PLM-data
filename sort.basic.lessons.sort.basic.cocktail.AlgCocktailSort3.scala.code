def cocktailSort3() {
  var beg=0
  var end=getValueCount()-2
  var swapped=false
  do {
    swapped=false
  	for (i <-beg to end) {
    	if (!isSmaller(i,i+1)) {
      	swap(i,i+1);swapped=true
    	}
  	}
    if (!swapped) {return}
    end-=1
  	for (i<-end to beg by -1) {
    	if (!isSmaller(i,i+1)) {
      	swap(i,i+1);swapped=true
    	}
   	}
    if (!swapped) {return}
    beg+=1
  } while (swapped)
}