def sortirMoria(){
  while (!estFaceMur()){
    retourne()
    avance()
    while (!estSurBiscuit()){
      avance()
      if (estFaceMur()){
        droite()
        avance()
        gauche()
        avance()
        break()
      }
    }
    prendBiscuit()
    retourne()
    while (!estSurBiscuit()){
      avance()
    }
    recule poseBiscuit()
  }
}