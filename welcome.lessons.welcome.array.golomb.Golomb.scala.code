def golomb(num:Int): Int = {
  if(num < 3)
  	return 0
  var G = new Array[Int](num*10)
  var last : Int = 3
  var compteur :Int = 2
  for (i <- 0 to G.length-1){
    G(i) = 0
  }
  G(1) = 1
  
  G(2) = 2
  G(num) = 0
  while( G(num)==0) {
    var valeur: Int = G(compteur)
    for (i <- 0 to valeur-1){
      G(last) = compteur
      last = last + 1
    }
    
    compteur = compteur +1
  }
  return G(num) 

  
}