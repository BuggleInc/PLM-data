while(isOverMessage()) {
	var msg:String = readMessage();
	msg match {
	  case "A" => forward()
	  case "B" => forward()
	  case "C" => forward()
	  case "D" => forward()
	  case "F" => forward()
	  case "E" => forward()
	  case "U" => forward()
	  case "V" => forward()
	  case "W" => forward()
	  case "Y" => forward()
	  case "Y" => forward()
	  case "Z" => forward()
	  case "L" => forward()
	  case "R" => forward()
	  case "I" => left(); left(); forward();
	  case _ => 
	}
}
