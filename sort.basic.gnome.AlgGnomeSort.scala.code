def gnomeSort() {
  var n = (getNombreValeurs()-1)
  var k=0
  while(k<=(n-2))
  	{
      if(!plusPetitQue((k+1),getValeur(k+2)))
         {
           echange(k+1,k+2)
           if(k==0)
           	{
              	if(!plusPetitQue(k,getValeur(k+1)))
              		{
                    echange(k,k+1)
                  }
              k=k+1
            }
           else{
           k = k-1
           }
         }
        else
         {
          k=k+1
        }
        
        
    }
  
}