def selectionSort() {
	var length = getNombreValeurs();
	var i =0;
	for(i <- 0 to length - 1){
		var j = i;
		var bonnePlace = 0;
		var min = 100000;
		var valeur = 0;
		while(j < length){

			if(getValeur(j) < min){
				bonnePlace = j;
				min = getValeur(j);		
			}
			j += 1;
		}
		echange(i, bonnePlace);
	}
}
