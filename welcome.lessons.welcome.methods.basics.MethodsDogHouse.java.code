/**
 * Fais la forme d'une "niche" (en réalité un carré 3x3), sans toutefois laisser de traînée, puis revient à son emplacement d'origine.
 */
 void dogHouse() {
 	int casesParcouruesTotales=0;
 	while (casesParcouruesTotales<8) {
 		 	for(int casesParcourues=0; casesParcourues<2; casesParcourues = casesParcourues + 1) {
 		avance();
 		casesParcouruesTotales = casesParcouruesTotales + 1;
 	} gauche();
 	int casesParcourues=0;
 	} 
 }
