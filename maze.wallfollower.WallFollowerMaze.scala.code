def keepHandOnSideWall(){
  var continuer:Boolean = true
  if (!estFaceMur()){
    gauche()
    if (estFaceMur()){
      droite()
      avance()
    }else{avance()}
  }else{
    while(continuer){
      gauche()
      gauche()
      if (estFaceMur()){
        droite()
        avance()
        continuer = false
      } else {
        avance()
        continuer = false
      }
      droite()
    }
  }
}

override def run() {
  while (!estSurBiscuit()){
    keepHandOnSideWall
  }
  prendBiscuit()
}