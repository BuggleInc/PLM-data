def isFacingTrail():
    if estFaceMur()==False:
       forward()
       if getCouleurSol()==Color.green:
           backward()
           return True
       else:
           backward()
           return False
    return False


while estSurBiscuit==False:
    if isFacingTrail()==True:
        avance()
        baisseBrosse()
        leveBrosse()
    else:
        left()    