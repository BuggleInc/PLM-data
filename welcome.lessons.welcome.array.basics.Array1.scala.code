override def run() {
  var T=new Array[Color](getMondeHauteur())
  T(0)=getCouleurSol()
  for (i<-1 to getMondeHauteur()-1) {
    avance;
    T(i)=getCouleurSol() 
  }
  for (i<- 1 to getMondeLargeur()) {
    avance;
    gauche;
    avance;
    droite;
    avance;
    for (i<-0 to getMondeHauteur()) {
      setCouleurBrosse(T(i));
      baisseBrosse;
      leveBrosse;
      avance
    } 
  }
}

