def combSort() {
  var n= getNombreValeurs()
  var ecart = n
  var m = true
  while (ecart>1 || m){ 
    if ( ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt    
    }
    var i = 0
    m = false
    do{
      if( plusPetit(i, i+ecart)){ 
        echange(i , i+ecart)
        i = i+1
        m = true
      } 
    } while (i + ecart < n )
  }   
}