override def run() {
  var cpt:Int=0
  ecritMessage(cpt)
  gauche()
  def test:Boolean = {
    var t1:Boolean=false
    var t2:Boolean=false
    if (getY()==getMondeHauteur()-1) {t1=true}
    if (getX()==getMondeLargeur()-1) {t2=true}
    return t1&&t2
  }
  while (!test) {
    if (!estDosMur()) {
      setPos(getX()+1,getY())
      cpt=cpt+1
    } else {
      setPos(0,getY()+1)
     	cpt=cpt+1  
      }
      
    ecritMessage(cpt)
  }
}