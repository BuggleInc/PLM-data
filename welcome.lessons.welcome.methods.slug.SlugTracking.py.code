def isFacingTrail():
    while estSurBiscuit()==False and porteBiscuit()==False:
        if estFaceMur()==False:
            baisseBrosse()
            leveBrosse()
            forward()
        if estFaceMur():
            left()
        if getCouleurSol()!=Color.green and porteBiscuit()==False:
            backward()
            left()
    if estSurBiscuit()==True:
        baisseBrosse()
        prendBiscuit()