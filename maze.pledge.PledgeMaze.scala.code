override def run() {
  var cmpt:Int = 0
  while(!estSurBiscuit){
    if((!(isDirectionFree(Direction.NORTH)))&&(!(cmpt==0))) {
      cmpt = keepHandOnSideWall() + cmpt
    } else {
      setDirection(Direction.NORTH)
      avance()
    }
  }
  prendBiscuit()
}



def keepHandOnSideWall():Int = {
  var dir:Int = 0
  gauche()
  dir = dir + 1
  if (estFaceMur()){
    droite()
    dir = dir -1
  }
  if (estFaceMur()){
    droite()
    dir = dir -1
  }
  if (estFaceMur()){
    droite()
    dir = dir -1
  }
  avance()
  return dir
}

def isDirectionFree(dir:Direction):Boolean = {
  var tmp:Direction = getDirection()
  setDirection(dir)
  if (estFaceMur) {
    setDirection(tmp)
    return false
  } else {
    setDirection(tmp)
    return true
  }
}