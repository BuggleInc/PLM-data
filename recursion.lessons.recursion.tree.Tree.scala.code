def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	if (steps==1) {
		avance(length)
		recule(length)
	} else {
		current(steps-1)
		avance(length)
		droite(angle)
		tree(steps-1,shrink*length,angle,shrink)
		gauche(2*angle)
		tree(steps-1,length*shrink,angle,shrink)
		leveCrayon()
		droite(angle)
		recule(length)
		baisseCrayon()
		
		
	}
}
