def cocktailSort2() {
var i = 0;
var echan=1;
var fin = getNombreValeurs()-2;
var debut = 0
while (echan==1) {
	echan=0
	while(i <= fin) {
		if (plusPetit(i+1,i)) {echange(i+1,i)
		echan=1
		}
		i=i+1
		fin=fin-1
	}
	while (i>=debut) {
		if (plusPetit(i,i-1)) {
			echange(i,i-1)
			echan=1
		}
	i=i-1	
	debut=debut+1
	}
}
}
