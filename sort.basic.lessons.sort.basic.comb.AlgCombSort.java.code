public void combSort() {
  double ecart = getNombreValeurs(); 
  double i = 0.0;
  boolean swap = true;
  do{
    if(ecart >1){
      ecart = ecart / 1.3;
    }
    i = 0.0 ;
    swap = false;
    while(i+ecart < getNombreValeurs()){
      if(plusPetit(i+ecart,i)){
        i++;
        echange(i+ecart,i);
        swap = true;
      }
        
    }
    
    
  }while((ecart > 1)|| (swap == true));
}