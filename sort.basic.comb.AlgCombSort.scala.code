def combSort()  {
  
  var long = getNombreValeurs -1 ;
  var ecart:Int = long;
  var a:Int=1;
  
  while (a!=0 || ecart >1 ) {
    a=0;
    for (i<- 0 to long - ecart) {
       if (plusPetit(i+ecart,i)) {
         echange(i,i+ecart);
         a=a+1
       }
    }
    if (ecart >1) {ecart = (ecart.toDouble / 1.3).toInt};
  }}