override def run() {
  var cpt=-1
    for (i <- 0 to getMondeLargeur()-1){
      if (i%2 == 0){
      	for (j <- 0 to getMondeHauteur()-1){
        
        setPos(j,i)
        cpt=cpt+1
        ecritMessage(cpt)
      }
     }
      else if (i%2 ==1){
        for (j <- getMondeHauteur()-1 to 0 by -1){
        
        setPos(j,i)
        cpt=cpt+1
        ecritMessage(cpt)
      }
        
      }
    }
}
