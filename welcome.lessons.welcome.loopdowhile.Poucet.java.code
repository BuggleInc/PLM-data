public void run() {
int countBaggles = 0;
int loops = 0;

	do{
		forward();
		
		if(isOverBaggle()){
			countBaggles++;
		}
		
		loops++;

		if(crossing()){
			if(countBaggles >= 3){
				left();
			}else if(countBaggles <= 2){
				right();
			}
			countBaggles = 0;
			loops = 0;
		}

		if(exitReached()){
			forward();
		}
		
	}while(loops < 5 && !exitReached()); 
}
