def keepHandOnSideWall() {
  if (estFaceMur()) {
    // On vérifie que l'on peut avancer à gauche.
    gauche()
    
    if (estFaceMur())
    	droite(); avance()
    
  } else {
    avance()
  }
}

override def run() {
  
  while (!estSurBiscuit()) {
    keepHandOnSideWall()
  }
  
  prendBiscuit()
}
