def shellSort()  {
	var gap=getNombreValeur/2
	while (gap>0) {
		def insertionSort() {
			for (i <- 1 to getNombreValeurs()-1) {
				val v=getValeur(i)
				var j=i
				while (j>0 && v<getValeur(j-1)) {
					copie(j-1,j)
					j-=1
				}
				setValeur(j,v)
			}
		}
	}
}


