def combSort() {
  var echangefait:Boolean = true
  var ecart:Int = getNombreValeurs()
  do {
    echangefait=false
    if (ecart>1) ecart=(ecart.toDouble / 1.3).toInt
    var i:Int = 0
    while(i+ecart<getNombreValeurs) {
      if (plusPetit(i+ecart, i)) {
        echange(i, i+ecart)
        echangefait=true
      }
      i+=1
    }
  } while (ecart>1 && echangefait)
}