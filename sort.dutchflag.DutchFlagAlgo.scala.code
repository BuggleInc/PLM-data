def solve() {
  
  var term = false
  
  do{
    term = true

    for (i <- 0 to getSize()-1){

       if((getColor(i) == BLUE) && (getColor(i+1) != BLUE)){
    		 swap(i,i+1)
         term = false
   		 }
      
      else if((getColor(i) == WHITE) && (getColor(i+1) != WHITE)){
        swap(i,i+1)
        term = false
      }
    
   	 }
   
    } while (term == false)
}