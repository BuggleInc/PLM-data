public void run() {
	searchWall();
	while(!isOverBaggle()) {
		keepHandOnSideWall();
	}
	pickUpBaggle();
}

void keepHandOnSideWall(){
	if(!isFacingWall()) {
		forward();
		left();
	}
	if(isFacingWall()){
		right();
	}
	else{
		right();
		right();
	}
}

void searchWall() {
	while(!isFacingWall()){
		left();
	}
	right();
}
