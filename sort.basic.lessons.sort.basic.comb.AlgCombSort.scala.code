def combSort() {
  var taille=getNombreValeurs()
  var trier=false
  var ecart=taille
  
  while (ecart>1){
    trier=true
    
      ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
   
    
    var i=0
    while (i+ecart<taille){
      if (plusPetit(i+ecart,i)==true){
        echange(i+ecart,i)
        trier=false
      }
    
    }
    i=i+1
  }
  
}