override def run() {
  var diag = 0
  var count = 0
  ecritMessage(count)
  setY(1)
  diag=1
  while (diag!=6){
    count=count+1
    ecritMessage(count)
    if (getX()==diag && getY()==0){
      diag=diag+1
      setX(0);setY(diag)
      
    }
    else{
      setX(getX()+1);setY(getY()-1)
    }
  }
}

