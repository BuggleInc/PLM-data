var diag = 0;
override def run() {
var c:Int=0


	do {
		for (y<-0 to  getMondeHauteur()-1)
		{
		for (x<-0 to getMondeHauteur()-1)
		{
			for (u<-0 to getMondeHauteur()-1){
			setPos(x,y)
			ecritMessage(c)
			c=c+1
			setPos(x,y+u)
			ecritMessage(c)
			c=c+1
			setPos(x+u,y)
			ecritMessage(c)
			c=c+1
			ecritMessage(c)
			c=c+1
		}
		
		
		}}
		}
		
	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-2
		)
}


