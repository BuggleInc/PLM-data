override def run() {
  var T = new Array[Color](getMondeHauteur()-1)
  for (i <- 0 to getMondeHauteur()-1){
    T(i) = getCouleurSol()
    avance()
  }
  
  def peindre(){
    for (j <- 0 to getMondeHauteur()-1){
    setCouleurBrosse(T(j))
    baisseBrosse()
    leveBrosse()
  }
  }
	
  for (k <- 1 to getMondeHauteur()-1){
    recule()
  }
  for (h <- 1 to getMondeLargeur()-1){
    gauche()
    avance()
    droite()
    avance()
    peindre()
  }
}