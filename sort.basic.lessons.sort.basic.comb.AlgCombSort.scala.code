def combSort() {
  var test=0
  var lgr = getNombreValeurs()
  var ecart = lgr
  do {
    test=1
  	if (ecart>1){
    	ecart =(ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
  	}
  	var i = 0
  	while ((i+ecart)<lgr){
    	if (!plusPetit(i,i+ecart)){
      	echange(i,i+ecart)
      	i+=1
        test=0
    	}
  	}
  } while ((ecart>1)||(test==0))
}