var diag = 1;
override def run() {
  
  var compteur=0
  var x=0
  var y=0
  
  for(i<- 0 to 12){
    
    do{
      writeMessage(compteur);
      x-=1;
      y-=1;
      setPos(x,y);
      compteur+=1 
    }while((x<6)&&(y<6))
    diag+=1;
    
    if(diag<6){
      x=0;
      y=diag;
    }
    else{
      y=diag-6;
      x=6;
    }
  }
}