override def run() {
  var cpt=0
  var x=0
  var y=0
  for (i <- 1 to 47) {
    ecritMessage(cpt)
    y+=1
    setPos(x,y)
    cpt+=1
    if (y==6) {
      x+=1
      y=0
      ecritMessage(cpt)
      setPos(x,y)
      cpt+=1
    }
  }
}

def fonctionTest() {
  var res:Boolean=false
  if (getMondeHauteur()==6 & getMondeLargeur()==6) {
    res=true
  }
  return res
}