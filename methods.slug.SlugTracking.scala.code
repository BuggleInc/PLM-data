def isFacingTrail():Boolean = {
    // Write your code here
 
  do {
    forward()
    if (getGroundColor() == Color.green){
      brushDown()
      brushUp()
    }
    else {
      backward()
      left()
    }
    if (isFacingWall()){
      left()
    }
  } while (isOverBaggle() == false)
  return true
}