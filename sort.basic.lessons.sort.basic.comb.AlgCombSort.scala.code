def combSort() {
var tri:Boolean=false
var ecart = getNombreValeurs()
 while(ecart>1 | !tri) {
   tri=true
   if (ecart>1) {
     ecart = (ecart.toDouble / 1.3).toInt
   }
  var i=0
  while (i+ecart < getNombreValeurs()) {
    if (getValeur(i + ecart) < getValeur(i)) {
      echange(i,i + ecart)
      i+=1
      tri=false
    }
  }
 }
}