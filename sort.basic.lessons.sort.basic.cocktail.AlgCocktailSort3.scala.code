def cocktailSort3() {
  val n = getNombreValeurs()
  var ec = 0
  var e = 0
  var d = 0
  var f = n-2
  do {ec=0
  for (i <- d to f) {
    if (plusPetit(i+1,i)) {echange(i,i+1);e=e+1}
  }
      f-=1
  if (ec==0) {return ()}
  for (i <- d to f) {
    var j = (f + d - i)
    if (!plusPetit(j,j+1)) {echange(j,j+1);e=e+1}
  }
    d+=1} while (e!=0)
}