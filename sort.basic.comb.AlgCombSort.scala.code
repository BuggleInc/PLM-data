def combSort() {
  var ecart=getNombreValeurs()
  var ec=true 
  var i=0
  do{
    ec=false
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
      ec=true
    }
    i=0
    while (i+ecart<getNombreValeurs()){
      if (plusPetit(i+1,i)){
        echange(i,i+1);ec=true
      }
      i=i+1
    }
  }while (ec)
    
          
}