override def run() {

	var couleur = new Array[Color](getMondeHauteur());
	
	for(i <- 0 to getMondeHauteur()-1) {
		couleur(i) = getCouleurSol();
		avance();
		
	}
	for(i <- 0 to getMondeLargeur()-2) {
		gauche();
		avance();
		droite();
		avance();
		
		for(j <- 0 to getMondeHauteur()-1) {
			setCouleurBrosse(couleur(j));
			baisseBrosse();
			leveBrosse();
			avance();
		}
		
	}
}
