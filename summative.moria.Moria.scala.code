def Moria() {
  retourne()
  while (!estFaceMur() || estSurBiscuit()) {
    while (!estSurBiscuit && !estFaceMur) {
      avance()
    }
    if (estSurBiscuit) {
      prendBiscuit()
      while (!estSurBiscuit) {
        recule()
      }
      avance()
      poseBiscuit()
      avance()
    }
  }
  droite()
  avance()
  gauche()
