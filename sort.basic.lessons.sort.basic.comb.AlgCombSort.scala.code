def combSort() {
	var ecart:Double =getNombreValeurs();
	var test:Boolean=true;
	do{
		test=false;
		if(ecart>(Double)1){
			ecart=ecart/1.3;
		}
		var i:Double=0;
		while(i+ecart<getNombreValeurs()){
			if(plusPetit(i+ecart,i)){
				echange(i+ecart,i);
				test=true;
			}
			i++;
		}
	}while(ecart>1 || test)
}
