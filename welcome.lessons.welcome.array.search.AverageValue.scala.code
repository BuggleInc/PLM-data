override def run() {
  var len:Int=getMondeHauteur()
  var T=new Array[Color](len)
  for (i<-0 to T.length-1){
    T(i)=getCouleurSol()
    avance;
  }
  var largeur:Int=getMondeLargeur()
  for (i<-1 to largeur-1){
    avance;gauche;avance;droite;
    for (j<-0 to T.length-1){
      setCouleurBrosse(T(j));
      baisseBrosse;leveBrosse
      avance
    }
  }
}