def isFacingTrail():
    if estFaceMur() == True :
        return False
    forward()
    if getCouleurSol() == Color.green :
        backward()
        return True
    else :
        backward()
        return False

def hunt():
    if isFacingTrail():
        baisseBrosse()
        avance()
        leveBrosse()
    else:
        gauche()
        if isFacingTrail():
            avance()
        else:
            droite()
            droite()

while not estSurBiscuit():
    hunt()
prendBiscuit()