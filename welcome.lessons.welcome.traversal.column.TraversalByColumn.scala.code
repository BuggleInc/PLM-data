override def run() {
	var cpt: Int = 0
	var x: Int = 0
	var y: Int = 0
	def fonctionTest(): Boolean = {
		var fin: Boolean = false
		if (getX() == getMondeLargeur() -1 && getY() == getMondeHauteur() -1) {
			fin = true
		}
		return fin
	}
	while (!fonctionTest()) {
		if (!estDosMur()) {
			y = y + 1
			setPos(x, y)
		}
		else {
			x = x + 1
			y = 0
			setPos(x, y)
		}
	}
}
