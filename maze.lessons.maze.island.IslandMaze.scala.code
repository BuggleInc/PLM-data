override def run() {
  while (!estSurBiscuit()) {
    while (!estFaceMur()) {
      setDirection(Direction.NORTH)
      avance()
    }
    keepHandOnSideWall()
    if (getDirection() == Direction.EAST) {
      droite()
    }    
  }
  prendBiscuit()
}

def keepHandOnSideWall() = {
    while (estFaceMur()) {
      droite()
    }
  	avance()
  	gauche()
}