def dDC(str:String):String = {
  val n = str.length
  return (str.charAt(n-2).toString+str.charAt(n-1).toString)
}

def last2(str:String):Int = {
  val n = str.length
  val dD = dDC(str)
  var b1 = false
  var b2 = false
  var c = 0
  for (i <- 0 to n-1) {
    b2 = b1 && str.charAt(i) == dD.charAt(1)
    b1 = str.charAt(i) == dD.charAt(0)
    if (b2) {c=c+1}
  }
  return c}