override def run() {
  var n=0
  val x = getWorldWidth()
  val y = getWorldHeight()
  while(! isInPosition(x: Int)) {
    writeMessage("n")
    n = n+1
    if (0 != getY()) {
      setPos(getX(), getY() - 1)
    }
    else {
      setPos(getX() + 1, y-1)
    }
  }
}


def isInPosition():Boolean = {
  if ( (x -1 == getX()) && (0 == getY())) {return true}
  else {return false}
}