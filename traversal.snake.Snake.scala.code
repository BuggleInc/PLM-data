override def run() {
  var test:Boolean = false
  brushDown();
  while (test ==false) {
    forward();
    if (getDirection() == Direction.EST) {
      left();
      if (estFaceMur) {
        test=true
      } else {
        left();
      }
      left();
    }
    if (getDiretion() == Direction.OUEST) {
      right();
      if (estFaceMur) {
        test=true
      } else {
        right();
      }
      right();
    }
	return test
  }
}
