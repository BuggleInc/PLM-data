override def run() {
  var dir:Direction=Direction.NORTH;
  var angle=1;
  while (!isOverBaggle()) {
    while (!isFacingWall()) {
      setDirection(Direction.NORTH)
      forward()
    }
    while (angle!=0) {
      if (!isFacingWall()) {
        right();angle=angle+1
        if (isFacingWall()) {
          left; angle=angle-1; forward()
        }
        else {
          forward()
        }
      }
      else {
        left(); angle=angle-1
      }
    }
  }
}