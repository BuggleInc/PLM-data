def cocktailSort() {
  val n = getNombreValeurs()
  var aInverse = true
  while (aInverse) {
    aInverse = false
    for (i <- 0 to n - 2) {
      if (getValeur(i) < getValeur(i+1)) {
        echange(i,i+1)
        aInverse = true
      }
    }
    for (j <- n-2 to 0 by -1) {
      if (getValeur(j+1) < getValeur(j)) {
        echange(j+1,j)
        aInverse = true
        }
    }
  }
}