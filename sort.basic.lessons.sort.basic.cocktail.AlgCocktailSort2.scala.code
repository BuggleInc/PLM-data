def cocktailSort2() {
var a=true
var b=true
  while (0<1){
    if (a==false && b==false){
      return
    }
a=false
b=false
  for (j <- 1 to getValueCount()-1){
    if (!isSmaller(j-1,j)){
      swap(j-1,j)
      a=true
      
		}
}
	for (j <- getValueCount()-1 to 1 by -1){
    if (!isSmaller(j-1,j)){
      swap(j-1,j)
      b=true
		}
}      
}
}
