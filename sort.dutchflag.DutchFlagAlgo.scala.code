def solve() {
  var i=0
  var debut=0
  var fin = getTaille()-1
  while (i<=fin){
    if (getCouleur(i)==BLEU){
      echange(i, debut)
      debut+=1
      i+=1
    } else if (getCouleur(i)==ROUGE){
      echange(i, fin)
      fin -=1
      if (getCouleur(i)==BLEU){
        echange(i, debut)
        debut+=1
        i+=1
      }
    }
  }
}