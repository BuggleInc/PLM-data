def combSort() {
  var ecart=getNombreValeurs()
  var i=0
  var nbechanges=1
  while (ecart>1){
    while (nbechanges!=0){
      ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
      i=0
      while (i+ecart<getNombreValeurs()){
        if (plusPetit(i,i+ecart)==false){
          echange(i,i+ecart)
          i=i+1
          nbechanges=nbechanges+1
        }
        else {
          i=i+1
        }
      }
    }
  }
}
