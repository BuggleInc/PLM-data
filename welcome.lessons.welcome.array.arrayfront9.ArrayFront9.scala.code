def arrayFront9(nums:Array[Int]): Boolean = {
  //if (nums==null) {return false}
  val l = nums.length
  l match {
    case 0 => false
    case 1  => nums(0)==9
    case 2 => ((nums(0)==9)||(nums(1)==9))
    case 3 => (((nums(0)==9)||(nums(1)==9))||(nums(2)==9))
    case _ => ((((nums(0)==9)||(nums(1)==9))||(nums(2)==9))||(nums(3)==9))
  }
}
