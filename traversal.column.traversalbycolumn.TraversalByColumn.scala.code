override def run() {
  var n = 0
  val x = getWorldWidth()
  val y = getWorldHeight()
  while(! isInPosition(x-1,y-1)) {
    writeMessage(n)
    n = n+1
    if (y-1 != getY()) {
      setPos(getX(), getY() + 1)
    }
    else {
      setPos(getX() + 1, 0)
    }
  }
}


def isInPosition(x: Int, y: Int):Boolean = {
  if ( (x == getX()) && (y == getY())) {
    writeMessage(x * y -1)
    return true
  }
  else {return false}
}