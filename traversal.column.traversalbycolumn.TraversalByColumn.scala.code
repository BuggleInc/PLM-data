override def run() {
  var n=0
  val x = getWorldWidth()
  val y = getWorldHeight()
  while(! isInPosition(x,y)) {
    writeMessage("n")
    n = n+1
    if (6 != getY()) {
      setPos(getX(), getY() + 1)
    }
    else {
      setPos(getX() + 1, 0)
    }
  }
}


def isInPosition(x: Int, y: Int):Boolean = {
  if ( (x -1 == getX()) && (y -1 == getY())) {return true}
  else {return false}
}