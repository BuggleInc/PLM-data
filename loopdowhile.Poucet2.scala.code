var sortie = false
var b = 0
var c = getCouleurSol
while(sortie == false){
  do{
    avance();
    if (estSurBiscuit()){
      b = b +1;
    }
  }while(crossing())
  if(b <= 2){
    droite();
    b = 0
  }else if(b >= 3){
    gauche();
    b=0
  }else if(c == Color.orange){
    avance();
    b = 0;
    sortie = true;
  }
}