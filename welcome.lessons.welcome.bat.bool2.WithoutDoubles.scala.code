def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
	if (noDoubles && die1 == die2 && 6 != die2  ) { return die1 +die2 +1 }
	else if (noDoubles && die1 == die2 && (die1 == 6) ){ die1=1; return die1 +die2 }
	else if (noDoubles && die1 == die2 && (die2 == 6) ){ die2=1; return die1 +die2 }
	else return dies1 +dies2
}
