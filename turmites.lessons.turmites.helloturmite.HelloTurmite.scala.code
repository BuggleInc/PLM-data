    var state = 0;

    def step(colors:Array[Color], rule:Array[Array[Array[Int]]] ) {

    		if(couleurActuelle==colors.length-1){
		setCouleurBrosse(colors(0))
		}
		else{
		setCouleurBrosse(colors(couleurActuelle+1))	
		}
		baisseBrosse
		leveBrosse
	if(rule(couleurActuelle)=='L'){
		gauche
	}
	else{
		droite
	}
		avance
	
		
    }

   def couleurActuelle():=Int{
   	i=0
   	while (getCouleurSol!=colors(i)){
		i=i+1
	}
	return i
   }
