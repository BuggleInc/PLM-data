override def run() {
  
  
  def fini:Boolean ={
    if(getMondeHauteur()==getX()+1){
      if(getMondeLargeur()==getY()+1){
        return true
      }else{
        return false
      }      
    }else{
      return false
    }
  }
  var x:Int=0
  var y:Int=0  
  def posS(x:Int,y:Int){
    
    if(!estFaceMur){
      y=y+1
      setY(y)
    }else{
      y=0
      x=x+1
      setPos(x,y)
    }
    
  }
  
while(fini==false){

  posS(x,y)
}
  
  
}