int max1020(int a, int b) {
if (est1020(a) && est1020(b)){
	return max(a,b);
}
if (est1020(a) && !est1020(b)){
	return a;
}
if (est1020(b) && !est1020(a)){
	return b;
}
else {
	return 0;
}
}

