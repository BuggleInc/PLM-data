def combSort()
{
  var n = getNombreValeurs();
  var isEchange = false;
	var ecart = n;
  var i = 0;
  
  do
  {
    isEchange = false;
    if (ecart>1) ecart = (ecart.toDouble / 1.3).toInt;
    
    i = 0;
    while (i+ecart<n)
    {
      if (plusPetit(i+ecart, i))
      {
        echange(i, i+ecart);
        isEchange = true;
      }
      i+=1;
    }
  }
  while (ecart > 1 || isEchange)
}