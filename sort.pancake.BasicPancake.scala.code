def solve() {
  val n = getTaillePile()
  var max = 0
  var idRet = n-1
  for(i <- n-1 to 1 by -1) {
    idRet = i
    max = getRayonCrepe(i)
    for(j <- n-1 to i by -1) {
      if(max<getRayonCrepe(j)) {
        idRet = j
        max = getRayonCrepe(j)
      }
    }
    retourne(idRet)
    retourne(i)
  }
}