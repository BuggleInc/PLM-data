def combSort() {
	var sorted : Boolean = false;
	var ecart : Int = getNombreValeurs();
	var i=0;
	do {
		sorted = true;
		if (ecart>1) {
			ecart=(ecart.toDouble/1.3).toInt;
		}
		i = 0;
		while (i+ecart < getNombreValeurs()) {
			if (!plusPetit(i,i+ecart)) {
				echange(i,i+ecart);
				sorted = false;
			}
			i +=1;
		}
	} while (!sorted && ecart>=1)
}
