override def run() {
var H =  getMondeHauteur()
var L =  getMondeLargeur()
var T = new Array[Color](H)
for( i <- 0 to H-1 )
{
	T(i) = getCouleurSol()
	if(!estFaceMur)
	{
	avance()
	}
}

for(i <- 1 to L-1)
{
	recule(H-i)
	gauche()
	if(!estFaceMur)
	{
	avance()
	}
	droite()
	if(!estFaceMur)
	{
	avance()
	}
	for(i <- 0 to H-1)
	{
		setCouleurBrosse(T(i))
		baisseBrosse()
		leveBrosse()
		if(!estFaceMur)
		{
		avance()
		}
	}	
}
recule(H)
}

