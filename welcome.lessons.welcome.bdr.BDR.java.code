while(getIndication()!='') {
	switch(getIndication()) {
		case 'R':
		right();
		forward();
		break;
		case 'L': 
		left();
		forward();
		break;
		case 'I':
		back();
		forward();
		break;
		case 'A':
		forward();
		break;
		case 'B':
		forward(2);
		break;
		case 'C':
		forward(3);
		break;
		case 'Z':
		backward();
		break;
		case 'Y':
		backward(2);
		break;
		case 'X':
		backward(3);
		break;
	}
}