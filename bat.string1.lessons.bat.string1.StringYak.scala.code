def stringYak(str:String):String = {
  
  var sortie:String = "";
  
    var i:Int = 0;
    while(i<=str.length-3){
			if(str.charAt(i)=='y' && str.charAt(i+2)=='k'){
        i+=3;
      }else{
      	sortie+=str.charAt(i);
        i+=1;
      }
    }
  
  while(i<=str.length-1){
      	sortie+=str.charAt(i);
        i+=1;
    }
  
  return sortie;
}