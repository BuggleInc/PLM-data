def marquer(c:Int){
  
  ecritMessage(c)
  
}
def posFinale():Boolean ={
  
  var x = getX()
  var y = getY()
  var x_max = getMondeLargeur()
  var y_max = getMondeHauteur()
  if (x==(x_max-1) && y==(y_max-1))
  	{
      return true
    }
  else
  	{
      marquer(48)
      return false
    }
}

override def run() {
  retourne()
  var c = 0
  while(!posFinale())
  	{
      	var x = getX()
  var y = getY()
      if(estFaceMur())
      	{
          marquer(c)
          setPos(x+1,0)
          
        c = c+1
        }
      else{
        marquer(c)
        setPos(x,y+1)
        
        c = c+1
      }
    }
  retourne()
}