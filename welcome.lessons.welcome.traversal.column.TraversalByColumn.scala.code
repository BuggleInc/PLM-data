override def run() {
  var comp = 0;
  var col = 0;
  var lign = 0;
  while(!(positionFinale())) {
  	if (lign==7) {
      col=col+1;
      setPos(0,col);
    } else {
      setPos(col-1,lign-1);
      lign = lign + 1;
    }
  }
}

def positionFinale():Boolean = {
  var res = false;
  if (getX()==7  && getY()==7) {
    res = true;
  }
  return res
}