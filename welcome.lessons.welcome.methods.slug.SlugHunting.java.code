public void hunt() {
	while(!isOverBaggle()){
		if(isFacingTrail()){
			brushDown();
			forward();
			brushUp();
		}else{
			left();
		}
	}

	pickUpBaggle();
}

boolean isFacingTrail() {
//boolean greenCell = getGroundColor();
	if(isFacingWall()){
		return false;
	}

	forward();

	boolean res = getGroundColor().equals(Color.green); 
	backward();
	return res;
}//end function 


