def max1020(a:Int, b:Int):Int = {
  if((a>=10 && a<=20) || (b>=10 && b<=20)) {
    if(a>b) {
      if(a<=20 && a>=10) {
        return a
      }
      else {
        return b
      }
    }
    else {
      if(b<=20 && b>=10) {
        return b
      }
      else {
        return a
      }
    }
  }
}