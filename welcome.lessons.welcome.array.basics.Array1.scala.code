override def run() {
	var H = getMondeHauteur()
	var Tab = new Array[Color](H)
	for (i<- 0 to H-1) {
		Tab(i) = getCouleurSol()
		avance()
	}
	for (i<- 1 to getMondeLargeur() - 1) {
		avance()
		gauche()
		avance()
		droite()
		avance()
		for (j <- 0 to H - 1) {
			setCouleurBrosse(Tab(j))
			baisseBrosse()
			leveBrosse()
			avance()
		}
	}
}
