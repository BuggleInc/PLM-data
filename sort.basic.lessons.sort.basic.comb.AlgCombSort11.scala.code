def combSort11() {

	var length:Int = getNombreValeurs();
	var ecart:Int = length;
	var modif:Boolean = true;
	var i:Int = 0;

	do
	{
		modif = false;

		if (ecart > 1) 
		{
			if (ecart == 9 || ecart == 10)
				ecart = (11. / 1.3).toInt;
			else ecart = (ecart.toDouble / 1.3).toInt;
		}
			

		i = 0;
		while(i + ecart < length)
		{
			if(!plusPetit(i, i + ecart))
			{
				echange(i, i + ecart);
				modif = true;
			}

			i += 1;
		}
		
	} while (ecart > 1 || modif);
	
}
