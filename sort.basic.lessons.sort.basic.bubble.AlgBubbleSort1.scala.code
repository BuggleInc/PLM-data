def bubbleSort()  {
  var trie:Boolean=false
  while (trie==false) {
    for(i<-0 to getNombreValeurs()-2){
      if(plusPetit(i, i+1)){
        echange(i,i+1)
      }
    }
    var a:Int=0
    for (i<-0 to getNombreValeurs()-2){
      if(plusPetit(i, i+1)){
        a=a+1
      }
    }
    if (a==getNombreValeurs()-1){
      trie = true
    }
  }
}