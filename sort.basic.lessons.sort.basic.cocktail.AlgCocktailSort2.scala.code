def cocktailSort2() {
  
  var test=true;
  var debut=0;
  var fin=getNombreValeurs()-2;
  
  while(test)
  {
    test=false;
    
     for(i<-debut to fin)
    {
        if(plusPetit(i+1,i))
        {
          echange(i+1,i);
          fin-=1;
          test=true;
        }

      }

      for(i<fin to debut by -1)
      {
        if(plusPetit(i+1,i))
        {
          echange(i+1,i);
          debut+=1;
          test=true;
        }

      }
  }
}