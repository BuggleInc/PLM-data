var diag = 0;
var cpt = 0;
override def run() {
	for (i <- 1 to 6){
		for (i <- 1 to diag){
			ecritMessage(cpt)
			cpt=cpt+1
			setPos(getX()+1, getY()-1)
		}
		ecritMessage(cpt)
		cpt=cpt+1
		setPos(0, getY()+diag+1)
		diag=diag+1
	}
	setPos(0,6)
	for (i <- 1 to 7){
		for (i <- 1 to diag+1){
			ecritMessage(cpt)
			cpt=cpt+1
			setPos(getX()+1, getY()+1)
		}
		ecritMessage(cpt)
		cpt=cpt+1
		setPos(0, getY()+diag-1)
		diag=diag-1
	}
}
