	for(i<- d to f){
    
		if (plusPetit(i,i+1)==false){
			x=true			
			echange(i,i+1)
						}
			if (x==false){
         	return
                  }
         }
	
     f=f-1
     
	for(i<- f to d by -1){

		if (plusPetit(i,i+1)==false){
