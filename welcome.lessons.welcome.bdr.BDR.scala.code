while(estSurMessage()){
		if (litMessage()=="A")
  		avance();

	 if (litMessage()=="L"){
  			gauche()
  			avance()};		

		if (litMessage()=="Z")
  		recule();

		if (litMessage()=="B")
  		avance(2);

		if (litMessage()=="R"){
  		droite()
			avance()};

		if (litMessage()=="I"){
  		retourne()
			avance()};

		 if (litMessage()=="C")
  		avance(3);

		if (litMessage()=="Y")
  		recule(2);

		if (litMessage()=="X")
  recule(3);
};
