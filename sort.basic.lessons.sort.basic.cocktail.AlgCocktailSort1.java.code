public void cocktailSort() {
  int nb = getValueCount();
  boolean swap = true;
  do{
    swap = false;
    for(int i = 0; i<nb-1; i ++ ){
      if(isSmaller(i+1,i)){
        swap(i+1,i);
        swap = true;
      }
    }
    
   /* for(int j = nb-2; j>0;j++){
      if(isSmaller(j+1,j)){
        swap(j+1,j);
        swap = true;
      }
      
    }*/
  }while(swap);
}