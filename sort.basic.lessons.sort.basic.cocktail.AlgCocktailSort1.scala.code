def cocktailSort()  {
	  var x=getNombreValeurs();
	  var estTrie=false;
	  var nombreTrie=0
	  while (!estTrie) {
	  	estTrie=true;
	  	for (i<-0 to x-2-nombreTrie) {
	  		if (!plusPetit(i, i+1)) {
	  			echange(i, i+1);
	  			estTrie=false;
	  		}
	  	}
	  	for (i<-x-2-nombreTrie to 0) {
	  		if (!plusPetit(i, i+1)) {
	  			echange(i, i+1);
	  			estTrie=false;
	  		}
	  	}
	  	nombreTrie=nombreTrie+1;
	}
} 