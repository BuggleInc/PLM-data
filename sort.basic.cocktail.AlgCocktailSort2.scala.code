def cocktailSort2() {
  var test=false
  var debut=0
  var fin=getNombreValeurs()-2
  var cpt=0
  do{
    test=false
    for(i <- debut to fin){
      if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        test=true
      }
    }
    fin-=1
    cpt=0
    for(j <- debut+1 to fin){
      if(plusPetit(fin-cpt,fin-cpt-1)==true){
        echange(fin-cpt,fin-cpt-1)
        test=true
      }
      cpt+=1
    }
    debut+=1
  }
  while(test==true)
}