var diag = 0;
override def run() {
	var x:Int = 0
	for (i <- 1 to 7) {
		while (!(getY()==0)) {
			ecritMessage(x);
			setPos(getX()+1,getY()-1);
			x=x+1;
		}
		if (getY()==0) {
			ecritMessage(x);
			setPos(0,i);
			x=x+1;
		}
	}
	for (i <- 1 to 7) {
		while (!(getX()==6)) {
			ecritMessage(x);
			setPos(getX()+1,getY()-1);
			x=x+1;
		}
		if (getX()==0) {
			ecritMessage(x);
			setPos(i,6);
			x=x+1;
	}
	}
}