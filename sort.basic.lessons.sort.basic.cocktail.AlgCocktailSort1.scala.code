def cocktailSort() {
  var a=true
  do {
    a=false
  var k=getNombreValeurs()-2
  for (i <- 1 to getNombreValeurs()-2){
    if (plusPetit(i+1,i)){
      echange(i,i+1)
      a=true
    }
  }
  while (k>=0){
    if (plusPetit(k+1,k)){
      echange(k,k+1)
      a=true
    }
    k=k-1
  }
}
  while (a)
}
