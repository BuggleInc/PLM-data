def arrayCount9(nums:Array[Int]): Int = {
  var nbDeNeuf:Int = 0
  for (i <- 0 to 3) {
    if (nums(i) == 9) {nbDeNeuf = nbDeNeuf + 1}
  }
  return nbDeNeuf
}

def arrayFront9(nums:Array[Int]): Boolean = {
  
  var longueurTableau:Int = nums.length
  
  if ((longueurTableau >= 4) & ((nums(0) == 9) | (nums(1) == 9) | (nums(2) == 9) | (nums(3) == 9))) {return true}
  
  else if ((longueurTableau <= 3) & arrayCount9(nums:Array[Int]) != 0) {return true}
  
  else {return false}

}