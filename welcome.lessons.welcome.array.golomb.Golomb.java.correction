$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class golomb extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( golomb((int)t.getParameter(0)) ); }
	int golomb(int num) {
		if(num==1){
			return 1;
		}else{
			return 1 + golomb( num - golomb( golomb( num-1 ) ) );
		}
	}
 }