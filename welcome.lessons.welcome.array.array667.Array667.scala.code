def array667(nums:Array[Int]): Int = {
	var x=0
	var y=0
	for (i<-0 to nums.length-1){
		if ((y==0 && nums(i)==6) || (y==1 && nums(i)==6) || (y==1 && nums(i)==7) || (y==0 && nums(i)==7)) y=y+1
		if (y==2) {
			x=x+1
			if (i!=nums.length) {
				if ((nums(i+1)!=6) || (nums(i+1)!=7)) y=0
				else y=1
			}
		}
	}
	return x
}
