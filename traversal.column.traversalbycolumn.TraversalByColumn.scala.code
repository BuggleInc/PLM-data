override def run() {
  var x = getX()
  var y = getY()
  var num = 0
  do{
    if(!estFaceMur){
      y = y+1
      setPos(x,y)
      num = num+1
    }else{
      y = 0
      x = x+1
      setPos(x,y)
      num = num +1
    }
    ecritMessage(num)
  }while(!posFinale(x,y))
  
}

def posFinale(x:Int,y:Int): Boolean ={
    return ((getX() == getMondeLargeur()) && (getY()== getMondeHauteur()))
}