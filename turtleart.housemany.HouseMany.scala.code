override def run() {
  for(i <-0 to 3){
    baisseCrayon()
    carre(30)
  	avance(30)
  	droite(30)
  	triangle(30)
  	droite(150)
  	avance(30)
  	droite(180)
  	droite(90)
    leveCrayon()
  	avance(50)
  	gauche(90)
  }
  
}

def triangle(n:Int){
  for(i<-0 to 2){
    avance(n)
    droite(120)
  }
}

def carre(n:Int){
  for(i<-0 to 3){
    avance(n)
    droite(90)
  }
}