override def run() {
  while (getY()!=0){
    keepHandOnWall()
    if (!estFaceMur){
      avance()
    }
  }
  while(!estSurBiscuit()){
    keepHandOnWall()
  }
  prendBiscuit()
}

def keepHandOnWall(){
  gauche()
  if (estFaceMur()){
    droite()
    if(estFaceMur()){
      droite()
    }else{
      avance()
    }
  }else{
    avance()
  }
}