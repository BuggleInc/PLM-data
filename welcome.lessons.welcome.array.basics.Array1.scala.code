override def run() {
  lireCol1()
  for(i<- 1 to getMondeLargeur()-1){
    gauche()
    avance()
    droite()
    avance()
  }
}

def lireCol1(){
  var n:Int=getMondeHauteur()
	var couleurs=new Array[Color](getMondeHauteur())
  for (i<- 1 to getMondeHauteur()){
    couleurs(i)=getCouleurSol()
    avance()
  }
  couleurs(getMondeHauteur())=getCouleurSol()
}