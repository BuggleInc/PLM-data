def cocktailSort2() {
var Debut :Int=0
var Fin:Int=getNombreValeurs()-2
var swap : Boolean = true
do{
	
	for(i<-Debut to Fin){
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap= true
		}
		Fin-=1
	}
	swap=false
	for(i<-Fin to Debut){
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap= true
		}
		Debut+=1
	}
	
}while(swap)
}
