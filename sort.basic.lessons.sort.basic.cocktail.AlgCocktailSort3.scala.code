def cocktailSort3() {
	var debut=0; var fin = getNombreValeurs()-2;
	var ech = false;
	do {
		ech = false;
		for(i<-debut to fin) {
			if(plusPetit(i+1, i)) {
				echange(i, i+1);
				ech = true;
			}
		}
		if(ech == false) {
			return;
		}
		fin-=1;
		for(i<-fin to debut by -1) {
			if(plusPetit(i+1, i)) {
				echange(i, i+1);
				ech = true;
			}
		}
		if(ech == false) {
			return;
		}
		debut+=1;
	}while(ech);
}