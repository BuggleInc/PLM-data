def cocktailSort2() {

	var debut=0
	var fin=getNombreValeurs()-2
	var x=1

	while (x!=0){
		x=0
		
		for(i<-debut to fin){
				if (getValeur(i+1)<getValeur(i)){
					echange(i,i+1)
					x=x+1
			}	
		}
		fin-=1
		for(i<-fin to debut by -1){
				if (getValeur(i+1)<getValeur(i)){
					echange(i,i+1)
					x=x+1
			}	
		}
		debut+=1
	}
}
