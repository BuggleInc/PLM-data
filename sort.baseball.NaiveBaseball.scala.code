import scala.collection.mutable.ListBuffer

override def run() {
  var nbBases = getBasesAmount()
  var holeBaseIndex = getHoleBase()
  var holeIndex = getHolePosition()
  
  var playersColor:ListBuffer[Int] = new ListBuffer()
  
  for(i <- 0 to nbBases) {
    playersColor += getPlayerColor(i, 0)
    playersColor += getPlayerColor(i, 1)
  }
}