def insertionSort() {

	var v = 0 ; 
	var j = 0;
	
	for ( i <- 1 to getNombreValeurs -1)
	{
		v = getValeur(i);
		j = i + 1 
		
		while (j != -1)
		{
			if (j == 0 && !plusPetitQue(0,v) )
			{
				copie(0 ,1); 
				setValeur (0,v);
				j = -1;				
			}
			else if (plusPetitQue(j,v))
			{
				j += 1
				setValeur (j,v);
				j = -1;
			}
			else if(!plusPetitQue(j,v))
			{
				copie(j , j + 1); 
			}

			j -= 1;
			
		}

		
		setValeur(j, v)
	}

}
