def cocktailSort2() {
  var debut:Int = 0
  var fin:Int = getNombreValeurs()-2
  var continue:Boolean = false
  do{
    continue = false
  	for (i <- debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        continue = true
      }
  	}
    fin = fin - 1
  	for (i <- fin to debut+1 by -1){
    	if (plusPetit(i,i-1)){
        echange(i,i-1)
      	continue = true
      }
  	}
    debut = debut + 1
  }while (continue)
}