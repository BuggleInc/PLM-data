def shellSort()  {
  var v:Int =0;
  var ecart:Int = getNombreValeurs()/2;
  
  while(ecart>0){
  	for(i <- 1 to getNombreValeurs()-1){
    	v=getValeur(i);
    	var j=i;
    		while(j>0 && !plusPetitQue(j-i*ecart,v)){
        	copie(j-1,j);
    			j=j-1;
  			}
   			setValeur(j,v); 
  	}
  	ecart=ecart/2;
  }
}