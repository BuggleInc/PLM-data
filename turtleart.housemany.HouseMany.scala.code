override def run() {
  for(i <- 0 to 1) {
    for(j <- 0 to 3) {
      faireMaison()
      droite(90)
      avance(50)
      gauche(90)
    }
    leveCrayon()
    setX(getX() - 200)
    setY(getY() - 150)
    baisseCrayon()
  }
  
  def faireMaison() {
      avance(30)
      droite(30)
      
      for(k <- 1 to 3) {
        avance(30)
        droite(120)
      }
      
      droite(60)
      
      for(k <- 1 to 3) {
        avance(30)
        droite(90)
      }
  }
}