def max2Value(nums:Array[Int]): Int = {
  var max:Int=nums(0)
  for(a<-1 to nums.length-1){
    if (nums(a)>max){
      max = nums(a)
    }
  }
   var max2:Int=nums(0)
  for(a<-1 to nums.length-1){
    if ((nums(a)>max2)&&(nums(a)<max)){
      max2 = nums(a)
    }
  }
  return max2
                        }