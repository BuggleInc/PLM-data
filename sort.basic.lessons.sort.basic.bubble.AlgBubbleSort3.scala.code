def bubbleSort3() {
	
    for(i<- getNombreValeurs()-2 to 0 by-1) {
    	
        var swapped:Boolean=false;
        
        for(j<- 0 to i ){
        
           if(plusPetit(j+1,j)){
               
               echange(j+1,j);
               swapped=true;
                              
           }else if(swapped==false){
           	
           	return;
           	
           }
           
        }
        
    }
    
}
