public void run() {
	findBaggle();
	escapeTheMaze();
}
public void findBaggle(){
int height = getWorldHeight();
int width = getWorldWidth();
int x = 0;
int y = 0;

	//loop rows
	for(int i = 0; i < height; i++){
		//loop columns
		for(int j = 0; j < width; j++){
			if(hasBaggle(i, j)){
				setIndication(i, j, 0);
				//save coordinates
				x = i;
				y = j;
				//stop loops when baggle is found
				i = 99;
				j = 99;
			}
		}
	}
	distanceToExit(x, y);
}
