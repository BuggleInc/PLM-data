def cocktailSort2() {
  var end:Boolean = true
  var debut:Int = 0
  var fin:Int = getNombreValeurs() - 2
  while (end) {
    end = false
    for (i <- debut to fin) {
      if (getValeur(i) > getValeur(i+1)) {
        echange(i,i+1)
        end = true
      }
    }
    fin -= 1
    for (j <- fin to debut by -1) {
      if (getValeur(j) > getValeur(j+1)) {
        echange(j,j+1)
        end = true
      }
    }
    debut += 1
  }
}