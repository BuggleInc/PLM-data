def gnomeSort() {
	var lgr = getNombreValeurs();
	//var i = 0;
	for(i <- 0 to lgr-1){
	//while(i != lgr-1){
		if(plusPetit(i, i+1))
			i += 1;
		else{
			echange(i, i+1);
			i -= 1;
		}
		if(i == -1)
		i += 1;			
	}
}


