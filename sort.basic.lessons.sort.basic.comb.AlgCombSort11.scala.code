def combSort11() {
  var ecart = getNombreValeurs()
  var inv = true
  var i = 0
  while ( ecart>1 || inv ) {
    inv = false
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
      if (ecart == 10 || ecart == 9) {
        ecart = 11
      }
    }
    i = 0
    while ((i+ecart)<getNombreValeurs()) {
      if (!(plusPetit(i,(i+ecart)))) {
        echange(i,(i+ecart))
        inv = true      
    	}
      i = i+1
    }   
  }
}
