var diag = 0;
var a=0
var b=0
var cpt =0
var d=1

override def run() {
   
    while (diag<=4) {
      ecritMessage(cpt)
      d-=1
      cpt+=1
      if (d==0) {
        diag+=1
        b=diag
        a=0
        d=diag+1
        setPos(a,b)
        
      }
      else {
        a+=1
        b-=1
        setPos(a,b)
      }
    }
    while (diag<=11) {
      ecritMessage(cpt)
      d-=1
      cpt+=1
      if (d==0) {
        diag+=1
        b=diag
        a=diag-6
        d=13-diag
        setPos(a,b)
      }
      else {
        a+=1
        b-=1
        setPos(a,b)
      }
    }
  
}