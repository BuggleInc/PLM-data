def bubbleSort() = {
  var tab_has_changed = false;
  var tmp_value = 0;
  
//  if (tab.length < 2)
//    exit();
  
  var i = 0;
    
  do{
    tab_has_changed = false;
  	for (i <- 0 until getNombreValeurs() - 1){
    	if (getValeur(i+1) > getValeur(i)){
				echange(i+1, i);
        tab_has_changed = true;
      }
  	}
  }while (tab_has_changed);
  
}