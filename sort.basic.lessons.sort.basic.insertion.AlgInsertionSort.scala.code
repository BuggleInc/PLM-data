def insertionSort() {
  for (i <- 1 to getNombreValeurs() - 1) {
    var v = getValeur(i)
    var k = i
    var flag = false
    while (k != 0 && flag == false {
      if (getValeur(k) > v) {
        copie(k-1,k)
      } else {
        flag = true
      }
      k = k-1
    }
    setValeur(k,v)
  }
}