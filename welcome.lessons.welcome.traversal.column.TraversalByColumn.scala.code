override def run() {

def fonctionTest():Boolean = {
	(getX() == getMondeLargeur() && getY() == getMondeHauteur())
}

var i = 1
var k = 0
var cpt = 1

ecritMessage(0)
while (!fonctionTest) {
	setPos(k,cpt)
	ecritMessage(i)
	i += 1
	cpt += 1
	if (i == getMondeHauteur()-1) {
		k += 1
		cpt = 0
	}
}

}
