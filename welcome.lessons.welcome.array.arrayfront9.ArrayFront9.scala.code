def arrayFront9(nums:Array[Int]): Boolean = {
  val n :Int=nums.length
  var cpt :Int =0
 	if (n<=4){
    for (i<- 0 to n-1){
      if (nums(i)==9){
        cpt=cpt+1
      }
    }
    if (cpt!=0){
      return true
    }
    else {
      return false
    }
    
  }
  else {
    for (i<- 0 to 3){
      if (nums(i)==9){
        cpt=cpt+1
      }
    }
    if (cpt!=0){
      return true
    }
    else {
      return false
    }
    
    
  }
}