var nbPas :Int = 0;
var dejaFait :Int = 0;
var fin = false;
while(!fin){
	

while(!estSurBiscuit() && !porteBiscuit()){
	avance()
	nbPas = nbPas+1;
	
}
if(estSurBiscuit()){
	prendBiscuit()
}
if(!estSurBiscuit() && porteBiscuit()){
	while(dejaFait != nbPas){
		recule()
		dejaFait=dejaFait+1;
	}
	if(dejaFait == nbPas){
		poseBiscuit()
		fin=true
	}
}
}