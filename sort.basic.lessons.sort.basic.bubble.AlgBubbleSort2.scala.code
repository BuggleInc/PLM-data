def bubbleSort2()  
{
	var i : Int = getNombreValeurs();
	var n : Int = 0;
	var echEffectue : Int = 1;
	while (echEffectue == 1)
	{
		echEffectue = 0;
		i=getNombreValeurs()-1-n;
		while (i>0)
		{
		  if (plusPetit(i,i-1))
		  {
		  	echange(i-1,i);
		  	echEffectue = 1;
		  }
		  i = i-1;
		  n = n+1;
		}
	}
}


