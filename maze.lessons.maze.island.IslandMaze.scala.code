override def run() {

	def keepHandOnSideWall() {
		gauche()
		if (!estFaceMur()) {avance()}
		else {
			droite()
			if (!estFaceMur()) {avance()}
			else {droite()}
		}
	}

	while(getY!=0 && !estSurBiscuit()) {
			while(!estFaceMur()) {avance()}
			droite()
			while(getDirection()!=Direction.NORTH && !estSurBiscuit()) {
				keepHandOnSideWall()}
		}

	while (!estSurBiscuit()) {
		keepHandOnSideWall()
	}

	prendBiscuit()

}
