def combSort() {
var ecart =getNombreValeurs()
var x=getNombreValeurs()
var y=1
var i=0
do {
	if(ecart > 1){
		ecart = (ecart.toDouble / 1.3).toInt
	}
	i=0
	y=0
	while( i+ecart < x){
		if (getValeur(i)>getValeur(i+ecart)){
			echange(i,i+ecart)
			i=i+1
			y=y+1
		}
	}
} while ((ecart >1) || ( y!=0))
}