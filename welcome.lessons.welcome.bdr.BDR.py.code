if estSurMessage():
    if litMessage()=="R":
        droite()
    if litMessage()=="L":
        gauche()
    if litMessage()=="I":
        retourne()
    if litMessage()=="A":
        avance()
    if litMessage()=="B":
        avance(2)
    if litMessage()=="C":
        avance(3)
    if litMessage()=="Z":
        recule()
    if litMessage()=="Y":
        recule(2)
    if litMessage()=="X":
        recule(3)

    