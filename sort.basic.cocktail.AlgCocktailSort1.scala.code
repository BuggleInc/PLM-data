def cocktailSort() {
  val n=getNombreValeurs()
  var compt = 1
  while(compt != 0) {
    compt = 0
    for (i <- 0 to (n-2)) {
      if (plusPetit(i+1,i)) echange(i,i+1)
      else compt += 1
    }
    for(i <- (n-2) to 0 by (-1)){
      if (plusPetit(i+1,i)) echange(i,i+1)
      else compt += 1
    }
  }
}