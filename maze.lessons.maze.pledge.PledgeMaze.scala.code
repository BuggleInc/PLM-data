def isDirectionFree(dir:Direction): Boolean = {
  var pastDirection=getDirection()
  setDirection(dir)
  if(estFaceMur()){
    setDirection(pastDirection)
    return false
  } else {
    return true
  }
}

def KeepHandOnSideWall(i:Int):Int={
  if(estFaceMur()==false){
    avance()
    gauche()
    return i+1
  } else{
    droite()
    return  i-1
  }
}
