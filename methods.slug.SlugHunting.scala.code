def hunt() {
    while(!estSurBiscuit){
      if(isFacingTrail())
        avance()
      else
      	gauche()
    }
}

def isFacingTrail():Boolean = {
  if(estFaceMur()){
    return false
  }
  avance()
  if(getCouleurSol()==Color.green){
    recule()
    return true
  }
  else{
    recule()
    return false
  }
  }