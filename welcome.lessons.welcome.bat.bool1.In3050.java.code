dans3040(a) {
	if ((a<=40)&&(a>=30)) {
		return true;
	} return false;
}

dans4050(a) {
	((a<=50)&&(a>=40)) {
		return true;
	} return false;
}

boolean in3050(int a, int b) {
	if (dans3040(a)&&dans3040(b)) {
		return true;
	} else if (dans4050(a)&&dans4050(b)) {
		return true;
	} return false;
}
