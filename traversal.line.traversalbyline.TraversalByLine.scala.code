override def run() {
  var cpt:Int=0
  while (!fini()){
    ecritMessage(cpt)
    cpt += 1
    tp()
  }
  ecritMessage(cpt)  
}

def fini():Boolean = {
  return (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1)
} 

def basDeColonne():Boolean={
  return (getX()==getMondeLargeur()-1)
}

def tp(){
  if(basDeColonne()){
    setPos(0,getY()+1)
  }
  else {setPos(getX()+1,getY())}
}