def step(rule:Array[Char], colors:Array[Color]) {
	var c=0
	for (i<- 0 to colors.length-1) {
		if (getCouleurSol()==colors(i)) {
			if (rule(i)=="R") {
				droite()
			} else {
				gauche()
			}
			c=(i+1)
			if (c==colors.length) {
				c=0
			}
			setCouleurBrosse(colors(c))
			baisseBrosse()
			leveBrosse()
			avance()
		}
	}
}
