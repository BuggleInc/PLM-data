def solve() {
  val n= getTaille()
  for (i<-1 to n-1)
  {var c=getCouleur(i)
  if (c== BLEU)
  {var j=i ;
    while (j>=0 && getCouleur(j)!=BLEU)
      {echange(j,j-1); j-=1}}
   else if (c=BLANC) {var j=i ; while ((j>=0 && getCouleur(j)!=BLEU) && getCouleur(j)!=BLANC )
      {echange(j,j-1); j-=1}}}
}
