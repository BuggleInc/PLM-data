def cocktailSort3() {
	
	var debut=0
	var fin=getNombreValeurs()-2
	var inverse = false
	
	do{
		inverse = false
		for(i<-debut to fin){
			if(!(plusPetit(i,i+1))){
				echange(i,i+1)
				inverse = true
			}
		}
		if(!inverse){
			return
		}
		fin-=1
		
		for(i<-fin to debut by (-1)){
			if(!(plusPetit(i,i+1))){
				echange(i,i+1)
				inverse = true
			}
		}
		if(!inverse){
			return
		}
		
		debut+=1	
		
	}while(inverse)
}