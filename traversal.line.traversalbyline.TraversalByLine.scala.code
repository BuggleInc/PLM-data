var compt:Int = 0

override def run() {
  
  
  var posFin:Boolean = false
  
  while(posFin != true){
    avancer()
    
    if(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1){
      
      posFin = true;
    }
  }

  }


def avancer() {
      for (y <- 0 to getMondeHauteur()-1){
  	for (x <- 0 to getMondeLargeur()-1){

      setPos(y, x)
      ecritMessage(compt)
      compt = compt + 1;
      
      
      
    }
  }
}