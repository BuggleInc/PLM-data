def cocktailSort2() {
  var debut = 0
  var m=getNombreValeurs()
  var n= false
  var fin = m -2
  do{
    n= false
    for (i<- debut to fin){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          fin -= 1
          n= true     
     }
    }
    for (j<- fin to debut by -1){
      if (!plusPetit(j,j+1 )){
        	echange(j, j+1)
          fin -= 1
          n= true      
     }
    }
  } while(n)
}
  
  
   
