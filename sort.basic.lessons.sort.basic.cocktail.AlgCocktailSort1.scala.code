def cocktailSort() {
  var i : Int = 0
  var a : Int = getNombreValeurs()
  var j : Int = 0
  var trie : Boolean = false
  while (trie==false){
    trie = true
    for (i<- 0 to a-2){
      if (plusPetit(i+1,i)== true) {
        echange(i+1,i)
        trie=false
      }
    }
    for (i<- a-2 to -1 by -1){
      if(plusPetit(i+1,i)==true){
        echange(i-1,i)
        trie=false
      }
    }
  }
}