def combSort() {
  var n = getNombreValeurs();
	var ecart = n;
  var echange = 0;
  var stop = false
  do{
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    };
    var i = 0;
    do{
      if(plusPetit(i+ecart,i)){echange(i,i+ecart);echange=echange+1;}
      i=i+1;
    }while(i+ecart<n)
  if (echange==0){stop=true}
  echange=0;
  }while(stop==false || ecart>1)
}