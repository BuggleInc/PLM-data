def cocktailSort2() {
  var a = 0
  var b = getValueCount() - 2
  var k = 0
  var t = false
  do {
  t = true
    
      
  for (i <- a to b) {
    if (isSmaller(i+1,i)){
      
      t = false
      swap(i,i+1)}
    
    
  } 
  k = 0
    while (k<=b){
		if (isSmaller(b-k+1,b-k)){
      t = false
      swap(b-k,b-k+1)
      k +=1
    }
    else{
      k+=2
    }
    
  }
  a+=1
    
  b-=1   
    


    
  
} while (t == false)
}