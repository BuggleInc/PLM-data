public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){
	//first, look for a LH wall
//	left();
	
	do{
		while(isFacingWall() ){
			right();
		}
		
		forward();
		left();			

		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall

