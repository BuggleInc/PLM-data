def selectionSort() {
  var finale = 0
  var temp = 10000
  var temp2 = 0
  var j = -1
  while (finale==0) {
    j+=1
    finale=1
    for (i <- j to getNombreValeurs-2) {
      if (plusPetitQue(i,temp)) {
        temp = getValeur(i)
        temp2=i
        finale=0
      }
    }
    if (finale==0) {
    	echange(temp2,j)
    }
  }
}