public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){
	//first, look for a LH wall
	left();
	
	do{
		while(isFacingWall() ){
			right();
		}
		
		forward();

		if(isOverBaggle()){
			pickUpBaggle();
		}else{
			left();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall

