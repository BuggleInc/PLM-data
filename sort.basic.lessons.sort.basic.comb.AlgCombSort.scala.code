def combSort() {
	var ecart : Int = getNombreValeurs();
	var stop : Boolean = true;
	var i : Int = 0;
	
	do{
		stop = true;
		
		if(ecart >1){
			ecart = (ecart.toDouble / 1.3).toInt;
		}
		
		i = 0;
		while( i+ecart < getNombreValeurs()){
			if (plusPetit(i+1, i)){
				echange(i+1, i);
				stop = false;
			}
			i = i+1;
		}
		
	}while( !stop || ecart > 1)
}


