public void run() { 
	int lap = 0;
	int step;
	int jog =0;
	while(lap<7)
	{
	while(jog < 4)
	{
		for(step =0; step <4;step++)
		forward();

		left();
		for(step=0;step<2;step++)
		forward();

		right();
		for(step=0;step<4;step++)
		forward();

		right();
		for(step=0;step <2;step++)
		forward();

		left();
		for(step=0;step<4;step++)
		forward();

		left();
		jog++;
	}
	jog = 0
	lap++;
	}
}
