def isFacingTrail():Boolean = {
	if (estFaceMur) return false
	else {
		avance()
		if (getCouleurSol() == Color.green) {
			recule()
			return true
		} else {
			recule
			return false
		}
	}
}

def hunt() {
	while (!estSurBiscuit) {
		if (isFacingTrail) {
			baisseBrosse()
			leveBrosse()
			avance()
		}
		else gauche()
	}
	baisseBrosse()
	leveBrosse()
	prendBiscuit()
}

