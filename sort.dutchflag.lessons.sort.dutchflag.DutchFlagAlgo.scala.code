def solve() {
var b=0
var w=getTaille()-1
var r=getTaille()-1
  while (b<=w) {
    if (getCouleur(b)==Color.white) {
      w=w-1
    }
    else if (getCouleur(b)==Color.blue) {
      echange(b,w)
      b+=1
      w-=1
    }
    else if (getCouleur(b)==Color.red) {
      echange(w,r)
      r=r-1
    }  
    }
  }