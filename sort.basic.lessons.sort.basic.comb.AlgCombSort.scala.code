def combSort() {
  var lgr : Int = getNombreValeurs()
  var ecart : Int = lgr-1
  var bool : Boolean = true
  var i = 0
  
  while ((ecart>1) | (bool = true)) {
    bool = false
    if (ecart>1) {ecart = (ecart.toDouble / 1.3).toInt}
    i = 0
    while (i+ecart < lgr) {
      if (plusPetit(i+ecart,i)) {
        echange(i,i+ecart)
        i+=1
        bool = true
      }
    }
  }
}