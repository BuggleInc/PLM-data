def shellSort()  {
  var ecart=lgr/2
  while (ecart>0){
    for (i <- 1 to getNombreValeurs()-1){
      var v=getValeur(i)
      n=1
      while(j>0 && !plusPetit(i-n*ecart,i-(n-1)*écart)){
        copie(i-n*ecart,i-(n-1)*écart)
        n+=1
      }
      setValeur(i,v)
    }
  }
}




def insertionSort() {
  for (i <- 1 to getNombreValeurs()-1){
    var v=getValeur(i)
    var j=i
    while(j>0 && !isSmallerThan(j-1,v)){
      copie(j-1,j)
      j-=1
    }
    setValeur(j,v)
  }
}