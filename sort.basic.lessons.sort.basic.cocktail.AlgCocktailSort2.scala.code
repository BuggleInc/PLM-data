def cocktailSort2() {
var swapped = false
var n = 2
var m = 0
do{
	swapped = false
	for(k <- m to getNombreValeurs()-n){
			if(plusPetit(k+1,k)){
				echange(k,k+1)
				swapped = true
			}
		}
		n=n+1
		for(i <- getNombreValeurs()-n to m by -1 ){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swapped = true
			}
		}
		m=m+1
		
}while(swapped == true)	
}
