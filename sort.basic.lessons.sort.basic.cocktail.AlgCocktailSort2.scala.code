def cocktailSort2() {
  var i : Int = 0
  var a : Int = getNombreValeurs()
  var start : Int = 0
  var end : Int = a-2
  var trie : Boolean = false
  while (trie==false){
    trie = true
    for (i<- start to end){
      if (plusPetit(i+1,i)== true) {
        echange(i+1,i)
        trie=false
      }
    }
    end = end-1
    for (i<- end to start by -1){
      if(plusPetit(i+1,i)==true){
        echange(i+1,i)
        trie=false
      }
    }
    start = start +1
  }
}