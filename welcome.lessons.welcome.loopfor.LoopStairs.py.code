stepper = 0
while stepper > 0:
    if (isFacingWall() != True):
        stepper = stepper +1
        forward()
        brushDown()
    elif (isFacingWall() == True):
        left()
        brushDown()
        brushUp()
        forward()
        right()
        forward()
    elif stepper > 0:
        while stepper > 0:
            stepper = stepper - 1
            forward()
            brushDown()
