override def run() {
  carre()
  avance(4)
  carre()
  gauche()
  avance(4)
  carre()
  recule(4)
  carre()
  avance(4)
  
}


def motif(c:Color) {
  setCouleurBrosse(c)
  gauche()
  avance()
  marqueSol()
  recule()
  droite()
  avance()
  marqueSol()
  avance(2)
  gauche()
}

def marqueSol() {
  baisseBrosse()
  leveBrosse()
}

def carre() {
  motif(Color.RED)
  motif(Color.BLUE)
  motif(Color.GREEN)
  motif(Color.YELLOW)
}
