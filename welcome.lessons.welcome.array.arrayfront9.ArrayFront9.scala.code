def arrayFront9(nums:Array[Int]): Boolean = {
var n = nums.length;
if (n < 4) {
	for ( k <- 0 to n ) {
		if (nums(k) == 9) {
			return true;
		} else {
			return false;
		}
	}
}
if (n > 4) {
	for ( k <- 0 to 3 ) {
		if (nums(k) == 9) {
			return true;
		} else {
			return false;			
		}
	}
}
return false;
}

