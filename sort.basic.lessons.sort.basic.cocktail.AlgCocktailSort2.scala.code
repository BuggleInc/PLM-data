def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var echanges = false
  do {   
    for(i<-debut to fin) {
      if(!plusPetit(i,i+1)) {
        echange(i,i+1)
        echanges = true
      }
    }
    fin = fin - 1
    for(i<-fin to debut by -1) {
      if(!plusPetit(i,i+1)) {
      echange(i,i+1)
        echanges = true
      }
    }
    debut = debut + 1
  }
  while(echanges)
}