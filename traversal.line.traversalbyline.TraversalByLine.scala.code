override def run() {
  var cpt:Int=0
  ecritMessage(0)
  while (positionTest()==false) {
    parcours()
    cpt+=1
    ecritMessage(cpt)    
    positionTest()
    gauche()

    
  }
  
}


def positionTest():Boolean={
  if(getMondeHauteur()-1==getY() && getMondeLargeur()-1==getX()){
    return true
  }else {
    return false
  }
  
}

def parcours(){
  var x:Int=0
  var y:Int=0

  x=getX()
  y=getY()
  droite()

    
  if (estFaceMur==false){
    setPos(x+1,y)
    
  }else{
    setPos(0,y+1)
  }
  
}