override def run() {
  var w = getWorldWidth();
  var h = getWorldHeight();
  
  var count:Int = 0;
  var direction = 1;
  
  var x = 0;
  var y = 0;
  
  
  do {
    writeMessage(i);
    
    count += 1;
    x += direction;
    
    if (x < 0) {
      y += 1;
      x = 0;
    } else if (x >= w) {
      y + = 1;
      x = w - 1;
    }
    
  } while (!isOnFinalDestination());
}

def isOnFinalDestination() : Boolean = {
  return getX() == getWorldWidth() && getY() == getWorldHeight();
}