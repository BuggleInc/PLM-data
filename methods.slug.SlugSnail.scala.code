def hunt(c:Color) {
  while (!estSurBiscuit()) {
    if (isFacingTrail(c)) {
      avance()
      baisseBrosse()
      leveBrosse()
    } else {
      gauche()
    }
  }
  prendBiscuit()
}
def isFacingTrail(c: Color):Boolean = {
  if (estFaceMur())
    return false
  avance()
  if (getCouleurSol()==c) {
    recule()
    return true
  }
  recule()
  return false
}