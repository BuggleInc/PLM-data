public void run() {
	colByCol();
}

void colByCol(){
int width = getWorldWidth();
int height = getWorldHeight();


	for(int x = 0; x < width; x++){
		for(int y = 0; y < height; y++){
			if(!finalPos()){
				setPos(x, y);
			}			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth()){
		return true;
	}

	return false;
}//end finalPos
