var sortie=false

while (sortie==false){
  avance()
  var biscuit=0
  var couloir=0

  for (couloir<-0 to 4){
    if (estSurBiscuit()){
      if (couloir!=0){
        biscuit=biscuit+1
      }
    }
    avance()
  }

  if (biscuit<=2){
    droite()
  }else{
    gauche()
  }
  sortie=sortieTrouvee()
}