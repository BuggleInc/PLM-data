var nbBiscuits = 0
var nbPas = 0
var DejaFait = 0

while (nbBiscuits != 4){
  while (!estSurBiscuit){
    avance()
    nbPas = nbPas + 1
  }
  if (estSurBiscuit){
    prendBiscuit()
    while (DejaFait < nbPas){
      recule()
      DejaFait = DejaFait + 1
    }
    poseBiscuit()
    nbBiscuits = nbBiscuits + 1
  }
}