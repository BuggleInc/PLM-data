override def run() 
{

def posfin(): Boolean=
{
	if (estFaceMur() && getDirection()==Direction.EST)
	{
		gauche()
		if (estFaceMur())
		{
			droite()
			return true
		} 
		else 
		{
			droite()
			return false
		}
	}
	} else return false
		
}

def serpent()
{
	baisseBrosse()
	while (posfin()==false)
	{
		if (estFaceMur() && (getDirection() == Direction.EST))
		{
			gauche()
			avance()
			gauche()
		}
		if (estFaceMur() && (getDirection() == Direction.OUEST))
		{
			droite()
			avance()
			droite()
		} else avance()
	}
}

serpent()
	
}
