override def run() {
  
  while (!estFin) {
   avance(6)
  	
  	if (getDirection()==Direction.EST ) {
      gauche()
      avance()
      gauche()
    }
    else if (getDirection()==Direction.OUEST) {
      droite()
      avance()
      droite()
    }
  }
  avance(6)
  droite()
  avance()
  droite()
  avance(6)
  
}


def estFin() :Boolean ={
  while (getY>1) {
    return false
  }
  return true
}

