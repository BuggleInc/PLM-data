def combSort() {
  var ecart=getNombreValeurs()
  var a=true
  do {
    a=false
    if (ecart>1){
      ecart=ecart/1.3
    }
    i=0
    while (i+ecart<getNombreValeurs()){
      if (plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        i+=1
        a=true
    }
  }
} while ((ecart>1)||(a==true))
}