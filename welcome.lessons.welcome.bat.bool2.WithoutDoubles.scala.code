def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
  if (noDoubles==true & die1==die2){
    if (die1==6)
    	return die2+1
    else if (die1==5)
    	return die2+2
   	else if (die1==4)
    	return die2+3
    else if (die1==3)
    	return die2+4
    else if (die1==2)
    	return die2+5
    else
    	return die2+6
  }
  else
  	return die1+die2
}