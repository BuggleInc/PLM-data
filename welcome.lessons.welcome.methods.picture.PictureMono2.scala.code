override def run() {
  for (cpt <- 1 to 3){
    faire3Motifs()
    gauche()
    avance(7)
    droite()
  }
}

def marquerSol(){
  baisseBrosse()
  leveBrosse()
}

def faireV(){
  avance(2)
  marquerSol()
  avance()
  gauche()
  avance()
  marquerSol()
  recule()
  droite()
  avance()
  marquerSol()
