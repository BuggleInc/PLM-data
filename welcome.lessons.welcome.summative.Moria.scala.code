def sortir() {
  setDirection(Direction.EST);
  while (getCouleurSol() != Color.GREEN) {
    if (estSurBiscuit()) {
      prendBiscuit();
      setDirection(Direction.OUEST);
  		while(!estSurBiscuit) {
    		avance();
  		}
  		recule();
  		poseBiscuit();
  		setDirection(Direction.EST);
    } else if (estFaceMur()) {
      droite();
      avance();
      gauche();
      avance();
    } else {
      avance();
    }
  }
}