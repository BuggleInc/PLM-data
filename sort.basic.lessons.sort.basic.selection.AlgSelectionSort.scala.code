def selectionSort() {
  var j:Int = 0
  var v:Int = 0
  for (k <- 0 to getNombreValeurs() - 2) {
    j = k
    v = getValeur(j)
    for (i <- k to getNombreValeurs() - 1) {
      if (getValeur(i) < v) {
        j = i
        v =getValeur(j)
      }
    }
    echange(k,j)
  }
} // mon code est trop efficace mais j'ai pas le temps de le changer donc tant pis //