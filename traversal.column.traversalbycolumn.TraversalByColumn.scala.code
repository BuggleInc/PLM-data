override def run() {
  while(!posFin){
    tp()
  }
  
  
}

var large : Int = getMondeLargeur()-1
var hauteur: Int = getMondeHauteur()-1

def posFin(): Boolean={
  if(getX() = large && getY() = hauteur){
    return true
  }else{
    return false
  }
}

def tp(){
  if(getY() != hauteur-1){
    setPos(getX(),getY()+1)
  }else{
    setPos(getX()+1,0)
  }
}