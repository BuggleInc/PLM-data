def keepHandOnSideWall(){
  
  if(isFacingWall()){
    right()
    if(!isFacingWall()){
      forward()
    		}
   	 
     }

  else{
    left()
        if(!isFacingWall()){
          forward()
        	}
        else{right(); forward()}
  }
}

