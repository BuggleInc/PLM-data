def cocktailSort2() {
  var debut = 0
  var cpt=0
  var j = 0
  var fin = getNombreValeurs-2
  do {cpt=0
    for (i<- debut to fin){
      if (plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt=1
      }
    fin=fin-1
    j=fin
    }
    while (j > debut){
      if (plusPetit(j,j+1)==false){
       	echange(j,j+1)
        j=j-1
        cpt=1
      }
    debut=debut + 1
    }
  }while (cpt==1)
}