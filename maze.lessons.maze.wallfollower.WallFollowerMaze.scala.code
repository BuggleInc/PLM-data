def keepHandOnSideWall() {
  var efm:Boolean = false
  gauche()
  if (estFaceMur()) {
    efm = true
  } else {
    efm = false
  }
  droite()
  if ((!estFaceMur()) && (efm == true)) {
    avance()
  } else if ((!estFaceMur()) && (efm == false)) {
    avance()
  } else if ((estFaceMur()) && (efm == true)) {
    gauche()
  }
}

override def run {
  while (!estSurBiscuit()) {
    keepHandOnSideWall()
  }
  prendBiscuit()
}
