def shellSort()  {
  var stockage=0
  var k=0
  var ecart=getNombreValeurs()/2
  while (ecart>=1) {
  	for (i<- 1 to getNombreValeurs()-1) {
    	stockage=getValeur(i)
    	k=i
    	while(k>=ecart && !plusPetitQue(k-ecart,stockage)) {
      	copie (k-ecart,k)
      	k-=ecart
    	}
    	setValeur(k,stockage)
  	}
	}
  ecart=ecart/2
}