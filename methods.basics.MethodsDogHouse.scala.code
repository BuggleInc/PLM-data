var bx = true
var i = 1

def dogHouse()
{
  if(bx)
  {
    bx = false
    
    for(i <- 1 to 6)
    {
      forward()
    }

    setPos(1, 6)

    left()

    for(i <- 1 to 6)
    {
      forward()
    }

    setPos(1, 6)
  }
  else
  {
		setPos(4, 0)
		setBrushColor(Color.white)
    setPos(3, 6)
    i += 1
    
    if(i == 3) right()
  }
}