var diag = 0
var cpt = 0
var x = 0
var y = 0
var max = getMondeLargeur()
override def run() {
  while(diag!=12){
    for (i <- 1 to 7){
      while (y!=0){
        ecritMessage(cpt)
        setPos(x+1,y-1)
        x=x+1
        y=y-1
        cpt = cpt+1
      }
      ecritMessage(cpt)
      setPos(0,x+1)
    }
  }
}