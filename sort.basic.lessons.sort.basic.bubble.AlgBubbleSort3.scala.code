def bubbleSort3()  
{
	var taille:Int = getNombreValeurs();
	var i:Int = 0;
	var j:Int = 0;
	var modif:Boolean = true;

	for (i <- (taille - 2) to 0 by -1)
	{
		modif = false;
		for (j <- 0 to i)
		{
			if (!plusPetit(j, j + 1)) 
			{
					echange(j, j + 1);
					modif = true;
			}
		}

		if (!modif) return 
	} 
}
