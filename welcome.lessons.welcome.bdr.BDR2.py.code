go = 1

while (go):
   m = readMessage()
   
   if   m=='A': forward()
   elif m=='B': forward(2)
   elif m=='C': forward(3)
   elif m=='D': forward(4)
   elif m=='E': forward(5)
   elif m=='F': forward(6)
   elif m=='I': back(); forward()
   elif m=='L': left(); forward()
   elif m=='R': right(); forward()
   elif m=='U': backward(6)
   elif m=='V': backward(5)
   elif m=='W': backward(4)
   elif m=='X': backward(3)
   elif  m=='Y': backward(2)
   elif m=='Z': backward()
   else: go = 0
