def insertion(ecart:Int) {
  for (i<- 1 to getNombreValeurs()-1)
      {
       var j = i
       var v = getValeur(i)
       
       while ((j>=ecart)&&(!plusPetitQue(j-ecart,v)))
           
          {copie(j-ecart,j); j=j-ecart}
       
        setValeur(j, v) }
}
    