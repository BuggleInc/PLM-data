def shellSort()  {
  var eca = getNombreValeurs()/2
  while (eca>0) {
    var t = true
    var v = getValeur(eca)
		var k = eca
    while (k+eca<getNombreValeurs() && t == true){
      t = false
      if (plusPetit(k+eca,k)){
        t = true
      copy(k,k+eca)
      k+=eca
    }
    }
		setValeur(k,v)
    eca = eca/2
  }
  
}