def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (isBirthday){
    if (speed<=65){
      return 0
    }else if (speed<=85 && speed>=66){
      return 1
    }else{
      return 2
    }
  }
  else if (speed<=60){
    return 0
  }else if (speed>=61 && speed <=80){
    return 1
  }else{
    return 2
    
  }
}
