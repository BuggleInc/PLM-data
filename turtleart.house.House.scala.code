override def run() {

  square(100.0);
  
  triangleEqui(100.0);
  
  
 def triangleEqui(taille:Double){
    forward(taille);
    right(120.0);
    forward(taille);
    right(120.0);
    forward(taille);

	}

  def square(taille:Double) {
    forward(taille);
    right(90.0);
    forward(taille);
    right(90.0);
    forward(taille);
    right(90.0);
    forward(taille);
    right(90.0);
  }
  
}