def insertionSort()
{
  var v:Int = 0
  var compteur:Int = 0
  
  for(i <- 1 to getNombreValeurs()-1)
  {
    v = getValeur(i)
    compteur =1
   	
    while((compteur<i)&&(!plusPetitQue(i-compteur,v)))
    {
      copie(i-compteur,i-compteur+1)
      compteur = compteur + 1
    }
    
    setValeur(i-compteur,v)
  }
}