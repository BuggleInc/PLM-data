boolean cigarParty(int cigars, boolean isWeekend) {
if(!isWeekend){
	if (estDans(40,60,cigars)){
		return true;
	}
	else {
		return false;
	}
}
else {
	if(cigars>39){	
	return true;
}
else {return false;}
}

}

boolean estDans(int i, int j, int a){
if(a>i-1 && a<j+1){
	return true;
}
else {
	return false;
}
}

