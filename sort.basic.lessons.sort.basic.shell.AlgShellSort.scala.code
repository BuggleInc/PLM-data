def shellSort()  {
  var valeur = 0;
  var test = 0;
  var ecart = getNombreValeurs()  
 	while (ecart>0) {
    ecart = (ecart.toDouble / 2).toInt;
  	for (i <- 1 to getNombreValeurs-1){
    	valeur = getValeur(i);
    	test = i-ecart;
   		while (test>=0 && getValeur(test)>valeur){
      		setValeur(test+ecart,getValeur(test));
      		test=test-ecart;
    	}
    	setValeur(test+ecart,valeur)
  	}
	}
}  
