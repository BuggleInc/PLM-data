def insertionSort()
{
  for (i <- 1 to getNombreValeurs() - 1)
  {
    var differenceCases = 1
    var derniereCase = getValeur(i)
    while (getValeur(i - differenceCases) > derniereCase && i - differenceCases > -1)
    {
      setValeur(i - differenceCases + 1, getValeur(i - differenceCases))
      differenceCases += 1
    }
    setValeur(i - differenceCases, derniereCase)
  }
}