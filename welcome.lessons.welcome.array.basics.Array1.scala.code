override def run() {
  remplir()
  droite()
  gauche()
  for (i<-0 to Y-1){
    recopier()
  }

}
var X:Int=getMondeLargeur()
var Y:Int=getMondeHauteur()
var T=new Array[Color](Y)

def remplir(){
  for (i<- 0 to Y-1){
    T(i)=getCouleurSol()
    avance()
  }
}

def recopier(){
  for (i<- 0 to T.length-1){
    setCouleurBrosse(T(i))
    avance()
    baisseBrosse()
    leveBrosse()
  }
}