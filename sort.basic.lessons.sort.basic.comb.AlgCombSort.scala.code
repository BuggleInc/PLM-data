def combSort() {
  var gap:Int = getValueCount();
  var sorting = true
  var i:Int = 0
  
  while (gap > 1 && sorting) {
    if (gap>1) {
      gap = (gap.toDouble /1.3).toInt;
    }
    
    i=0
    sorting = false
    while (i+gap < getValueCount()) {
      if (! isSmaller(i, i+gap)) {
        swap(i, i+gap);
        sorting = true
      }
      i+=1
    }
    
    println(gap, sorting)
  } 
}