override def run() {
	var T=new Array[Color](getMondeHauteur())
	for (i<-0 to T.length-2) {
	T(i)=getCouleurSol()
	avance();
	}
	T(T.length-1)=getCouleurSol();
	recule(getMondeHauteur()-1);
	for (i<-1 to getMondeLargeur()-1) {
		gauche();
		avance();
		droite();
		avance();
		for(i<-0 to T.length-1) {
			setCouleurBrosse(T(i))
			baisseBrosse();
			leveBrosse();
			avance();
		}
	}
}