def cocktailSort2() {
val fin=getNombreValeurs()-2
var swap=true
var deb=0
do {
	swap=false
	for (i<-deb to fin){
		if (plusPetit(1+i,i)){
			echange(i,i+1)
			swap=true
			fin-=1
		}
	}
	swap=false
	for (i<-fin to deb by -1){
		if (plusPetit(i+1,i)){
			echange(i,i+1)
			swap=true
			deb+=1
		}		
	}
	
}while (swap==true)
}
