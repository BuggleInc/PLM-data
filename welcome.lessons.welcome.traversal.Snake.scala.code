override def run() {
	var fin:Boolean = false;
	def verifFin() : Boolean = {
		if (estFaceMur() == true) {
			if (getDirection() == Direction.EST) {
				gauche();
				if (estFaceMur() == true) {
					fin = true;
				} else { 
					fin = false;
				}
				droite();
			} 
			else if (getDirection() == Direction.OUEST) {
				droite();
				if (estFaceMur() == true) {
					fin = true;
				} else { 
					fin = false;
				}
				gauche();
			}
		} else {
			fin = false
		}
		if (fin == false) {
			return false;
		} else {
			return true;
		}
	}
	baisseBrosse();
	while (!verifFin()) {
		for (i <- 1 to getMondeHauteur()-1) {
			avance(getMondeLargeur()-1);
			verifFin();
			if (getDirection() == Direction.EST) {
				gauche();
				avance();
				gauche();	
			} else if (getDirection() == Direction.OUEST) {
				droite();
				avance();
				droite();
			}
		}
	}
}
