def combSort() {
  val n = getNombreValeurs()
  var ecart = n
  var flag = true
  var i = 0
  while(ecart>1 || flag)  {
    flag = false
    while(i+ecart<n) {
      if(plusPetit(i+ecart,i)) {
        echange(i,i+ecart)

        flag = true
      }
      i += 1
    }
    if (ecart > 1) {
      ecart = (ecart.toDouble / 1.3).toInt}
   	i=0
  }
}