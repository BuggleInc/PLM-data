override def run() {
  while (!estSurBiscuit()) {
    while (getDirection != Direction.NORTH) {
      gauche()
    }
    while (!estFaceMur) {
      avance()
    }
    do {
      keepHandOnSideWall()
    } while (estFaceMur() && getDirection != Direction.NORTH)
  }
}

def keepHandOnSideWall() = {
  if (!estFaceMur()) {
    gauche()
  }
  while (estFaceMur()) {
    droite()
  }
  avance()
}

