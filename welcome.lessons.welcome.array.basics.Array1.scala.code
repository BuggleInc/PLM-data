override def run() {
var hauteur = getMondeHauteur();
var hau = hauteur -1;
var largeur = getMondeLargeur();
var lar = largeur-1;
var tab = new Array[Color] (hauteur)
for (i<- 0 to hau ) {tab(i) = getCouleurSol();avance()} 
for (i<-0 to lar-1) {gauche();avance();droite();avance()
for(h<- 0 to hau) {setCouleurBrosse(tab(h));baisseBrosse();leveBrosse();avance()}}
}
