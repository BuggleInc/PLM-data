def array123(nums:Array[Int]): Boolean = {
  var est1=false
  var est2=false
  var est3=false
  for (i<-0 to nums.length-1){
    var x=nums(i)
    if (x==1){
      est1=true
    }
    else if(x==2){
      est2=true
    }
    else if(x==3){
      est3=true
    }
  }
  if (est1==true && est2==true && est3==true){
    return true
  }
  else return false
}
