def selectionSort() {
  
  
  var test=false;
  var j=0;
  for(j<-0 to getNombreValeurs()-2)
  {
    var min=getValeur(j)
    var ind=j;
    
    for(i<-j+1 to getNombreValeurs()-1)
    {
      if(min>getValeur(i))
      {
        min=getValeur(i)
        ind=i;
      }
    }
    echange(j,ind)
  }
}