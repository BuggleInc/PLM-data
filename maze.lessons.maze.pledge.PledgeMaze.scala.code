override def run() {
var angle:Int = 0;

def Init(){
	while(!estSurBiscuit){
		if(!estFaceMur && angle == 0){
			setDirection(Direction.NORTH);
			while(!estFaceMur){
				avance();
			}
			droite();
		}
		else{
			garderMur();
		}
	}
}

def garderMur(){
	gauche();
	angle = angle + 1;
	if(estFaceMur){
		while(estFaceMur){
			droite();
			angle = angle - 1;
		}
		avance();
	}
	else{
		avance();
	}
}

avance();
while(!estSurBiscuit){
garderMur();
}

}

