def alarmClock(day:Int, vacation:Boolean): String = {
	if (vacation == true && day=0) {
		return "off"
	} else if (vacation == true && day=6) {
		return "off"
	} else if (vacation == true && day>0 && day<6) {
		return "10:00"
	} else if (vacation == false && day>0 && day<6) {
		return "7:00"
	} else
	return "10:00"
}
