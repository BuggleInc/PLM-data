var diag=0
override def run() {
	var cpt = 0
  ecritMessage(cpt)
  cpt = 1
  setPos(0,1)
  diag = 1
	for(i<-2 to 13){
    cpt = diagonale(cpt)
  }
}

def diagonale(cptV:Int): Int = {
  var cpt = cptV
  for(i<- 1 to diag){
    ecritMessage(cpt)
    cpt = cpt+1
    setPos(getX()+1,getY()-1)
  }
  diag = diag+1
  return cpt
}