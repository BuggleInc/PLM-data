def shellSort()  {
  var ecart=getNombreValeurs()/2
  while (ecart>0){
    for (i<-1 to getNombreValeurs()-1){
      var v=getValeur(i)
      var j=i
      while ((j-i*ecart)>0 && getValeur(j-i*ecart)>v){
        setValeur(j,getValeur(j-ecart))
        j=j-ecart
      }
      setValeur(j,v)
    }
    ecart=ecart/2
  }
}