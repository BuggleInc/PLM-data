def tree(steps:Int, length:Double, angle:Double, shrink:Double){
	if(steps == 0){
		return;
	}
	current(steps);
	avance(length);
	gauche(angle);
	tree(steps - 1, length * shrink, angle, shrink);
	droite(angle*2);
	tree(steps - 1, length * shrink, angle, shrink);
	gauche(angle);
	droite(180);
	avance(length);
	gauche(180);
}
