def shellSort()  {
  var gap:Int = getValueCount() / 2
  var v:Int = 0
  while(gap >= 0){
    for(i <- gap to getValueCount() - 1){
       v = getValue(i)
       var j:Int = i
       while(j >= gap && getValue(j - gap) > v){
         copy(j - gap, j)
         j -= gap
       }
       setValue(j, v)
    }
    gap /= 2
  }
}
