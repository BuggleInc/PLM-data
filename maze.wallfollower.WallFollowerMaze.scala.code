override def run() {
  while(!isOverBaggle()){

    	while(!isFacingWall() && wallToLeft()){
        forward()
      }
      if(wallToLeft()){
        right()
      }
      else{
        left()
        forward()
        left()
        forward()
      }
  
    
    //right()
  }
  pickupBaggle()
}

def wallToLeft() : Boolean = {
  left()
  var mur : Boolean = isFacingWall()
  right()
  return mur
}