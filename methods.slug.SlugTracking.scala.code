def isFacingTrail():Boolean = {
  	var vuVert: Boolean = false
    if (getCouleurSol()==Color.green) {
      vuVert=true
    } else {
      vuVert=false
    }
  	return vuVert
}

while (estSurBiscuit==false) {
    if (estFaceMur()==true) {
      left();
    }
  	forward();
  	if (isFacingTrail()==true) {
      backward(); brushDown();brushUp(); forward();
    } else {
      backward(); left();
    }
}