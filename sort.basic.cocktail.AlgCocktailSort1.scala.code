def cocktailSort() {
	var n : Int = getNombreValeurs()
  var aBouger : Boolean = true
    
  while (aBouger == true){
    for (k <- 0 to n-2){
      if (plusPetit(k, k+1) == false){
        echange(k,k+1)
        aBouger = true
      }
    }
    for (k <- 0 to n-2){
      var i : Int = n - 2 - k
      if (plusPetit(i, i+1) == false){
        echange(i,i+1)
        aBouger = true
      }
    }
  }
}