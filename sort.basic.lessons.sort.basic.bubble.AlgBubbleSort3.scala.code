def bubbleSort3() {

var trie : Boolean = false;
var nb : Int = 0;

	while (!trie) {
		for (j <- getNombreValeurs() - 2 to 0 by -1) {
			nb = 0;
			for (i <- 0 to j) {
				if (!plusPetit(i,i+1)) {
					echange(i,i+1);
					nb = nb + 1;
				}
				if (nb == 0) {
					return;
				}
			}
		}
		if (nb == 0) {
			trie = true;
		}
	}

}
