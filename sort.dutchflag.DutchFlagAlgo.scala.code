def solve() {
  val n= getTaille()
  for (i<-1 to n-1)
  {var c=getCouleur(i)
  if (c== BLEU)
    {var j=i-1 ;
     while (j>=0 && getCouleur(j)!=BLEU)
      {echange(j,j-1); j-=1}}
  else if (c==BLANC) 
     {var j=i-1 ; 
      while ((j>=0 && getCouleur(j)==ROUGE))
      
          {echange(j,j-1); j-=1}}}
}
