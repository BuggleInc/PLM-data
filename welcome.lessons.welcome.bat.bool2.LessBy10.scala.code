def lessBy10(a:Int, b:Int, c:Int):Boolean = {
  if ((a>b) && ((a-b)>=10)) {return true} 
  else if ((b>a) && ((b-a)>=10)){return true}
	else if ((a>c) && ((a-c)>=10)) {return true}
	else if ((c>a) && ((c-a)>=10)) {return true}
	else if ((b>c) && ((b-c)>=10)) {return true}
	else if ((c>b) && ((c-b)>=10)) {return true}
	else {return false}
}