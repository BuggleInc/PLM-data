def insertionSort() {
  var k:Int =1
  var a:Int =0
  var e:Int =1
  for(i<- 1 to getNombreValeurs()-1){
    a=getValeur(i)
    k=1
    for(j<-1 to i){
      if(e=1){
       if(getValeur(i-j)>a){
   			k=i-j   
        copie(i-j,i-j+1)  
      }
        else{e=0}
      }
      
     
    }
    
      if(k!=1 && i !=1){
        setValeur(k,a)
      }
        
      

                   }
}



























































































































































































































