def cocktailSort2() {
  var n=1
  var m=1
  var beg=0
  var end = getValueCount() -2
  while (n!=0 || m!=0) {
    n=0
    m=0
    for (i <- beg to end) {
      if (isSmaller(i+1,i)) {swap(i, i+1); n=1}
    }
    end = end - 1
    for (i <- end to beg by -1) {
      if (isSmaller(i+1, i)) {swap(i,i+1); m=1}
    }
    beg = beg + 1
  }
}