def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
var s= steps
var l=length
	if(s!=1){
		current(s)
		avance(length)
		droite(angle)
		s=s-1
		l=l*shrink
		tree(s,l,angle,shrink)
	}
	else{
		var x=getX()
		var y=getY()
		current(s)
		avance(l)
		setPos(x,y)
		gauche(angle)
		gauche(angle)
		avance(l)
	}
}
