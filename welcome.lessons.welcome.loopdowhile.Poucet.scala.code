var sortie=false
var nbBaggel=0;
while (sortie==false) {
	for (compteur <- 1 to 5) {
	avance();
	if (estSurBiscuit()) {
		nbBaggel=nbBaggel+1;
	}
	}
	if (nbBaggel>=3) {
		gauche();
		nbBaggel=0;
	}
	else if (nbBaggel<=2) {
		droite();
		nbBaggel=0;
	}
	if (sortieTrouvee()) {
		sortie=true;
		avance();
	}
}