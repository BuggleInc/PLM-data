def haveBaggle():Boolean = {
	var vuBiscuit : Boolean = false
  var nbPas = 0
  do
  {
   for (i <- 1 to 6)
    {
      if (estSurBiscuit())
      {
        vuBiscuit = true
      }
      avance()
      nbPas = nbPas + 1
    }
  } while (vuBiscuit != true)
  for (i <- 1 to nbPas)
  	recule()
  if (vuBiscuit==true)
  	return true
  else
  	return false
  
}