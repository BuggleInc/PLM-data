def cocktailSort() {
	
	var n : Int =getNombreValeurs()
	var i :Int =n-2	
	var c:Int=0

	do{
		for(i <- 0 to n-2 ){
			c=0
			if(!plusPetit(i,i+1)){
				echange(i+1,i)
				c=c+1		
						}
				}
	
		while(i>=0){
			c=0
			if(!plusPetit(i,i+1)){
				echange(i+1,i)
				c=c+1		
			}
			i=i-1
		}



	} while(c!=0)
}
