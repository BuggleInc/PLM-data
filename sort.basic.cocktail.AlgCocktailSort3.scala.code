def cocktailSort3() {
  var c=1
  var d=1
  var debut=0
  var fin=getNombreValeurs()-2
  while(c==1&&d==1) {
    c=0
  	for (i<-debut to fin) {
    	if (plusPetit(i+1:Int,i:Int)) {
      	echange(i+1:Int,i:Int)
      	c=1
    	}
  	}
    fin-=1
    d=0
    for (i<-fin to debut by -1) {
    	if (plusPetit(i+1:Int,i:Int)) {
      	echange(i+1:Int,i:Int)
      	d=1
    	}
  	}
    debut+=1
  }
}