def combSort() {
  var a: Boolean=true;
  var b: Boolean=true;
  var ecart: Int=getNombreValeurs();
  var lgr: Int=ecart;
  while(ecart>1 || a==true)
  {
    if(ecart>1)
    {
      ecart=ecart/1.3;
    }
    var i: Int=0;
    while(i+ecart < lgr)
    {
      if(getValeur(i)>getValeur(i+1))
      {
        echange(i,i+1);
        i=i+1;
      }
    }
  }
}