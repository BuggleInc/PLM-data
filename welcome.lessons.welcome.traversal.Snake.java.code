public void run() {
	boolean fin = false;
	while (!fin){
		while (!estFaceMur()){
			avance();
		}
		if (getDirection()==Direction.EST){
			gauche();
			if (estFaceMur()){
				fin = true;
				droite();
			}
			else {
				avance();
				gauche();
			}
		}
		else{
			droite();
			avance();
			droite();
		}
	} 
	
}
