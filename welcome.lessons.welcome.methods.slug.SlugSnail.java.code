public void hunt(Color c) {
 	while(!isOverBaggle()){
 		
 		if(isFacingTrail(c)){
 			brushDown();
 			forward();
 			brushUp();
 		}else{
 			left();
 		}
 	}

 	pickUpBaggle();
     
}//end hunt()

boolean isFacingTrail(Color c) {
boolean cell = false;

	if(isFacingWall()){
		return cell;
	}

	forward();
	cell = getGroundColor().equals(c); 
	backward();
	
	return cell;
}//end isFacingTrail()

