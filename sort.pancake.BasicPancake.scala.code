def solve() {
  var length:Int = getTaillePile()-1
  var max:Int = 0
  do{
    if (length > 0){
      for (i <- 0 to length){
        if (getRayonCrepe(i) >= getRayonCrepe(max)){
          max = i
        }
      }
      if (max < length){
        retourne(max+1)
        retourne(length+1)
      }
      length -= 1
    }
    else{
      retourne(length)
    }
  }while(!estTriee())
}