override def run() {
  
  while( ! positionFinale()){
    ecritMessage( ""+(getY() + getX()*7))
    parcours()
  }
  
}


def positionFinale(): Boolean ={
  return getMondeHauteur()-1 == getY() && getMondeLargeur()-1 == getX() 
}

def parcours(){
  if (estFaceMur()){
    setPos(getX() +1 , 0)
  } else{setPos(getX(), getY()+1)}
}