public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){
	do{
/*		if(isFacingWall() ){
			right();
		}else{
			forward();
			left();			
		}
*/

		while(isFacingWall() ){
			right();
		}

		forward();
		left();

		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall
