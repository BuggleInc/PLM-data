override def run() {
  var i=0
  var j=0
  var place=0
  while (!positionFinale()){
    for (j <- 0 to 6){
      for (i <- 0 to 6){
        ecritMessage(place)
        place +=1
        setPos(i,j)
      }
    setPos(0,j)
    }
  }
  place+=1
  ecritMessage(place)
}

def positionFinale():Boolean={
  if (getX()==6 && getY()==6){
    return true
  }
  return false
}