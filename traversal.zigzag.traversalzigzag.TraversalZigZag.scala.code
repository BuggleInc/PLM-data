def posTest():Boolean = { 
  if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {
  	return true
  }
  return false
}



def colonne() {
	var j=0
  var cpt=0
  
  while (!posTest()) {
    for(i<-0 to 5) {
      setPos(i,j)
      
      ecritMessage(cpt:Int)
    	cpt=cpt+1
    }
    
    for(i<-0 to 6) {
      
      setPos(6-i,j+1)
      ecritMessage(cpt:Int)
    	cpt=cpt+1
    
  
     
    }
    
    	j=j+2
    
   
}}






override def run() {
  colonne()
  
}