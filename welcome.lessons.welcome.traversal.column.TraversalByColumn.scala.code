def finpos():Boolean = {return ((getMondeHauteur()-1 == getX() && (getMondeLargeur() -1== getY())))}
def nexcase() {var x= getX()
              var y = getY()
              if (y==getMondeLargeur()-1) {x=x+1;y=0} else y=y+1
              setPos(x,y)}



override def run() {var c=0
                    while (!finpos()) {ecritMessage(c.toString);nexcase();c+=1}
                    ecritMessage(c.toString)
}
