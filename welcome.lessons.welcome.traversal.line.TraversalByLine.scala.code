override def run() {



var steps:Int = 2;

ecritMessage(0);
	while(!estArrive())
	{

		
		setPos(getX() + 1, getY());
		ecritMessage(steps - 1);
		steps = steps + 1;
		if(getX() == 6 && getY() < 6)
		{	
			setPos(0,getY() + 1);
			ecritMessage(steps-1);
			steps = steps + 1;
			
		}

	
	}
	
		

def estArrive() : Boolean = {

	var result = false;

	if(getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur() - 1)
	{
		result = true;
	}

		return result;

	
}






}
