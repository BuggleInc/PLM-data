

def cocktailSort() { 

		var swap : Boolean =false 

		
		for (i<-0 to getNombreValeurs()-2) {
			swap=false
			
			if (plusPetit (i+1, i)){
				echange (i, i+1)
				swap == true
			}
			
		}

		for (i<-getNombreValeurs()-2 to 0 by -1) {
			
			if (!swap) {
				
				if (plusPetit (i+1,i)){
					echange(i,i+1)
					swap==true
				}
				
			}
		}
}
