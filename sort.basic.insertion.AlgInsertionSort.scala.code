def insertionSort() {
  var len = getNombreValeurs()
  var rgpositif:Boolean = true
  var v = 0
  var rg = 0
  for (i <- 1 to len-1) {
    rg = i-1
    v = getValeur(i)
    while (rgpositif) {
    	if (v<getValeur(rg))  {
    		copie(rg,rg+1)
      if (rg>0)
      	rg-=1
      else
      	rgpositif = false
    }
    setValeur(rg+1,v)
  }
}
    	