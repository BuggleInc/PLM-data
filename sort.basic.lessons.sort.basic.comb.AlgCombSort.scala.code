def combSort() {
  
  val l = getNombreValeurs()
  var ecart = getNombreValeurs()
  var i : Int
  
  do{
    
  	if (ecart>1){
      ecart = ecart/1.3
    }
    i = 0
    
  	while (i+ecart < l){
    	if (plusPetit(i+ecart,i)){
      	echange(i,i+ecart)
      
    	}
    	i += 1
  	}
    
  }while(ecart > 1  || i >= 1)
  
}