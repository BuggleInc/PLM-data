public void cocktailSort() {
  boolean chg = true;
  while(chg){
    chg=false;
    for(int i = 0; i< getNombreValeurs()-1; i++){
      if(plusPetit(i+1,i)){
        echange(i+1,i);
          chg=true;
      }
    }
    for(int j = getNombreValeurs()-1; j >=0; j--){
      if(plusPetit(j+1,j)){
        echange(j+1,j);
        chg=true;
      }
    }
  }
}