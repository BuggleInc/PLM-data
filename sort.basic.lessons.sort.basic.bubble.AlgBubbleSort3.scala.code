def bubbleSort3()  {
  var estTrie:Boolean = false
  for (j <- 0 to getNombreValeurs()-1) {
    estTrie = true
    for (i <- 0 to getNombreValeurs()-2-j) {
      if (plusPetitQue(i+1,getValeur(i))) {
        echange(i,i+1)
        estTrie = false
      }
    }
    if (estTrie) {
      return
    }
  }
}