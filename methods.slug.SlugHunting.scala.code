def hunt() {
    while(isOverBaggle() == false){
      if(isFacingTrail == true){
        forward()
      }
      else {left()}
    }
}

def isFacingTrail():Boolean = {
   		if(isFacingWall()){
      return false
      }
 			 
      else {forward}
  		if (getGroundColor() == Color.green){
        backward()
        return true
      }
  		else {backward() 
            return false}
  
}