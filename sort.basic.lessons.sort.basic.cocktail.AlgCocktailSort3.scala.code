def cocktailSort3() {
  var tri = true
  var deb = 0
  var fin = getNombreValeurs()-1
  while (tri) {
    tri = false
    for (i<-deb to fin-1) {
      if (!(plusPetit(i,i+1))) {
        echange(i,i+1)
        tri = true
      }
    }
    if (!tri) {
    	fin = fin - 1
    	for (j <- fin to deb+1 by -1) {
      	if (plusPetit(j,j-1)) {
        	echange(j,j-1)
        	tri = true
      	}
    	}
    	deb = deb +1
    }
  }
}
