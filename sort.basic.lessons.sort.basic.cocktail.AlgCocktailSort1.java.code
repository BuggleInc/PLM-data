public void cocktailSort() {
	boolean swapped;
	do {
		swapped = false;
		int i = 0;
		while(i < getValueCount() - 1) {
			System.out.println("-> i:  " + i);
			if (isSmaller(i + 1, i)) {
				swap(i, i+1);
				swapped = true;
			}
			i++;
		}
		if (true) break;
		while(i > 0) {
			if (isSmaller(i + 1, i)) {
				swap(i, i + 1);
				swapped = true;
			}
			i--;
		}
	} while(swapped);
}

