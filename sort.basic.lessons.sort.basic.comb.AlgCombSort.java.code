public void combSort() {
  double ecart = getValueCount(); 
  boolean swap = true;
  do{
    if(ecart >1){
      ecart = (ecart/1.3);
    }
    int gap = (int)ecart;

    swap = false;
    for(int i = 0; i < getValueCount()-gap;i++){
      if(!isSmaller(i,i+gap)){
        swap(i+gap,i);
        swap = true;
      }
      
    }
  }while((ecart > 1)||swap);
}