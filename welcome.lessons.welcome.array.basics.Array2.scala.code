override def run() {
	var T = new Array[Color](getMondeHauteur)
	
	for (i <- 0 to T.length - 2) {
		T(i)=getCouleurSol()
		avance()
	}
	T(T.length-1)=getCouleurSol();
	
	for(i<-2 to getMondeLargeur()) {
		gauche()
		avance()
		droite()
		for (j <- 0 to getMondeHauteur() - 2) {
			setCouleurBrosse(T(j))
			baisseBrosse()
			leveBrosse()
			avance()
		}
	}

}
