def cocktailSort2() {
	var inverse=false
	var N=getNombreValeurs()
	var O=0
	do{
		inverse=false
		for (i<- 0 to N-2) {
			if (plusPetit(i+1,i)) {
			echange(i,i+1)
			inverse=true
			N=N-1
			}
		}
		for (i<-getNombreValeurs()-2 to O by -1) {
			if (plusPetit(i+1,i)) {
			echange(i,i+1)
			inverse=true
			O=O+1
			}
		}
	} while (inverse)
}
