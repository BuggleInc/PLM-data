override def run() {
def keepHandOnSideWall(){
	gauche();
	if(getDirection()==Direction.SOUTH||getDirection()==Direction.EST||getDirection()==Direction.WEST){
	if(estFaceMur){
		droite(); if(estFaceMur){
			droite(); if(estFaceMur){
				droite()}else{avance}
			}else{avance}
		}else{avance}
	}
}
def trouveNord(){
	if(getDirection==Direction.NORTH && estFaceMur==false){avance()}
}

var etat = 0

while(getY>0){
etat match{
	case 0 => trouveNord()
			if(estFaceMur()==true && getDirection()==Direction.NORTH){
				etat=1 
				
			}
	case 1 => keepHandOnSideWall()
			if(getDirection()==Direction.NORTH){
				etat=0
			}
}



}
}



