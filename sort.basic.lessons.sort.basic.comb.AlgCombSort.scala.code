def combSort() 
{
	var ecart:Int=getNombreValeurs()
	var i=0
	var modifie=false
	do
	{
		modifie=false
		if(ecart>1)
			ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
		while(i+ecart<getNombreValeurs())
		{
			if(getValeur(i)>getValeur(i+ecart))
			{
				echange(i,i+ecart)
				modifie=true
			}
			i=i+1
		}
	}
	while(modifie | ecart>1)
}
