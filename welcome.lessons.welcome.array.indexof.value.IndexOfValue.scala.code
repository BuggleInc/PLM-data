def indexOfValue(nums:Array[Int] ,value:Int): Int = {
	var p:Int =nums.length
	if (nums.length == 0){
		return -1
	}
	if (nums.length == 1){
		if(nums(0)==value){
			return 0
		}else{
			return -1
		}
	}
	for (j<-0 to nums.length -1){
		if(nums(j)==value){
			return j
		}else{
			return -1
		}
	}else{
		return -1
	}
	

}
