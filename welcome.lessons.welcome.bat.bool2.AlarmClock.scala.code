def alarmClock(day:Int, vacation:Boolean): String = {
  if (vacation = false && day <= 5 && day >= 1) {
    return "7:00"
  }
  else if (vacation = false && day = 6 || day = 0){
    return "10:00"
  }
  else if (vacation == true && day = 1 || day = 2 || day = 3 || day = 4 || day = 5){
    return "10:00"
  }
  else if (vacation == true && day = 6 || day = 0){
    return "off"
  }
}