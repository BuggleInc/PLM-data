def combSort() {
  val len=getNombreValeurs()
  var n=0
  var ecart= len
  var i=0
  do{
    n=0
    i=0
    ecart=(ecart.toDouble/1.3).toInt
    while (ecart+i<len){
      n=change(i, i+ecart, n)
      i+=1
    }
  } while(ecart>1)
  if (n>0){
    while (n>0){
      n=0
      for (j<- 0 to len-2){
        n=change(j, j+1, n)
      }
    }
  }
}



def change(n:Int, m:Int, o:Int):Int={
  if (!plusPetit(n, m)){
    echange(m, n)
    return o+1
  }
  return o
}