def shellSort()  {
  var lgr=getNombreValeurs
  var ecart=lgr/2
  while (ecart>0) {
    insertionSort(ecart)
    ecart=ecart/2
  }
}

def insertionSort(ecart:Int) {
  var lgr=getNombreValeurs
  for (i <- 1 to lgr-1) {
    var v=getValeur(i)
    var j=i
    while ((j>0) && (!plusPetitQue(j-ecart,v))) {
      copie(j-ecart,j)
      j-=ecart
    }
    setValeur(j,v)
    }
  }