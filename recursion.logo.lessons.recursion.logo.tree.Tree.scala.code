def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  var tot:Int= steps
  if (steps==0) {
    if(steps==tot){}
    recule(length)
    gauche(angle)
    avance(length)
    tree(steps+1,length/shrink,angle,shrink)
  }
  avance(length)
  droite(angle)
  tree(steps-1, length*shrink, angle,shrink)
}
