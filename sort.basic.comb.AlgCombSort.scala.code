def combSort() {
  var lgr = getNombreValeurs()
  var ecart = lgr
  var i =0
  var aumoinsunechange = 0
  while(ecart >1 || aumoinsunechange == 1){
    aumoinsunechange = 0
    if(ecart>1){
      ecart = (ecart.toDouble/1.3).toInt
    }
    i = 0
    while(i+ecart < lgr){
      if(plusPetit(i+ecart,i)){
        echange(i,i+ecart)  
        aumoinsunechange = 1
      }
      i = i+1
    }
  }
}