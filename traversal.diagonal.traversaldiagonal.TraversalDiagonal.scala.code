var diag = 0;
var a : Int = 1
//var y : Int = 0
var counter : Int = 0;
var finalpos : Boolean = true;
override def run() {
      for(y <- 0 to 6){
        for (x <- 0 to y){
          setPos(x,y-x)
          writeMessage(counter)
          counter = counter + 1
          diag = diag + 1
      }
 
    }
    for (y <-  a to 6){
      for (x<- a to y){
      setPos(x,6-y+x)
      writeMessage(counter)
      counter = counter + 1
        diag = diag+1
        a = a+1

  }
      
    
  }
}
