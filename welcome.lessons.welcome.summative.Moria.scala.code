def sortir() {
  retourne();
  while (getCouleurSol() != Color.GREEN) {
    if (estSurBiscuit()) {
      prendBiscuit();
      retourne();
  		while(!estSurBiscuit) {
    		avance();
  		}
  		recule();
  		poseBiscuit();
  		retourne();
    } else if (estFaceMur()) {
      droite();
      avance();
      gauche();
      avance();
    } else {
      avance();
    }
  }
}