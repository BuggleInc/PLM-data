def cocktailSort2() {
var lg= getNombreValeurs()
var tri=true
var fin =lg-1
var debut= 0
do { tri=true
	for (i<- debut to fin-1){
	if (plusPetit (i+1, i)){
		echange(i, i+1)
		tri=false
	}
} 
fin=fin-1
for (i<- fin to debut+1 by -1){
	if (plusPetit(i, i-1)){
		echange(i, i-1)
		tri=false
	}
}
debut=debut+1
}
while ( tri==false)
}