override def run() {
  retourne()
  var compteur:Int=0
  while (getX() !=(getMondeLargeur()) && getY() != getMondeHauteur()-1) {
    ecritMessage(compteur)
    setPos(getX(),getY()+1)
    compteur+=1
    if (estFaceMur()){
      ecritMessage(compteur)
      setPos(getX()+1,0)
    }    
  }
  compteur+=1
  ecritMessage(compteur)
}