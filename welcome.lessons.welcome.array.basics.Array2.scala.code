override def run() {
	var t=new Array[Color](getMondeHauteur());
	var w:Int=0;
	var i:Int=1;

	for (x<-0 to getMondeHauteur()-1) {
			t(x)=getCouleurSol();
			if (x< getMondeHauteur()-1) {
				avance();
			}
			w=w+1
	}
	
	for (y<-1 to getMondeHauteur()-1) {
	recule(w);
	gauche();
	avance();
	droite();
	baisseBrosse();
	setCouleurBrosse(t(i));
	leveBrosse();
	i=i+1;
	}
}
