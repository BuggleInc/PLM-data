override def run() {
  var cpt = 0
  var y = 0
  while (finale==false) {
		for (i<-1 to 6) {
      ecritMessage(cpt)
      cpt = cpt + 1
      setPos(i,y)
  	}
    y = y + 1
    setPos(0,y)
  }
}

def finale(): Boolean = {
  if (getX==6 && getY==6) {
    return true
  } else {
    return false
  }
}
