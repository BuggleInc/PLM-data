  var blancRouge= getTaille() -1
  var apresBlanc= getTaille() -1
  while(!estTrie){
  	getCouleur(apresBleu) match{
      case Color.BLUE => apresBleu = apresBleu +1
      case Color.WHITE => echange(apresBleu,apresBlanc);apresBlanc=1+apresBlanc
