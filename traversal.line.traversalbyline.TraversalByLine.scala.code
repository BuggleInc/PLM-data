override def run() {
  var x=0
  var y=0
  var compteur=0
  while (y!=getMondeHauteur()){
  	while (x!=getMondeLargeur()){
    	ecritMessage(compteur)
      compteur = compteur + 1
      if (x!=6){
      	x = x + 1
    		setPos(x,y)
      }
      else {
        x=x+1
        if (y!=6){
        	y=y+1
        	setPos(0,y)
        }
        else {
          y=y+1
        }
      }
  	}
    x=0
  }
}