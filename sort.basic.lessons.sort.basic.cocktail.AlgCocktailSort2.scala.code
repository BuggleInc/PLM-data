def cocktailSort2() {
var debut:int=0
var fin:int= getNombreValeurs()-2
var swap=false
	do{ 
	
		for(i<-debut to fin by 1 ){
			swap=false
				if(plusPetit(i+1,i)){
					echange(i,i+1)
					fin=fin+1
					swap=true
			
			}
		}
		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				debut=debut+1
				swap=true
				
			
		}
		
	}while(swap)
}
