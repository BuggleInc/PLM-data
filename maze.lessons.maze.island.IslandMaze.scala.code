override def run() {
def keepHandOnSideWall() {
	gauche
	if(estFaceMur) {
		droite
		if(estFaceMur) {
			droite
			if(estFaceMur) {
				droite
			} else {
				avance
			}
		} else {
			avance
		}
	} else {
