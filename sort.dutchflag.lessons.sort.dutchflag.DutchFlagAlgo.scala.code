def solve() {
  
  var blue=0;
  var white=0;
  var red=0;
  
  for(i<-0 to getSize()-1){
    
    if(getColor(i)==0){
			swap(i,blue);
      blue += 1;
    }
    if(getColor(i)==1){
			swap(i,white+blue);
      white += 1;
    }
    if(getColor(i)==2){
			swap(i,red+blue+white);
      red += 1;
    }
    
  }

}