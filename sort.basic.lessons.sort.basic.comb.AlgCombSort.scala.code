def combSort() {
	var ecart = getNombreValeurs();
	var swaped = true;
	var i = 0;
	do{
		if(ecart > 1){
			ecart = (ecart.toDouble / 1.3).toInt;
			println(ecart);
		}
		i = 0;
		swaped = false;
		while(i + ecart < getNombreValeurs()){
			if(plusPetit(i + ecart, i)){
				echange(i, i+ecart);
				swaped = true;
			}
			i += 1;
		}
	}while(ecart >1 && swaped);
}
