def arrayFront9(nums:Array[Int]): Boolean = {
  var  L=nums.length
  
  
  if (L<4) {
    for (i  <- 0 to (L-1)) {
      if (nums(i)==9) {
        return true
      }
      else {
        return false
      }
    }
    
  }
  else {
    return ((nums(0)==9)||(nums(1)==9)||(nums(2)==9)||(nums(3)==9))
  }
}