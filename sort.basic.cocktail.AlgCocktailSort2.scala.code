def cocktailSort2() {
  var c=1
  var debut=0
  var fin=getNombreValeurs()-2
  while(c==1) {
    c=0
  	for (i<-debut to fin) {
    	if (plusPetit(i+1:Int,i:Int)) {
      	echange(i+1:Int,i:Int)
      	c=1
        fin-=1
    	}
  	}
    for (i<-debut to fin) {
    	if (plusPetit(getNombreValeurs()-1-i:Int,getNombreValeurs()-2-i:Int)) {
      	echange(getNombreValeurs()-1-i:Int,getNombreValeurs()-2-i:Int)
      	c=1
        debut+=1
    	}
  	}
  }
}