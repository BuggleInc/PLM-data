String alarmClock(int day, boolean vacation) {
	if((day<=5 && day >0) && vacation == false)
	return "7:00";
	if((day<=5 && day >0) && vacation == true)
	return "10:00";
	if((day==6 && day ==0) && vacation == false)
	return "10:00";
	if((day==6 && day ==0) && vacation == true)
	return "off";
	return "off";
}
