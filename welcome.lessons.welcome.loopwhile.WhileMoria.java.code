public void run() { 
	while(!isBackingWall()){
		while(!isOverBaggle() || isBackingWall()){
			backward();
		} 
		if(isOverBaggle()){
			pickupBaggle();
			if(isCarryingABaggle()){
				while(!isOverBaggle()){
					forward();	
				}	
				backward();
				dropBaggle();
				backward();
			}
		}
	}
	right();
	while(!isBackingWall()){
		backward();
	}
	right();
	while(getGroundColor() != Color.green){
		forward();
	}
}
