def cocktailSort2() {
  var debut = 0
	var fin = getNombreValeurs()-2
	var mem = 0
  do {
    mem=1
    for (i<-debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
        mem=0
      }
    }
    fin=fin-1
    for (i<-debut+1 to fin) {
      if (plusPetit(fin+1-i+1,fin+1-i)) {
        echange(fin+1-i+1,fin+1-i)
        mem=0
      }
    }
    debut=debut+1
  } while (mem==0)
}