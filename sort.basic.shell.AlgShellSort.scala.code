def shellSort()  {
  var ecart=getNombreValeurs()/2
	while (ecart>0){
    for (i<-1 to getNombreValeurs()-1) {
      var v=getValeur(i)
      var j=i
      while (j>0 && !plusPetitQue(j-i*ecart,v) ) {  
        copie(j-i*ecart,j-(i-1)*ecart)
        j-=1    
      }
      setValeur(j,v)
    }	
  }
 
}