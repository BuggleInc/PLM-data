def close10(a:Int, b:Int): Int = {
  var deltaa:Int = Math.abs(10 - a)
  var deltab:Int = Math.abs(10 - b)
  
  (deltaa, deltab) match {
    case (deltaa, deltab) if (deltaa == deltab) => return 0
    case (deltaa, deltab) if (deltaa < deltab) => return a
    case (deltaa, deltab) if (deltab < deltaa) => return b
  }
}