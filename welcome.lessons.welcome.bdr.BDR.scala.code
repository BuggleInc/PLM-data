var x: String = "";
if(estSurMessage()) {
	x = litMessage()
	if ( x=="R" ) { right();}
	if ( x=="L"  ) {  left();}
	if ( x=="I" ) {  retourne();}
	if ( x=="A") {  avance();}
	if ( x=="B") {  avance();avance;}
	if ( x=="C") {  avance();avance();avance();}
	if ( x=="Z"  ) { recule();}
	if ( x=="Y") {  recule();recule;}
	if ( x=="X"  ) {  recule();recule;recule;}
}





