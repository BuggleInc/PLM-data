def combSort() {
	var ecart = getNombreValeurs();
	var verif=false;
	do {
		verif=false;
		if (ecart>1) {
			ecart = (ecart.toDouble / 1.3).toInt;
		}
		var i=0;
		while (i+ecart < getNombreValeurs()) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				verif=true
			}
			i=i+1;
		}
	} while (ecart >1 | verif)
}
