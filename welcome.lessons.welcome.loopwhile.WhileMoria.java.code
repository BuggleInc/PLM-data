public void run() {
	left();
	left();	
	while (isFacingWall() == false) {
		while (isOverBaggle() == false) {forward();}
			pickupBaggle();
			while (isOverBaggle() == false) {
			backward();}
			forward();
			dropBaggle();
			if (isFacingWall() == false) {
			forward();}
			else {
			right();
 			forward();
 			left();
 			forward();
 			}
	}
	
}

	/* isFacingWall() isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().*/
	 
