def cocktailSort3() {
	var deb=0;
	var fin=getNombreValeurs-2;
	var verif=false;
	do{
		verif=false;
		for (i <- deb to fin by 1) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				verif=true;
			}
		}
		fin=fin-1;
		if (!verif){
			return;
		}
		for (i <- fin to deb by -1) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				verif=true;
			}
		}
		deb=deb-1;
		if (!verif) {
			return;
		}
	} while (verif)
}
