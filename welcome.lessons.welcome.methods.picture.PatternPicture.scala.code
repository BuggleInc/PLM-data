  avance(2);
  faireCoteDroit(Color.BLUE);
  gauche();avance(2);droite();
  faireCoteGauche(Color.GREEN);
  recule(2);
  faireCoteDroit(Color.YELLOW);
  avance(3);droite();avance(2);gauche();recule(3);
}


def faireCote(){
