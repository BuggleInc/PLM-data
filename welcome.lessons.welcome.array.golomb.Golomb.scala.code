def golomb(num:Int): Int = {
	var c = 1
	var T = new Array[Int](num)
	var r = 0
	var e = 0
	var comp = 0

	T(0) = 1

	while (c < num)
	{

		r = T(c - 1)
		comp = 0

		for (e <- 0 to num - 1)
		{
			if (T(e) == c)
			{
				comp = comp + 1
			}
		}

		if (r - comp > 0)
		{
			T(c) = c
		}
		else 
		{
			T(c) = c + 1
		}
		c = c +1
		
	}

	return T(num - 1)
	

	
} 
