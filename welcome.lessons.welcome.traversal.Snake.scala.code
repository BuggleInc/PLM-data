override def run() {
  baisseBrosse()
  while (!posfin()) {
    snakeWalk()
  }
}



def posfin():Boolean = {
  if ((getDirection()==Direction.EST) && estFaceMur()) {
    gauche()
    if (estFaceMur()==true) {
      return true
    }
  } else {
    droite()
    return false
  }
  return false
}

def snakeWalk() {
  if (!estFaceMur()) {
    avance()
  } else if (getDirection()==Direction.EST) {
    gauche()
    avance()
    gauche()
  } else {
    droite()
    avance()
    droite()
  }

}