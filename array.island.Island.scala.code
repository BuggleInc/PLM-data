def island(num:Array[Int]): Int = {
  var len : Int = longueur(num)
  var compteur : Int = 0
  var lvl = 0
  while (compteur <= len) {
    compteur += 1
    while (num(lvl+1)>num(lvl)) {
    lvl+=1
  }
  }
}


def longueur(num:Array[Int]): Int = {
  var taille = 0
  while (num(taille)<100) {
    taille += 1
  }
};return longueur