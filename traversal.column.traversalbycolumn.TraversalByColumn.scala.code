def estPosiFin():Boolean={
  if(getX()==getMondeHauteur() && getY()==getMondeLargeur()){return true}
  return false}
def avancer(){
  var x=getX()
  var y=getY()
  y=y+1
  if (y>getMondeHauteur()){x=0;y=y+1}
  setPos(x,y)
}


override def run() {
  var mark=0
  do{ecritMessage(mark)
     mark=mark+1
    avancer()
  }while(!estPosiFin())
}