def cocktailSort2() {
  var lgr = getNombreValeurs()
  var debut = 0
  var fin = lgr-2
  var echang = 0
  do{
    echang = 0
    for(i<- debut to fin){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        echang = 1
      }
    }
    fin = fin -1
    for(j<- fin to debut){
      if(plusPetit(j+1,j)){
        echange(j,j+1)
        echang = 1
      }
    }
    debut = debut +1
  }while(echang == 1)
}