def noTriples(nums:Array[Int]): Boolean = {
	var taille: Int = nums.length;
	var n: Int = 0;
	var nb: Int = 0;
	var occur: Int = 0;

	for(n <- 0 to taille - 1){
		if(nums(n) != nb){
			nb = nums(n);
			occur = 0;
		}
		else{
			occur = occur + 1;
		}
			
	}
	if(occur == 3){
		return false;
	}
	else{
		return true;
	}
	
}
