override def run() {
  while (!posFinale()){
    parcours()
  }
}

def posFinale():Boolean={
  val l=getMondeLargeur()
  val h=getMondeHauteur()
  
  return ((getX()==l)&&(getY()==h))
}

def parcours()={
  val h=getMondeHauteur()
  while(!(getY()==h)){
    setPos(getX(),getY()+1)
  }
  setPos((getX()+1),0)
}