override def run() {
  var x=0
  var y=0
  var compteur=0
  while (x!=getMondeLargeur()){
  	while (y!=getMondeHauteur){
    	ecritMessage(compteur)
      compteur = compteur + 1
      if (y!=6){
      	y = y + 1
    		setPos(x,y)
      }
      else {
        y=y+1
        if (x!=6){
        	x=x+1
        	setPos(x,0)
        }
        else {
          x=x+1
        }
      }
  	}
    y=0
  }
}