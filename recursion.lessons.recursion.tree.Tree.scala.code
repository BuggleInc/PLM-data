def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {

	if(steps!=0) {
		avance(length)
		droite(angle)
		avance(length)
		tree(steps-1,shrink*length,angle,shrink)
		recule(length)
		gauche(2*angle)
		avance(length)
		tree(steps-1,shrink*length,angle,shrink)
		recule(length)
		droite(angle)
	}

}
