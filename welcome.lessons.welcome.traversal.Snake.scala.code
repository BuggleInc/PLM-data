def positionFinale():Boolean = {
		if ((estFaceMur()) && (getDirection == Direction.EST)){
			gauche()
			if (estFaceMur()){
				return true
			}
		}
		return false
	}
override def run() {
baisseBrosse()
	while (positionFinale == false){
		while (!estFaceMur()){
				avance() 
			}
			if (getDirection == Direction.EST){
				gauche()
				avance()
				gauche()
			}
			}
			if (getDirection == Direction.OUEST){
				droite()
				avance()
				droite()
			}
  		}
		}
}
