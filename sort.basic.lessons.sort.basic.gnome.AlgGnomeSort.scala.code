def gnomeSort() {
  var longueur:Int = getNombreValeurs()
  var posGraincheux:Int = 0
  
  while (posGraincheux < longueur) {
    // Graincheux v?rifie les pots sont dans le bon ordre.
    if (plusPetit(posGraincheux + 1, posGraincheux))
      echange(posGraincheux + 1, posGraincheux)
    else
    	posGraincheux += 1
  }
}