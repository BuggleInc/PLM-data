def insertionSort() {
  
  for (i <- 1 ro getNombreValeurs()-1) {
    
    var v = getValeur(i)
    var last = 0
    
    for (c <- 1 to i) {
      
      if (!plusPetitQue(i-c,v)) {
        
        setValeur(i,getValeur(i-c))
        last = c
        
      }
      
    }
    
    set(i-c,v)
    
  }
  
}
