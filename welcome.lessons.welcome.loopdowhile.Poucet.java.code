public void run() { 
	int baggle = 1;
	while(!exitReached())
	{
		do
	{
		forward();
		if(isOverBaggle())
		{
			baggle++;
		}
	}while(!crossing());
	if(baggle <3)
	{
		right();
		baggle = 0;
	}
	else
	{
		left();
		baggle = 0;
	}
}
