def bubbleSort2() {
  var n = getValueCount();
  var swapped = false;
	
  do
  {
    swapped = false;
    for( i <- n-2 to 0 by -1)
    {
      for (j <- 0 to i)
      {
      	if (!isSmaller(i,i+1))
      	{
       	 swap(i,i+1);
        	swapped = true;        
      	}
      }
    }
  }while(swapped);
  
}