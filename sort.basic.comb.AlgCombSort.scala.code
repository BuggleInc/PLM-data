def combSort() {
  var ecart = getNombreValeurs()-1
  var nbEchanges = 0
  var i = 0
  while(ecart> 1 | nbEchanges >= 1) {
    nbEchanges=0
    if(ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    i=0
    while(i+ecart < getNombreValeurs()) {
      if(plusPetit(i+ecart,i)) {
        echange(i,i+ecart)
        nbEchanges= nbEchanges+1
      }
      i=i+ecart
    }
  }
}