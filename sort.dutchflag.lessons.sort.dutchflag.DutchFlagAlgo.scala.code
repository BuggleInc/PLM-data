def solve() {
  var apresBleu=0;
  var avantBlanc = getTaille()-1;
  var blancRouge = getTaille()-1;
  while(apresBleu <= avantBlanc){
    if(getCouleur(apresBleu)==BLEU){
      apresBleu += 1;
    }else{
      if(getCouleur(apresBleu)==ROUGE){
        echange(blancRouge,apresBleu)
        blancRouge -= 1;
      	avantBlanc -= 1;
      }else{
     	 	echange(apresBleu,avantBlanc);
      	avantBlanc -= 1;
      }
    }
  }
}