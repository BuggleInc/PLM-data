def extrema(nums:Array[Int]): Int = {  
  var max = nums(0)
  var min = nums(0)
  var compteur = 0
  while (compteur<nums.length)
  {
    if (nums(compteur)>max)
    {
      max = nums(compteur)
    }
    if (nums(compteur)<min)
    {
      min = nums(compteur)
    }
    compteur += 1
  }
  return max - min
}