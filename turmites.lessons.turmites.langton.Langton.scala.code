def step() {
  
  if(getCouleurSol()==Color.BLACK)
  {
    setCouleurBrosse(Color.WHITE)
    
  	baisseBrosse()
  	leveBrosse()
    gauche();
    avance()
  }else if(getCouleurSol()==Color.WHITE)
  {
    setCouleurBrosse(Color.BLACK)
    
  baisseBrosse()
  leveBrosse()
    droite();
    avance()
  }
  
}
