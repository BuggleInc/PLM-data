def bubbleSort()  {
  var listeTriee:Boolean=true
  var x=0
  x=getNombreValeurs
  do {
    for (i <- 0 to x-2) {
       if (plusPetit(i+1,i)) {
         echange(i+1,i)
         listeTriee=false
       } else {
         listeTriee=false
       }
    }
  } while (test()==true)
}

def test() {
  var liste:Boolean=true
  var x=0
  x=getNombreValeurs
  for (i <- 0 to x-2) {
    if (plusPetit(i+1,i)) {
      liste=false
    }
  }
  return liste
}