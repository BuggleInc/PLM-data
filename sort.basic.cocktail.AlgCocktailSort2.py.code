lgr=getNombreValeurs()
debut=0
fin=lgr-1
trie=False
while not trie:
  trie=True
  for i in range (debut,fin):
    if plusPetit(i+1,i):
      echange(i,i+1)
      trie=False
  fin=fin-1
  for i in range (fin,debut,-1):
    if plusPetit(i,i-1):
      echange(i-1,i)
      trie=False
  debut=debut+1
   
    
    
