def max2Value(nums:Array[Int]): Int = {
  var c = nums(0)
  var b = nums(1)
  var max1 = Math.max(b,c)
  var max2 = Math.min(c,b)

    for (i<- 2 to nums.length){
    if (i>=max1 && i>max2){
      max2 = max1
      max1 = i
    }else if (i>max2 && i<max1){
      max2 = i
    }
    
  }
  
  return max2
}