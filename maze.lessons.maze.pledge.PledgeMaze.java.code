public void run() {
	escapeTheMaze();
}

public void escapeTheMaze(){
int turns = 0;
int random = random3();
setDirection(Direction.EAST);

	do{	
		//go north
		if(turns == 0){
			while(!isFacingWall() && getDirection() == Direction.NORTH){
				forward();
			}
		}

		if(isFacingWall()){
			right();
			turns--;
		}else{
			forward();
			left();
			turns++;
		}

		//pick up baggle
		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
		
	}while(!isCarryingBaggle());
}//end escapeTheMaze
