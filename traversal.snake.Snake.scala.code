override def run() {
  brushDown();
  
  while(!isFacingWall() && getDirection() != Direction.NORTH) {
    if(!isFacingWall()) {
      forward();
    }
    
    if(isFacingWall()) {
      if(getDirection() == Direction.EAST) {
        left();
      }
      
      if(getDirection() == Direction.WEST) {
        right();
      }
    }
  }
}