def combSort() {
  var ecart:Int = getNombreValeurs();
  var incr:Boolean=false;
  var i:Int = 0;
  
  do{
    incr=false;
    if(ecart > 1){
      ecart = (ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int];
    }
    i=0;
    while(i+ecart < getNombreValeurs()){
      echange(3,4);
      if(plusPetit(i+ecart,i)){
        echange(i+ecart,i);
        i=i+1;
        incr=true;
      }
    }
    
  }while(ecart > 1 || incr==false);
}