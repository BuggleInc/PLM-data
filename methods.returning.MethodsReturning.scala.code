def haveBaggle():Boolean = {
  
  var vuBiscuit:Boolean = false
  
  	while (!vuBiscuit) {
  
  		for (i <- 1 to 6) {
    
    		if (estSurBiscuit()) {
      
      		vuBiscuit = true 
      
    		}
        
        avance();
        
      }
    
  		for (i <- 1 to 6) {
    
    		recule();
        
      }  
    
			if (vuBiscuit) {
    
    		return vuBiscuit
    
  		} 
    
			else {
    
  		droite();
  		avance();
  		gauche();
    
  		}
    
    }
  
  return vuBiscuit
  
}