void goAndGet(){
int stepFwd = 0;
int stepBwd = 0;

	while(!isOverBaggle()){
		forward();
		stepFwd++;
		
		if(isOverBaggle()){
			pickupBaggle();
			while(isCarryingBaggle()){
				backward();
				stepBwd++;
			
				if(stepFwd == stepBwd){
					dropBaggle();
				}

				if(!isCarryingBaggle()){
					stepFwd = 0;
					stepBwd = 0;
				}
			}
		}
	}
}
