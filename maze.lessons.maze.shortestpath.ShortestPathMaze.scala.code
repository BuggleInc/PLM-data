override def run() {
  marquage()
  def marquage() {
    var x=0
    var y=0
    while (!aBiscuit(x,y)) {
      if (x<getMondeLargeur()) {x+=1}
      else {x=0;y+=1}
    }
    setIndication(x,y,0)
    var a=0
    var b=0
    var p=0
    for (b<-0 to getMondeHauteur()-1) {
      for (a<-0 to getMondeLargeur()-1) {
        p=((a-x)%getMondeLargeur()+(b-y)%getMondeHauteur())
        setIndication(a,b,p)
      }
    }
  }
}