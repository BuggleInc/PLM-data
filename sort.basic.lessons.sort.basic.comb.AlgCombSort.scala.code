def combSort() {
  var ecart:Int = getNombreValeurs()
  var swapped:Boolean = false
  
  while (ecart > 1 || swap) {
    if (ecart > 1) {
    	ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    }
    
    var i:Int = 0
    
    while (i + ecart < getNombreValeurs()) {
      if (plusPetit(i + ecart, i))
      	echange(i + ecart, i)
      
      i += 1
    }
  }
}