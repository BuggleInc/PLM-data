def cocktailSort3() {
	var chgtFait : Boolean = true
	var debut : Int = 0
	var fin : Int = getNombreValeurs()-2
	while (chgtFait) {
		chgtFait = false
		for (i <- debut to fin) {
			if (plusPetit(i+1, i)) {
				echange(i, i+1)
				chgtFait = true
			}
		}
		if (chgtFait == false) {
				return
			}
		fin -= 1
		for (i <- fin to debut by -1) {
			if (plusPetit(i+1, i)) {
				echange(i, i+1)
				chgtFait = true
			}
		}
		if (chgtFait == false) {
				return
			}
		debut += 1
	}
}
