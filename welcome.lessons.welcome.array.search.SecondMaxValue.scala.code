def max2Value(nums:Array[Int]): Int = {
  var longueurT = nums.length
  var max = nums(0)
  var maxDeux = nums (0)
  
  
  
  //On doit avoir ? la fin du programme le 2e plus grand nombre, en un seul tour
  
  for (i<- 1 to longueurT-1){
    var temp = max
    if (nums(i) > max)
    	maxDeux = temp
    	max = nums(i)
    
  }
  return maxDeux
}