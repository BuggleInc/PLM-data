def combSort11() {

  var taille=getNombreValeurs()
  var trier=true
  var ecart=getNombreValeurs()
  
  while (ecart>1 || trier==true){
    if(ecart>1){
		  ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]      
    
   	  if (ecart==10){
      ecart=11
    }
    }
    
    var i =0
    trier=false
   
    while (i+ecart< taille ){
      if (plusPetit(i+ecart,i)==true){
        echange(i+ecart,i)
        trier=true
      }
    i=i+1
    }
    
  }
  
}