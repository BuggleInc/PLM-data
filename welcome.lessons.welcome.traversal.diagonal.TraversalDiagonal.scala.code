var diag = 0;
override def run() {
  var comp = 0;
  var col = 0;
  var lign = 0;
  while (!(positionFinale())) {
    for (i <- 0 to 11) {
      col = i;
      for (j <- 0 to 11) {
        lign = j;
      	setPos(col-lign,lign);
      }
    }
  }
}



def positionFinale():Boolean = {
  var res = false;
  if (getX()==6  && getY()==6) {
    res = true;
  }
  return res
}