$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class twoAsOne extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
