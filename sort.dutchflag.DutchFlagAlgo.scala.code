def solve() {
  while (!estTrie){
   for (i <- 0 to getTaille-2){
     for (j <- 0 to getTaille-i-2){
       if (getCouleur(i)==2){
         if (getCouleur(getTaille-j-1)==0 || getCouleur(getTaille-j-1)==1){
           echange(i,getTaille-j-1)
         }
     	 } else if (getCouleur(i)==1) {
         if (getCouleur(getTaille-j-1)==0){
           echange(i,getTaille-j-1)
         }
       }
   		}
		}
	}
}