def moveInter(height:Int, src:Int, other1:Int, other2:Int, dst:Int) {
  
  height match {
    
    case 0 => { () }

    case n => {
      moveInter((n - 1), src, dst, other2, other1)
     	move(src, other2)
      move(src, dst)
      move(other2, dst)
      
    }
  }
}

