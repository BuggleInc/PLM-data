var diag = 0;
override def run() {
  var cpt:Int = 0
  ecritMessage(cpt)
  cpt+=1
  for (i <- 0 to 47) {
    if(estFaceMur() || murADroite()) {
      if (diag<6){
        setPos(0,(diag+1))
      }
      else {
        setPos((diag-5),6)
      }
      diag+=1
    }
    else {
      setPos(getX()+1,getY()-1)
    }
    ecritMessage(cpt)
    cpt+=1
  }
}

def murADroite():Boolean = {
  droite()
  if (estFaceMur()) {
    gauche()
    return true
  }
  else {
    gauche()
    return false
  }
}