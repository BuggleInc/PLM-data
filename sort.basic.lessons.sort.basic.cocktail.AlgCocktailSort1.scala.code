def cocktailSort() {
  var m=getNombreValeurs()
  var n= false
  do{
    n= false
    if (i<- 0 to m-2){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          n= true
      
    }
    if (i<- m-2 to 0){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          n= true      
    }
  } while (true)
}




    
      
    	