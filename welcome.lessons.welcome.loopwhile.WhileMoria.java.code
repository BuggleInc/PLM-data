public void run() { 
	right();
	right();

	while(!isFacingWall()) {
		while(!isOverBaggle()&&!isFacingWall()) {
			forward();
		}
		if(isOverBaggle()) {
			pickupBaggle();
		} else {
			escape();
			return;
		}

		while(!isOverBaggle()) {
			backward();
		}
		if(isCarryingBaggle()) {
			forward();
			dropBaggle();
			forward();
		}
	}
}

public void escape() {
	right();
	forward();
	left();
	forward();
}


