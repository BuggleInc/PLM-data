def positionFinale():Boolean = {
		if ((estFaceMur()) && (getDirection() == Direction.EST)){
			gauche()
			if (estFaceMur()){
				return true
			} else {
				droite()
				gauche()
				avance()
				gauche() 
			}
		}
		if ((estFaceMur()) && (getDirection() == Direction.EST)){
		gauche()
		droite()
		avance()
		droite()
		}
		return false 
}		
override def run() {
baisseBrosse()
	while ((positionFinale() == false) && (!estFaceMur)){
				avance()
  	}
}
