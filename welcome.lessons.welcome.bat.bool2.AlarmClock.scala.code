def alarmClock(day:Int, vacation:Boolean) : String ={
 
  if (vacation == false && (day <= 5 && day >= 1)) {
    return "7:00"
  }
  else if (vacation == false && (day > 5 || day < 1)){
    return "10:00"
  }
  else if (vacation == true && (day <= 5 && day >= 1)){
    return "10:00"
  }
  else  {
    return "off"
  }
}
