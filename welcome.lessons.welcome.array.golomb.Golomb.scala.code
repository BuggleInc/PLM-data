def golomb(num:Int): Int = {
  var avance=1
  var test =1
  var nombre=0
  var T= new Array[Int](num)
  T(0)=test
  while (avance<num){
    nombre =nombre+1
    test =T(nombre)
    while (test !=0 && avance < num){
      test=test-1
      avance=avance+1
      T(avance-1)=nombre
    }
  }
  return T(num-1)
}