def cocktailSort2() {
	var debut = 0
	var fin = getNombreValeurs - 2
	var swap = true
	while (swap){
		for (i <- debut to fin){
			swap = false
			if (plusPetit(i+1,i)){
				echange(i,i+1)
				swap=true
				fin = fin - 1
			}
		}
		for (i <- fin to debut by -1){
			if (plusPetit(i+1,i)){
				echange(i,i+1)
				swap = true
				debut = debut + 1
			}
		}
		
	}
}