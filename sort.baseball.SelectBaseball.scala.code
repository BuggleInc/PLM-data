override def run() {
  chercheJoueur(0,0)
  deplace(4,0)
}


def chercheJoueur(couleur:Int, depart:Int) {
  
  var baseTestee:Int = (depart+1)%5
  
  while ((getCouleurJoueur(baseTestee,0)!=couleur) && (getCouleurJoueur(baseTestee,1)!=couleur)) {
    deplace(baseTestee,0)
    baseTestee = (baseTestee+1)%5
  }
  
  
}