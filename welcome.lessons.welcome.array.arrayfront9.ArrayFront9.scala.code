def arrayFront9(nums:Array[Int]): Boolean = {
  var taille = nums.length
  if (taille < 4)
  {
    for (i <- 0 to taille -1)
    {
      if (nums(i) == 9)
      	{
          return true
          
        }
    }
    
  }
  else
  		{
        for (i <- 0 to 3)
        	{
            if (nums(i) == 9)
            	{
                return true
              }
          }
      }
  return false }