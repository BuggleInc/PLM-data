var diag = 0;
override def run() {
	var x=0
	val gML=getMondeLargeur-1
	val gMH=getMondeHauteur-1	
	var i=0
	var j=0
	var y= -1
	var z=1
	while (diag!=2*gML){
		do {
			if (diag<=gML) setPos(i,j)
			else setPos(j,i)
			ecritMessage(x)
			x+=1
			j-=z
			i+=z
		}while (i!=diag-y)
		diag+=1
		if (diag<=gML) {i=0;j=diag}
		else {j=diag-gML;i=gMH;y=2*gML-diag;z= -1}
	}
}
