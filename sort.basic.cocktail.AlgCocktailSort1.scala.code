def cocktailSort() {
  var n:Int=getNombreValeurs()
  var i:Int=0
  var compteur:Int=1 
  while (compteur!=0) {
    compteur=0
    for (k <- 0 to (n-2)){
      if (!plusPetit(k,k+1)){
        echange(k,k+1)
        compteur=1
      }
    }
    for (k <- (n-2) to 0){
      if(!plusPetit(k,k+1)) {
        echange(k,k+1)
        compteur=1
      }
    }
  }
}