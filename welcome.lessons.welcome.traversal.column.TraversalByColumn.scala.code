override def run() {
  exo()
}

def positionFinale():Boolean={
  if((getX()== getMondeLargeur() -1) && (getY()== getMondeHauteur -1)){
    return true
  }
  return false
}

def exo(){
  var n = 0
  while(!positionFinale){
    if(getY() == getMondeHauteur -1){
      setPos(0,getY()+1)
      ecritMessage(n)
      n=n+1
    }
    else if(1 == 1){
      setPos(getX()+1,getY())
      ecritMessage(n)
      n=n+1
    }
  }
}