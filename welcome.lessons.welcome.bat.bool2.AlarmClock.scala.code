def alarmClock(day:Int, vacation:Boolean): String = {
	if ((day==0 || day==6)&& vacation == true ){return "off"}
	if ((day==0 || day==6)&& vacation == false ){return "10:00"}
	if ((day==1 || day==2 ||day==3 || day==4 ||day==5)&& vacation == true ){return "10:00"}
	if ((day==1 || day==2 ||day==3 || day==4 ||day==5)&& vacation == false ){return "10:00"}
}
