def combSort() {
  var ecart:Int = getNombreValeurs()
  var aInverse:Boolean = false
  do {
    aInverse = false
    if (ecart>1) {
      ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    }
    var i:Int = 0
    while (i+ecart < getNombreValeurs()) {
      if (getValeur(i)>getValeur(i+1)) {
        echange(i,i+1)
        aInverse = true
      }
      i += 1
    }
  } while (ecart>1 || aInverse == true)
}