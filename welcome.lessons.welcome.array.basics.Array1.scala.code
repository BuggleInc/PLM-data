override def run() {
var H =  getMondeHauteur()
var L =  getMondeLargeur()
var T = new Array[Color](H)
for( i <- 0 to H-1 )
{
	T(i) = getCouleurSol()
	avance()
}

recule(H)
gauche()
avance()
droite()
avance()

for(i <- 1 to L-2)
{
	recule(H-i-1)
	gauche()
	avance()
	droite()
	avance(i)
	for(i <- 0 to H-1)
	{
		setCouleurBrosse(T(i))
		baisseBrosse()
		leveBrosse()
		avance()
	}	

}
}
