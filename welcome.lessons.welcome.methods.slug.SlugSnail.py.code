def isFacingTrail(color):
    if not estFaceMur():
        forward()
        if getCouleurSol()==color:
            backward()
            return True
        else:
            backward()
            return False

def hunt(color):
    while not estSurBiscuit():
        while not isFacingTrail(color):
            left()
        forward()
        baisseBrosse()
        leveBrosse()
    prendBiscuit()