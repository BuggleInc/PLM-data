override def run() {

	def finish():Boolean = {
		if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {return true}
		return false
	}

	ecritMessage(0)

	var cpt =1

	while (!finish()) {
		if (!estDosMur) {
			setPos(getX(), getY()-1)
		} else {
			setPos(getX()+1, 0)
		}
		ecritMessage(cpt)
		cpt = cpt + 1
	}

	ecritMessage(cpt)

}
