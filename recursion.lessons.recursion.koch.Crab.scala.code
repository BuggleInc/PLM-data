def crab(levels:Int, length:Double) {
gauche(45)
if(levels==0){
		avance(length)
	}else{
		crab(levels-1,length/Math.sqrt(2) )
		gauche(90)
		crab(levels-1,length/Math.sqrt(2))
		droite(45)
		crab(levels-1,length/Math.sqrt(2))
		droite(45)
		crab(levels-1,length/Math.sqrt(2))
		gauche(90)
		crab(levels-1,length/Math.sqrt(2))
	}	
}
