def dateFashion(you:Int, date:Int):Int = {
	if (you >= 8 && you <= 10) {
		if (date > 2 && date <= 10) {
			return 2
		} else {
			if (date >= 0 && date <= 2) {
				return 0
			} else {
				return 1
			}
		}
	}	
	if (date >= 8 && date <= 10) {
		if (you > 2 && you <= 10) {
			return 2
		} else {
			if (you >= 0 && you <= 2) {
				return 0
			} else return 1
		}
	}
	if ((you > 2 && you < 8) && (date > 2 && date < 8)) {
		return 1
	} else return 0
}	