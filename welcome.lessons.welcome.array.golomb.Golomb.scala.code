def golomb(num:Int): Int = {
  var G = Array[Int](num*3)
  var last : Int = 2
  var compteur :Int=2
  G(0) = 0
  G(1) = 1
  G(2) = 2
  G(num) = 0
  while( G(num)==0) {
    var valeur: Int = G(compteur)
    for (i <- 0 to valeur-1){
      G(last) = compteur
      last = last + 1
    }
    
    compteur = compteur +1
  }
  return G(num)
}