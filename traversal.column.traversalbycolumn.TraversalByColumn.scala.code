override def run() {
  var cpt:Int=0
  ecritMessage(cpt)
  while ((getY()!=getMondeHauteur())&&(getX()!=getMondeLargeur())) {
    if (!estDosMur()) {
      setPos(getX(),getY()+1)
      cpt=cpt+1
    } else if (getX()!=getMondeLargeur()) {
      setPos(getX(),0)
      cpt=cpt+1
    }
    ecritMessage(cpt)
  }
}