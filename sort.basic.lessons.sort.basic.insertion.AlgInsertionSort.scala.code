def insertionSort() {
  var i:Int=1
  var v:Int=0
  val N:Int=getNombreValeurs()
  var j:Int=0
  while (i<N){
    v=getValeur(i)
    j=i-1
    while(!plusPetitQue(j,v) && j>=1){
      if(!plusPetitQue(j,v)){
        setValeur(j+1,getValeur(j))
        j=j-1
      }
      
    }
    setValeur(j+1,v)
    i=i+1
  }
}


