override def run() {
  while (positionFinale()==false){
    positionSuivante()
  }
}

def positionFinale(){
  if (getX()==getMondeLargeur() && getY()==getMondeHauteur()){
    return true
  }
  return false
}

def positionSuivante(){
  var cpt : Int = 0
  for (i <- 0 to getMondeLargeur()){
    for (j <- 0 to getMondeHauteur()){
      cpt=cpt+1
      setPos(i,j)
      ecritMessage(cpt)
    }
  }
}