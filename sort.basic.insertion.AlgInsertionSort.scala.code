def insertionSort() {
  
  for (i <- 1 to getNombreValeurs()-1) {
    
    var v = getValeur(i)
    var last = 0
    
    for (c <- i to 0 by -1) {
      
      var a = getValeur(c)
      
      if (a>v) {
        
        setValeur(c+1,a)
        last = c
        
      }
      
    }
    
    setValeur(last,v)
    
  }
  
}