def angleBleu():
    setCouleurBrosse(Color.BLEU)
    for b in range(2):
        baisseBrosse()
        leveBrosse()
        left()
        forward()
        left()

def angleRouge():
    setCouleurBrosse(Color.RED)
    for r in range(2):
        forward()
        baisseBrosse()
        leveBrosse()
        left()
        forward()
        left()

angleRouge()