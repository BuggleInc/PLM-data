def gnomeSort() {
	var position = 0
	var swap = true
	while(position != getNombreValeurs()-1 && swap){
		swap = false
		if(position == 0){
			position += 1
		}else{
			if(plusPetit(position,position-1)){
				echange(position,position-1)
				swap = true
				position -= 1
			}else{
				position += 1
			}
		}
	}
}
