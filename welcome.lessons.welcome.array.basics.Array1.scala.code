override def run() {
	var T = new Array[Color](getMondeHauteur)
	T(0)=getCouleurSol()
	for(c <- 0 to T.length - 1) {
		avance()
		T(c) = getCouleurSol
	}
	for(c2 <- 0 to T.length - 2) {
		gauche()
		avance()
		droite()
		avance()
		for(c3 <- 0 to T.length - 2) {
			setCouleurBrosse (T(c3))
		}
	}
}
