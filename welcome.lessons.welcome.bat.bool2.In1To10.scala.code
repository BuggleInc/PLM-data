def in1To10(n:Int, outsideMode:Boolean):Boolean = {
  var in = true
  if ((n<=10)&&(n>=1)) {
    in = true
  } else {
    in = false
  }
  if ((outsideMode)&&(in)&&((n==10)||(n==1))) {
    return true
  } else if ((outsideMode)&&(in)) {
    return false
  }  else if ((outsideMode)&&(in==false)&&((n==10)||(n==1))) {
    return true
  } else if ((outsideMode==false)&&(in==false)) {
    return false
  } else {
    return true
  }
}
