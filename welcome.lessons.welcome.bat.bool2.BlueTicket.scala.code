var x:Int=0
var y:Int=0
var z:Int=0


def blueTicket(a:Int, b:Int, c:Int):Int = {
  x=a+b
  y=b+c
  z=a+c
  if (x==10){
    return 10
  }else if (y==10){
    return 10
  }else if (z==10){
    return 10
  }else if ((x-y)==10){
    return 5
  }else if (((x-z)==10)){
    return 5
  }else{
    return 0
  }
}