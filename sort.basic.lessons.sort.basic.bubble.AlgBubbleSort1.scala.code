def bubbleSort()  {
  var i : Int = 0
  var a : Int = getNombreValeurs()
  var j : Int = 0
  var trie : Boolean = false
  while (trie==false){
    for (i<- 1 to a){
      if (plusPetit(i,i-1)== true) {
        echange(i,i-1)
        j=j+1
      }
      if (j==0){
        trie==true
      }
     
    }
  }
}

    