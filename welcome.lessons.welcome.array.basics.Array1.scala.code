override def run() {
  var i = getMondeHauteur()
  var T = new Array [Color](i)
  for (j<- 0 to T.length-1) {
  	var m = getCouleurSol
  	T(j) = m
  	avance()
	}
	gauche()
	avance()
	droite()
	avance()
  for (n<- 0 to T.length-1){
		setCouleurBrosse(T(n))
		baisseBrosse()
		leveBrosse()
    avance()
	}
}


