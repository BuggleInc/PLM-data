override def run() 
{

def keepHandOnSideWall()
{
	while(!estSurBiscuit())
	{
		if (estFaceMur())
		{
			gauche()
			if (estFaceMur() && !estDosMur())
			{
				droite()
			} 
			else if (estFaceMur() && estDosMur())
			{
				gauche()
				if (estFaceMur())
				{
					gauche()
					gauche()
					avance()
				}
				else
				{
					avance()
				}
				avance()
			}
			else
			{
				avance()
			}
		} else avance()
	}
}

keepHandOnSideWall()

}
