def cocktailSort3() {
  var n = getNombreValeurs();
  var j = 1;
  var c1 = 1;
  var c2 = 1;
  while ( c1 != 0  || c2 != 0 ){
    c1 = 0;
    c2 = 0;
    for ( i <- j-1 to n-j-1 ){
      if (plusPetit(i+1:Int,i:Int)){
        echange(i+1,i);
        c1 += 1;
      }
    }
    if ( c1 == 0 ){
      return;
    }
    for ( i <- j+1 to n-j ){
      if (plusPetit(n-i:Int,n-i-1:Int)){
        echange(n-i-1,n-i);
        c2 += 1;
      }
    }
    j += 1;
  }
}