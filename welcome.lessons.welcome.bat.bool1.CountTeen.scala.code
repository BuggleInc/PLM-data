def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var nbAdo :Int = 0 ;
  if (estAdo(a)==true) {nbAdo = nbAdo +1} ;
  if (estAdo(b)==true) {nbAdo = nbAdo +1} ;
  if (estAdo(c)==true) {nbAdo = nbAdo +1} ;
  if (estAdo(d)==true) {nbAdo = nbAdo +1} ;
  return nbAdo}

def estAdo(a:Int):Boolean =
		{if (a>=13 && a<=19) {return true}
     else {return false}
    }
