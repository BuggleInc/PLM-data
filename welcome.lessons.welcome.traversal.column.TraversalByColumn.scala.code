override def run() {
  val h=getMondeHauteur()
  
  while (!posFinale()){
    if ((getY())==h){
    setPos((getX()+1),0)
  }
  else{
    setPos(getX(),getY()+1)
  }
  }
    
  
  }


def posFinale():Boolean={
  val l=getMondeLargeur()
  val h=getMondeHauteur()
  
  return ((getX()==l)&&(getY()==h))
}