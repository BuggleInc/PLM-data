def noTriples(nums:Array[Int]): Boolean = {
  var pred:Int=nums(0)
  var occurrence:Int=1
  for (i<-1 to nums.length-1){
    if (nums(i)==pred){
      occurrence=occurrence+1
      if (occurence==3){
        return false
      }
    }else{
      pred=nums(i)
      occurence=1
    }
  return true
  }
}
  