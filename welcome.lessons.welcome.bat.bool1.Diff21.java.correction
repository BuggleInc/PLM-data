$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class diff21 extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( diff21((Integer)t.getParameter(0)) );		 }
	int diff21(int n) {
		if (n>21)
			return 2*(n-21);
		return 21-n;
	}
 }