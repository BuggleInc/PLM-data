var fini = false
var mess = ""

while(!fini){
  
  if (estSurMessage()){
    
		mess = litMessage()
    if(mess == "R") {
      droite ; avance ;
    }
    
    if(mess == "L"){
      gauche ; avance;
    }
    
    if(mess == "I"){
      droite; droite; avance;
    }
    
    if(mess == "A") avance;
    
    if(mess == "B"){
      avance ; avance;
    }
    if(mess == "C"){
      avance ; avance ; avance;
    }
       
    if(mess == "Z") recule;
       
    if(mess == "Y") {
      recule ; recule
    }
       
    if(mess == "X") {
      recule ; recule ; recule;
    }
    
  }
  else fini = true ;
  
}