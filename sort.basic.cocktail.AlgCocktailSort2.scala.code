def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var permutation = 0
  do {
    for (i <- debut to fin){
      if (plusPetit(i,i+1)==false) {
        permutation=1
        echange(i,i+1)
        fin-=1
      }
    }
   for (j <-fin to debut){
     if (plusPetit(j,j+1)==false){
       permutation=1
       echange(j,j+1)
       debut+=1
     }
   }
  } while (permutation==1)
}