def step(rule:Array[Char], colors:Array[Color]) {
  var cou=Color.white
 	var dir = 0
  cou = getCouleurSol()
  for (i <- 0 to rule.length-1){
    if (cou==colors(i)){
      dir = i
    }
  }
  setCouleurBrosse(colors((dir+1)%(rule.length)))
 	baisseBrosse()
 	leveBrosse()
  if (rule(dir)=='L'){
   	gauche()
    avance()
  }
  else {
    droite()
    avance()
  }
}