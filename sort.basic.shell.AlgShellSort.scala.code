def shellSort()  {
  
  var ecart = getNombreValeurs()/2
  
  while (ecart>0) {
  
    for (i <- 1 to (getNombreValeurs()/ecart).toInt) {
      
			var last = 0
      var c = 1
      var v = getValeur(i*ecart-(c-1)*ecart)
      

      while (i*ecart-(c+1)*ecart >= 0 && !plusPetitQue(i*ecart-c*ecart,v)) {

        copie(i*ecart-c*ecart,i*ecart-(c+1)*ecart)
        last = c
        c = c + 1

      }

      setValeur(i*ecart-last*ecart,v)

    }
    
    ecart = ecart/2
  
  }
  
}