override def run() {
  def marquerSol(){
    baisseBrosse()
    leveBrosse()
  }
  def faireV(){
    avance(2)
    marquerSol()
    avance()
    gauche()
    avance()
    marquerSol()
    recule()
    droite()
    avance()
    marquerSol()
    avance(2)
    gauche()
  }
  
  def faireMotif(){
    var compt:Int=0
    for (compt<-1 to 4){
      faireV()
    }
  }
  
  var nbMotif:Int=0
  for (nbMotif<-1 to 3){
    faireMotif()
    avance(7)
  }

}