def passeObstacle() {
  while(estFaceMur()){
    droite()
    if(estFaceMur){
      gauche()
      if(!estFaceMur){
        avance()
      }
      else{
        gauche()
        if(!estFaceMur){
          avance()
          
        }
        else{
          gauche()
          avance()
        }
      }
    }
    else{
      avance()
    }
  }
}

override def run() {
  while(!estSurBiscuit){
    while(getDirection != Direction.NORTH){
      droite()
    }
    while(!estFaceMur){
      avance()
    }
    passeObstacle()
  }
  prendBiscuit()
}