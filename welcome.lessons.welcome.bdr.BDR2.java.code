while(getIndication()!=' '){
switch (getIndication()){
    case'R':
    droite();
    avance();
    break;	// just go around while 
    case'L':
    gauche()
    avance();
    break;
    case'I':     //print.ln.out.walker 
    retourne();
    avance();
    break;
    case'A':
    avance();
    break;
    case'B':
    avance(2);
    break;
    case'C':
    avance(3);
    break;
    case'D':
    avance(4);
    break;
    case'E':
    avance(5);
    break;
    case'F':
    avance(6);
    break;
    case'Z':
    recule();
    break;
    case'Y':
    recule(2);
    break;
    case'X':
    recule(3);
    break;
    case'W':
    recule(4);
    break;
    case'V':
    recule(5);
    break;
    case'U':
    recule(6);
    break;
    default:
}
}
