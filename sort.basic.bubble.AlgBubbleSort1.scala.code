def bubbleSort(tab: Array[Int]) = {
  var tab_has_changed = false;
  var tmp_value = 0;
  do{
  	for(int i = 0; i < tab.length - 2; i++){
    	if(tab[i+1] > tab[i]){
        tmp_value = tab[i+1];
        tab[i+1] = tab[i];
        tab[i] = tmp_value;
        tab_has_changed = true;
      }
  	}
  }while(tab_has_changed);
  
}