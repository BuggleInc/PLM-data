def cocktailSort() {
  
  var trier:Boolean=false
  
  
  do{
    for(i<-0 to getNombreValeurs()-2){
      if(plusPetit(i,i+1)){
        echange(i,i+1)
        trier=true
      }
      trier=false
    }
    for(i<- getNombreValeurs()-2 to 0 by -1){
      if(plusPetit(i,i+1)){
        echange(i,i+1)
        trier=true
      }
      trier=false
    }
    
  } while(trier)
  
}