def golomb(num:Int): Int = {
	var T = new Array[Int](num)
	var n = 0
	var cpt = 0
	T(0)=0
	T(1)=1
	for (i<-2 to num-1) {
		n = T(i-1)
		cpt = 0
		for (j<-1 to i-1) {
			if (n==T(j)) {
				cpt=cpt+1
			}
		}
		if (cpt==T(n)) {
			T(i)=n+1
		} else {
			T(i)=n
		}	
	}
	return T(num)
}

def occurrences(nums:Array[Int],lookingFor:Int):Int = {
	var compteur:Int = 0
	for (i<-0 to nums.length-1) {
		if (nums(i)==lookingFor) {
			compteur = compteur +1
		}
	}
	return compteur
}