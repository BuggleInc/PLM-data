override def run() {
  var posfinale=false
  setCouleurBrosse(Color.blue)
  baisseBrosse()
  while(!posfinale){
  	while(!estFaceMur()){
    	avance()}
 
  	if((estFaceMur()) && (getDirection()==Direction.EST)){
    	gauche()
    	if(!estFaceMur()){
    		avance()
    		gauche()
    	}else{posfinale=true}
  	}
  
  	if((estFaceMur()) && (getDirection()==Direction.OUEST)){
  		droite()
    	if(!estFaceMur()){
  			avance()
  			droite()
    	}else{posfinale=true}
  	}
  } droite()
}
