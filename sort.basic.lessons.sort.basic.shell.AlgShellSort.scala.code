def shellSort() {
	val l=getNombreValeurs();
	var v=0;
	var w=0;
	var j=0;
	var b=true;
	var ecart=l/2;
	while (ecart>0) {
		for (i<-1 to l-1) {
			v=getValeur(i);
			b=true;
			j=i;
			while ((b)&&(j>=0)) {
				b=false;
				if (j<ecart) {
					setValeur(j,v);
				} else if (plusPetitQue(j-ecart,v)) {
					setValeur(j,v);
				} else {
					copie(j-ecart,j);
					j-=ecart;
					b=true;
				}
			}
		}
		ecart/=2;
	}
}
