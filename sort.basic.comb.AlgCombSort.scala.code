def combSort() {
  var n : Int =getValueCount()
  var gap : Int = n
  var stop : Boolean = false
  do{
    
    
    
    var it : Int = 0
    stop=false
    
    while (it+gap<n){
      if(isSmaller(it+gap,it)){
      	swap(it+gap,it)
        stop=true 
      }
      it+=1
      
    }
    
    if(gap>1)
    	gap=(gap/1.3).toInt
    
  }while(gap>=1 || stop)
}