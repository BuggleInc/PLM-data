def insertionSort() {
  
  var i = 0;
  var j = 0;
  var v = 0;
  
  for (i <- 1 to getNombreValeurs()-1) {
    v = getValeur(i);
    j = i-1;
    
    while (plusPetitQue(j,v) == false) {

      copie(j, j+1)
      if (j > 0) {
         j -= 1

      } else {
         j += 1
      }



    } 

    setValeur(j+1, v);

    
  }
  
  
}