override def run() {
  var height =  getWorldHeight()
  var width = getWorldWidth()
  var compteur = 0
  
  for (y <- 0 to height-1){
    for (x <- 0 to width -1){
      if (y % 2 == 0){
      	setPos(x,y)
      	writeMessage(compteur)
      	compteur = compteur +1
      }else{
        setPos(height-1-x,y)
      	writeMessage(compteur)
      	compteur = compteur +1        
      }
      
    }
  }
}
