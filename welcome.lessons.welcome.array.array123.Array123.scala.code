def array123(nums:Array[Int]): Boolean = {
  var a:Boolean = false
  var b:Boolean = false
  var c:Boolean = false
  for (i<- 0 to nums.length-1) {
    if (nums(i)==1) {
      a=true
      }
    if (nums(i)==2) {
      b=true
    }
    if (nums(i)==3) {
      c=true
    }
  }
  if (a==true & b==true & c==true) {
    return true
  }
  return false
}