override def run() {

def finish():Boolean = {
		if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {return true}
		return false
	}

	ecritMessage(0)

	var cpt =1

	while (!finish()) {
		if ((getX()==getMondeLargeur()-1 && getY()%2==0) || (getX()==0 && getY()%2==1)) {
			setPos(getX(), getY()+1)
		} else {
			if (getY()%2==0) {setPos(getX()+1, getY())}
			else {setPos(getX()-1, getY())}
		}
		
		ecritMessage(cpt)
		cpt = cpt + 1
	}

}
