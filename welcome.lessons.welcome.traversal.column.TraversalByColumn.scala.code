override def run() {
  var posfinale = false
  while(!posfinale){
    if (setPos(getX(),getY()) == setPos(getMondeLargeur()-1, getMondeHauteur()-1)){
      posfinale=true}
    move()
  }
}



def move(){
  for (i <- 0 to getMondeLargeur()-2){
    setPos(getX()+1, getY())
    for(j <- 0 to getMondeHauteur()-3){
      setPos(getX(), getY()+1)
    }
  }
}