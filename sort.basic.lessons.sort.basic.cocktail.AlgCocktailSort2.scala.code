def cocktailSort2() {
var length = getNombreValeurs(); 
var debut=0; var fin=length-2;
var échangé:Boolean=false;
do
{
	for(i<-debut to fin){
		if(plusPetit(i,i+1)){
			echange(i,i+1);
			échangé=true;
		}
		fin-=1;
	}
	for(i<-fin to debut by -1){
		if(plusPetit(i,i-1)){
			echange(i,i-1);
			échangé=true;
		}
		debut += 1;
	}
}
while(échangé)
}
