def isFacingTrail():        
    if isFacingWall():
        return False
        
    else:
        forward()
    if getGroundColor()==Color.green:
        brushDown()
        brushUp()
        backward()
        return True
    else:
        backward()
        return False

def Hunt():
    if isFacingTral()==True:
        forward()
    else:
        left()

Hunt()
        