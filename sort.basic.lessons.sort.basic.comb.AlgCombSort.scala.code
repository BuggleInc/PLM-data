def combSort() {
	var ecart = getNombreValeurs()-2
	var sorted = false
	var i = 0
	do {
		sorted = true
		if (ecart > 1){
			ecart = (ecart.toDouble / 1.3).toInt	
		} 
		i = 0
		for (i <- 0 to getNombreValeurs()-1-ecart){
			if (plusPetit(i,i+ecart) == false){
				echange(i,i+ecart)
				sorted = false
			}
			i = i+1
		}
	} while ((ecart > 1) || (sorted == false))
}
