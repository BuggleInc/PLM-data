def cocktailSort() {
  for (i <- 0 to getNombreValeurs()){
    var c:Int =0
    for (j <- 0 to getNombreValeurs()-i -2){
      
      if (plusPetit(j+1,j)){
        echange(j,j+1)
        c+=1
        plusPetit(j+1,j)
        plusPetit(j+1,j)
      }
      
    }
    if (c==0){
        return
      }
  }
}
