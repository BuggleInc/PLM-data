message=litMessage();
while(estSurMessage()):
    if message==("R"):
        droite()
        avance()
    if message==("L"):    
        gauche()
        avance()
    if message==("I"):
        retourne()
        avance()
    if message==("A"):
        avance()
    if message==("B"):
        avance(2)
    if message==("C"):
        avance(3)
    if message==("Z"):
        recule()
    if message==("Y"):
        recule(2)
    if message==("X"):
        recule(3)