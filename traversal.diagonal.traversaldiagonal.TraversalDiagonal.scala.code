override def run() {
  var diag = 0
  var count = 0
  while (diag!=12){
    ecritMessage(count)
        count=count+1
    if(getX()==getMondeLargeur()-1 || getY()==0 ) {
      diag=diag+1
      if (diag<getMondeHauteur()-1){
      setPos(0,diag)
      }
      else{
        setPos(diag-6,getMondeHauteur()-1)
      }
    }
    else {
      setPos(getX()+1,getY()-1)
    }
  }
  ecritMessage(count)
}

