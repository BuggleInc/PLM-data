def solve(src,dst,other, height=-1):
    if height==-1: # initial case
        height=getSlotSize(src)
    if  getTaillePiquet(dst)==0:
        deplace(src,dst)
        return solve(other,src,dst, height)
    else:
        deplace(src,other)
        return solve(dst,other,src, height)
        

        
         
