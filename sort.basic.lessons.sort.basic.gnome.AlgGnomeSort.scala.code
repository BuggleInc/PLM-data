def gnomeSort() {
    var sorted : Boolean = false;
    var x=1;
    while (!sorted) {
        if (x==0) {
            x=1;
        }
        if (x==getNombreValeurs()) {
            return;
        }
        else if (plusPetit(x-1,x)) {
            x=x+1;
        }
        else {
            echange(x-1,x);
            x=x-1;
        }
    }
}
