def golomb(num:Int): Int = {	
  var t = new Array[Int](num)
  t(0)=1
  var x:Int = 1
  var y:Int = 2
  var z:Int = 1
  var v:Int = 2
  while(t(num-1) == 0){
    if(x+y<num){  
      for(i<- x to x+y){
        t(i)=v
        z += 1
      }
      x = z
      y = t(y+1)
    }else{
      for(i<- x to num-1){
        t(i)=v
      }
    }
  }
  return(t(num-1))
}