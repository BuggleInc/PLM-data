var diag = 0;
override def run() {
  var i=0
  var j=0
  var k=0
  var place=0
  while (diag!=12){
    for (j<- 0 to 6){
      for (k<- 0 to j){
        setPos(k,j-k)
        ecritMessage(place)
        place+=1
      }
    }
    for (j<- 0 to 5){
      for (k<- 0 to (6-j)){
        setPos(k+j+1,6-(k))
        ecritMessage(place)
        place+=1
      }
    }
    diag=12
  }
}