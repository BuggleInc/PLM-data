public void bubbleSort()  {
	boolean swapped = false;
	do {
		swapped = false;
		for (int i = 0; i < getValueCount() - 1; i++) {
			if (!isSmaller(i, i+1)) {
				swap(i, i+1);
				swapped = true;
				System.out.println("Swapped: " + i + " and " + (i + 1));
			} else {
				System.out.println(i + ", " + i+1);
			}
		}
	} while(!swapped);
}
