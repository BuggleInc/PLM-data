def cocktailSort2() {
  var inversion = false;
  var debut = 0 ; 
  var fin = getNombreValeurs() - 2;
  do {
    inversion = false;
    for (i<- debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1);
        inversion = true;
      }
    fin = fin - 1;
    }
    for (i<- fin to debut by -1) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1);
        inversion = true;
      }
    debut = debut + 1;
    }
  } while (inversion);
}