var nbBiscuit = 0
while(!sortieTrouvee())
	{
    do{
      for(compteur<-1 to 5)
      	{
          if(estSurBiscuit())
          	nbBiscuit = nbBiscuit + 1
            
          avance()
        }
      
    }while(crossing())
    
    if(nbBiscuit<3)
    	droite()
      
    else
    	gauche()
    
    nbBiscuit = 0
  }