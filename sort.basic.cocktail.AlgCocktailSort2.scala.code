def cocktailSort2() {
  var d:Int=0
  var c:Int=1
  var f:Int=getNombreValeurs()-2
  while( c==1){
    c=0
  for (i <-d to f){
    if(plusPetit(i+1,i)){
      echange(i,i+1)
      
      c=1
    }
  }   
  for (j <-d+1 to f){
    if(plusPetit(f-j+1,f-j)){
      echange(f-j+1,f-j)
      c=1
      
}
       }

    f-=1
    d+=1
       }
}