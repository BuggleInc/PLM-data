def cocktailSort() {
  var lgr:Int=getNombreValeurs()
  var c:Int=1
  while (c!=0) {
    c=0
    for (i <- 0 to lgr-2) {
      if (plusPetit(i:Int,(i+1):Int)){
        c=1
        echange(i:Int, (i+1):Int)
      }
    }
    for (i <- 0 to lgr-2) {
      if (plusPetit(lgr-1-i:Int, lgr-1-i-1)) {
        c=1
        echange((lgr-1-i):Int, (lgr-1-i-1):Int)
      }
    }
  }
}