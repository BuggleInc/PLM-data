override def run() {

	var couleur = new Array[Color](getMondeHauteur());
	couleur(0) = getCouleurSol();
	
	for(i <- 0 to getMondeHauteur()-2) {
		avance();
		couleur(i) = getCouleurSol();
	}

	for(i <- 0 to getMondeLargeur()-2) {
		for(i <- 0 to getMondeHauteur()-2)
			recule();
		gauche();
		avance();
		droite();
		for(i <- 0 to getMondeHauteur()-2)
			avance();
		
	}
}
