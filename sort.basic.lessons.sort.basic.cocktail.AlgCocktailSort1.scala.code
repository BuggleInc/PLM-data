def cocktailSort()
{
  var sortie:Boolean = false
  var compteur:Int = 0
  var m:Int = 0
  
  while(!sortie)
  {
    sortie = true
    
    for(i <- 0 to getNombreValeurs()-2-compteur)
    {
      if(plusPetit(i+1,i))
      {
        echange(i,i+1)
        sortie = false
      }
    }
    
    for(j <- 0 to getNombreValeurs()-2-compteur)
    {
      m = getNombreValeurs()-2-compteur - j
      
      if(plusPetit(m+1,m))
      {
        echange(m,m+1)
        sortie = false
      }
    }
    
    compteur = compteur + 1    
  }
}
