def insertionSort() {




  var nbVal = getNombreValeurs()

  for(i<-0 to nbVal-1){
    var copier = true
    
    var tmp = getValeur(i)
    
 		var n=0
    println()
    while((copier)&&(i-n>0)){
      println("i-n-1 : "+(i-n-1)+"	i:"+i+"	n:"+n)
      if(plusPetit(i-n,i-n-1)){
        copie(i-n-1,i-n)
        n+=1
      }else{
        setValeur(i-n,tmp)
        copier=false
      }
    }
    
    
  }

}