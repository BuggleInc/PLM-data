def cocktailSort2() {
  
  var sorted:Boolean = false;
  var deb:Int = 0;
  var fin:Int = getNombreValeurs()-2;
  
  do{
    
    sorted = false;
  
    for(i <- 0 to getNombreValeurs()-2){

      if(plusPetit(i+1, i)){

        echange(i, i+1);
        sorted = true;

      }

    }
    
    fin -= 1;

    for(i <- getNombreValeurs()-2 to 0 by -1){

      if(plusPetit(i+1, i)){

        echange(i, i+1);
        sorted = true;

      }

    }
    
    deb += 1;
    
  }while(sorted);
    
}