public void combSort() {
  double ecart = getValueCount(); 
  int i = 0;
  boolean swap = true;
  do{
    if(ecart >1){
      ecart = (ecart/1.3);
    }
    int gap = (int)ecart;
    i = 0 ;
    swap = false;
    while(i+gap < getValueCount()){
      if(isSmaller(i,i+gap)){
        swap(i+gap,i);
        swap = true;
      }
      i++;
    }
  }while((ecart > 1)||swap);
}