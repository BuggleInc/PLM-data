override def run() {
  var i = 0
  for (y <- 0 to getMondeLargeur()-1) {
    	if (y%2 == 0) {
      for (x <- 0 to getMondeHauteur()-1) {
      setPos(x,y)      
      ecritMessage(i)
      i+= 1
      }
    	} else {
      for (x <- 0 to getMondeHauteur()-1) {
      setPos(getMondeHauteur() - x - 1,y)      
      ecritMessage(i)
      i+= 1 
      }
    }
  }
}