/**
 *  Fait des V
 */
void faireV() {
  avance(2);
	marqueCase();
	avance();
	gauche();
	avance();
  marqueCase();
	retourne();
	avance();
  gauche();
  avance();
	marqueCase();
  avance(2);
  gauche();
}

/**
 * Marque 1 seule case
 */
void marqueCase(){
	baisseBrosse();
	leveBrosse();
}
public void run() {
  for (int j = 0; j < 3; j++){
		for (int i = 0; i < 3; i++){
			faireV();
      avance(7);
		}
    gauche();
    avance(7);
    droite();
  }
}
