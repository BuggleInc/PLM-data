def gnomeSort() {
  var lgr=getNombreValeurs()
  var changement=true
  var i=0
  while (changement==true){
    changement=true
    if(plusPetit(i+1,i)==true){
      echange(i+1,i)
      if (i==0){
        i=i+1
      }else{
        i=i-1
      }
    }else{
      if (i==lgr-2){
        changement=false
      }else{
        i=i+1
      }
    }
  }
}
    
    
    
      
      
     