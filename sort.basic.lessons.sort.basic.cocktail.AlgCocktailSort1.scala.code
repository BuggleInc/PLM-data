def cocktailSort() {
	var swap=false
	do {
		swap= false
		for (i <- 0 to getNombreValeurs()-2) {
			if ( plusPetit(i+1,i) ) {
				echange (i+1,i);
				swap=true 	
			}
		}
		for (j <- getNombreValeurs()-2 to 0 by -1) {
			if ( plusPetit(j+1,j) ) {
				echange (j+1,j);
				swap=true
	
			}
		}
	} while( swap==true)
}
