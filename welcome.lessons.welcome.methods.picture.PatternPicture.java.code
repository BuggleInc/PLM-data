/**
 * doSide : effectue une partie du motif puis revient à la position d'origine.
 */

void doSide() {
	gauche();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
	droite();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
}

public void run() {
	setCouleurBrosse(Color.RED);
	doSide ();
}
