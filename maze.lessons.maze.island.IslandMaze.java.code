public void run() {
	escapeTheMaze();
}

public void escapeTheMaze(){

	do{	
		//go north
		while(!isFacingWall() && getDirection() == Direction.NORTH){
			forward();
		}
		
		while(isFacingWall()){
			right();
		}
		
		forward();

		//pick up baggle
		if(isOverBaggle()){
			pickUpBaggle();
		}else{
			left();
		}
		
	}while(!isCarryingBaggle());
}//end escapeTheMaze
