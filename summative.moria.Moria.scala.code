def rangement() {
  retourne()
  while (!estFaceMur) {
    avance()
    if (estSurBiscuit & !porteBiscuit) {
      prendBiscuit()
      retourne()
    } else if (estSurBiscuit & porteBiscuit) {
      recule()
      poseBiscuit()
      retourne()
    }
  }
  droite();avance();gauche();avance()
}

rangement