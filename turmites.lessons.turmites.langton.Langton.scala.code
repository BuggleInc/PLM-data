def step() {
  if (getCouleurSol()==Color.black)
  {
    setCouleurBrosse(Color.white)
    paint()
    gauche()
    avance()
  }
  else if (getCouleurSol()==Color.black)
  {
    setCouleurBrosse(Color.black)
    paint()
    droite()
    avance()
  }
  else
   setCouleurBrosse(Color.black)
}

def paint() {
  baisseBrosse()
  leveBrosse()
}

for(i<-1 to 12000)
	step()