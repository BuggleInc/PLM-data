def cocktailSort3() {
  var c:Boolean = true
  var n:Int = getNombreValeurs()-1
  var f:Int = 0
  while (c){
    c=false
    f+=1
    for (i<- 0 to n-f){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        c = true
      }
    }
  }
  c=true
  f=0
  while (c){
    c=false
    f+=1
    for (i <- n-f to 0){
      if (plusPetit(i+1,i)){
        echange(i-1,i)
        c = true
    	}
     }
   }
  
}