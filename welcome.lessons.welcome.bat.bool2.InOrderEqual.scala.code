def inOrderEqual(a:Int, b:Int, c:Int, equalOk:Boolean):Boolean = {
  if (equalOk==true && (a==b && a>c)) {
    return false
  }
  if (equalOk==true && (a==b || (b==c & b<a) || (a==b && b==c))) {
    return true
  }
  if (a<b && b<c && equalOk==false) {
    return true
  } else {
    return false
  }
}