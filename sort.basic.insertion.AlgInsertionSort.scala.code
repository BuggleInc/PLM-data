def insertionSort() {
  var lgr = getNombreValeurs()
  var v = 0
  var test =false
  var casePourV = 0
  for (i <- 1 to lgr -1){
    v=getValeur(i)
    casePourV = 0
    for (j <-1 to i){
      test = plusPetitQue(i-j,v)
      if (!test){
        copie(i-j,i-j+1)
        casePourV = i-j
      }
      setValeur(casePourV , v) 
    }
  }
}