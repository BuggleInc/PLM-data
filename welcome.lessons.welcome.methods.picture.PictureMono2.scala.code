def marqueSol() {
	baisseBrosse();leveBrosse();
}

def faireV() {
	avance(); marqueSol();
	avance(); gauche(); avance(); marqueSol();
	droite(); avance(); droite(); avance(); marqueSol();
	gauche(); avance(); gauche();
}

def faireMotif() { 
	for (x <- 1 to 4) {faireV()};
	for (y <- 1 to 5) {avance()};
}

def faireTriplemotif() {
	for (x <- 1 to 3) {faireMotif()};
	gauche();
	for (y <- 1 to 5) {avance()};
	droite()
}

override def run() {
	for (x <- 1 to 3) {faireTriplemotif()}
}
