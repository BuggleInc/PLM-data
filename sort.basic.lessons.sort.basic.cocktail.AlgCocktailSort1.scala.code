def cocktailSort()
{
  var sortie:Boolean = false
  
  while(!sortie)
  {
    sortie = true
    
    for(i <- 0 to getNombreValeurs()-2)
    {
      if(plusPetit(i+1,i))
      {
        echange(i,i+1)
        sortie = false
      }
    }
    
    for(m <-getNombreValeurs()-2 to 0 by -1 )
    {
      if(plusPetit(m+1,m))
      {
        echange(m,m+1)
        sortie = false
      }
    } 
  }
}
