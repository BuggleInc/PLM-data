override def run() {

def positionFinale(x:Int, y:Int):Boolean = {
	if (x==getMondeLargeur()-1 && y==getMondeHauteur()-1) {
		return true
	}
	else {
		return false
	}
}

	var x=0
	var y=0
	ecritMessage(0);
	var cpt=0
	while (!positionFinale(x, y)) {
		while (x!=getMondeLargeur-1) {
		x = x+1
		y = y ;
		}
		if (x == getMondeLargeur()) {
			y = y+1
			x = x
		}
		setPos(x, y)
		cpt=cpt+1
		ecritMessage(cpt);				
	}
	

}
