def cocktailSort3() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var n= false
  do{
    n= false
    for (i<- debut to fin){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          
          n= true     
     } 
    }
    fin = fin-1
    if (n == false){
      return
     }
    
    for (j<- fin to debut by -1){
      if (!plusPetit(j,j+1 )){
        	echange(j, j+1)
          
          n= true      
      }      
    }
    debut = debut + 1
    if (n == false){
      return
    	}
    
  } while(n)
}