override def run() {
	def posFin() = Boolean {
		if (getDirection() == Direction.EST) {
			gauche();
			if (estFaceMur() == true) {return true}
		}
	}

	def virage() = {
		if (getDirection == Direction.EST) {
			gauche();
			avance();
			gauche();
		} else {
			droite();
			avance();
			droite();
		}
	}
	
	baisseBrosse()
	while (!posFin()) {
		while (!estFaceMur) {
		avance();
		}
		if (estFaceMur) {virage()}
	}
}
