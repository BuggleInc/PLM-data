def cocktailSort3() {
	var length = getNombreValeurs();
	var debut = 0;
	var fin = length-2;
	var tri1 = true;
	var tri2 = true;

	while(tri1 || tri2) {
		tri1 = false;
		tri2 = false;
		for(i<-debut to fin) {
			if(plusPetit(i, i+1) == false) {
				echange(i, i+1);
				
				tri1 = true;
			}
			
		}
		fin -= 1;
		if(tri1 == true) {
			for(i<-fin to debut by -1) {
				if(plusPetit(i, i+1) == false) {
					echange(i, i+1);
					
					tri2 = true;
				}
				
			}
			debut += 1;
		}
	}
}
