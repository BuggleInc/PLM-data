String alarmClock(int day, boolean vacation) {
	if(sleepIn(isWeekday(day),vacation)) {
		return "10:00";
	}return "7:00";
}
boolean sleepIn(boolean weekday, boolean vacation) {
	if(vacation) {
		return true;
	} else {
		if(!weekday) {
			return true;
		} else {
			return false;
		}
	}
}
boolean isWeekday(int day) {
	if(day==0 || day==6){
		return true;
	}return false;
}
