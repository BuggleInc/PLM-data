def combSort() {
  var taille=getNombreValeurs()
  var trier=false
  var ecart=taille
  
  while (trier==false){
    trier=true
    if (ecart>1){
      ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
   
    }
    var i=0
    while (i+ecart<taille){
      if (plusPetit(i,i+ecart)==true){
        echange(i,i+ecart)
        trier=false
      }
    i=i+1
    }
  }
  
}