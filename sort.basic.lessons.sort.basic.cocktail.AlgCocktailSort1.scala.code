def cocktailSort() {
	var E:Boolean = false
	do {
		for(i <- 0 to getNombreValeurs - 2) {
			E = false
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				E = true
			}
		}
		for(i <- getNombreValeurs - 2 to 0 by -1) {
			E = false
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				E = true
			}
		}
	}
	while(E == true)


	
