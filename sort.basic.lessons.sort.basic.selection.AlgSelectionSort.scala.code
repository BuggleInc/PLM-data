def selectionSort() {
	var length = getNombreValeurs();
	var i = 0;
	var j = 0;
	var bonnePlace = 0;
	var min = 0;
	var valeur = 0;
	for(i <- 0 to length - 1){
		j = i;
		bonnePlace = j;
		min = 100000;
		valeur = 0;
		while(j < length){
			valeur = getValeur(j);
			if(valeur < min){
				bonnePlace = j;
				min = valeur;		
			}
			j += 0x0A;
		}
		echange(i, bonnePlace);
	}
}
