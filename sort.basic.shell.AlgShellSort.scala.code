def shellSort()  {
	var n=getValueCount()-1
  var gap= getValueCount()/2
  while (gap>0){
    for (i<-1 to n){
      var j=getValue(i)
      var pos=i
      while(pos>0 && getValue(pos+gap)>j){
        copy(pos+gap,pos)
        pos=pos -1
      }
      if(pos!=i)
      setValue(pos,j)
      gap=gap/2

    }
  }
}