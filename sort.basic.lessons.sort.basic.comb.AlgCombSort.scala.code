def combSort() {
  var n= getNombreValeurs()
  var ecart = n
  do{
    if ( ecart>1){
      ecart = ecart / 1.3
    }
    var i = 0
    var m = false
    do{
      if( plusPetit(i, i+ecart)){ 
        echange(i , i+ecart)
        i = i+1
        m = true
      } 
    } while (i + ecart < n )
  } while (ecart>1 or m) 
}