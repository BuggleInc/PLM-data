def shellSort()  {
  var ecart=getNombreValeurs()/2
  while(ecart>1){
    for(i <- 1 to getNombreValeurs()-1){
      val v= getValeur(i)
      var j = i-ecart
      while(j>0 && getValeur(j-ecart)>v){
        copie(j-ecart,j)
        j-=ecart
      }
      setValeur(j, v)
    }
    ecart=ecart/2
  }
}