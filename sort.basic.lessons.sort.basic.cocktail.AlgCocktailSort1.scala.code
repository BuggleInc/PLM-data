def cocktailSort() {
  var n = getNombreValeurs()
  var est_triee = false
  while (est_triee == false){
    est_triee = true
    for (i<-0 to n-2){
      if (plusPetit(i+1,i)){
        echange(i, i+1)
        est_triee = false
      }
    }
    for (i<-n-2 to 0 by -1){
      if (plusPetit(i+1,i)){
        echange(i, i+1)
        est_triee = false
      }
    }
    
  }
}