override def run() {
	while (estFaceMur()) {
		gauche();
	}
	droite();
	while (!estSurBiscuit) {
		keepHandOnSideWall()
	}
    prendBiscuit();
}

def keepHandOnSideWall() {
	if(!estFaceMur()) {
		gauche();
		if(estFaceMur()) {
			droite();
			avance();
		} else {
			avance();
		}
	} else {
		gauche()
		if(estFaceMur()) {
			retourne();
			if (estFaceMur()) {
				droite();
				avance();
			} else {
			avance();
			}
		} else {
			avance();
		}
	}
}
