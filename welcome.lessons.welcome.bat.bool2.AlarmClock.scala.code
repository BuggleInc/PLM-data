def alarmClock(day:Int, vacation:Boolean): String = {
	if (vacation==false){
    day match{
      case 5 || 1 || 2 || 3 || 4  => return "7:00" 
      case 6 || 0 => return "10:00"
    }
  }
  else {
    day match{
      case 5 || 1 || 2 || 3 || 4  => return "10:00" 
      case 6 || 0 => return "off"
    }
  }
}