def faireV(c:Color,cc:Color){
  setCouleurBrosse(c)
  forward()
  baisseBrosse()
  leveBrosse()
  setCouleurBrosse(cc)
  forward()
  baisseBrosse()
  leveBrosse()
  forward()
  left()
}
