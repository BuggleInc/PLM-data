def combSort() {

	var ecart: Int = getNombreValeurs()-1;
	var i : Int = 0;
     var sp : boolean = false
	do {
		ecart = (ecart.toDouble / 1.3).toInt;
		i = 0;
		while(i+ecart<getNombreValeurs()) {
			if(getValeur(i+ecart) < getValeur(i)) {
				echange(i+ecart,i);
			}
			i=i+1;
			sp = false;	
		}
		
	} while(ecart >1 || sp==false  ) 
}
