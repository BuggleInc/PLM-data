forward();
do{
  var count:Int=0
  while (crossing()==false){
    forward();
    if (isOverBaggle==true){
      count=count+1
    }
  }
  if (crossing()==true && isOverBaggle()==true){
    count=count+1
  }
  if (count>2){
    left();forward();count=0
  } else {
    right();forward();count=0
  }
} while(exitReached()==false)
forward();
