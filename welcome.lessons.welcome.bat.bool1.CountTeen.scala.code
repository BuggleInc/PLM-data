def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var nbAdos = 0
    if (((13<=a)&&(a<=19))){
    	nbAdos = nbAdos + 1
  }
    if (((13<=b)&&(b<=19))){
    	nbAdos = nbAdos + 1
  }
    if (((13<=c)&&(c<=19))){
    	nbAdos = nbAdos + 1
  }
    if (((13<=d)&&(d<=19))){
    	nbAdos = nbAdos + 1
  }
  return nbAdos
}