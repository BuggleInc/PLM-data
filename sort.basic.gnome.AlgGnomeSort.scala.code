def gnomeSort() {
  var i = 0;
  var l = getNombreValeurs;
  var J = getValeur(1);
  var I = getValeur(0);
  
  
  while(i < l-2){
    
    if(I <= J){
      i += 1;
      I = J;
      J = getValeur(i+1);
    }
    else{
      echange(i,i+1);
      if(i > 0){
        i -= 1;
        J = I;
        I = getValeur(i);
      }
    }
  }
}