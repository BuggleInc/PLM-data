override def run() {
  
  val xpos = getX();
  val ypos = getY();
  
  
  carre(xpos, ypos, 100.0);
  carre(xpos-100, ypos, 100.0);
  carre(xpos-100, ypos-100, 100.0);
  carre(xpos, ypos-100, 100.0);
  
  setX(xpos); setY(ypos);
  setCap(0);
  
}


def carre(fromx:Double, fromy:Double, taille:Double) = {
  setX(fromx); setY(fromy);
  setCap(0);
  
  avance(100);
  droite(90);
  avance(100);
  droite(90);
  avance(100);
  droite(90);
  avance(100);
  
}