def cocktailSort2() {
var a=true
var b=true
var d=0
var f=getValueCount()-2
  while (0<1){
    if (a==false && b==false){
      return
    }
a=false
b=false
  for (j <- d to f){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true 
      f=f-1
		}
}
	for (j <- f to d by -1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      b=true
      d=d+1
		}
}      
}
}