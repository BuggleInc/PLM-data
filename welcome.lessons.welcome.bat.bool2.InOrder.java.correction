$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class inOrder extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( inOrder((Integer)t.getParameter(0), (Integer)t.getParameter(1), (Integer)t.getParameter(2), (Boolean)t.getParameter(3)) ); }
	boolean inOrder(int a, int b, int c, boolean bOk) {
		return (bOk || (b > a)) && (c > b);
