def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (isBirthday ==false){
    if (speed<=60){
      return 0
    }
    if( speed<=80 & speed>61){
      return 1
    }
    else{
      return 2
    }
  }
  else{
    if (speed<=65){
      return 0
    }
    if( speed<=85 & speed>66){
      return 1
    }
    else{
      return 2
    }
    
  }
}