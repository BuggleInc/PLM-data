def goAndGet():
    stepper = 0
    while isOverBaggle() != True:
        stepper = stepper + 1
        forward()
    if (isOverBaggle()):
        pickupBaggle()
    for stepper in range(stepper):
        backward()
    dropBaggle()

for i in range(7):
