def cocktailSort2() {
  var mouv:Boolean=true
  var N=getNombreValeurs()-2
  var deb=0
  while(mouv){mouv=false
             for(x<-deb to N){if(plusPetit(x+1,x)){echange(x,x+1);mouv=true}}
              N-=1
             for(x<-deb to N){
               var j=N-x-1
               if(plusPetit(j+1,j)){echange(j,j+1);mouv=true}}
              deb+=1
             }
}