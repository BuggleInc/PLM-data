def cocktailSort2() {
	var taille: Int = getNombreValeurs();
	var tri: Boolean = false;
	var j; Int = 0;
	do{
		tri = false;
			for(i <- 1 + j to taille - j - 1){
				if(!plusPetit(i - 1, i)){
					echange(i - 1, i);
					tri = true;
				}
			}
			for(i <- taille - j - 1 to 1 + j by -1){
				if(plusPetit(i, i - 1)){
					echange(i, i - 1);
					tri = true;
				}
			}
		j = j + 1;
	}while(tri)
}
