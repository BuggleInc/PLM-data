def cocktailSort3() {
  var debut=0
  var fin =getNombreValeurs()-2
  var inversement=false
  do{
    for (i<-debut to fin){
      inversement=false
    	if (plusPetit(i+1,i)){
        echange(i,i+1)
        inversement=true
      }
    }
    fin=fin-1
     for (i<- fin to debut by -1){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        inversement=true
      }
    }
    debut=debut+1
  }while(inversement)
}
