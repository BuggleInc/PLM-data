def solve() {
  var zonebleue=1
  var zonerouge=getTaille()
  var zoneblanche=1
  while (zonebleue<zonerouge){
    if (getCouleur(zonebleue)==ROUGE){
      for (i<-zonebleue to zonerouge-1){
        echange(i,i+1)
      }
      zonerouge-=1
    }
    if (getCouleur(zonerouge)==BLEU){
      for (i<-zonerouge to zonebleue+1){
        echange(i,i-1)
      }
      zonebleue+=1
    }
    if (getCouleur(zonebleue)==BLANC){
      zonebleue+=1
    }
    if (getCouleur(zonerouge)==BLANC){
      zonerouge-=1
    }
  }
  
}
