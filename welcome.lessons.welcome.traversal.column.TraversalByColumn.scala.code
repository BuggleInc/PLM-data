def fin():Boolean = 
{
  if (!estDosMur())
  	return false
  
  var ok = false
  droite()
  
  if (estFaceMur())
  	ok = true
  
  gauche()
  return true
}

override def run() {
  var idCase = 0
  ecritMessage("" + idCase)
  
  while (!fin())
  {
    if (getY() > getHauteurTerrain() - 1)
    	setPos(getX() + 1, 0)
    
  	setPos(getX(), getY() + 1)
    idCase += 1
    ecritMessage("" + idCase)
  }
}