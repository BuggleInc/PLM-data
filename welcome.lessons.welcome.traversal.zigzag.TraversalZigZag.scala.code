override def run() {
	var x=0
	var y=0
	var cpt=0
	def positionFinale():Boolean= {
	if(cpt==getMondeHauteur()*getMondeLargeur()-1) {
		return true
	} else {
		return false
	}
	}
	while (!positionFinale) {
		if(getX()==getMondeLargeur()-1) {
			y+=1
			if (y%2==0) {
				x+=1
			} else {
				x-=1
		}
		} else if (y%2==0) {
			x+=1
		} else {
			x-=1
		}
		ecritMessage(cpt);
		cpt+=1
		setPos(x,y);
	}
	ecritMessage(cpt);
}
