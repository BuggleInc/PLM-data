def testPos() : Boolean ={
  var h: Int= getMondeHauteur()
  var l: Int = getMondeLargeur()
  if (getX == h & getY == l) {
    return true
  }
  return false
}



def deplace(){
  var x : Int=0
  var y : Int=0
  if (estFaceMur() == false){
    x= x+1
    setPos(x, y)
  }
  else{
    x=0
    y=y+1
    setPos(x, y)
  }
}



override def run() {
  ecritMessage(0)
  do {
    deplace();
    var c : Int=1
    ecritMessage(c)
    c = c+1
  }
  while (!testPos)
}
