override def run() {
  var cpt:Int=0
  ecritMessage(cpt)
  while (!positionFinale()){
    if (getX()==getMondeHauteur()-1){
      setPos(getX(),getY()+1)
    }
    if (getX()==0 && getY()!=0){
      setPos(getX(),getY()+1)
    }else{
      if (getY()%2==0){
        setPos(getX()+1,getY())
      }else{
        setPos(getX()-1,getY())
      }
    }
    cpt=cpt+1
    ecritMessage(cpt)
  }
}

def positionFinale():Boolean={
  return getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1
}