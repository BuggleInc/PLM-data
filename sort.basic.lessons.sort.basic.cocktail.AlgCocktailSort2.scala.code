def cocktailSort2() {
  var chg = true;
  var debut = 0;
  var fin = getNombreValeurs()-2;
  while(chg){
    chg=false;
    for(i <- debut to fin){
      if(plusPetit(i+1,i)){
        echange(i+1,i);
          chg=true;
      }
    }
      fin -= 1;
    for(j <- fin to debut by -1){
      if(plusPetit(j+1,j)){
        echange(j+1,j);
        chg=true;
      }
    }
      debut += 1;
  }
}