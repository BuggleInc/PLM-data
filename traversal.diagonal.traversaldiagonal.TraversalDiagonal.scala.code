var diag = 1;
override def run() {
  
  var compteur=0
  var x=0
  var y=0
  
  writeMessage(compteur);
  
  for(i<- 0 to 12){
    
    if(diag<6){
      x=0;
      y=6-diag;
    }
    else{
      y=0;
      x=diag-6;
    }
    
    do{
      writeMessage(compteur);
      x+=1;
      y+=1;
      setPos(y,x);
      compteur+=1 
    }while((x<6)&&(y<6))
    diag+=1;

  }
}