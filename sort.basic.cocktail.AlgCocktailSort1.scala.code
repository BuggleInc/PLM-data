def cocktailSort() {
  var n = getValueCount();
  var j = 0;
  while (j < n) {
    for (i <- 0 to n-2-j) {
      if (isSmaller(i+1, i)) {
        swap(i, i+1);
      }
    }
    j += 1
    for (i <- (n-2-j) to 1 by -1) {
      if (isSmaller(i, i-1)) {
        swap(i, i+1);
      }
    }
    j += 1
  }
}