override def run() {
  setDirection(Direction.NORTH)
  while(!estSurBiscuit()){
    while(!estFaceMur()){
      avance()
    }
    while(estFaceMur()){
      droite()
      keepHandOnSideWall()
      setDirection(Direction.NORTH)
    }
  }
}

def keepHandOnSideWall(){
  if (estFaceMur()){
    droite()
  }
  else {
    avance();gauche()
    while (!estFaceMur()){
      avance();gauche()
    }
    droite()
  }
}