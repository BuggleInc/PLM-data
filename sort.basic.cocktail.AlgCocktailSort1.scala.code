def cocktailSort() {
  var l=getValueCount()
  var i=0
  var k=0
  for (i <- 0 to (l-2)) {
    k=0
    if (isSmaller(i,i+1)==false){
      swap(i,i+1)
      k=1
    }
    for (i <- (l-2) to 0 by -1){
      if (isSmaller(i,i+1)==false){
        k=1
        swap(i,i+1)
      }
    }
  if(k==0){
    return()
  }
  }
}