def combSort() {
var tri:Boolean=false
var ecart = getNombreValeurs()
 while(ecart>1| !tri) {
   if (ecart>1) {
     ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
   }
  i=0
  while (i+ecart < getNombreValeurs()) {
    if (getValeur(i+ecart)<getValeur(i)) {
      echange(i,i+ecart)
      i+=1
    }
  }
 }
}