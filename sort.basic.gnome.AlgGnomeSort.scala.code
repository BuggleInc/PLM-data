def gnomeSort() {
  
 var n : Int = getValueCount()
 var i : Int = 0
 var pos : Int = 0
 while(i<n){
   
   if(isSmaller(i+1,i)){
     swap(i,i+1)
     i-=1
   }
   else{
     
     i+=1
     pos=i
   }
   
   if(i<0)
   	i=pos
   
 }
