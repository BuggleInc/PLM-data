def bubbleSort3() {
  
  var n:Int = getNombreValeurs()
  var echange:Boolean = true
  var i:Int = 0
  
  while ((i <= n-2) && echange)
    
    for (j <- 0 to n-2-i){
			echange = false
      if(plusPetit(j+1, j)){
        var tampon:Int = getValeur(i)
        setValeur(i, getValeur(i+1))
        setValeur(i+1, tampon)
        echange = true
      }
    }

  
}