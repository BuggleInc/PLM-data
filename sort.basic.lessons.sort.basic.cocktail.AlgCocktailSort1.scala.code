def cocktailSort() {

		val hasSwap=true;
	for (i<- 0, getNombreValeurs()-2)
	{
		if(hasSwap==true)
		{
			hasSwap=false;
			if( plusPetit(i+1,i)
			{
				echange(i,i+1);
				hasSwap=true;
			}
		}
	}
	for (i<- getNombreValeurs()-2 to 0 by -1)
	{
		if(hasSwap==true)
		{
			hasSwap=false;
			if(plusPetit(i+1,i)
			{
				echange(i,i+1);
				hasSwap=true;
			}
		}
	}
	
}
