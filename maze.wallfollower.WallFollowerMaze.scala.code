override def run() {
  while (!estSurBiscuit) {
    while (estFaceMur) {
      droite()
    }
    if (keepHandOnSideWall && !estFaceMur) {
      avance()
    }
  }
  prendBiscuit()
}

def keepHandOnSideWall(): Boolean = {
  gauche()
  if (estFaceMur) {
    droite()
    return true
  } else {
    retourne()
    if (estFaceMur) {
      gauche()
      return true
    }
  }
  droite()
  return false
}