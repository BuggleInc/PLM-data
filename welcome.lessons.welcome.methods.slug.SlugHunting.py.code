def hunt():
    while estSurBiscuit() == False: 
        if isFacingTrail():
            baisseBrosse()
            forward()
            leveBrosse()
        else:
            gauche()
    prendBiscuit()
    
def isFacingTrail():
    if estFaceMur():
        return False
    forward()
    vert = getGroundColor() == Color.green
    backward()
    return vert
