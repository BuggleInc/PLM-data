public void findBuggle(){
int height = getWorldHeight();
int width = getWorldWidth();

	//loop rows
	for(int i = 0; i < height; i++){
		//loop columns
		for(int j = 0; j < width; j++){
			if(hasBaggle(i, j)){
				setIndication(i, j, 0);
				print(i);
			}
		}
	}
}
