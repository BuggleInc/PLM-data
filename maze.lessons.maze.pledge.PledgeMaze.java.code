public void run() {
	while(!isOverBaggle()){
		setDirection(Direction.NORTH);
		while(!isFacingWall()){
			forward();
		}
		int counter = 0;
		Direction direction = getDirection();
		while(isFacingWall()){
			right();
			counter++;
		}
		if (getDirection() == direction && counter == 0){
			while(!isFacingWall()){
				forward();
			}
				break;
			}
		}
	}	
}}