def solve() {
  
  var blue=0;
  var white=0;
  var red=0;
  
  for(i<-0 to getSize()/2-1){
    
    if(getColor(i)==BLUE){
			swap(i,blue);
      blue += 1;
    }
    if(getColor(i)==WHITE){
			swap(i,white);
      white += 1;
    }
    if(getColor(i)==RED){
			swap(i,red);
      red += 1;
    }
    
  }

}