def solve() {
  var beforeRed:Int = 0
  var afterBlue:Int = getSize() - 1
  var white:Int = getSize() - 1
  while(beforeRed < white){
    if(getColor(beforeRed) == RED)
      beforeRed += 1
    if(getColor(beforeRed) == BLUE){
      swap(white, afterBlue)
      swap(beforeRed, afterBlue)
      afterBlue -= 1
      white -= 1
    }
    if(getColor(beforeRed) == WHITE){
      swap(white, beforeRed)
      white -= 1
    }
  }
}