def cocktailSort2() {
	var b=true;
	val l=getNombreValeurs() ;
	var debut=0;
	var fin = l-1;
	while(debut<=fin) {
		b=false;
		for (i<-0 to l-2) {
			if (plusPetit(i+1,i)) {
				fin-=1;
				echange(i,i+1);
			}
		}
		for (i<-0 to l-2) {
			if (plusPetit((l-1-i),(l-2-i))) {
				debut+=1;
				echange((l-1-i),(l-2-i));
			}
		}
	}
}
