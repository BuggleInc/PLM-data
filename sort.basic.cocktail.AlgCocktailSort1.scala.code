def cocktailSort() {
  var n : Int =getValueCount()
  
  var i : Int = n/2
  
  while(i >= 0 ){
    
    var j : Int = 0
    
    for(j<-0 to n-2){
      
      if( isSmaller(j+1,j)){
	    	swap(j,j+1)
      }
    }
    
    j=n-1
    while(j>0){
      
      if( isSmaller(j,j-1)){
	    	swap(j,j-1)
      }
      j-=1
    }
    
    i=i-1
  }

}