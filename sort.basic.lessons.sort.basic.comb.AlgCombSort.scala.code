def combSort() {
  var ecart = getNombreValeurs()
  var tri= true
  while(tri || ecart>1){
    tri = false
    ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    var i = 0
    while(i+ecart<getNombreValeurs()){
      if(!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        i+=1
        tri = true
      }
    }
  }
}