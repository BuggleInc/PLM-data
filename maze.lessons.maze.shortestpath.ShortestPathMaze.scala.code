    		if(aBiscuit(x,y)){ return (x,y)}
    		else{for(c <- 0 to getMondeHauteur - 1){
    			y = c    		
    			if(aBiscuit(x,y)){ return (x,y)}
    		}
    }
    }

setIndication(x,y,0)
indic(x,y)
    
}
