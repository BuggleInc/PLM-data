def cocktailSort2() {
  var ok:Boolean = false
  var max:Int = getNombreValeurs()-2
  var min:Int = 0
  while(ok == false){
    ok=true
    for(i<-min to max){
      if( !plusPetit(i,i+1)){echange(i,i+1);ok=false; }      
    }
    max+= -1
    for(i<- 0 to max-min){
      if( !plusPetit(max-i,max-i+1)){echange(max-i,max-i+1);ok=false; }      
    }
    min+=1
  }

}