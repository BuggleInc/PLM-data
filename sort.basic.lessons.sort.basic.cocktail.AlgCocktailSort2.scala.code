def cocktailSort2() {
var debut=0
var fin= getNombreValeurs()-2
var swap=false
	do{ 
	
		for(i<-debut to fin by 1 ){
			swap=false
				if(plusPetit(i+1,i)){
					echange(i,i+1)
					swap=true
			
			}
		}
		fin-=1
		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swap=true
			}
			
		}
		debut+=1
	}while(swap)
}
