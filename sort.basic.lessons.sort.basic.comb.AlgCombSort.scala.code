def combSort() {
  var i=0
  var changement=true
  var ecart=getNombreValeurs()-1
  do{
    changement=false
    if (ecart>1){
      ecart=(ecart.toDouble/1.3)toInt
    }
    i=0
    while ((i+ecart) < getNombreValeurs()-1){
      if (plusPetit((i+ecart),i)){
        echange(i+ecart,i)
      }
      i=i+1
    }
    
  }while(ecart>1 || changement==true)
}