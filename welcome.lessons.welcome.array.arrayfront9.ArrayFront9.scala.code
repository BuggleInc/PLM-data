def arrayFront9(nums:Array[Int]): Boolean = {
	def minValue(tab:Array[Int]): Int = {
	var i = tab.length ; var y=0;
	for (x<- 0 to i-1) {
		if (tab(x)>y) {
			y=tab(x)
		}
	}
	return y
}
	var j = nums.length ; var y=0; var T= Array(3, j-1);
	for (x<- 0 to minValue(T)) {
		if (nums(x)==9) {
			y=y+1
		}
	}
	if (y!=0) {
		return true
	}else {
		return false
	}
}
