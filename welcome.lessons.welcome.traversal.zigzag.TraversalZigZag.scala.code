override def run() {
  var hauteur=getMondeHauteur()
  var largeur=getMondeLargeur()
  var compteur=0
  var x=0
  var y=0
  while (x!=largeur && y!=hauteur){
    if (x==0){
      for(i<-0 to largeur-1){
        setPos(i,y)
        ecritMessage(compteur)
        compteur+=1
      }
      x=hauteur-1
    }else{
      if (x==hauteur-1){
        for(j<-0 to largeur-1){
        setPos(largeur-1-j,y)
        ecritMessage(compteur)
        compteur+=1
        }
      }
      x=0
    }
    y+=1
  }
}