def cocktailSort2() {
  var n = getNombreValeurs()
  var end = true
  var tour = 0
	
  var debut = 0
  var fin = n
  
  while(end)
  {
    end = false
    for(i<- debut to fin-2)
    {
      if (!plusPetit(i,i+1))
      {
        echange(i,i+1)
        end = true
      }
    }
    fin -= 1
    for(i<- debut to fin)
    {
      var j = n - i
      if (!plusPetit(i,i+1))
      {
        echange(i,i+1)
        end = true
      }  
    }
    debut += 1
  }
}

