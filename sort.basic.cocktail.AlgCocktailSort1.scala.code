def cocktailSort() {
  var n = getNombreValeurs()
  var d = false
  do {
    d = false
    for (i <- 0 to n-2) {
      if (!plusPetit(i,i+1)) {
        echange(i,i+1);d = true
      }
    }
    for (i <- n-2 to 0) {
      if (!plusPetit (i-1,i)) {
        echange(i-1,i);d = true
      } 
  }
  }
  while (d)}

  
  
  
  
  
  
 