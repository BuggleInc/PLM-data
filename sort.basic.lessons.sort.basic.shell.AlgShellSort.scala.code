def shellSort()  {
  var ecart = getNombreValeurs()/2
  while (ecart>0) {
    var v=0
  
  for (i<-ecart-1 to getNombreValeurs()-1) {
    v=getValeur(i)
    var place = i
    while(place>0 && place-ecart>=0 && !plusPetitQue(place-ecart,v)) {
      copie(place-ecart,place)
      place-=ecart
    }
    setValeur(place,v)
  }
    ecart = ecart/2
  }
}


