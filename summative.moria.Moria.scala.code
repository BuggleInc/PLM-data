def run() {
  back();
  while (finished()) {
		if (!isOverBaggle && !isCarryingBaggle) {
			forward();
    }
    else if (isOverBaggle && !isCarryingBaggle) {
      pickupBaggle();
      back();
    }
    else if (isCarryingBaggle && !isOverBaggle) {
			forward();
    }
    else {
      back();
			forward();
      dropBaggle();
      
    }
  }
}

def finished(): Boolean = {
  var width:Int = getWorldWidth;
  var x:Int = getX;
  if (x == width -3) {
    return true
  }
  return false
}