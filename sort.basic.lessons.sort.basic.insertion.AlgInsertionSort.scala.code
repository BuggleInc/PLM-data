def insertionSort() {
  var lgr = getNombreValeurs()
  var diffcase = 1
  for (i <- 1 to lgr-1){
    var v = getValeur(i)
  while(plusPetit(v,i-diffcase) && i-diffcase >= 0){
    if (plusPetit(v,i-diffcase)){
      setValeur(i,getValeur(i-diffcase))
  }
    diffcase=diffcase+1
  }
    setValeur(lgr-1,v)
  }
}
  	