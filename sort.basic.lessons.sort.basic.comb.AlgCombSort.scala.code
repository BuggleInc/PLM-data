def combSort() {
  var ecart=getNombreValeurs()
  var test=false
  while (ecart>1 || !test){
    test=true
  	if (ecart>1){
    ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
  	}
  	var i=0
  	while (i+ecart<lgr){
    	if (plusPetit(i,i+ecart)){
      	echange(i,i+ecart)
        test=false
    	}
    	i+=1
  	}
  }
}
