def cocktailSort2() {
  var switch=false
  for(i<- 1 to getNombreValeurs()-1 by 2) {
    switch=false
    for (k <- i/2 to getNombreValeurs()-(i/2+2)) {
      if (plusPetit(k+1,k)) {echange(k,k+1);switch=true}
    }
    
    for (k <- getNombreValeurs()-(i/2+3) to i/2+1 by -1) {
      if (plusPetit(k+1,k)) {echange(k,k+1);switch=true}
    }
    if (!switch) {return}
  }
}