def hunt(c:Color) {
  while (!estSurBiscuit()){
    if (isFacingTrail(c)){
      avance()
      baisseBrosse()
      leveBrosse()
    }else {
      gauche()
    }
  }
  prendBiscuit()
 }

def isFacingTrail(couleur:Color):Boolean = {
  if (estFaceMur() == true ){
    return false
  } else {
    avance()
  	getCouleurSol()
  	if (getCouleurSol() == couleur) {
    	recule()
      return true
  	}else {
      recule()
    	return false
  }
  }
}
