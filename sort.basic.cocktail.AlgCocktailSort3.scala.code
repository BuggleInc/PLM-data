def cocktailSort3() {
  var end=getValueCount()
  var beg=0
  var k=0
  for (i <- beg to end){
    k=0
  	if isSmaller(i,i+1)==(false) {
    	swap(i,i+1)
      k=k+1
    }
    if (k==0){
      return()
    }
  }
  end=end-1
  for (i <- end to beg by -1){
    k=0
    if isSmaller(i-1,i)==(false) {
      swap(i-1,i)
      k=k+1
    }
    if (k==0){
      return()
    }
  }
   beg=beg+1
}