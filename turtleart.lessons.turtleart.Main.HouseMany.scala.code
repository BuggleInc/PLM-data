override def run() {
	def maison(taille:Int) ={
		avance(taille)
		droite(30)
		var i = 0
		for (i<-0 to 2) {
			avance(taille)
			droite(120)
		}
		droite(60)
		for (i<-0 to 2) {
			avance(taille)
			droite(90)
		}
	}
	var k = 0
	var j = 0
	for (k<-0 to 2) {
		for (j<-0 to 3) {
			if ((k != 1 && j != 1) || (k != 1 && j != 2))	maison(30)
			droite(90)
			leveCrayon
			avance(50)
			baisseCrayon
			gauche(90)
		}
		gauche(90)
		avance(200)
		droite(90)
		avance(75)
	}
}
