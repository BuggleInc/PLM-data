def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	if(steps != 0){
		current(steps)
		avance(length)
		droite(angle)
		tree(steps-1, length*shrink, angle , shrink)
		current(steps)
		gauche(2*angle)
		tree(steps-1, length*shrink, angle , shrink)
		current(steps)
		droite(angle+180)
		avance(length)
		droite(180)
	}
}
