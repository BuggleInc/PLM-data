override def run() {
  baisseBrosse()
  for (i <- 0 to 48) {
    if (estFaceMur == true) {
      if (getDirection() == Direction.EST) {
        gauche(); avance(); gauche()
      }
      if (getDirection() == Direction.OUEST) {
        droite(); avance(); droite()
      }
    } else {
      avance()
    }
  }
  leveBrosse()
}