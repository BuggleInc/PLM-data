public void run() {
  int nbBiscuit = 0;
  int nbCase = 0;
    
  do{
    if(estSurBiscuit()){
      nbBiscuit++;
    }
    
    if (nbCase == 0){
      avance();
      nbCase = 1;
    }
    
    if(crossing() && nbBiscuit >=3 && !sortieTrouvee()){
      gauche();
      nbBiscuit++;
      avance();
    }else if (crossing() && nbBiscuit <=2 && !sortieTrouvee()){
      droite();
      nbBiscuit++;
      avance();
    }
    
    nbBiscuit = 0;

    
    
  }while(!crossing()); 
  
}