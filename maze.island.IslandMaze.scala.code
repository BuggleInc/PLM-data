def isWallAtLeft () : Boolean = {
  left();
  var answer = isFacingWall();
  right();
  return answer;
}

def followTheLeftSide() {
  while (!isOverBaggle()) {
  	if (!isFacingWall()) {
    	forward();
     	if (!isWallAtLeft()) {
      	left();
       	forward();
     	 	if (!isWallAtLeft()) {
       		left();
       		forward();
       }
   		}
  	} else { right(); } 
	}
}

override def run() {
  followTheLeftSide();
  pickupBaggle();
}