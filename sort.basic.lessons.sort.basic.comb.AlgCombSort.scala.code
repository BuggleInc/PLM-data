def combSort() {
	var ecart = getNombreValeurs()
	var is_echange = false
	do {
		is_echange = false
		if (ecart > 1) {
			ecart = (ecart.toDouble /1.3).toInt}
		i = 0
		while ((i+ecart) < getNombreValeurs()) {
			if (plusPetit(i,i+ecart) {
				echange(i,i+ecart)
				i +=1
				is_echange = true
			}
		}
	} while ((ecart>1) || ischange)
}
