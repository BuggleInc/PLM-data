def combSort(){
	aux(getNombreValeurs);
}

def aux(ecart:Int){

  var l = getNombreValeurs;
  var ecart_ = ecart;
  var changement = false;
  
  if(ecart_ > 1) ecart_ = (ecart_.toDouble/1.3).toInt;

  for(i <- 0 to l - ecart_ - 1){
    if(plusPetit(i+ecart_, i)){
      echange(i, i+ecart_);
      changement = true;
    }
  }

  if(changement || ecart > 1){
    aux(ecart_);
  }
}