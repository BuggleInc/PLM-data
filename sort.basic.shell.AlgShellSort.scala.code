def shellSort()  {
  var ecart = (getNombreValeurs)/2
  var j = 1
  while (ecart > 1){
   for ( i <- 0 to getNombreValeurs-2){
     var a = getValeur(i+(j-1)*ecart)
     var b = getValeur(i +j*ecart)
     while (b < a && ( (i + j*ecart < getNombreValeurs-1 )|| (i+(j-1)*ecart>0))){
       echange(i+j*ecart, i+(j-1)*ecart)
       if ((i+ (j+1)*ecart) < getNombreValeurs){
         j += 1
       } else if ((i + (j-2)*ecart)>0){
         j -= 1
       }
     }
   }
  	ecart = ecart / 2
	}
}