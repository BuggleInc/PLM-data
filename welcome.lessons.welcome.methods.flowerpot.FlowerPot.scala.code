def marquer()
{
	baisseBrosse();
	leveBrosse();
}

def growFlowers(c: Color)
{
setCouleurBrosse(Color.YELLOW);
marquer();
setCouleurBrosse(c);
avance();
marquer();
recule();
gauche();
avance();
marquer();
avance();
recule(2);
marquer();
avance();
gauche();
avance();
marquer();
}
growFlowers(Color.blue);