var T = new Array[Color](getMondeHauteur())
for (i <- 0 to T.length-2){
   T(i)=getCouleurSol();
   avance();
}
T(T.length-1)=getCouleurSol()

for (i<- 1 to getMondeLargeur-1){
  gauche();
  avance();
  droite();
  do {avance();setCouleurBrosse(T.length-i)} while (estFaceMur==false);
  for (j <- 1 to getMondeHauteur-i){recule()};
