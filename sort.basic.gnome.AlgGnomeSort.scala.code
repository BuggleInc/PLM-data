def gnomeSort() {
  plusPetit(0:Int,1:Int) //pour avoir deux lectures en plus
  var i=0
  while(i<getNombreValeurs()-2) {
  	if (plusPetit(i:Int,i+1:Int)) {
      i+=1
    }
    else {
      if (i==0) {
        echange(i:Int,i+1:Int)
        i+=1
      }
      else {
        echange(i:Int,i+1:Int)
        i-=1
      }
    }
      
    
  }
}