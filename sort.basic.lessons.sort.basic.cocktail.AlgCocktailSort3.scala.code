def cocktailSort3() {

  var debut=0;  var fin=getNombreValeurs()-2
  var a=true

  do {
    a=false
    for (i <- debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        a=true
      }
    }
      fin=fin-1
    
    for (i <- fin to debut by -1){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        if (a==true){
          a=false}
      }
    }
      debut=debut+1
    
  }
  while (!a)
}
