def combSort() {
  var ecart=getNombreValeurs()
  var stop=false
  
  while (ecart>1 || stop!=true){
    if (ecart>1){
      ecart=(ecart.toDouble/1.3).toInt
      if (ecart==9 || ecart==10){
        ecart=11
      }
    }
    
    var i=0
    var echanger=0
    
    while(i+ecart<getNombreValeurs()){
      if (plusPetit(i+ecart,i)){
        echange(i+ecart,i)
        echanger+=1
      }
      i+=1
    }
    if (echanger<1){
      stop=true
    }
  }
}