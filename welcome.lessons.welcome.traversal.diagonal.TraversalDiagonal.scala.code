var diag = 0;
override def run() {
  var compteur = 0;
  var ligne = 0;
  var colonne = 0;
  ecritMessage(compteur)
  do{
    compteur=compteur+1;
    ligne=ligne-1;
    colonne=colonne+1;
    setPos(colonne,ligne);
    ecritMessage(compteur);
  }while(!(colonne==diag))
  diag = diag + 1;
  ligne = diag;
  colonne = 0;
  compteur = compteur + 1;
  setPos(colonne,ligne);
  ecritMessage(compteur);
	}while (!(getX==getMondeLargeur()-1 && getY==getMondeHauteur()-1))
}