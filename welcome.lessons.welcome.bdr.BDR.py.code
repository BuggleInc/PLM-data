while True:
    x = readMessage()
    if x == "Z":
        backward()
    if x == "L":
        left()
        forward()
    if x == "A":
        forward()
    if x == "B":
        forward(2)
    if x == "X":
        backward(3)
    if x == "R":
        right()
        forward()
    if x == "I":
        right()
        right()
        forward()      

