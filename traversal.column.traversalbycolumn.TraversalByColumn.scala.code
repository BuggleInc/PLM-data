override def run() {
  def positionFinale():Boolean={
    var c=0
    while ((getX()!=getWorldWidth()) && getY()!=getWorldHeight()){
      writeMessage(c)
      if (getY()!=7){
        setPos(getX(),getY()+1)
        c=c+1
      }
      else {
        setPos(getX()+1,0)
        c=c+1
      }
    }
    return(true)
  }
  
positionFinale()
}