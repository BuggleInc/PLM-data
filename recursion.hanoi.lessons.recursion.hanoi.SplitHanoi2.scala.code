def hanoi(height:Int, src:Int, other:Int, dst:Int) {
  if (height>1) {
    hanoi(height-1, src, dst, other)
 	  move(src, dst)
    hanoi(height-1, other, src, dst)
  }
  else {
    move(src, dst)
  }
}

def splitHanoi(height:Int, src:Int, other:Int, dst1:Int, dst2:Int) {
  if (height > 0) {
    splitHanoi(height-2, src, dst1, dst2, other)
    move(src, dst1)
    hanoi((height-2)/2, dst2, src, dst1)
    move(src, dst2)
    hanoi((height-2)/2, other, src, dst2)
  }
}