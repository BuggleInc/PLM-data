override def run() {
  setDirection(Direction.NORTH)
  while(getY()!=0){
    if(!estFaceMur){
      avance()
    }
    else{
      droite()
      if(!estFaceMur){
        avance();gauche()
      }
    }
  }
  gauche()
  while(!estSurBiscuit){
    while (estFaceMur){
      gauche()
      }
    avance()
    droite()
    while(!estFaceMur){
      avance();droite()
    }
  }
  prendBiscuit()
}