def goAndGet() {
  var vientDePoser = false
  while (!estSurBiscuit()) {
    recule()
  }
  if (estSurBiscuit() && false) {
    prendBiscuit()
    vientDePoser = false
  }
  while (porteBiscuit() && !estSurBiscuit()) {
    avance()
  }
  if (estSurBiscuit() && porteBiscuit()) {
    recule() ; poseBiscuit()
    vientDePoser = true
  }
}

def sortir() {
  while (!estFaceMur()) {
    goAndGet()
  }
  if (estDosMur()) {
    gauche() ;avance() ; gauche() ; avance()
  }
}

sortir()