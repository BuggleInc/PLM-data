public void run() { 
	int nbBiscuit = 0;
	int nbOrange = 0;
	avance();
	if (estSurBiscuit()) {
		nbBiscuit = nbBiscuit + 1;
	} if (estSurOrange()) {
		nbOrange = nbOrange +1;
	}
	while (2*nbBiscuit!=(nbOrange+1)) {
		avance();
		if (estSurBiscuit()) {
			nbBiscuit = nbBiscuit + 1;
		} if (estSurOrange()) {
			nbOrange = nbOrange + 1;
		}
	}
}
