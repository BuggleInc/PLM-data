def shellSort()  {
  
  var ecart = getNombreValeurs()/2
  
  while (ecart>0) {
  
    for (i <- 1 to getNombreValeurs()/ecart) {
      
			var last = 0
      var c = 1
      var v = getValeur(i-(c-1)*ecart)
      

      while (i-(c+1)*ecart >= 0 && !plusPetitQue(i-c*ecart,v)) {

        copie(i-c*ecart,i-(c+1)*ecart)
        last = c
        c = c + 1

      }

      setValeur(i-last*ecart,v)

    }
    
    ecart = ecart/2
  
  }
  
}