def bubbleSort2() {
  var tri = false 
  while (!tri) {
    var n = 0
    var m=getNombreValeurs()
    for (j <- 0 to m-2){
    	for (i <- 0 to m-2-j){
      	if (!plusPetit(i, i+1)){
        	echange(i, i+1)
        	n += 1        
      	}
    	}
   }  
    if (n == 0){
        tri = true
  }
   
 }
}