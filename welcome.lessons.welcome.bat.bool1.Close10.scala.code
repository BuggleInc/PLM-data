def close10(a:Int, b:Int): Int = {
  if ( Maths.abs(10-a)< Maths.abs(10-b)){
    return a
  }
  else {
    if (Maths.abs(10-a)> Maths.abs(10-b)){
      return b
    }
    else (Maths.abs(10-a)= Maths.abs(10-b)){
      if (Maths.abs(a)< Maths.abs(b)){
        return a
      }
      else {
        return b
      }
    }
  }
}