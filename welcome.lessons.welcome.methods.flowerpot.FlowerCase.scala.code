def fleur(c:Color){setCouleurBrosse(c);baisseBrosse();avance();avance();recule();gauche();avance();recule();recule();avance();setCouleurBrosse(Color.YELLOW);droite();leveBrosse();recule()}

def jardiniere(){fleur(Color.RED);avance();avance();avance();avance();fleur(Color.CYAN);recule();recule();recule();recule();droite();avance();avance();avance();avance();gauche();fleur(Color.PINK);avance();avance();avance();avance();fleur(Color.GREEN);recule();recule();recule();recule()}
                 
def jardiniere2(){gauche();avance();avance();droite();avance();avance();fleur(Color.ORANGE);avance();avance();avance();avance();fleur(Color.BLUE);avance();avance();avance();avance();fleur(Color.ORANGE);recule();gauche();avance();avance();droite();recule()}

def growFlowers(){jardiniere();jardiniere2();jardiniere();gauche();avance();droite();recule();recule();recule();recule();recule();recule();recule();recule();droite();avance();avance();avance();avance();avance();gauche();jardiniere();jardiniere2();jardiniere()}