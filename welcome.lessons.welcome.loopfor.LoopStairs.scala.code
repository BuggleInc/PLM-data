var heaven = false
var compteurRose = 0

while (heaven == false){
  
  //On avance
  while(!isFacingWall && !heaven){
    forward()
  }
  //On monte la marche
  left()
  forward()
  right()
  
  if (getGroundColor == Color.pink){
    compteurRose = compteurRose + 1
  }
  
  if (compteurRose == 4){
  	heaven = true
  }
}