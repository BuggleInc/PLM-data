override def run() {
  baisseBrosse()
  var x = true
  while(x){
    avance()
    if(getDirection() == Direction.EST && estFaceMur()){
        gauche()
       if(!estFaceMur){
        avance()
        gauche()
      } 
    }
    else if(getDirection() == Direction.OUEST && estFaceMur() ){
        droite()
      if(!estFaceMur){
        avance()
        droite()
      } 	
    		}	
    if(getCouleurSol() == Color.red && estFaceMur){
      x = false
    }
  }
  droite()
}