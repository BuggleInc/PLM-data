def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps!=0) {
    var nb:Int = steps
    var l:Double = length
    current(nb)
    avance(l)
    droite(angle)
    tree(nb-1, l*shrink, angle, shrink)
    gauche(angle)
    recule(l)
    nb += 1
    l /= shrink
    gauche(2*angle)
    tree(nb-1, l*shrink, angle, shrink)
	}
}