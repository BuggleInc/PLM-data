def max2Value(nums:Array[Int]): Int = {
  var max:Int=nums(0)
  var deux:Int=Integer.MIN_VALUE
  val n:Int = nums.length
  for (i<-1 to n-1){
    if (nums(i)>deux){
      if (nums(i)> max){
        deux=max
        max=nums(i)
        
      }
      else{
        deux= nums(i)
      }
    }
    }
  return deux
  }