def marqueSol()
{
  baisseBrosse()
  leveBrosse()
}

def faireV()
{
  avance(2)
  marqueSol()
  avance()
  gauche()
  avance()
  marqueSol()
  recule()
  droite()
  avance()
  marqueSol()
  avance(2)
  gauche()
}

override def run() {
  setCouleurBrosse(Color.lightGray)
  for(i <- 1 to 4) { faireV() }
}