def fini():Boolean = {
  if(getX()==6 && getY()==6){
    return true
  }
  return false
}

override def run() {
  
  var nb:Int = 0;
  var x:Int = 0;
  var y:Int = 0;
    
  while(!fini()){
    
    x = nb%7;
    y = nb-7*(nb%7)
    
    setPos(x,6);
    
    ecritMessage(nb%7);
    
    nb += 1;
    
  }
  
}