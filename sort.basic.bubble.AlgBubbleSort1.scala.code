def bubbleSort()  {
  
  var x=getValueCount()
  var i = 0
  var echange = true
  

  while ((echange==true) && (i<x)){
    
    echange = false;
    
    for (j <- 0 to x-i-2){
      
      if (isSmaller(getValue(j), getValue(j+1))){
        
        swap(j,j+1);
        echange = true;
        
      }  
      
    }
    
    i=i+1
    
  }
  
}