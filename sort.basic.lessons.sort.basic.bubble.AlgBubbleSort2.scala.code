def bubbleSort2() {
	for (i <- getNombreValeurs()-2 to 0) {
		for (j <- 0 to i) {
			if (plusPetit(j+1,j) {
				echange(j+1,j);
			}
		}
	}
}

def bubbleSort()  {
	var inverse = true;
	
	do{
		inverse = false;
		for(i <- 0 to getNombreValeurs()-2) {
	         if(plusPetit(i+1,i)) {
			echange(i+1,i);
			inverse = true;
	         }
		}
      }while( inverse )
      
}
