override def run() {
	var T = new Array[Color](getMondeHauteur())
	for (i <- 0 to getMondeHauteur() -2) {
		T(i) = getCouleurSol()
		avance()
	}
	T(getMondeHauteur()-1) = getCouleurSol()
	recule(5)
	for (i <- 0 to getMondeLargeur()-2) {
		gauche()
		avance()
		val decalage = litMessage().toInt
		droite()
		for (i <- decalage to getWorldHeight()-1) {
			setCouleurBrosse(T(i))
			baisseBrosse()
			leveBrosse()
			avance()
		}
		if (!estDosMur) {recule()}
	}
}
