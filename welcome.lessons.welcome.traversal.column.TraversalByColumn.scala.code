override def run() {
  var cpt:Int =0
  var x:Int = 0
  var y:Int =0
  while(!estFinal) {
    if (!estFaceMur) {
      setPos(x,y)
      y=y+1
    } else {
      y=y+1
      setPos(0,y)
    }    
    cpt = cpt+1;
    
    ecritMessage()
  }
}

def estFinal () :Boolean={
  if (getX()==getMondeLargeur() && getY==getMondeHauteur()) {
    return true
  } else return false
}