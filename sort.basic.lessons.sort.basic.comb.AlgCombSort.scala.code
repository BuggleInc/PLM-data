def combSort() {
  
  var ecart=getNombreValeurs();
  var test=true;
  while(test || ecart>1)
  {
    test=false;
    if(ecart>1)
    {
      ecart=ecart/1.3;
    }
    
    i=0;
    while(int(i+ecart)<getNombreValeurs())
    {
      if(plusPetit(i+ecart,i))
      {
        echanger(i+ecart,i);
        i+=1;
        test=true;
      }
    }
  }
}