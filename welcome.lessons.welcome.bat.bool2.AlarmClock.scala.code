def alarmClock(day:Int, vacation:Boolean): String = {
  if(vacation==true &&( day ==1 || day==2 ||day==3||day==4||day==5)){
    return "10:00"
  }
  else if (vacation==true && (day==0||day==6){
    return "off"
  }
  else if (vacation == false && (day==1 || day==2 ||day==3||day==4||day==5)){
    return "7:00"
  }
  else{
    return "10:00"
  }
}