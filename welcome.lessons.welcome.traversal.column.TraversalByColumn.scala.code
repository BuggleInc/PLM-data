override def run() {
	var x:Int = 0
	def posFin():Boolean = {
		var b = false 
		if (getX() == getMondeHauteur()-2 && getY() == getMondeLargeur()-2) {b = true}
		return b
	}
	while (!posFin) {
		if (!estDosMur()) {
			ecritMessage(x);
			setPos(getX(),getY()+1);
			x=x+1;
		}
		if (estDosMur) {
			ecritMessage(x);
			setPos(getX()+1,0);
			x=x+1;
		}
	}
}
