override def run() {
  var colonne = 0;
  var ligne = 0;
  var compteur = 0;
  ecritMessage(compteur);
  do {if(getX==getMondeLargeur-1){colonne=0;ligne=ligne+1;setPos(colonne,ligne)}
      else {colonne=colonne+1;setPos(colonne,ligne)};compteur=compteur+1;ecritMessage(compteur)}
  while (!(getX==getMondeLargeur-1 && getY==getMondeHauteur-1))
}
