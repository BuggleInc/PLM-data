override def run() {
  dessine(Color.RED)
  avance(3)
  gauche()
  dessine(Color.BLUE)
  avance(3)
  gauche()
  dessine(Color.GREEN)
  avance(3)
  gauche()
  dessine(Color.YELLOW)
}

def dessine(c: Color) {
  setCouleurBrosse(c)
  avance()
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  gauche()
  avance()
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  gauche()
}