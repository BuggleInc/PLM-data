def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if (speed<60) {
		return 0
		} 
		else if ((60<speed && speed<81) && isBirthday==false) {
			return 1
		} else if (speed>80 && isBirthday==false) {
			return 2
		} else if (speed<65 && isBirthday) {
			return 0
		} else if ((65<speed && speed<86) && isBirthday) {
			return 1
		} else { 
			return 2
		}
}
