def cocktailSort() {
	do {
		var break = false;
		while (!break) {
			for (i <- 0 to getNombreValeurs()-2) {
				if (plusPetit(i+1,i)) {
					echange(i+1,i);
					break = true;
				}
			}
			for (i <- getNombreValeurs()-2 to 0 by -1) {
				if (plusPetit(i+1,i)) {
					echange(i+1,i);
					break = true;
				}
			}
		}
	}
}