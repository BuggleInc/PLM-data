var diag = 0;
override def run() {
  var c:Int=0
  for (j<-0 to (2*getMondeHauteur-1)){
    for (i<-0 to diag){
      if (diag<7){
        setPos(diag/getMondeHauteur()+i,diag % getMondeHauteur()-i)
        ecritMessage(c)
        c=c+1
    } else {
        setPos(diag/getMondeHauteur()+i,getMondeHauteur()-i)
        ecritMessage(c)
        c=c+1
      }
    diag=diag+1
    }
  }
}