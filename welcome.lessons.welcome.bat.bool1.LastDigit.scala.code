def lastDigit(a:Int, b:Int):Boolean = {
          if ( Math.abs( ( dec(a) - dec (b) )) == 0 ){
          	return true;
          }else{
          	return false;
          }
}



 def  dec(a:Int):Int = {
 	return  (a % 10);
 	
 }

