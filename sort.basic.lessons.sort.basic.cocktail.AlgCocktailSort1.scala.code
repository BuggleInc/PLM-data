def cocktailSort() {
  var tri = true
  var comp = 0
	while (tri) {
    tri = false
    comp = 0
  	for (i<- 0 to getNombreValeurs()-2) {
    	if (!(plusPetit(i,i+1))) {
      	echange(i,i+1)
   			comp = 1
      }
    }
  	for (j <- getNombreValeurs() to 2) {
    	if (plusPetit(j,j-1)) {
      	echange(j-1,j)
				comp = 1
      }
  	}
    if (comp ==1) {
      tri = true
    }
  }
}