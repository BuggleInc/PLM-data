override def run() {
  while(getY!=0 || !estSurBiscuit()){
    searchExternalWall()
  }
  while(!estSurBiscuit()){
    keepHandOnWall()
  }
  prendBiscuit()
}

def keepHandOnWall(){
  gauche()
  if (estFaceMur()){
    droite()
    if(estFaceMur()){
      droite()
    }else{
      avance()
    }
  }else{
    avance()
  }
}

def searchExternalWall(){
  while(!estFaceMur()){
    avance()
  }
  gauche()
  while(getDirection()!=Direction.NORTH){
    keepHandOnWall()
  }
}