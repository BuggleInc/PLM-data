def growFlowers(){
	fleur(Color.RED);avance();avance();avance();avance();fleur(Color.CYAN);recule();droite();avance(2);
	
}
def fleur(v: Color){
	marque(v);avance();marque(Color.YELLOW);avance();marque(v);recule();droite();avance();marque(v);recule(2);marque(v);avance();gauche();recule()
}
def marque(c: Color){setCouleurBrosse(c);baisseBrosse();leveBrosse()}
