def combSort() {
	var ecart=getNombreValeurs()
	var i=0
	var inverse=false

	do {
		if (ecart>1) {
			ecart = (ecart.toDouble / 1.3).toInt
		} 
		i=0	
		inverse=false
		 while (i+ecart<getNombreValeurs()) {
		 	if (plusPetit(i+ecart,i)) {
		 		echange(i,i+ecart)
		 		inverse=true
		 	}
		 	i++
		 }
	} while (ecart>1 || inverse)
	
	
}
