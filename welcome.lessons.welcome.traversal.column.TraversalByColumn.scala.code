def isInFinalPosition():Boolean = {
  
  if ((getX() == getMondeLargeur() -1) && getY() == getMondeHauteur()-1) {return true}
  
  else {return false}
  
}

def allerProchainePosition() {
  if (getY() == getMondeHauteur() -1) {
    setPos(getX() +1, 0)
  }
  else {setPos(getX(), getY() +1)}
}


