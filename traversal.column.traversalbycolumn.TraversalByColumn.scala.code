def posTest():Boolean = { 
  if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {
  	return true
  }
  return false
}



def colonne() {
	var i=0
  var cpt=0
  ecritMessage(cpt:Int)
  while (!posTest()) {
    for(j<-0 to 6) {
      setPos(i,j)
      
      ecritMessage(cpt:Int)
    	cpt=cpt+1
    }
    
    i=i+1
  }
}
      
















override def run() {
  colonne()
}