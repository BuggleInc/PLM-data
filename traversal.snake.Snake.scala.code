override def run() {
  setBrushColor(Color.RED);
  brushDown();
  var fin=false;
  while (fin==false){
    while (isFacingWall==false){
      forward();
    }
    if (getDirection()==Direction.EAST){
      left();
      if (isFacingWall==false){forward();left();
                              } else {
        fin=true;
      } }else {
      right();forward();right();
    }
}
}