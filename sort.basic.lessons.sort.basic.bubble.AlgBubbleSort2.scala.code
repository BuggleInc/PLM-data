def bubbleSort2()  {
  var echanges = true
  var etapes = 0
  while (etapes < getNombreValeurs()){
    echanges = false
    for (k <- 0 to getNombreValeurs() -2 - etapes ){
      if (getValeur(k) > getValeur(k+1)){
        echange(k, k+1);
        echanges = true;
      }
    }
    etapes += 1;
  }
}
