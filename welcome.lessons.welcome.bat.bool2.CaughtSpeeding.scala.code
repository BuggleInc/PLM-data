def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if(!isBirthday){
    if(speed<=60){
      return 0
    } else if(speed>=61 && speed<=80){
      return 1
    } else{
      return 2
    }
  } else{
    if(speed<=60+5){
      return 0
    } else if(speed>=61 && speed<=80+5){
      return 1
    } else{
      return 2
    }
  }
}
