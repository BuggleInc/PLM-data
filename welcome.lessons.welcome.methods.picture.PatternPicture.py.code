def trait(a,b):
    forward()
    setBrushColor(a)
    brushDown()
    brushUp()
    forward()
    setBrushColor(b)
    brushDown()
    brushUp()
    forward()
def carre():
    trait(Color.red,Color.blue)
    left()
    trait(Color.blue,Color.green)
    left()
    trait(Color.green,Color.yellow)
    left()
    trait(Color.yellow,Color.red)
    forward()
def motif(i):
    for l in range(i):
        carre()
motif(4)
