def bubbleSort3() {	
  var n:Int=getNombreValeurs()
  var i:Int=0
  var nbBoucle:Int=0
  var compteur:Int=0
  while (nbBoucle<(n-1)){
    while (i<(n-1-nbBoucle)){
      if (!plusPetit(i,i+1)){
        echange(i,i+1)
        i=i+1
        compteur=1
      } else{
        i=i+1

      }

    }
    if (compteur==0){
      return
    } else {
      nbBoucle=nbBoucle+1
      i=0
    }
  }
}
