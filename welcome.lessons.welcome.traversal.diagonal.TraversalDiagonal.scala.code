var suce = 0;
override def run() {
	for(i<- 0 to getMondeHauteur-1){
		for(j<- 0 to i){
			setPos(j,i-j)
			ecritMessage(suce)
			suce+=1
			
		}
			
	}
		for(i<- 0 to getMondeHauteur-1){
		for(j<- getMondeHauteur-1 to 0){
			setPos(j+1,getMondeHauteur-1-j-1)
			ecritMessage(suce)
			suce+=1
		}
			
	}
}
