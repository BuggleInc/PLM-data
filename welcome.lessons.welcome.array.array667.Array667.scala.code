def array667(nums:Array[Int]): Int = 
{
  var compteur = 0
  for (i <- 1 to nums.length) 
  {
    if (i < nums.length) 
    {
      if (nums(i-1) == 6 && nums(i) == 6) 
      {
        compteur = compteur + 1
      }
      if (nums(i-1) == 6 && nums(i) == 7) 
      {
        compteur = compteur + 1
      }
    }
  }
  return compteur
}