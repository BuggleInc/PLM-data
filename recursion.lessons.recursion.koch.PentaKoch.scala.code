def pentaKoch(levels:Int, length:Double) {
	if(levels!=0){
		pentaKoch(levels-1,length/3)
		gauche(120)
		pentaKoch(levels-1,length/3)
		droite(85)
		pentaKoch(levels-1,length/3)
		droite(85)
		pentaKoch(levels-1,length/3)
		droite(85)
		pentaKoch(levels-1,length/3)
		gauche(120)
		pentaKoch(levels-1,length/3)
		
	}else{
		avance(length)
	}
}
