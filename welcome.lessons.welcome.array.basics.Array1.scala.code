override def run() {
  var T =Array[Int](getCouleurSol)
  avance()
  while ( T(0)!=getCouleurSol ){
    T=T+(getCouleurSol)
    avance()
  }
  for (i<-0 to T.lenght-1){
    gauche()
    avance()
    droite()
    for (j<- 0 to T.lenght-1){
      baisseBrosse()
      setCouleurBrosse(T[j])
      avance()
    }
  }
}