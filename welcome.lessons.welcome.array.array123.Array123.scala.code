def array123(nums:Array[Int]): Boolean = {
  var voir_un = false
  var voir_deux = false
  var voir_trois = false
  for (i<- 0 to nums.length-1){
    if (nums(i)==1){
      voir_un = true
    }
    if (nums(i)==2){
      voir_deux = true
    }
    if (nums(i)==3){
      voir_trois = true
    }
  }
  return (voir_un && voir_deux && voir_trois)
}