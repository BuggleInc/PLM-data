def sortirMoria(){
  var a:Boolean=true
  var fin:Int=0
  while (fin==0){
    retourne()
    avance()
    while (a){
      if (estFaceMur()){a=false}
      if (estSurBiscuit()){a=false}
      avance()
    }
    a=true
    if (estSurBiscuit()){
      prendBiscuit()
    	retourne()
    	while (!estSurBiscuit()){
      	avance()
    	}
    	recule()
      poseBiscuit()
    }else{droite();avance();gauche();avance();fin=1}
	}
}
sortirMoria()