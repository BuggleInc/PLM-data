def max2Value(nums:Array[Int]): Int = {
  var taille : Int = nums.length
  var maxi1 : Int = Math.max(nums(0) , nums(1))
  var maxi2 : Int = Math.min(nums(0) , nums(1))
  for (x <- nums) {
    if (x > maxi1) {
      maxi2 = maxi1; maxi1 = x}
    else if (x > maxi2) {maxi2 = x}
    }
  return maxi2
}
  