def combSort11() {
  var ecart = getNombreValeurs()
  var taille = getNombreValeurs()
  var inverse = true
  while (ecart >1 || inverse==true)
  {
    inverse = false
    if (ecart>1)
    {
      ecart = (ecart.toDouble / 1.3).toInt
      if (ecart==9 || ecart == 10)
      {
        ecart = 11
      }
    }
    var i=0
    while (i+ecart < taille)
    {
      if (!plusPetit (i,i+ecart))
      {
        echange(i,i+ecart)
        inverse =true
      }
      i = i+1
    }
  }
}