def golomb(num:Int): Int = {
  var avance=1
  var test =0
  var nombre=1
  var T= new Array[Int](num+1)
  T(0)=nombre
  while (avance<num){
    nombre =nombre+1
    test =T(nombre-1)
    while (test !=0 && avance < num){
      test=test-1
      avance=avance+1
      T(avance-1)=nombre
    }
  }
  return T(num-1)
}