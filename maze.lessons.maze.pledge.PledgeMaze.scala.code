override def run() 
{

	def keepHandOnSideWall()
{
	gauche()
	if (estFaceMur())
	{
		droite()
		if (estFaceMur())
		{
			droite()
			if (estFaceMur())
			{
				droite()
				avance()
			} else avance()
		} else avance()
	} else avance()	
}

def goNord()
{
	if (getDirection()==Direction.NORTH && !estFaceMur())
	{
		avance()
	}
	else
	{
		gauche()
		if (!estFaceMur())
		{
			avance()
		}
		else
		{
			gauche()
		}
	}
}

while (!estSurBiscuit()
{
	goNord()
	droite()
	keepHandOnSideWall()
}
