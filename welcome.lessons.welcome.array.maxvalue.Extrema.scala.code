def extrema(nums:Array[Int]): Int = {
	var suce=nums(0)
	var sucegrave=0
	var sucegravedeouf=nums(0)
	var sucesuce=0
	for (i<- 1 to nums.length-1){
		if (nums(i)>suce){
			suce=nums(i)
			sucegrave=i
		}
			if (nums(i)>sucegravedeouf){
			sucegravedeouf=nums(i)
			sucesuce=i
		}
	}
	return sucegravedeouf-suce

}
