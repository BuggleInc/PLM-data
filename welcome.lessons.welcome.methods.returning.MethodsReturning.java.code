		for (int j = 0 ; j < 6 ; j = j + 1) {
			if (estSurBiscuit()) {
				dejaVu = true;
			}
			avance();
	}
	
	for (int j = 0 ; j < 6 ; j = j + 1) {
		recule();
	}

