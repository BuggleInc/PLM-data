public void run() { 
while (true)
{
	right();
	right();
	forward();
	if (isOverBaggle())
	{
		pickupBaggle();
		right();
		right();
		forward();
		forward();
		forward();
		isCarryingBaggle();
		
		back();
		dropBaggle();
	}
}
}
/*isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle()
*/
