def shellSort()  {
  var ecart = getNombreValeurs()/2
  while(ecart>0) {
    var k = 0
    var v = 0
    var c = 0
    for (i<- 1 to getNombreValeurs() -1) {
      c=1
      k=ecart
      v=getValeur(i)
      while((i-k > 0) && (!plusPetitQue(k,v))) {
        copie(k,i)
        c+=c
        k=c*k
      }
      setValeur(k,v)
    }
    ecart = ecart / 2
  }
  
}