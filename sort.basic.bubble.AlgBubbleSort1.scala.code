def bubbleSort()  {
  val n = getNombreValeurs()
  while(!estTrie())
  {
    for(i<- 0 to n-2)
    {
      if(plusPetit(i+1, i))
      	echange(i, i+1)
    }
  }
}

def estTrie() : Boolean =
{
  for (i<- 0 to getNombreValeurs()/2)
  {
    if (!plusPetit(i, i+1))
    	return false
  }
  return true
} 