def gnomeSort() {
	var i:Int = 1
	var last:Int = 0
	
	while (i < getNombreValeurs()-1) {	
		if (plusPetit(i,i-1)) {
			echange(i-1,i)
			if (i > 1) {
				if (last == 0)
					last = i
				i = i-1
			} else
				i = i + 1
		} else {
			if (last != 0) {
				i = last
				last = 0
			}
			i = i + 1
		}
	}
}
