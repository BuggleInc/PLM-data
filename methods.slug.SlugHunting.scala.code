def hunt() {
  while (!estSurBiscuit()) {
    if (isFacingTrail()) {
      avance()
      baisseBrosse()
      leveBrosse()
    } else {
      gauche()
    }
    
  }
}
def isFacingTrail():Boolean = {
  if (estFaceMur())
    return false
  avance()
  if (getCouleurSol()==Color.green) {
    recule()
    return true
  }
  recule()
  return false
}