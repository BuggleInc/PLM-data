def cocktailSort() {
  var switch:Boolean =true
  
   do {
     
    switch=false
    
    for(i<- 0 to getNombreValeurs()-2 ){
      if (plusPetit(i+1,i)){echange(i+1,i);switch=true}}
    
    
     for(j<- getNombreValeurs()-2 to 0){
       if(plusPetit(j,j-1)){echange(j,j-1);switch=true}}
     

    
  }while (switch)
}