override def run() {
		def faireV(c: Color){
		setCouleurBrosse(c);
		avance();
		baisseBrosse();
		leveBrosse();
		avance();
		gauche();
		avance();
		baisseBrosse();
		leveBrosse();
		recule();
		droite();
		avance();
		baisseBrosse();
		leveBrosse();
		avance();
		gauche();
	}

		def faireMotif(){
			faireV(Color.YELLOW);
			faireV(Color.RED);
			faireV(Color.BLUE);
			faireV(Color.GREEN);
		}

	faireMotif();
}
