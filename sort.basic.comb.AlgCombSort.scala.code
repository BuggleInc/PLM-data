def combSort() {
  var ecart:Int=getNombreValeurs()
  var x:Int=getNombreValeurs()
  var t=0
  while (t==0 && ecart>=1) {
  ecart=(ecart.toDouble/1.3).toInt
  var i:Int=0
  while (i+ecart < x) {
    if (plusPetit(i,i+ecart)==false) {
      echange(i,i+ecart)
      t=1
    }
    i=i+1
  }
    if (t!=0) {
      t=0
    }
    else return
  }
}