var debut = 0
var fin = getNombreValeurs()-2
def cocktailSort2() {
  var b = false
  do
  {
    b = false
    for(i <- debut to fin)
    {
      if(plusPetit(i+1,i))
      {
        echange(i+1,i)
        b=true
      }
    }
    fin-=1
    for(i <- fin to debut by -1)
    {
      if(plusPetit(i+1,i))
      {
        echange(i+1,i)
        b=true
      }
    }
    debut+=1
  }while(b);
}