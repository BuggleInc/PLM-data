override def run() {
  while(!estFaceMur()){
    if (getDirection==direction.OUEST){
      retourne()
    }
    while (!estSurBiscuit()){
      avance()
    }
    prendBiscuit()
    retourne()
    while (!estSurBiscuit()){
      avance()
    }
    recule()
    poseBiscuit()
  }
  droite()
  avance()
  gauche()
  avance()
}