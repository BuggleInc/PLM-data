override def run() {
  findNorthWall()
  findSideWall()
  while(!estSurBiscuit) {
    keepHandOnSideWall()
  }
  prendBiscuit()
}

def keepHandOnSideWall() {
  if (estFaceMur) {
    (droite)
  }
  else {
    avance()
    gauche()
    while (!estFaceMur) {
      avance()
      gauche()
    }
    droite()
  }
}

def findSideWall() {
  while (!estFaceMur) {
    avance()
  }
  droite()
}

def findNorthWall() {
  var etat:Int = 0
  var y:Int = getY()
  while (getY()!=0) {
    etat match {
      case 0 => setDirection(Direction.NORTH)
                while (!estFaceMur) {
                  avance()
                }
                etat = 1
      case 1 => y=getY()
                while (y<=getY()) {
                  keepHandOnSideWall()
                }
                etat = 0
      case _ => println("Ce cas ne devrait pas arriver.")
    }
  }
}