def solve() {
  var apresBleu=0
  var blancRouge=getTaille()-1
  var avantBlanc=getTaille()-1
  while (apresBleu<=avantBlanc) {
    for (i<-apresBleu to avantBlanc) {
      if (getCouleur(i)==0) {
        echange(i,apresBleu)
        apresBleu=apresBleu+1
      }
      if (getCouleur(i)==2) {
        echange(i,avantBlanc)
        avantBlanc=avantBlanc+1
      }
    }
  }
}