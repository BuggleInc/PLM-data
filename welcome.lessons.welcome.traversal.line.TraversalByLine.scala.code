override def run() {
var msg = 0
ecritMessage(""+msg)
msg += 1
var x = 1
var y = 0
while (!positionFinale())
	{if (x!=getMondeLargeur())
			{setPos(x,y)
			ecritMessage(""+msg)
			msg += 1
			x += 1}
		else {x=0
			y=y+1
			setPos(x,y)
			ecritMessage(""+msg)
			msg += 1
			x += 1}
	}
}

def positionFinale():Boolean= {
		(getX(),getY()) match {
			case (x,y) if (x==getMondeLargeur()-1 && y ==getMondeHauteur()-1) =>
						return true
			case _	=>	return false
		}
}
