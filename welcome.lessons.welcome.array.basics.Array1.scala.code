override def run() {
	val H = getMondeLargeur();
	var T = new Array[Color](H+1)
	for (i <- 1 to H) {
		T(i) = getCouleurSol() ;
		avance();
	}
	for (i <- 1 to H-1) {
		for (tmp <- 1 to H) {
			gauche(); avance(); droite();
			avance();
			setCouleurBrosse(T(tmp));
			baisseBrosse();
			leveBrosse();
		}
	}
	
}
