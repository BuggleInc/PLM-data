override def run() {
  var h : Int = getMondeHauteur()
  var i : Int = 0
  var T : Array[Color] = new Array[Color](h)
  for (i<- 0 to h-1){
    avance()
    T(i)=getCouleurSol()
    
  }
  for (i<- 0 to h-1){
    avance()
    gauche()
    avance()
    droite()
    avance()
    setCouleurBrosse(T(i))
    baisseBrosse
  }
}