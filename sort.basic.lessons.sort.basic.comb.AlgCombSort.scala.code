def combSort() {
  var ecart=getNombreValeurs()
  var swap=true
  do {
    swap=false
    if (ecart>1) {
      ecart=ecart/1.3
    }
    var i=0
    while (i+ecart < getNombreValeurs()){
      if (!plusPetit(i,i+1)) {
        echange(i,i+1)
        swap=true
      }
      i+=1
    }
  } while (ecart>1 || swap)
}