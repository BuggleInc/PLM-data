def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps==0){
    return 0
  }
  else{
    avance(length)
    droite(angle)
    return tree(steps-1,length/shrink,angle, shrink-1)
    gauche(2*angle)
    return tree(steps-1,length/shrink,angle, shrink-1)
    droite(angle)
    recule(length)
  }
  }