def inOrder(a:Int, b:Int, c:Int, bOk:Boolean):Boolean = {
	if(!bOk){
		if ( order(a,b)  && order(b,c)){
		return true;
		}else{
			return false;
		}
	}else{
		if ( order(b,c) ){
		return true; 
		}else{
			return false;
		}
	}
}


def order(a:Int , b:Int):Boolean = {
	if ((b - a) > 0 ){
		return true;
	}else{
		return false;
	}
}

