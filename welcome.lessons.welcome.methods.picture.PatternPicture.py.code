def fairemotif():
    couleur=[Color.RED,Color.BLUE,Color.GREEN,Color.YELLOW]
    droite()
    backward()
    for i in range (len(couleur)):
        color=couleur[i]  
        motif(color)  
        


def motif(color):
    setCouleurBrosse(color)
    baisseBrosse
    leveBrosse()
    forward()
    gauche()
    forward()
    baisseBrosse()
    leveBrosse()
    forward()
    

fairemotif()
    
