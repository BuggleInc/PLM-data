var n:Int=0
var col:Color=Color.BLACK

def step(rule:Array[Char], colors:Array[Color]) {

do{
	col=colors(n)
	n=n+1
	
}while (col==getCouleurSol())
n=n-1
if (rule(n)=='R'){
	right();
}else{	
	left();
}
if(n<colors.length-2){
	setCouleurBrosse(colors(n+1))
}else{
	setCouleurBrosse(colors(0))
}
baisseBrosse();
leveBrosse();
avance();
}
