override def run() {
	var h:Int = getMondeHauteur()
	var l:Int = getMondeLargeur()
	var c:Int = 0
	for (i<- 0 to l-1) {
		if (getY() == 0) {
		setPos(0,i)
		
		ecritMessage(c)
		
		for (j <- 1 to h-1) {
		setPos (j,i)
		c = c+1 
		ecritMessage(c)}
		c=c+1}
		
		else if(getY() == 6)
		{ setPos(l-1,i)
		ecritMessage(c)
		for (h<- 1 to h-1) {
	setPos (l-1-h,i)
		c = c+1 
		ecritMessage(c)}
	c=c+1	
	
}
}}
