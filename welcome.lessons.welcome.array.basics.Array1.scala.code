override def run() {
  var taille:Int=getMondeHauteur()
  var T=new Array[Color](taille)
  
  for (i<-0 to (taille-1)){
    T(i)=getCouleurSol()
  }

  
  for (i<-0 to ((getMondeLargeur())-1)){
    gauche()
    avance()
    droite()
    avance()
    
    for (j<-0 to (taille-1)){
      dessine(T(j))
    	avance()
    }
  } 
  
}



def dessine(c:Color){
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
}

