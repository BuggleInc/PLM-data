def combSort() {
	val longeur:Int = getNombreValeurs();
	var ecart:Int = longeur;
	var test:Boolean = false;
	var i:Int = 0;
	do{
		if(ecart > 1 ){
			ecart = (ecart.toDouble / 1.3).toInt
		}
		i=0;
		while(i+ecart < longeur){
			test = false;
			if (!plusPetit(i,i+ecart)){
				echange(i,i+ecart);
				test = true;
			}
			i+=1;
		}
	} while (ecart >1 || test)
}
