def solve() {
  	val N=getTaillePile()
  def gCrepeF(n:Int){
    var plusG=getRayonCrepe(0)
    var hCrepe=0
    for(h<-1 to n){if(getRayonCrepe(h)>plusG){hCrepe=h
                                               plusG=getRayonCrepe(h)}}
  	retourne(hCrepe+1)
  	retourne(n+1)}
  for(x<-1 to (N-1)){gCrepeF(N-x)}
}