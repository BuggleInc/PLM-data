var sortie=false

while (sortie==false){
  var biscuit=0
  var couloir=0

  for (couloir<-0 to 4){
    avance()
    if (estSurBiscuit()){
      if (couloir!=0){
        biscuit=biscuit+1
      }
    }
  }

  
  //direction selon le nombre de biscuit
  if (biscuit<=2){
    droite()
  }else{
    gauche()
  }
  sortie=sortieTrouvee()
}