override def run() {
  var nbr:Int = 0;
  var width:Int = getWorldWidth();
  var  height:Int = getWorldHeight();
  var x = 0; var y = 0;
  while (!finished(x,y,width,height)) {
    var x:Int = getX;
    var y:Int = getY;
    writeMessage(""+nbr);
    if (y != height-1) {
      setPos(x,y+1);
    }
    else {
      setPos(x+1,0);
    }
    nbr = nbr +1 ;
  }
}

def finished(x:Int,y:Int,width:Int,height:Int):Boolean = {
  if (x==width-1 && y==height-1) {
    return true
  }
  return false
}