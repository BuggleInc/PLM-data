def combSort()
{
  var sortie:Boolean = false
  var ecart:Int = getNombreValeurs()
  var i:Int = 0
  
  while((ecart>1)||(!sortie))
  {
    sortie = true
    
    if(ecart>1)
    {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    
    i=0
    
    while(i+ecart < getNombreValeurs())
    {
      if(plusPetitQue(i,i+ecart))
      {
        echange(i,i+ecart)
        sortie = false
      }
      
      i=i+1
    }   
  }
}