while (!isFacingWall()){
  
  getDirection() match{
    case Direction.EAST =>
    	if (isOverBaggle()){
        pickupBaggle()
        setDirection(Direction.WEST)
      }else{
        forward()
      }
    
    case Direction.WEST =>
    	if (isOverBaggle()){
        pickupBaggle()
				setDirection(Direction.EAST)
      }else{
        forward()
      }
  }
}