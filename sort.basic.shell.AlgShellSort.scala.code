def shellSort()  {
  var ecart:Int= getValueCount()/2
  while(ecart>0){
    insertionSort();
    ecart=ecart/2;
  }
}

def insertionSort(){
  for(i<-1 to getValueCount()-1){
    var x:Int=getValue(i);
    var j:Int=i-1;
    while(j>=0 && getValue(j)>x){
      setValue(j+1,getValue(j))
      j=j-1
    }
    setValue(j+1,x)
  }
}