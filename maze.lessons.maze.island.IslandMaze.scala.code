override def run() {
  while (!estSurBiscuit()) {
		if (!estFaceMur() && getDirection() == Direction.NORTH)  {
   		avance()
  	} else if (!estFaceMur() && !(getDirection()==Direction.NORTH)) {
  		gauche()
      if (!estFaceMur()) {
        avance()
      }
		} else keepHandOnSideWall()
}
  prendBiscuit()
}


