def golomb(num:Int): Int =
{
  var tab = new Array[Int](2*(num+1))
  
  for(i <- 0 to tab.length-1)
  {    
    tab(i)=0
  }
  
  var aMettre:Int = 0
  var indice:Int = 3
  var valeur:Int = 3
  
  tab(0)=0
  tab(1)=1
  tab(2)=2
  tab(3)=2
  
  while(tab(num) == 0)
  {    
    aMettre=tab(valeur)
    for(i <- indice+1 to indice + aMettre)
    {
      tab(i)=valeur
    }
    valeur = valeur + 1
    indice = indice + aMettre
  }
  
  return tab(num)
}
