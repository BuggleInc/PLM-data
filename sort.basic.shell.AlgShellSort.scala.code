def shellSort() {
  var length:Int = getNombreValeurs()
  var v:Int = 0
  var a:Int = 0
  var j:Int = 0
  var ecart:Int = length/2
  for (i <- 1 to length-1){
    a = i
    v = getValeur(i)
    j = 1
    while(j<=i && !plusPetitQue(i-j,v)){
      copie(i-(j*ecart),i-(j-1)*ecart)
      a = i-j*ecart
      j += 1
    }
    ecart = ecart/2
    setValeur(a,v)
  }
}