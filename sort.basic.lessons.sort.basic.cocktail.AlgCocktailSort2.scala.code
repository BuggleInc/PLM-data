def cocktailSort2() {
  var a=0
  var d=0
  var f=getNombreValeurs()-2
 	do {
   	a=0
   	for (i<-d to f) {
     	if (plusPetit(i+1,i)) {
     	  echange(i,i+1)
      	a=1
   	  }
     	f=f-1
  	 }
  	 for (i<-f to d) {
   	 	if (plusPetit(i+1,i)) {
         echange(i,i+1)
   	     a=1
    	}
     	d=d+1
  	 }
  	} while (a==1)
}