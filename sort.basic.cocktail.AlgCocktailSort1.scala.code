def cocktailSort() {
  var fin:Boolean = false
  var n:Int = getNombreValeurs() - 2
  do {
    fin = true
    for(i <- 0 to n) {
      if(plusPetit(i + 1, i)) {
        echange(i + 1, i)
        fin = false
      }
    }
    
    for(i <- n to 0 by -1) {
      if(plusPetit(i + 1, i)) {
        echange(i + 1, i)
        fin = false
      }
    }
  } while(!fin)
}