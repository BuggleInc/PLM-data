def golomb(num:Int): Int = {
	var cv = 2;
	var cp = 3;
	var comp = 0;
	var k = 0;
	var T = new Array[Int](1000)
	

	T(0) = 0
	T(1) = 1
	T(2) = 2
	T(3) = 2

	while (cv < 19)
	{
		for (k <- 0 to 17)
		{
			if (T(k) == cv)
			{
				comp = comp + 1
			}
		}

		while (comp != T(cv))
		{
			comp = comp +1 ; T(cp) = cv; cp = cp + 1;
		}

		cp = cp;
		cv = cv +1;
		comp = 0;
	}

print("cherche "+num+" trouve "+T+"\n")
	return T(num)
	

	
} 
