n=getNombreValeurs()-1
case1=0
case2=1

while case!=n:
  if plusPetit(case1,case2)==True:
    case1=case1+1
    case2=case2+1
  
  if plusPetit(case1,case2)==False and case1!=0:
    echange(case1,case2)
    case1=case2
    case2=case1-1
  
  if case1=0 and plusPetit(case1,case2)==False:
    echange(case1,case2)
    case1=case1+1
    case2=case2+1
  