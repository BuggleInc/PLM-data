def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
	while(isAsleep){
		return false;
	}
	while(!isMorning){
		return true;
	}
	if(!isAsleep && isMorning && isMom){
			return true;
	}
	if(!isAsleep && isMorning && !isMom){
			return false;
	
	}else{
			return true;
		}
		
	
}


