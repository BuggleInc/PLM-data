override def run() {
var n =  (getMondeHauteur()) ;
var T = new Array[Color](n);
for (i <- 0 to n-1) {
	T(i) = getCouleurSol();
	avance();
}
for (j <- 0 to n-1) {
	gauche();
	avance();
	droite();
	var aux = T(n-1);
	for (c <- n-2 to 0) {
		var x = T(c);
		T(c) = T(c-1);
		T(c+2) = x;
	}
	T(0) = aux ;
	for (k <- 0 to n-1) {
		setCouleurBrosse(T(k))
		baisseBrosse();
		leveBrosse();
		avance();
	}
	for (r <- 0 to n-1) {
		recule();
	}
}
}
