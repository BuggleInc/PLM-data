def combSort() {

	var ecart = getNombreValeurs()
	var end = 0

	while (ecart > 1 || end != 0) {
		end=0
		
		if (ecart > 1) {
			ecart = (ecart.toDouble / 1.3).toInt
		}
		
		for (i<-0 to getNombreValeurs()-ecart-1) {
			if (plusPetit(i+ecart, i)) {
				echange(i+ecart, i)
				end+=1
			}
		}
		
	}

}
