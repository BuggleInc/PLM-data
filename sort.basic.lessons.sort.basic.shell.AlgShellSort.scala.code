def shellSort()  {
  var ecart:Int = getNombreValeurs() / 2  // largeur du peigne //
  var v:Int = 0  // pour m?moriser la valeur //
  var j:Int = 0  // i - k * ecart //
  while (ecart > 0) {
    for (i <- ecart-1 to getNombreValeurs() - 1) {
      v = getValeur(i)
      j = i - ecart
      while (j >= 0 && getValeur(j) > v) {
        setValeur(j + ecart, getValeur(j))
        j -= ecart
      }
      setValeur(j + ecart,v)
    }
    ecart = ecart / 2
  }
}
