override def run() {
  while(!isSorted) {
    for (i <- 1 to 4) {
      var B:Int=getHoleBase()
      if (B==0) {
        move(3,1)
      } else {
        move(B-1,1)
      }
    }
    for (i<- 1 to 4) {
      var B:Int=getHoleBase()
      if (B==0) {
        move(3,0)
      } else {
        move(B-1,0)
      }
    }
  }
}