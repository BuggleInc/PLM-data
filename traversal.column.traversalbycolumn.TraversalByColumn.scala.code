override def run() {
  var cpt = 0
  ecritMessage(cpt.toString)
  while(!estPosFinale()){
    if(getY()==6){
      setPos(getX()+1,0)
    }
    else{
      setPos(getX(),getY+1)
    }
    cpt += 1
    ecritMessage(cpt.toString)
  }
}

def estPosFinale():Boolean = {
  if(getX()==getMondeLargeur()-1 && getY()==getMondeHauteur-1){
    return true
  }
  else{
    return false
  }
}