def paintTile(){
  brushDown()
  brushUp()
}


def makeV(){
  forward();forward()
  paintTile()
  forward()
  left()
  forward()
  paintTile()
  backward()
  right()
  forward()
  paintTile()
  forward();forward();left()
}

def makePattern(){
 for(i <- 1 to 4){
    makeV()
  } 
}


override def run() {
 for(i <- 1 to 9){
   makePattern()
 } 
}