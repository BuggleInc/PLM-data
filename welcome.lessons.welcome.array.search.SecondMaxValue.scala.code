def max2Value(nums:Array[Int]): Int = {
  var max1=nums(0)
  
  for (i <- 0 to nums.length-1){
    if (max1<nums(i)){
      max1=nums(i)
    }
  }
    for (i <- 0 to nums.length-1){
      if (nums(i)==max1){
        nums(i)=Integer.MIN_VALUE
      }
    }
  var max2=nums(0)
  
  for (i <- 0 to nums.length-1){
    if (max2<nums(i)){
      max2=nums(i)
    }
  
  return max2
}