def cocktailSort() {
  var nb : Int = getNombreValeurs()
  var bool : Boolean = true
  
  while (bool == true) {
    bool = false
    for (i<- 0 to nb-2) {
     	if (plusPetit(i+1,i)) {
        echange(i,i+1)
        bool = true
      }
    }
    
    for (i<- nb-2 to 0 by -1) {
     	if (plusPetit(i+1,i)) {
        echange(i,i+1)
        bool = true
      }
    }
    
  }
}
  