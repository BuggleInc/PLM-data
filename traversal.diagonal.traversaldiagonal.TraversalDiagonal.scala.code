var diag:Int = 1;
override def run() {
  var x:Int = 0
  var d:Int =diag
		ecritMessage(x)
   while (getMondeHauteur()-1 !=getY() || getMondeLargeur()-1 != getX()){
     if (diag<getMondeHauteur()){
				for (i <- 0 to diag){
 				 x+=1
 				 setPos(i,diag-i)
  				ecritMessage(x)
				}
       diag+=1
       d=diag
     }
     
     else{
       for (i <- 0 to d-1){
         print(i + "   ")
  				x+=1
  				setPos(i+1,d-i)
  				ecritMessage(x)
     	}
       d-=1
     }
     
      
}
}