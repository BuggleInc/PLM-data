def in3050(a:Int, b:Int):Boolean = {
  if ( (in34(a) && in34(b)) || (in45(a) && in45(b)) ) {return true} else {return false}
}  
  
  
  
  
  
def in34(a:Int):Boolean = {
	if ((a>=30)&&(a<=40)) {return true} else {return false}
}
  
def in45(a:Int):Boolean = {
	if ((a>=40)&&(a<=50)) {return true} else {return false}
}
