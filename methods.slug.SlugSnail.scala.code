def hunt(c:Color) {
  while(!estSurBiscuit()){
    baisseBrosse()
  	leveBrosse()
    if (estFaceMur()){
      gauche()
    }
    if (isFacingTrail(c:Color) == true){
  	avance()
  	}else{
    gauche()
  	}
	}
  prendBiscuit()
  baisseBrosse()
}

def isFacingTrail(c:Color):Boolean = {
   if (!estFaceMur()){
   	avance()
    if(getCouleurSol()==c:Color){
      recule()
      return true
    }else{
      recule()
      return false
    }
  }
  return false 
}