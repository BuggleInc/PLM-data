def shellSort()  {
  var ecart = getNombreValeurs()/2
  for(i <-0 to getNombreValeurs()-2){
    ecart = ecart/2
    var tampon = getValeur(i+1)
    var j = 0
    while(i-j*ecart>=0 && !plusPetitQue(i-j*ecart,tampon)){
      copie(i-j*ecart,i-(j+1)*ecart)
      j=j+1
    }
    
    setValeur(i-(j-1)*ecart,tampon)
  	
      
    }
}