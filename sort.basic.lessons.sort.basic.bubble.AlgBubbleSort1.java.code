public void bubbleSort()  {
	int k=1;
	boolean  aucunEchange = false;
	int tab = getNombreValeurs();
	while(!aucunEchange)
	{
		aucunEchange = true;
		for(int i=0; i<tab-1 ; i++)
		{    
			
			if(getValeur(i)>getValeur(i+1))
			{
				echange(i,i+1);
				aucunEchange = false;
				
			}
			
		}
	}
	
	
}
