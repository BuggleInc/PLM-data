override def run() {
  var cpt = 0
  while(!fin()){
    if(getY()<getMondeHauteur()-1){
      setPos(getX(),getY()+1)
      cpt+=1
    }
    else if(getX()<getMondeLargeur()-1){
      setPos(getX()+1,0)
      cpt+=1
    }
    ecritMessage(cpt)
  }
}

def fin():Boolean={
  var posf = false
  if((getX(),getY())==(getMondeLargeur()-1,getMondeHauteur()-1)){
    posf=true
  }
  return posf
}