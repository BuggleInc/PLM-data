def shellSort()  {
	var ecart = getNombreValeurs()/2
	while (ecart >0) {
		for (i <- 1 to getNombreValeurs()-1) {
			var v = getValeur(i)
			var j = i
			while ((j-ecart>0)&&(getValeur(j-ecart)>v)) {
				copie(j-ecart,j)
				j -= ecart
			}
			setValeur(j,v)
		}
		ecart = ecart/2
	}
}


