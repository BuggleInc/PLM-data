def cocktailSort() {
  var sorting:Boolean = true;
  
  while (sorting == true) {
    sorting = false;
    
  	for (i <- 0 to getValueCount()-2 ) {
      
      if (! (isSmaller(i, i+1)))  {
        swap(i, i+1);
        sorting = true;
      }
    }
    
  	for (i <-getValueCount()-2 to 0) {
      
      if (isSmaller(i, i+1))  {
        swap(i, i+1);
        sorting = true;
      }
    }
    
  }
}
