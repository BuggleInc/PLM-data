/**
 * Avance jusqu'à trouver un biscuit et le récupère avant de revenir à son emplacement d'origine et le poser.
 * 
 * Ne détecte pas la présence de murs, ne pas utiliser en présence de murs.
 */
 void goAndGet() {
 	int nbPas=0
 	while (!estSurBiscuit()) {
 		avancer();
 		nbPas = nbPas + 1;
 	} for (; nbPas>0; nbPas=nbPas-1) {
 		recule();
 	} poseBiscuit();
 }
