override def run() {
  var cpt = 0
  var y = 0
  while (finale==false) {
		for (i<-0 to 6) {
      setPos(y,i)
      ecritMessage(cpt)
      cpt = cpt + 1
  	}
    y = y + 1
    setPos(y,0)
    finale()
  }
}

def finale(): Boolean = {
  if (getX==6 && getY==6) {
    return true
  } else {
    return false
  }
}
