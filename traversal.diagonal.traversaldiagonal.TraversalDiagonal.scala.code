var diag = 0;
override def run() {
  var c=0
  ecritMessage(0)
  while(!posFin){
    tp()
    c +=1
    ecritMessage(c)
  }  
}



def posFin(): Boolean = {
  if( getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1){
    return true
  }else{
    return false
  }
}

def tp(){
  if(estFaceMur){
    setPos(0,diag+1)
    diag +=1
  }else{
    setPos(getX()+1,getY()-1)
  }

}