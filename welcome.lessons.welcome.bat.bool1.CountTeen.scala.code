def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  def ado(a:Int):Boolean={
    return (a>=13 && a<=19)
  }
  var nbAdo:Int = 0
  if (ado(a)){
    nbAdo = nbAdo + 1
  }
  if (ado(b)){
    nbAdo = nbAdo + 1
  }
  if (ado(c)){
    nbAdo = nbAdo + 1
  }
  if (ado(d)){
    nbAdo = nbAdo + 1
  }  
  return nbAdo
  }