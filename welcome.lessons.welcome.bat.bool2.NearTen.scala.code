def nearTen(num:Int):Boolean = {
  
  var reste=num
  var ordre=0
  var taille=1
  while (reste>9) {
    reste=reste%10
    ordre=ordre+1
  }
  for (i<- 1 to ordre) taille = 10*taille
  if (taille==1) return (num<=2 || num>=8)
  if (((num%taille)<=2) || ((num%taille)>=8)) return true
  else return false
  
}

