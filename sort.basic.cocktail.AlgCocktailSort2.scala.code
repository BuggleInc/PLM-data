def cocktailSort2() {
  var len:Int = getNombreValeurs()
  var end:Boolean = false
  var debut:Int = 0
  var fin:Int = (len - 2)
  do {
    end = true
    for (i <- (debut) to (fin)) {
      if (plusPetit((i+1),i)) {
        echange(i,(i+1))
        end = false
      }
    }
    fin -= 1
    for (i <- (fin) to (debut) by -1) {
      if (plusPetit((i+1),i)) {
        echange(i,(i+1))
        end = false
      }
    }
    debut += 1
  } while (!end)
}