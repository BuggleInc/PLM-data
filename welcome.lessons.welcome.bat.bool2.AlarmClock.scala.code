def alarmClock(day:Int, vacation:Boolean): String = {
  (day,vacation) match{
    case (day,vacation) if ( day<= 5 && day >= 1 && vacation==false ) => return "7:00"
    case (6,false) => return "10:00"
    case (0,false) => return "10:00"
    case (6,true) => return "off"
    case (day,vacation) if (day<=5 && day>=1 && vacation==true) => return "10:00"
    case (0,true) => return "off"
    case (_,_) => return ""
  }
}
