def solve() {
  val n = getTaille()
  var apresBleu = 0
  var blancRouge = n-1
  var avantBlanc = n-1
  while (!estTrie() && apresBleu < avantBlanc) {
   if (getCouleur(avantBlanc) == BLEU) {
    echange(apresBleu,avantBlanc)
    apresBleu += 1
   } else {
    		if (getCouleur(avantBlanc) == ROUGE) {
     			echange(avantBlanc,blancRouge)
     			avantBlanc -= 1
          blancRouge -= 1
    } else {
     		avantBlanc -= 1
      }
    }
 	}
  echange(avantBlanc,blancRouge)
}

  


