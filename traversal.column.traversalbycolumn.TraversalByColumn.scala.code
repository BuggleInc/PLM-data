override def run() {
  var cpt:Int = 1
  while (!estSurFin) {
    parcours()
    ecritMessage("cpt")
  }
}

def estSurFin():Boolean = {
  if ((getMondeHauteur()-1 == getY)&&(getMondeLargeur-1 == getX)){
    return true
  } else {
    return false
  }
}

def parcours():Unit = {
  if (getY() == getMondeHauteur()-1){
    setPos((getX +1), 0)
  } else {
    setPos(getX, getY + 1)
  }
}