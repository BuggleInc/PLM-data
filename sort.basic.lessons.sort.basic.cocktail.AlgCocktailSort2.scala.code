def cocktailSort2() {
	var fin:Int = getNombreValeurs - 2
	var debut:Int = 0
	var E:Boolean = false
	do {
		E = false
		for(i <- debut to fin) {
			if(plusPetit(i + 1 , i)) {
				echange(i, i + 1)
				fin = fin - 1
				E = true
			}
		}
		for(i <- fin to debut by -1) {
			if(plusPetit(i - 1 , i)) {
				echange(i, i - 1)
				debut = debut + 1
				E = true
			}
		}
	}
	while(E == true)
}
