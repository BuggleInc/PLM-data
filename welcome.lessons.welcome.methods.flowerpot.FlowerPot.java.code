void run{
	shiftRight();
}//end run

void shiftRight{
int steps = 4;

	//go right 4 steps
	for(i = 0; 1 < steps; i++){
		forward();
	}
}//end shiftRight

void shiftUp{
int steps = 4;

	left();
	//go up 4 steps
	for(i = 0; 1 < steps; i++){
		forward();
	}

	right();
}//end shiftUp

void shiftLeft{
int steps = 4;
	//go left 4 steps
	back();
	
	for(i = 0; 1 < steps; i++){
		forward();
	}

	right()
}//end shiftLeft
