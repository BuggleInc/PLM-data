def insertionSort() {
  var len = getNombreValeurs()
  var rgpositif:Boolean = true
  var v = 0
  var rg = 0
  for (i <- 1 to len-1) {
    rg = i-1
    v = getValeur(i)
    rgpositif = rg>=0
    while (rgpositif) {
      if (getValeur(rg)>v) {
      	copie(rg,rg+1)
        rg-=1
      }
      else
        rgpositif = false
    	setValeur(rg,v)
      
    }
	}
}