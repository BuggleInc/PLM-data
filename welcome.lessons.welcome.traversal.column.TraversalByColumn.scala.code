override def run() {
  var cpt:Int = 0
  ecritMessage(0)
  while (!posfin()) {
    cpt = cpt + 1
    if ((getY()==6) && (getX()!=6)) {
      setPos((getX()+1),0)
    }
    setPos(getX(),(getY()+1))
    ecritMessage(cpt)
  }
}

def posfin():Boolean = {
  if ((getX()==6) && (getY()==6)) {
    return true
  } else {
    return false
  }
} 