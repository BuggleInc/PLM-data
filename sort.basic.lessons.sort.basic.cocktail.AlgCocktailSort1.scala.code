def cocktailSort() {
	var swaped = true;
	val length = getNombreValeurs();
	var i = 0;
	do{
		swaped = false;
		for(i <-0 to length-2){
			if(plusPetit(i+1, i)){
				echange(i+1, i);
				swaped = true;
			}
		}
		i = length-2;
		while(i >= 0){
			if(plusPetit(i+1, i)){
				echange(i+1, i);
				swaped = true;
			}
			i = i - 1;
		}
		
	}while(swaped);
}
