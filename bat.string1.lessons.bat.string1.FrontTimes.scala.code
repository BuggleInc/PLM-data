def frontTimes(str:String, n:Int):String = 
{
	var chaine = ""
	var coupe = ""

	str.length match
	{
		case 0 => coupe = ""
		case 1 => coupe = str.substring(0,1)
		case 2 => coupe = str.substring(0,2)
		case _ => coupe = str.substring(0,3)
	}
	
	for(i <- 1 to n)
	{
		chaine = chaine + coupe
	}

	return chaine
}
