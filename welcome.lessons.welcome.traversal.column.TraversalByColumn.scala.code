override def run() {
  var posfinale = false
  while(!posfinale){
    if (setPos(getX(),getY()) == setPos(getMondeLargeur()-1, getMondeHauteur()-1)){
      posfinale=true}
    move()
  }
}



def move(){
  var raw = 0
  for (i <- 0 to getMondeLargeur()-1){
    if (raw!=0){
      setPos(getX()+1, getY())}
    for(j <- 0 to getMondeHauteur()-1){
      setPos(getX(), getY()+1)
      raw=raw+1
    }
  }
}