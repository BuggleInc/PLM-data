def greenTicket(a:Int, b:Int, c:Int):Int = {(a,b,c) match {
  																							case (_,a,b) => return 20
  																							case (_,_,a) => return 10
                                                case (_,a,_) => return 10
  																							case (_,_,b) => return 10
  																							case (_,_,_) => 0}
}