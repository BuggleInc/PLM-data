def cocktailSort3() {
  var fin=getNombreValeurs()-2
  var stop=false
  var debut=0
  
  while(stop!=true){
    stop=true
    for (i<- debut to fin){
      if (plusPetit(i+1,i)){
          echange(i,i+1)
        stop=false
      }
    }
    if (stop==true){
      return
    }else{
      fin-=1

      for(i<- fin to debut by -1){
        if (plusPetit(i+1,i)){
            echange(i,1+i)
          stop=false
        }
      }
    }
    debut+=1
  }
}