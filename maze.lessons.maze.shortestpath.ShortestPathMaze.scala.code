override def run() {
  compteur();// Your code here
}
def compteur():(Int, Int) = {
	var x:Int = 0;
	var y:Int = 0;
	var i:Int = 0;
	while(!aBiscuit(x, y) {
		if (x < getMondeLargeur() -1) {
			x = x+1;
		}
		else {
			y = y+1;
			x = 0;
		}
	}
	ecrit(x, y, i);
}
def ecrit (x:Int, y:Int, i:Int) = {
	if (getIndication(x,y) != 9999) {
		setIndication(x,y,i)
	}
	
}
