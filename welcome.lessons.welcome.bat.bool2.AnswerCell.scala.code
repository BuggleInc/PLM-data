def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
  if (isAsleep == true) {
    return false
  }
  else {
    if (isMorning == false) {
      return true
    }
    else {
      if (isMom == true) {
        return true
      }
      else {
        return false
      }
    }
  }
}