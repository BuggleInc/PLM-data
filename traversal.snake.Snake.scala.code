def finalePosition(): Boolean{
 	var res: Boolean = false
  
  res = isFacingWall()
  left()
  res = res && (getDirection() == NORTH && isFacingWall())
  left()
  
  return res
}

def snakeMove(){
  if(!isFacingWall()){
    forward()
  } else if(getDirection() == Direction.EAST){
    left();forward();left()
  } else {
    right();forward();right()
  }
}


override def run() {
  while(!finalePosition()){
    snakeMove()
  }
}