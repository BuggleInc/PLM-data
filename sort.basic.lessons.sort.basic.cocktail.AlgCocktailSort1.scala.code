def cocktailSort() {
  var tri = true
	while (tri) {
    tri = false
  	for (i<- 0 to getNombreValeurs()-2) {
    	if (!(plusPetit(i,i+1))) {
      	echange(i,i+1)
   			tri = true
      }
    }
  	for (j <- 1 to getNombreValeurs()-1) {
    	if (plusPetit(getNombreValeurs()-j,getNombreValeurs()-j-1)) {
      	echange(getNombreValeurs()-j-1,getNombreValeurs()-j)
				tri = true
      }
  	}
  }
}
