def isFacingTrail():Boolean = {
  var biscuit = 0
  while (biscuit==0){
    forward()
    if (estFaceMur()){
      return false
    }
    if (getGroundColor()==Color.green){
      if (estSurBiscuit()){
        prendBiscuit()
        biscuit = biscuit + 1
      }
      baisseBrosse()
      leveBrosse()
    }
    else {
      backward()
      left()
    }
  }
  return true
}