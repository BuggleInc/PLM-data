
def PositionFinale():Boolean={
  if (estFaceMur()){
    if (estFaceMur()){
      return true
    } else {
      return false
    }
  }
  return false
}

def PasSerpent(){
  if (!estFaceMur){
    avance()
  }else if (getDirection()==Direction.OUEST){
    droite()
    avance()
    droite()
  } else {
    gauche()
    avance()
    gauche()
  }
}

override def run() {
  baisseBrosse()
  while(PositionFinale()==false){
    PasSerpent()
  }
}

