def cocktailSort2()  {
  var debut = 0
  var fin = getNombreValeurs -2
  var nbEchanges = 1
  while(nbEchanges!=0) {
    nbEchanges=0
    for(i<- debut to fin) {
      if(plusPetit(i+1, i)) {
        echange(i,i+1)
        nbEchanges=nbEchanges+1
      }
    }
    fin=fin-1
    for(i<- fin to debut by -1) {
      if(plusPetit(i+1, i)) {
        echange(i,i+1)
        nbEchanges=nbEchanges+1
      }
    }
    debut=debut+1
  }
}