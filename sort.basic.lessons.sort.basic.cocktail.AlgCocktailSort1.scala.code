def cocktailSort() {
  var compteur:Int=0
  do { 
    for (i<-getNombreValeurs-2 to 0 by -1) {    
    if (plusPetit(i+1,i)) {      
      echange(i+1,i)
      compteur +=1}}
    for (i<- 0 to getNombreValeurs()-2) {
    if (plusPetit(i+1,i)) {
      echange(i,i+1)
      compteur+=1}}}
  while (compteur!=0)
}