def bubbleSort()  {
  var echanges:Boolean=true;

  while(echanges==true){
    echanges=false
    for(i<-0 to getValueCount()-2){
    	if (isSmaller(i+1,i)){
        echanges=true
      	swap(i,i+1)
    	}
  	}
	}
}
