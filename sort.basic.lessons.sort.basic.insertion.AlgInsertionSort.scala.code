def insertionSort() {
  var v = 0
  var c = -1
    for (i <- 1 to getNombreValeurs()-1) {
      c = -1
      v = getValeur(i)
      for (k <- 1 to i) {
        if (plusPetit(v, i-k)) {
          echange(i-k, i-k+1);
          c = i-k
        }
      }
      if (c != -1) {
        setValeur(c, v)
      }
    }
}