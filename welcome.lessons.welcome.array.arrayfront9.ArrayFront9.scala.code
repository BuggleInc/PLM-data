def arrayFront9(nums:Array[Int]): Boolean = 
{
  var canard = false
  if (nums.length < 4) 
  {
    for (i <- 1 to nums.length) 
    {
      if (nums(i-1) == 9) 
      {
        canard = true
      }
    }
  } else 
  {
    for (i <- 1 to 4) 
    {
      if (nums(i-1) == 9) 
      {
        canard = true
      }
    }
  }
}