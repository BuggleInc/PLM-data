def combSort() {
	var ecart  = getNombreValeurs() ; 
	var i = 0;
	var changed = true ; 
	do{
		changed = false ; 
		if (ecart > 1 ) {
			 ecart = (ecart.toDouble / 1.3).toInt;
		}
		while (i+ecart < getNombreValeurs()){
			if(plusPetit(i+ecart,i)){
					echange(i+ecart,i);
					changed = true ;
					
			}
			i +=1;
		}
	}while (ecart > 1 || changed )
}
