def isTeen(a:Int): Boolean = {
	return (a >= 13 && a <= 19)  
}

def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var teenCount:Int = 0
  
  if (isTeen(a)) {
    teenCount += 1
  }
  
  if (isTeen(b)) {
    teenCount += 1
  }
  
  if (isTeen(c)) {
    teenCount += 1
  }
  
  if (isTeen(d)) {
    teenCount += 1
  }
  
  return teenCount
}