def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
var nb:Int = 0;
if (a >= 13 && a <= 19) {
	(nb = nb + 1);
	return nb;
}
if (b >= 13 && b <= 19) {
	(nb = nb + 1);
	return nb;
}
if (c >= 13 && c <= 19) {
	(nb = nb + 1);
	return nb;
}
if (d >= 13 && d <= 19) {
	(nb = nb + 1);
	return nb;
}
return nb;
}
