def isWallLeft():
    left()
    if isFacingWall():
        right()
        return True
    else:
        right()
        return False
def keepHandOnSideWall():
    isWallLeft()
    if isWallLeft():
        if not isFacingWall():
            forward()
        else:
            left()
    else:
        left()
        forward()
while not isOverBaggle():
    keepHandOnSideWall()