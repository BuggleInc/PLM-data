def cocktailSort2() {
  var debut=2
  var fin=getNombreValeurs()-2
  var test=false
  while (!test){
    test = true
    for (i <- debut to fin){
      if(plusPetit(i+1,i)){
        test = false
        echange(i,i+1)
      }
      
    }
    fin -=1
    var i=fin
    while(i!=debut){
      i-=1
      if (plusPetit(i,i-1)){
        test = false
        echange(i-1,i)
    }
    }
    debut+=1
  }
}