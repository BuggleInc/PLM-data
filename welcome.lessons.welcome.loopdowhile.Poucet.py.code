premiereFois=True
nbBiscuit=0
while (sortieTrouvee())==False:
    while premiereFois==True or (croisement())==False:
        if estSurBiscuit():
            nbBiscuit=nbBiscuit+1
        forward()
        premiereFois=False
    if estSurBiscuit():
        nbBiscuit=nbBiscuit+1
    if (nbBiscuit>=3):
        gauche()
        nbBiscuit=0
        forward()
    else:
        droite()
        nbBiscuit=0
        forward()
