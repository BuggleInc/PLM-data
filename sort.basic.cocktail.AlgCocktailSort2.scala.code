def cocktailSort2() {
  var a = 0
  var b = getValueCount() - 2
  var t = false
  do {
  t = true
  for (i <- a to b) {
    if (isSmaller(i+1,i)){
      
      t = false
      swap(i,i+1)}
    
  } 
    b-=1
  for (i <- b to a-1)	{
		if (isSmaller(i,i-1)){
      t = false
      swap(i,i+1)
    }
    
  }
  a+=1
    


    
  
} while (t == false)
}