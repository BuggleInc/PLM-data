def cocktailSort() {
  var nbValeurs=0
  var fin = 0
  var test = false
  nbValeurs=getNombreValeurs()
  do {
     fin=0
     for (i<- 0, nbValeurs -2){
       test = plusPetit(i,i+1)
       if(test){
         echange(i,i+1)
         fin =fin +1
       }
     }
      for(i<- nbValeurs -2,0){
         test = plusPetit(i,i+1)
         if(test){
           echange(i,i+1)
           fin = fin +1
         }  
      }
   } while(fin !=0)
}