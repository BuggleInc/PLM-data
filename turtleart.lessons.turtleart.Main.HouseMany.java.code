public void run() {
  shapeRun();

  setPos(50, getY());
  setPos(50, 175);
  shape();
  setPos(200, 175);
  shape();
  setPos(50, 100);
  shapeRun();
  setPos(50, 25);
}

void shapeRun(){
  for(int i = 0; i < 4; i++){
    shape();
    setPos(getX()+20, getY());
  }	
}
public void shape(){
	forward(30);
	right(30);
	forward(30);
	right(120);
	forward(30);
	right(120);
	forward(30);
	backward(30);
	left(90);
	forward(30);
	right(90);
	forward(30);
	backward(30);
	right(90);
}
