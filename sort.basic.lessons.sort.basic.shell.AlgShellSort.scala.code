def shellSort()  {
var ecart = getNombreValeurs()
ecart = (ecart.asInstanceOf[Double] / 2).asInstanceOf[Int]
while (ecart>0) {
  var v = 0
  var k = 1
    for (i <- 1 to getNombreValeurs()-1) {
      v = getValeur(i);
      k = i
      while ( (k>ecart-1)&&(!plusPetitQue((k-ecart), v)) ) {
        setValeur(k, getValeur(k-ecart));
        k=k-ecart
      }
      setValeur(k,v)
    }
  ecart = (ecart.asInstanceOf[Double] / 2).asInstanceOf[Int]
}
}

