def combSort() {
  var test=true
  var lgr = getNombreValeurs()
  var ecart = lgr
  do {
    test=false
  	if (ecart>1){
    	ecart =(ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
  	}
  	var i = 0
  	while ((i+ecart)<lgr){
    	if (plusPetit(i,i+ecart)){
      	echange(i+ecart,i)
      	i+=1
        test=true
    	}
  	}
  } while ((ecart>1)||(test))
}