
override def run() {
  var diag = 0
  var count = 0
  while (PositionFinale()==false){
    if (getX()==diag && getY()==0){
      setX(0);setY(getY()+1)
      diag=diag+1
      
    }
    else{
      setX(getX()+1);setY(getY()+1)
    }
    ecritMessage(count)
    count=count+1
  }
}


def PositionFinale() : Boolean = {
  var H=getMondeHauteur()
  var L=getMondeLargeur()
  if (getX()+1==L && getY()+1==H){
    return true
  }
  return false
}