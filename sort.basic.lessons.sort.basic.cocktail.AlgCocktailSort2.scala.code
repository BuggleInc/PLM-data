

def cocktailSort2() {
  var taille=getNombreValeurs()
  var trier=false
  var debut = 0
  var fin = taille-2
  while(trier==false){
    trier=true
    for(j<- debut to fin){
      if(plusPetit(j+1,j)==true){
        echange(j+1,j)
        trier=false
      }
      
    }
    fin = fin-1
    for (i<-fin to debut by -1){
      if ( plusPetit(i+1,i)==true){
        echange(i+1,i)
        trier=false
      }
    }
    debut=debut+1
  }
}



