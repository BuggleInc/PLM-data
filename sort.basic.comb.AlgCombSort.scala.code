def combSort() {
  var echanger = true
  var len = getNombreValeurs()
  var ecart = getNombreValeurs()
  while(ecart>1 | echanger = true) {
    	if (ecart>1) {
      	ecart = (ecart.toDouble / 1.3).toInt
    	};var i = 0
      while(i+ecart<len) {
    	if (plusPetit(i+ecart,i)) {
      	echange(i+ecart,i)
      	echanger = true
    	}
  	}
}
}