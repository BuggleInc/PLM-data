def lessBy10(a, b, c):
    if (0<=a<=10 and 0<=b<=10 and (c>10 or c<0)) or (0<=b<=10 and 0<=c<=10 and (a>10 or a<0)) or (0<=a<=10 and 0<=c<=10 and (b>10 or b<0)):
        if (a==10 or b==10 or c==10) and (a>1 or b>1 or c>1):
            return False
        else:
            return True
    else:
        return False