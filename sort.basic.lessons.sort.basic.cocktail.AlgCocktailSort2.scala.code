def cocktailSort2() {
	var flag = true;
	var debut = 0;
	var fin = getNombreValeurs()-2;
	do {
		flag = true;
		for (i <- (debut to fin)) {
				if (!plusPetit(i, i+1)) {
					echange(i,i+1);
					flag = false;
					fin -= 1;
				}
		}
		for (i <- (fin to debut by -1)) {
			if (!plusPetit(i, i+1)) {
					echange(i,i+1);
					flag = false;
					debut += 1;
				}
		}
	}
	while(!flag);
}
