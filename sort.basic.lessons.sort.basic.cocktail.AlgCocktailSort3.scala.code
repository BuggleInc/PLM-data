def cocktailSort3() {
	var b=true;
	var a=true;
	val l=getNombreValeurs() ;
	var debut=0;
	var fin = l-2;
	var i=0;
	while (b&&b) {
		b=false;
		a=false;
		while (i<fin) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				b=true;
			}
			i+=1;
		}
		fin-=1;
		while ((i>debut)&&b) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);
				a=true;
			}
