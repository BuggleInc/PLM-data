override def run() {
  var hauteur=getMondeHauteur()
  var largeur=getMondeLargeur()
  var compteur=0
  for (i<-0 to hauteur){
        for (j<-0 to largeur-1){
          if (i==hauteur-1){
          	setPos(j,largeur-i)
          	ecritMessage(compteur)
          	compteur+=1
          }else{
            if (j==0){
          		for (j<-0 to largeur-1){
          			setPos(j,i)
          			ecritMessage(compteur)
          			compteur+=1
          		}
          	}
        	}
        }
  }
}

