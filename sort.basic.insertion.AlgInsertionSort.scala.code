def insertionSort() {
  var x:Int=getNombreValeurs()
  var v=0
  var y=0
  var t=0
  for (i<-1 to x) {
    y=i
    t=i
    v=getValeur(i)
    for (j<-i-1 to 0 by -1) {
      if (plusPetit(j,t)==false) {  
        setValeur(i,getValeur(j))
        t=t-1
        y=j
      }
    }
    setValeur(y,v)
   }
}