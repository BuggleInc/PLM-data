override def run() {
  while (fini()==false){
    serpute()
  }
}

def fini(){
  if (estFaceMur()==true){
    gauche()
    if (estFaceMur()==true){
    return true
    }
  }
  droite()
  return false
}

def serpute(){
  while (estFaceMur()==false){
    avance()
  }
  if (getDirection()==Direction.EST){
    gauche()
    avance()
    gauche()
  }
  if (getDirection()==Direction.OUEST){
    droite()
    avance()
    droite()
  }
}