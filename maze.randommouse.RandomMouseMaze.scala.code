override def run() 
{
  while(!porteBiscuit())
  	{
  		var dir=random3()
      if (dir==0 && !estFaceMur())
      	{
          avance()
        }
      else if(dir==1)
      	{
          droite()
        }
      else
      	{
          gauche()
        }
      if (estSurBiscuit())
				{
          prendBiscuit()
        }      
    }
  
}