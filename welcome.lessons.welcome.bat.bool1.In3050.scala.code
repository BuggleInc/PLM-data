def in3050(a:Int, b:Int):Boolean = {
	var apA3040:Boolean = (a>=30 && a <=40)
	var apB3040:Boolean = (b>=30 && b <=40)
	var apA4050:Boolean = (a>=40 && a <=50)
	var apB4050:Boolean = (b>=40 && b <=50)

	return (apA3040 && apB3040) || (apA4050 && apB4050)
}

