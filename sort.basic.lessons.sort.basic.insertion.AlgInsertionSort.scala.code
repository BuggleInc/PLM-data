def insertionSort() {
  var lgr=getNombreValeurs()
  var v=0
  var i=0
  
  
  for (compteur<-1 to lgr-1){
    
    i=compteur
    
    v=getValeur(i)
    
    while (i>1){
    	
      if (plusPetit(v,getValeur(i-1))==true){
      	
        echange(i-1,i)
    	}
      
      i=i-1
     
    }
   	 setValeur(i,v)
  }
}