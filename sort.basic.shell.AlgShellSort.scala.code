def shellSort() {
  val lgr=getNombreValeurs()
  var dernierecase:Boolean=true
  var i:Int=0
  var v:Int=0
  var j:Int=0
  var ecart:Int = (lgr.toDouble / 2).toInt
 	
  
  
  
  
  for (i <- 1 to lgr-1){
    dernierecase=true
    v = getValeur(i)
    j = 1
    
    
    while (dernierecase){
      if (i-j>=0&&getValeur(i-j)>=v){
        copie(i-j,i+1-j)
        j+=1
      }
      else{
        dernierecase=false
        setValeur(i-j+1, v)
      }
      }
    }
}