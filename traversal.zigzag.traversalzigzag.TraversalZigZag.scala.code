override def run() {
  var cpt: Int = 0
  ecritMessage(cpt);
  while (getMondeHauteur()-1 != getY() || getMondeLargeur()-1 != getX()) {
    cpt += 1;
    if (getX() != getMondeLargeur()-1) {
      setPos(getX()+1,getY()); ecritMessage(cpt);
    } else if (getX() == getMondeLargeur()-1) {
      setPos(getX(),getY()-1); ecritMessage(cpt);
    }
  }
}