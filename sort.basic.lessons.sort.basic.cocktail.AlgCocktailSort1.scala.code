def cocktailSort() {
	var f = false
	while(f == false){
		for(i<- 0 to getNombreValeurs - 2){
			f = true
			for(j <- i to getNombreValeurs - 2){
				if(getValeur(j)>getValeur(j+1)){echange(j,j+1); f = false}
			}
		}
		for(i<- getNombreValeurs - 2 to 0 by -1){
			f = true
			for(j <- 0 to i){
				if(getValeur(j)>getValeur(j+1)){echange(j,j+1); f = false}
			}
		}
	}
}
