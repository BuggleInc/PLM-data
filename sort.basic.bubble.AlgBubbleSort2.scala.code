def bubbleSort2()  {
  var listeTriee:Boolean=true
  var x=0
  x=getNombreValeurs
  do {
    listeTriee=true
    for (i <- x-2 to 0) {
      for (j <- 0 to i) {
         if (plusPetit(j+1,j) & i!=0) {
           echange(j+1,j)
       }
      }
      if (i==0) {
        listeTriee=false
      }
    }
  } while (listeTriee)
}