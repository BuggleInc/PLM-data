$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class stringTimes extends BatEntity {  public void run(BatTest t) {
		t.setResult( stringTimes((String)t.getParameter(0), (Integer)t.getParameter(1)) ); }
	String stringTimes(String str, int n) {
		String result = "";
		for (int i=0; i<n; i++) {
			result = result + str;  
		}
		return result;
	}
 }