def cocktailSort2() {
  var boole=false
  var debut=0
  var fin=getValueCount()-2
  do{
    boole=false
  	for (i <- debut to fin){
    	if(isSmaller(i+1,i)){
      	swap(i,i+1)
      	boole=true
        
      } 
      
    }
    fin=fin-1
    for (i <- fin to debut by (-1)){
    	if(isSmaller(i+1,i)){
      	swap(i,i+1)
      	boole=true
        
        
      }
      
    }
    debut=debut+1 
   }
  while(boole)
}