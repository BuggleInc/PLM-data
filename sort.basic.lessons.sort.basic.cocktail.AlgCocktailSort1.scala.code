def cocktailSort() {
var a=true
var b=true
    if (a==false && b==false){
      return
    }
a=false
b=false
  for (j <- 0 to getValueCount()-2){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true
		}
}
	for (j <- getValueCount()-2 to 0 by -1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      b=true
		}
}    
}
