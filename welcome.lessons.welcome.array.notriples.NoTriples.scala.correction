$package
import plm.universe.bat.{BatEntity,BatWorld,BatTest}; 
import plm.universe.World; 
class noTriples extends BatEntity { 
   override def run(t: BatTest) {
   t.setResult( noTriples( t.getParameter(0).asInstanceOf[Array[Int]]) )
   }
def noTriples(nums:Array[Int]): Boolean = {
  var count=0
  for (i <- 0 to nums.length-3)
    if ( (nums(i) == nums(i+1)) && (nums(i+1) == nums(i+2)) )
      return false
  return true
} }