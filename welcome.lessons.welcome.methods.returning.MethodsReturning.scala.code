def haveBaggle():Boolean = {
  var vuBiscuit=false
  while (vuBiscuit==false){
    for (i <- 0 to 5){
      if (estSurBiscuit()==true){
        vuBiscuit=true
      }
      avance()
    }
    if (vuBiscuit==false){
      for (i <- 0 to 5){
        recule()
      }
      droite()
      avance()
      gauche()
    }
    if (vuBiscuit==true){
      for (i <- 0 to 5){
        recule()
      }
    }
  }
  return vuBiscuit
}
