	var i = 0 ;
	var j = 0 ; 
	var changed = true ; 
	
		
		for ( i <- (getNombreValeurs()-2 to 0 by -1)){
			while (changed){
				for (j<- 0 to i){
					changed = false ; 
					if(plusPetit(j+1,j)){
						echange(j,j+1);
						changed = true ; 
					}
					
				}
			}
				
		}
		
	
}
