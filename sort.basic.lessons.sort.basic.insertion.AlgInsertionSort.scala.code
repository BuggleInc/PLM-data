def insertionSort() {
  var lgr=getNombreValeurs()
  var v=0
  var i=0
  var changement=
  
  for (compteur<-1 to lgr-1){
    
    i=compteur
    
    v=getValeur(i)
    
    while (i>0 && plusPetit(v,getValeur(i-1))==true){
    	
      echange(i-1,i)
      i=i-1
    }
    
      
    setValeur(i,v)
  }
}