def blueTicket(a:Int, b:Int, c:Int):Int = {
  var ab = a+b
  var bc = b+c
  var ac = a+c
  (ab,bc,ac) match {
    case (ab,bc,ac) if ab == 10 => 10
    case (ab,bc,ac) if bc == 10 => 10
    case (ab,bc,ac) if ac == 10 => 10
    case (ab,bc,ac) if ab == bc + 10 => 5
    case (ab,bc,ac) if ab == ac + 10 => 5
    case (_,_,_) => 0 
  }
}