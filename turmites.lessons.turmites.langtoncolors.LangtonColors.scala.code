var i=0
def step(rule:Array[Char], colors:Array[Color]) {
	
	while (getCouleurSol!=colors(i)){
		i=i+1
	}
	if(rule(i)=="L"){
		if(i==colors.length){
		setCouleurBrosse(colors(0))
		}
		else{
		setCouleurBrosse(colors(i+1))	
		}
		baisseBrosse
		leveBrosse
		gauche
		avance
	}
	else{
		if(i==colors.length){
		setCouleurBrosse(colors(0))
		}
		else{
		setCouleurBrosse(colors(i+1))	
		}
		baisseBrosse
		leveBrosse
		droite
		avance	
	}
	
}


