def fini():Boolean = {
	val n = getNombreValeurs()
	var j = 0
	for(i<-0 to n-2){
		if(plusPetit(i,i+1) == true){
			j = j+1;
		} else {}
	}
	if (j == n-1){
		return true;
	}
	return false;	
}


def bubbleSort()  {
	val n = getNombreValeurs()
	var échangé = true
	while (échangé){
		échangé = false
		for(i<-0 to n-2){
			if(plusPetit(i,i+1) == false){
				echange(i,i+1)
				échangé = true
			} 
		}
	}
}
