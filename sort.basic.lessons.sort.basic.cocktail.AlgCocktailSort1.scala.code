def cocktailSort() {
var test:Boolean = false;
var test2:Boolean = false;
	val longeur:Int = getNombreValeurs();
	do {
		test = false;
		for (i <- 0 to longeur-2){
			if (!plusPetit(i,i+1)){
				echange(i,i+1);
				test = true;
			} 
		}
		test2 = false;
		for (j <- longeur -2 to 0){
			if (!plusPetit(j,j+1)){
				echange(j,j+1);
				test2 = true;
			} 
		}
	} while (test || test2)
}



	