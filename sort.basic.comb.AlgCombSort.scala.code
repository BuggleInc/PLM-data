var condition = false
def combSort() {
  var ecart = getNombreValeurs()
  do{
    condition=false
    if(ecart>1)
      ecart = (ecart.toDouble / 1.3).toInt
    var i=0
    while(i+ecart<getNombreValeurs()){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        i=i+1
        condition=true
    }
    }
  }while(ecart>1 || condition)
}