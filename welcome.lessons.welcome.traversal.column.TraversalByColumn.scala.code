override def run() {
var c:Int=0


	do {
		for (y<-0 to getMondeHauteur()-1)
		{
			setPos(0,y)
				ecritMessage(c)
				c=c+1
		}
		for (x<-1 to  getMondeHauteur()-1)
		{
			for (y<-0 to getMondeHauteur()-1)
			{
				setPos(x,y)
				ecritMessage(c)
				c=c+1
			}
			setPos(x,0)
		}
		setPos(getMondeHauteur()-1,getMondeHauteur()-1)
		}
	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-2
		)
}
