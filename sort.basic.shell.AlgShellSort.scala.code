def shellSort()  {
  


var ecart = (getNombreValeurs().toDouble / 2.0).toInt
while (ecart > 0)
  {for (i<- 1 to getNombreValeurs()-1)
      {
       var j = i-ecart
       var v = getValeur(i)
       
       while ((j>0)&&(!plusPetitQue(j,v)))
           
          {copie(j,j+ecart); j=j-ecart}
       
        setValeur(j, v) }}
    ecart = (ecart.toDouble / 2.0).toInt
  
  
  }