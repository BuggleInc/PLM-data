override def run() {
  var sommeAngle:Int=0
  setDirection(Direction.NORTH)  
  while(!estSurBiscuit()){
    while (!estFaceMur() && getDirection()==Direction.NORTH){
      avance()
    }
    do {  
      if (estFaceMur()){
        droite()
        sommeAngle=sommeAngle-1
      }
      else {
        avance();gauche()
        while (!estFaceMur()){
          avance();gauche()
          sommeAngle=sommeAngle+1
        }
        droite()}
    } while(sommeAngle!=0 && !isDirectionFree(Direction.NORTH))  
  }
  prendBiscuit()
}



  def isDirectionFree(dir:Direction):Boolean ={
    var d:Direction=getDirection()
    setDirection(dir)
    if(estFaceMur()){
      setDirection(d)
      return false
    }
    else {
      setDirection(d)
      return true
    }
  }