def paint():
    brushDown()
    brushUp()

def move_and_check():
    move_forward()
    if getGroundColor() == Color.green:
        paint()
    else:
        backward()
    
def hunt():
    while not isOverBaggle():
        if not isFacingWall():
            check_square()
        else:
            right()
            check_square()