override def run() {
  var i=0
  var x=0
  var y=1
  retourne()
  ecritMessage(i)
  setPos(x,y)
  while (!posFinale()) {
    if (!estFaceMur()) {
    	ecritMessage(i)
    	if (y<getMondeHauteur()) {y+=1;setPos(x,y)}      
    }
		else if (!posFinale() && estFaceMur()) {x+=1;setPos(x,y)}
    i+=1
  }
  
  def posFinale() : Boolean = {
    if (estFaceMur()) {
      gauche()
      if (estFaceMur()) {droite; return true}
      else {droite();return false}
  	}
    else {return false}
  }
}