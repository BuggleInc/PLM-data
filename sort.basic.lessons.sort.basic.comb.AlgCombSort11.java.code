public void combSort11() {

	int lgr = getNombreValeurs();
	int ecart = lgr;
	boolean inversion = true;
	int i=0;
	do
	{
		inversion = false;
		
		if(ecart>1)
		{
			ecart = (int) (ecart / 1.3) ;
			if(ecart == 9 | ecart == 10)
			{
				ecart = 11;
				
			}
		}
		i=0;
		while(i+ecart  < lgr)
			{
				
				if(getValeur(i)>getValeur(i+ecart))
				{
					echange(i,i+ecart);
					inversion = true;
				}
				i++;
						
				
			}
		
	}while(inversion);
}
