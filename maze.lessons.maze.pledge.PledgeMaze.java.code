public void run() {
	escapeTheMaze();
}

public void escapeTheMaze(){

	do{	
		//go north
		while(!isFacingWall() && getDirection() == Direction.NORTH){
			forward();
		}

		if(isFacingWall()){
			right();
		}else{
			forward();
			if(!isFacingWall()) { left(); }			
		}

		//pick up baggle
		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
		
	}while(!isCarryingBaggle());
}//end escapeTheMaze
