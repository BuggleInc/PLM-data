def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (isBirthday==true){
    speed match{
      case speed if speed < 66 => return 0
      case speed if speed < 86 => return 1
      case _ => return 2
    }}
  else {speed match{
    case speed if speed < 61 => return 0
    case speed if speed < 81 => return 1
    case _  => return 2
  }}}
