override def run() {
  while (!posFinale()){
    parcours()
  }
}

def posFinale():Boolean={
  val l=getMondeLargeur()
  val h=getMondeHauteur()
  
  return ((getX()==l)&&(getY()==h))
}

def parcours()={
  val h=getMondeHauteur()
  if (getY()==h)){
    setPos((getX()+1),0)
  }
  else{
    setPos(getX(),getY()+1)
  }
  
}