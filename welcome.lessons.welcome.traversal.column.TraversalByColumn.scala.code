override def run() {
  var cpt=0
	var i=0
while (i!=getMondeLargeur()-1){
  while(!estDosMur()){
    ecritMessage(cpt)
    setPos(i,(cpt+1)%getMondeHauteur())
    cpt=cpt+1
  }
 	ecritMessage(cpt)
  if (i<=6)i=i+1
  setPos(i,0)
  cpt=cpt+1
}
}





