def shellSort()  {
  var longueur = getValueCount()
  var temp = 0
  var k = 0
	var ecart = (longueur/2).toInt
  
  while(ecart > 0)
  {
    for(i<- 1 to longueur-1)
    {
      temp = getValue(i)
      k = i-ecart
      while(k>=0 && temp<getValue(k))
      {
        copy(k,k+ecart)
        k-=ecart

      }
      setValue(k+ecart,temp)
    }
    ecart = (ecart.toDouble/2).toInt
  }
  
  
}