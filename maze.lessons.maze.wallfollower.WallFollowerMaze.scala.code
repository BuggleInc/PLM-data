override def run() 
{

def keepHandOnSideWall()
{
	while(!estSurBiscuit())
	{
		if (estFaceMur())
		{
			gauche()
			if (estFaceMur() && !estDosMur())
			{
				droite()
			} 
			else if (estFaceMur() && estDosMur())
			{
				gauche()
				gauche()
			}
			else
			{
				avance()
			}
		} else avance()
	}
}

keepHandOnSideWall()

}
