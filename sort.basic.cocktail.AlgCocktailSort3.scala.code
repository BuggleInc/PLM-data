def cocktailSort3() {
  
  var debut = 0;
  var fin = getNombreValeurs()-2;
  var i = 0;
  var a = 0;
  
    
  for (i <- debut to fin){
    if (plusPetit(i, i+1) == false) {
      echange(i, i+1);
      a = 1
    }
    
  }
  fin -= 1
  if ( a  == 0) {
      return
  }
  a =0

  for (i <- fin to debut by -1) {

    if (plusPetit(i, i+1) == false) {
      echange(i, i+1);
      a = 1

    }
    
  }
  debut +=1
  
  if ( a  == 0) {
      return
  }

  
}