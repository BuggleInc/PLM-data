def pentaKoch(levels:Int, length:Double) {
	if (levels==0) {
			avance(length)
		
		} else {
			squareSide(levels-1, length/3)
			left(90)
			squareSide(levels-1, length/3)
			right(90)
			squareSide(levels-1, length/3)
			right(90)
			squareSide(levels-1, length/3)
			left(90)
			squareSide(levels-1, length/3)
		}
}
