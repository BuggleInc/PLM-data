def combSort() {
  val lgr:Int = getNombreValeurs()
  var ecart:Int = lgr
  var changement = true
  var i:Int = 0
  while (ecart>1 || changement){
    changement = false
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt}
    if (ecart==9||ecart==10){ecart=11}
    i = 0
    while (i+ecart < lgr){
      if (plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        changement = true
      }
      i += 1
    }
    
  }
}