def solve() {
  var n = getStackSize()
  
  while(n>0){
    
    var max = getPancakeRadius(0)
    var maxId = 0
    for(it <- 1 to n-1){
      if(max<getPancakeRadius(it)){
        max=getPancakeRadius(it)
        maxId=it
      }
    }
    flip(maxId+1)
    if(!isPancakeUpsideDown(1))
    	flip(1)
    flip(n)
    n-=1
    
  }
  
}