override def run() {
  while(!isOverBaggle()){
      keepHandOnSideWall()
    forward()
}

def keepHandOnSideWall(){
	while(isFacingWall()){
    right()
  }
  forward()
  
  left()
  if(isFacingWall()){
    right()
    forward()
  }
  
  else{forward()
       left()
       if(isFacingWall(){
         right()
         forward()
         
       }
          else{right()
              forward()}
     	}
  

}
}