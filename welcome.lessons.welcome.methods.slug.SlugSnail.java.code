 public void hunt(Color c) {
 	while(!isOverBaggle()){
 		if(isFacingTrail()){
 			forward();
 		}else{
 			left();
 		}
 	}

 	pickUpBaggle();
     
 }

boolean isFacingTrail() {
boolean greenCell = false;

	if(isFacingWall()){
		return false;
	}

	forward();
	boolean res = getGroundColor().equals(Color.PINK); 
	backward();
	
	return res;
}//end isFacingTrail()

