override def run() {
  var x = getX()
  var y = getY()
  var num = 0
  do{
    ecritMessage(num)
    if(!estFaceMur && y < getMondeHauteur()){
      y = y+1
      setPos(x,y)
      num = num+1
    }else {
      if(x<getMondeLargeur()){
         x = x +1
      	y = 0
      	setPos(x,y)
      	num = num +1
      }
    }
  }while(!posFinale(x,y))
  
}

def posFinale(x:Int,y:Int): Boolean ={
    return ((getX() == getMondeLargeur()) && (getY()== getMondeHauteur()))
}