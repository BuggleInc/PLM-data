public void tree(int steps, double length, double angle, double shrink)  {
	if(steps==0){tree(steps-1,length*shrink,-angle,shrink);}
  else
    {
    current(steps);
    avance(length);
    droite(angle);
    tree(steps-1,length*shrink,angle,shrink);
    tree(steps-1,length*shrink,-angle,shrink);
    gauche(angle);
    recule(length);
  }
}