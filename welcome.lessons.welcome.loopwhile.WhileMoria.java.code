public void run() {
	setDirection(Direction.EAST);
	while (isOverBaggle() == false) {
		forward(); {
			while (isOverBaggle() == true) {
				pickupBaggle();
				setDirection(Direction.WEST);
				while (isOverBaggle() == false) {
					forward(); {
						while (isCarryingBaggle() == true) {
							setDirection(Direction.EAST);
							forward(); {
								while (isOverBaggle() == false) {
									dropBaggle(); 
								}
							}
						}
					}
				}
			}
		}
	}
}
