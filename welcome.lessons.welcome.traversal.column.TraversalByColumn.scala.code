override def run(){Colonne()}


def fonctionTest() : Boolean={
  var L=getMondeLargeur()
  var H=getMondeHauteur()
  var X=getX()
  var Y=getY()
  if ((X,Y)!=(H-1,L-1)){return false} 
  else {return true}}


def Colonne(){
  var mess = 0
  var L=getMondeLargeur()
  var H=getMondeHauteur()
  while (!fonctionTest){
  for (j <- 0 to L-1){
    for (i <- 0 to H-1){
      ecritMessage(mess)
      setPos(j,i)
      mess = mess + 1 
    }
  }
  }}