public void bubbleSort3()  {
 boolean estEnOrdre = false ;
for(int j = 1; j<=getNombreValeurs();j++){
  estEnOrdre = true;
    for(int i = 0; i<getNombreValeurs()-j;i++){
      if(plusPetit(i+1,i)){
        echange(i,i+1); 
        estEnOrdre = true ;
      	}
      if (!estEnOrdre) break ;
    }      
  }
}