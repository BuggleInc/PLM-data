var diag = 0;
override def run() {
  var nbCase = 0
  var x = 0
  var y = 0
  while (!positionFinale()) {
    ecritMessage(nbCase)
    if (diag < 6) {
      diag += 1
    } else {
      diag -= 1
    }
    for (compteur <- 1 to diag + 1) {
     	setPos(compteur - 1, diag - compteur)
      nbCase += 1
      ecritMessage(nbCase)
      }
  }
}

def positionFinale():Boolean = {
  return(getX() == 6 && getY() == 6)
}