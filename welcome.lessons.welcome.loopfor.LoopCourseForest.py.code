stepper = 0

while stepper < 448:
    stepper = stepper +1
    forward()
    if (stepper != 448):
        if (stepper % 4 == 0):
            left()
        elif (stepper % 6 == 0):
            right()
        elif (stepper % 10 == 0):
            right()
        elif (stepper % 12 == 0):
            left()
        elif (stepper % 16 == 0):
            left()
