override def run() {
  var compteur =0
  for (diag <-0 to getMondeHauteur()-1){
    for (i <- 0 to diag){
      setPos(i,diag-i)
      ecritMessage(compteur)
      compteur +=1
    }
  }
   for (diag <-1 to getMondeHauteur()-1){
    for (i <- 1 to diag){
      setPos(i+diag-1,getMondeHauteur()-i)
      ecritMessage(compteur)
      compteur +=1
		}
   }
}