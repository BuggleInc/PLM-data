def combSort() {
  var n:Int = getValueCount();
  var gap:Int = n;
  var i:Int = 0;
  var verif:Boolean = false;
  do {
    
    verif = false;
    
    if (gap >1) {
      gap = (gap.toDouble/1.3).toInt;
    }
    i = 0;
    
    while (i+gap < n) {
      if (isSmaller(i+gap,i)) {
        swap(i,i+gap);
        verif = true;
      }
      i = i+1;
    }
    
  } while (verif || gap>1)
}