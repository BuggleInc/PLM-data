override def run() {
  var x:Int = getMondeHauteur()
  var T = new Array[Color](x+1)
  for(k<- 1 to x){
  	T(0)=getCouleurSol()
    for(i<- 1 to T.length-1){
    	avance()
      T(i)=getCouleurSol()
  	}
  	T(x)=T(0)
  	gauche()
  	avance()
  	droite()
  	setCouleurBrosse(T(x))
    baisseBrosse()
    leveBrosse()
    avance()
    for(j<- 1 to T.length-1){
    	setCouleurBrosse(T(j-1))
    	baisseBrosse()
    	leveBrosse()
    	avance()
  	}
	}
  recule()
  droite()
  avance()
  gauche()
}
