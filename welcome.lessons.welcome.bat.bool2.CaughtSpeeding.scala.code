def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  var speed2:Int=0
  if (!isBirthday) {speed2=speed+5 
                    if (speed2<=60) {return 0}
                    else if ((61<=speed2) && (speed2<=80))
                    {return 1}
                    else {return 2 }}
  else { 
    if (speed<60) {return 0}
    else if ((61<=speed) && (speed<=80)) {return 1}
    else {return 2 }    
  }
}