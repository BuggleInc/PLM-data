def cocktailSort2() {
var debut:Int=0
var fin:Int=getNombreValeurs-2
var fini:Boolean=false
while (fini == false) {
	fini=true
	for (i <- debut to fin) {
		if (getValeur(i) > getValeur(i+1)) {
			echange(i,i+1)
			fini=false
		}	
	}
	fin=fin-1
	for (i <- fin+1 to debut+1 by -1) {
		if (getValeur(i) < getValeur(i-1)) {
			echange(i,i-1)
			fini=false
		}
	}
	debut=debut+1
}
}
