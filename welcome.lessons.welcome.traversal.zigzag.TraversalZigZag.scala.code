override def run() {
  var i=0
	while(posFinale()==false){
    ecritMessage(i+"")
    nextPos1()
    nextPos2
    i=i+1
  }
  ecritMessage(i+"")
}

def posFinale(): Boolean={
  if (getX()==getMondeLargeur-1 & getY()==getMondeHauteur()-1)
  	return true
  return false
}

def nextPos1(){
  if (getX()==getMondeLargeur()-1){
    setPos(getX(),getY()+1)
  }
  else{
    setX(getX()+1)
  }
}

def nextPos2(){
  if (getX()==0){
    setPos(0,getY()+1)
  }
  else
  	setPos(getX()-1,getY())
}