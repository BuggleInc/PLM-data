override def run() {
  var test:Boolean = false
  brushDown();
  while (test ==false && !estFaceMur) {
    forward();
    if (getDirection()==Direction.EST) {
      left();
      if (estFaceMur) {
        test = true
      } else {
        forward(); left();
      }
    }
    if (getDirection()==Direction.EST) {
      right();
      if (estFaceMur) {
        test=true
      } else {
        forward(); right();
      }
    }
	return test
  }
}
