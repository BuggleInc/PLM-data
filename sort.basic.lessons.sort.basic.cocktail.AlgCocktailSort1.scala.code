def cocktailSort()  {


	var estTrie = false;
	var invIndice = getNombreValeurs();
	
	do
	{

	estTrie = true;
	
		for(i <- 0 to getNombreValeurs() - 2)
		{
			if(!plusPetit(i,i+1))
			{
				echange(i,i+1);
				estTrie = false;
			}
		}
		
		if(!estTrie)
		{

				for(i <- 2 to getNombreValeurs())
			{

				invIndice = getNombreValeurs() - i;
				
				if(plusPetit(invIndice+1,invIndice))
				{
				echange(invIndice+1,invIndice);
				}
			}


		
		}
	}while(!estTrie)

}
