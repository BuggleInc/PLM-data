        	return true;
      } else if getColor(x) == "WHITE" {
        if getColor(y) == "BLUE" {
          return false;
        }else{
          return true;
        }
      } else if getColor(x) == "RED" {
        return false;
      }

  }

  for (i <- 1 until getSize()) {
