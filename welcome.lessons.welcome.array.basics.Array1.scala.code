override def run() {
var H =  getMondeHauteur()
var L =  getMondeLargeur()
var T = new Array[Color](H)
for( i <- 0 to H-1 )
{
	T(i) = getCouleurSol()
	avance()
}

for(i <- 0 to L-2)
{
	for(i <- 0 to H-1)
	{
		setCouleurBrosse(T(i))
		baisseBrosse()
		leveBrosse()
		avance()
	}	
	recule(H-i-1)
	gauche()
	avance()
	droite()
	avance(i+1)
}
}
