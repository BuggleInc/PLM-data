def growFlowers() {
  
  def marqueSol(c:Color) {
    setCouleurBrosse(c)
    baisseBrosse()
    gauche(); droite()
    leveBrosse()
  }
  
  def flower(c:Color) {
    setCouleurBrosse(c)
    baisseBrosse()
    avance(2)
    recule()
    gauche()
    avance()
    recule(2)
    leveBrosse()
    avance()
    marqueSol(Color.YELLOW)
    droite()
    recule()
    
  }
  
  flower(Color.RED)
      
}