
override def run() {
	var cpt : Int =0
	ecritMessage(cpt);
	droite();

	while(!posF  && getX()<6 && getY()<6){
		if(estDosMur){
			for(k<-1 to 6 ){
			setPos(getX()+1,getY());
			}
			droite();
			setPos(getX(),getY()+1);
			gauche();
			for(k<-1 to 6 ){
			setPos(getX()-1,getY());
			}
			droite();
			setPos(getX(),getY()+1);
			gauche();
			
		}else{
			setPos(getX(),getY());
		}
		
	}
		
}





def posF():Boolean ={
	if( getX()== (getMondeLargeur()-1)  && getY()==(getMondeHauteur()-1)){
		return true
	}else{
		return false
	}
}
