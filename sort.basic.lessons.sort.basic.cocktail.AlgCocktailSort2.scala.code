def cocktailSort2() {
var swapped = true
var debut = 0 
var fin = getNombreValeurs()

while (swapped) {
for (i <- debut to fin){
	if (plusPetit(i+1,i)) {
	echange(i,i+1)	
	fin -= 1
	swapped=true
	}
}

for (i <- fin to debut by -1){
	if (plusPetit(i+1,i)) {
	echange(i,i+1)
	debut += 1	
	swapped=true
	}
}
}
}
