def cocktailSort() {
  var echanger = true
  var len = getNombreValeurs()
  while (echanger == true) {
    echanger = false
    for (i <-0 to len -2) {
      if (plusPetitQue(i+1,i)) {
        echange(i+1,i)
        echanger = true
      }
    };for (i <-0 to len -2) {
      if (plusPetitQue(len -1-i,len -2-i)) {
        echange(len-1-i, len-2-i)
        echanger = true
      }
    } 
  }

}