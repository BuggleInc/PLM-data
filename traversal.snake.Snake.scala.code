override def run() {
  var test:Boolean = false
  brushDown();
  while (test ==false) {
    forward();
  }
  if (getDirection() == Direction.EST) {
    left(); 
    if (!estFaceMur) {
      forward(); left();
    } else {
      test=true
    }
    left();
  }
  if (getDirection() == Direction.OUEST) {
    right();
    if (!estFaceMur) {
      forward(); right();
    } else {test=true}
    right();
  }
  return test
}