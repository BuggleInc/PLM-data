override def run() {
  var cpt=0
	var i=0
while (i!=getMondeLargeur()){
  while(estFaceMur()){
    ecritMessage(cpt)
    setPos(cpt+1,i)
    cpt=cpt+1
  }
  tpc(i)
  if (i <= getMondeLargeur()){i=i+1}
}
}




def tpc(x:Int){
  setPos(x,0)
}

