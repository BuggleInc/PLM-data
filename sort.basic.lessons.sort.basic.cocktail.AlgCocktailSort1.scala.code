def cocktailSort() {
	var inverse=true
	do{
		inverse=false
		for(i<-0 to getNombreValeurs()-2){
			if(plusPetit(i+1,i)){
				inverse=true
				echange(i,i+1) 
			}
		}

		for(i<-getNombreValeurs()-2 to 0 by -1){
			if(plusPetit(i+1,i)){
				inverse=true
				echange(i,i+1) 
			}
		}
	} while(inverse)
}
