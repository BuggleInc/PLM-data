def cocktailSort() {
  var n:Int = getNombreValeurs()
  var continue:Int = 1
  while (continue != 0){
    continue = 0
  	for (i <- 0 to n-2){
    	if (plusPetit(i+1,i)){
      	echange(i,i+1)
      	continue = continue + 1
    	}
    }
  	for (i <- n-2 to 0 by -1){
    	if (plusPetit(i+1,i)){
      	echange(i,i+1)
      	continue = continue + 1
    	}
    }
  	if (continue == 0){return}
	}
}
