override def run() {
  var xMax:Int = getWorldWidth();
  var yMax:Int = getWorldHeight();
  var x:Int=0;
  var y:Int=0;
	(x,y) = findBaggle(xMax,yMax);
}


def findBaggle(xMax:Int, yMax:Int){
  var i:Int =0;
  var j:Int =0;
  while (!hasBaggle(i,j)){
    if (i==xMax) {
      i = 0;
      j += 1
    }
    else {
      i += 1
    }
  }
  return (i,j)
  
}