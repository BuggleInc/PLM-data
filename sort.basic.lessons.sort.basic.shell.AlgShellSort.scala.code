def shellSort()  {
  var lgr:Int= getNombreValeurs()
  var ecart = (lgr.toDouble / 2).toInt
  while (ecart>0){
    for (i<-1 to lgr-1) {
      var v=getValeur(i:Int)
      var j:Int=1
      while (i-j*ecart>=0 && v<getValeur(i-j*ecart)){
        setValeur(i-(j+1)*ecart:Int, getValeur(i-j*ecart:Int):Int)
        j=j+1
      }
      setValeur(i-(j+1)*ecart,v)
    }
    ecart = (ecart.toDouble / 2).toInt
  }
}