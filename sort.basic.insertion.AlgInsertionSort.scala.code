def insertionSort() {
  var length:Int = getNombreValeurs()
  var v:Int = 0
  var a:Int = 0
  var j:Int = 0
  var continuer:Boolean = true
  for (i <- 1 to length-1){
    a = i
    v = getValeur(i)
    j = i-1
    print(i +" ")
    while(j>=0){
      continuer = false
      if (!plusPetitQue(j,v)){
        copie(j,j+1)
        a = j
        continuer = true
        print("a" + i + "a ")
      }
      j -= 1
      if (j < 0){continuer = false}
    }
    setValeur(a,v)
  }
}


