override def run() {
var n =  (getMondeHauteur()) ;
var T = new Array[Color](n);
for (i <- 0 to n-1) {
	T(i) = getCouleurSol();
	avance();
}
for (j <- 1 to n) {
	gauche();
	avance();
	droite();
	var aux = T(n-1);
	for (c <- 0 to n-2) {
		T(c+1) = T(c)
	}
	T(0) = aux ;
	for (k <- 1 to n-1) {
		setCouleurBrosse(T(k))
		baisseBrosse();
		leveBrosse();
		avance();
	}
	for (r <- 0 to n-1) {
		recule();
	}
}
}
