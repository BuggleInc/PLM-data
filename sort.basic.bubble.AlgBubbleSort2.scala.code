  var len = getNombreValeurs()
  while (echanger == true) {
    var not =0
    var j = 2
    for (i <- 0 to len -j) {
    	if (plusPetit(i+1, i)) {
      	echange(i, i+1)
      };else { 
        not +=1
      };j += 1
      };if (not == len-j-1) {
      		echanger = false
    	}	
}
}