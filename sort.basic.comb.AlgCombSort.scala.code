def combSort() {
  var gap = getValueCount()
  var swapped = false
  
  do {
    swapped = false
    
    if(gap > 1) {
      gap = (gap.toDouble / 1.3).toInt
    }
    
    var i = 0
    
    while(i + gap < getValueCount()) {
      if(isSmaller(i, i + gap)) {
        swap(i, i + gap)
        swapped = true
        i += 1
      }
    }
  } while (gap > 1 || swapped == true)
}