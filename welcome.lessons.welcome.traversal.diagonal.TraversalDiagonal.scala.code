override def run() {
  var diag=0
  var y=0
  var x=0
  var compteur=0
  ecritMessage(compteur)
  compteur+=1
  y+=1
  diag=1
  while (diag!=getMondeHauteur()-1){
    while (x<=diag){
      ecritMessage(compteur)
      compteur+=1
      setPos(x,y)
      y=y-1
      x+=1
    }
    x=0
    diag+=1
    y=diag
  }
}