def arrayFront9(nums:Array[Int]): Boolean = {
  var res : Boolean = false;
  if (nums.length < 4) {
    for (i <- 0 to nums.length -1) {
      if (nums(i) == 9) {
        res = true;
      }
    }
  } else {
    for (j <- 0 to 3) {
      if (nums(j) == 9) {
        res = true;
      }
    }
  }
  return res;
}