override def run() {
  while(!posfinale())
  	serpent()
}

def posfinale():Boolean={
  if(estFaceMur()){
    droite()
    if(getDirection()==Direction.EST){
      gauche()
      if(estFaceMur()){
        gauche()
        return true
      }
    }
    retourne()
    if(getDirection()==Direction.OUEST){
      droite()
      if(estFaceMur()){
        droite()
        return true
      }
    }
  }
  return false
}

def serpent(){
  if(!estFaceMur()){
    avance()
  }
}