def solve() {
  var apresBleu:Int = 0
  var avantBlanc:Int = getTaille() -1
  var blancRouge:Int  = getTaille() -1
  while (!estTrie()){
    if (getCouleur(apresBleu) == BLEU){
      apresBleu = apresBleu +1
    } else if (getCouleur(apresBleu) == ROUGE){
      echange(apresBleu, blancRouge)
      echange(apresBleu, avantBlanc)
      blancRouge = blancRouge -1
      avantBlanc = avantBlanc -1
    } else{
      echange(apresBleu, avantBlanc)
      avantBlanc = avantBlanc -1
    }
  }
}