def bubbleSort3() {
  var aInverse = false
  val n = getNombreValeurs()
  var i = n - 1
  var nbPassage = 0
  while (nbPassage != n) {
    aInverse = false
    i -= 1
    for (j <- 0 to i - 1) {
      if (getValeur(j+1) < getValeur(j)) {
        echange(j+1,j)
        aInverse = true
      }
    }
    if (!aInverse) {
      return
    }
    nbPassage += 1
  }
}
