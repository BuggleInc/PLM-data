override def run() {
	
val decalage = litMessage().toInt
var tab = new Array[Color] (getMondeHauteur())

def remplirTableau() {
	for (i <- 0 to getMondeHauteur - 1) {
		tab(i) = getCouleurSol()
		avance()
	}
}

def faireColonne() {
	for (i <- (i + 5) % getMondeHauteur to getMondeHauteur() - 1 + (i + 5) % getMondeHauteur) {
		setCouleurBrosse(tab(i))
		baisseBrosse()
		leveBrosse()
		avance()
	}
}

remplirTableau()

	
}
