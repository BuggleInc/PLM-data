def cocktailSort() {
  var count = -1
  var n = getNombreValeurs()
  do {
    count=0
    for (i <- 0 to n-2){
      if (getValeur(i)>getValeur(i+1)){
        echange (i,i+1)
        count=count+1
      }
    }
    for (i <- n-2 to 0 by -1){
      if (getValeur(i)>getValeur(i+1)){
        echange (i,i+1)
        count=count+1
      }
    }
    
  } while (count!=0)
}
