def golomb(num:Int): Int = {
  var tab = new Array[Int](num)
  tab(0) = 1
  var flag = 1
  for (k <- 1 to num-1) {
    if (k==1) {
      tab(k) = 2
    }
    var nb = tab(k)
    for (j <- flag to (flag+nb)) {
      if (j <= num-1) {
      	tab(j) = k
      }
    }
    flag = tab(k)
  }
  return tab(num-1)
}