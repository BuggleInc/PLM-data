var diag = 0;
override def run() {
def fonctionTest():Boolean={
	if (getX()==getMondeLargeur-1 && getY==getMondeHauteur-1){return true}
	else{return false}
}
var n:Int = 0
while (fonctionTest()==false){
	ecritMessage(n.toString);
	n=n+1
	diag=diag+1
	getX(); getY();
	var x:Int = getX()
	var y:Int = getY()
	if(getX==getMondeLargeur-1){
		setPos(diag-6 ,getMondeHauteur-1)
	}
	else{if(getY==0){setPos(0, diag)}
		 else{setPos(x+1, y-1)}
	}
	

}
ecritMessage(n.toString)
}
