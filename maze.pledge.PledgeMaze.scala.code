override def run() {
  var virage:Double = 0
 	while (!estFaceMur())
  	avance()
  while (!estSurBiscuit())
  	virage = keepHandOnSideWall(virage)
  prendBiscuit()
}

def keepHandOnSideWall(virage:Double):Double = {
  gauche()
  var v:Double = 0
  if (estFaceMur()) {
  	droite()
    if (estFaceMur()) {
    	droite()
    	v=virage-1
    }
    else
    	avance()
  }
  else {
  	avance()
  	v=virage+1
  }
  return v
}