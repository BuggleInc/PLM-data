override def run() {
  val hauteur = getMondeHauteur()
  val largeur = getMondeLargeur()
  var cpt = 0
  
  def posFinale(): Boolean = {
    if (getX() == largeur-1 && getY() == hauteur-1) {
      return true
    }
    return false
  }
  
  while (!posFinale()) {
      if (getX() != largeur-1) {
        ecritMessage(cpt)
        cpt = cpt + 1
        setPos(getX()+1,getY())
      } else {
        ecritMessage(cpt)
        cpt = cpt + 1
        setPos(0,getY()+1)
      }
    }
  ecritMessage(cpt)
}
