firstTime = True
countSteps = 0
countBaggles = 0
while (exitReached() == False):
    while firstTime or not crossing():
        firstTime = False
        if (countBaggles <= 2):
            right()
        elif (countBaggles >= 3):
            left()
    while countSteps < 5:
        forward()
        if isOverBaggle() == True:
            countBaggles = countBaggles + 1
        if countSteps == 5:
            firstTime = True
  
    
