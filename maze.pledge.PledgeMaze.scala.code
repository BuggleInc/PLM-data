override def run() {
  var sommeAngle : Int =0
  while(!estSurBiscuit()){
    setDirection(Direction.NORD)
    while(!estFaceMur()){
      avance()
    }
    do{
      sommeAngle=keepHandOnSideWall(sommeAngle)
    }
    while(sommeAngle!=0 || isDirectionFree(Direction.NORD)==false)
  }
  prendBiscuit()
}

def keepHandOnSideWall(sommeAngle : Int) : Int = {
  gauche()
  sommeAngle=sommeAngle+1
  while(estFaceMur()==true){
    droite()
    sommeAngle-=1
  }
  avance()
  return sommeAngle
}

def isDirectionFree(dir : Direction) : Boolean = {
  setDirection(dir)
  if(!estFaceMur()){
    return false
  }else{
    return true
  }
}