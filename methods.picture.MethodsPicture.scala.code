override def run() {
  def makeV(c:Color){
    setBrushColor(c:Color)
    brushDown();
    brushUp();
    forward();
    left();
    forward();brushDown();brushUp();backward();
    right();forward();brushDown();left();
    right();brushUp();forward();forward();
    left();forward();forward()
  }
  def Patternc(){
    forward();forward();
    makeV(Color.yellow);
    makeV(Color.red);
    makeV(Color.blue);
    makeV(Color.green);
  }
}
