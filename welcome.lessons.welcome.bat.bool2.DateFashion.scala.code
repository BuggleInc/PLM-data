def dateFashion(you:Int, date:Int):Int = {
	if (max(you, date)>8 || max(you, date)==8 ){
		if(min(you, date)<2 || min(you, date)==2 ){
			return 0;
		}else{
			return 2;
		}
	}else{
		if(min(you, date)<2 || min(you, date)==2 ){
			return 0;
		}else{
			return 1;
		}
}
}






def min (a:Int , b:Int): Int = {
	return   (  (a + b - Math.abs(b - a))/2);
}
def max (a:Int , b:Int): Int = {
	return   (  (a + b + Math.abs(b - a))/2);
}
