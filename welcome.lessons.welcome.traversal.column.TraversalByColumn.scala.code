override def run() {
  var compteur:Int=0
  ecritMessage(compteur)
  while (getX() !=(getMondeLargeur()-1) && getY() != getMondeHauteur()-1) {
    avance()
    compteur+=1
    ecritMessage(compteur)
    if (estFaceMur()){
      recule(getMondeHauteur()-1)
      gauche()
      avance()
      droite()
    }    
  }
  compteur+=1
  ecritMessage(compteur)
}