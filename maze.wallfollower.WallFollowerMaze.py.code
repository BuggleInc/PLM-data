def surMurG() :
  
  '''
  Verifie que mur ? gauche
  '''
  
  ok = False
  left()
  if estFaceMur():
    ok = True  
  right()
  
  return ok
  
    

def keepHandOnSideWall() :
  '''
  Avance d'une case en gardant patte sur mur de gauche
  '''
  while surMurG() :
    
    if estFaceMur() :
      while estFaceMur() :
        left()
   	
    avance()
    
    if not surMurG() :
      right()
    
    
    

keepHandOnSideWall()
    
  