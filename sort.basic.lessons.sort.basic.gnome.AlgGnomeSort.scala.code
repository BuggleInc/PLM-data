def gnomeSort() {
  var i = 0;
  var nbOps = 0;
  do {
    while (i <= getNombreValeurs() -2) {
    	if (i == 0) {
      	i += 1;
   	 } else {
      	if (plusPetit(i+1, i)) {
       	 echange(i, i+1);
          nbOps += 1;
       	 i -= 1;
      	} else {
        	i += 1;
        }
      }
    }
  } while (nbOps > 0)
}