def cocktailSort3() {
	var swap = true
	var debut = 0
	var fin = getNombreValeurs()-2
	while(swap){
		swap = false
		for(i<-debut to fin){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swap = true
			}
		}
		if(!swap){
			return
		}
		fin -= 1
		swap = false
		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swap = true
			}
		}
		if(!swap){
			return
		}
		debut += 1
	}
}