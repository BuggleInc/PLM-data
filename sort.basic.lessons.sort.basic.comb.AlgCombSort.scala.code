def combSort() {
  var taille=getNombreValeurs()
  var trier=false
  var ecart=getNombreValeurs()
  
  while (ecart>1){
    trier=true
    cart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
    if(ecart<1){
      ecart=(ecart.asInstanceOf[Double]+1).asInstanceOf[Int])
    }
    var i =0
   
    while (i<= taille - ecart){
      if (plusPetit(i+ecart,i)==true){
        echange(i+ecart,i)
        trier=false
      }
    i=i+1
    }
    
  }
  
}