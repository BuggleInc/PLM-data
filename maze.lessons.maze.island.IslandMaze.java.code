public void run() {
	searchWall();
	while(!isOverBaggle()) {
		keepHandOnSideWall();
	}
	pickUpBaggle();
}

void keepHandOnSideWall(){
	if(!isFacingWall()) {
		forward();
	}
	if(isFacingWall()){
		right();
		if(isFacingWall()) {
			forward();
			left();
		}
	}
}

void searchWall() {
	while(!isFacingWall()){
		left();
	}
}
