def shellSort() {
  var ecart = getNombreValeurs()
  var v = 0
  var k = 0
  while (ecart>0){
    ecart=(ecart.toDouble / 2).toInt
    for (i<-getNombreValeurs()/2 to getNombreValeurs()-1){
    	v = getValeur(i-k*ecart)
    	var j = i-(k+1)*ecart
    	while(j>=0 && !plusPetitQue(j,v)){
      	copie(j,j+1)
      	j-=1
    	}
    	j+=1
    	setValeur(j,v)
  	}
  }
}