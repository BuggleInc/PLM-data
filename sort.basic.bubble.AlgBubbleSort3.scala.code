def bubbleSort3()  {
  var max = getNombreValeurs();
  for(i <- 0 to max-1 ){
    var count = max-1;
    var permute = false;
    while(count > i+1){
       var a = getValeur(count);
      var b = getValeur(count-1);
      if(a<b){
        echange(count,count-1);
        permute = true;
      }
      count +=1;
      
    }
  if(!permute) break;
  }
}