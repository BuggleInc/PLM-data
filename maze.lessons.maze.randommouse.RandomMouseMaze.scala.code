def fin(): Boolean = {
  if(estSurBiscuit()) {
    return true
  }
  else {
    return false
  }
}

def murDroite(): Boolean= {
  droite()
  if(estFaceMur()) {
    gauche()
    return true
  }
  else {
    gauche()
    return false
  }
}

override def run() {
  while(!fin()) {
    if(!estFaceMur()) {
      avance()
    }
    else {
      if(murDroite()) {
        gauche()
        avance()
      }
      else {
        droite()
        avance()
      }
    }
  }
  prendBiscuit() 
}
