def cocktailSort2() {
  var inversion = false
  var debut = 0 ; 
  var fin = getNombreValeurs() - 2;
  do {
    for (i<- debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1);
      }
    fin = fin - 1;
    inversion = true;
    }
    for (i<- fin to debut by -1, j<-debut to i-1) {
      if (plusPetit(j,j+1)) {
        echange(j,j+1);
      }
    debut = debut + 1;
    inversion = true;
    }
  } while (!inversion)
}
