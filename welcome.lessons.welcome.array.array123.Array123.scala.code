def array123(nums:Array[Int]): Boolean = {
  var n = nums.length
  var o1=false
  var o2=false
  var o3=false
  for (i <- 0 to (n-1))
  {
    if (nums(i)==1)
    {
      o1=true
    }
    if (nums(i)==2)
    {
      o2=true
    }
    if (nums(i)==3)
    {
      o3=true
    }
  }
  if (o1==true && o2==true && o3==true)
  {
    return true
  }else {
    return false
  }
}