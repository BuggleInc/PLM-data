def northlibre():Boolean = {
  if(getDirection()==Direction.NORTH && !estFaceMur()){
    return true
  }
  droite();
  if(getDirection()==Direction.NORTH && !estFaceMur()){
    return true
  }
  gauche();
  return false
}

def keepHandOnSideWall(){
  while(estFaceMur()){
    droite();
  }
  avance();
}

override def run() {
  while(!estSurBiscuit()){
    keepHandOnSideWall();
    if(northlibre()){
      setDirection(Direction.NORTH);
    }
  }
  prendBiscuit();
}