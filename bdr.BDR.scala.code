var finished = false
while (!finished) {
  if (isOverMessage()) {
    var msg = readMessage() 
    if (msg == "R") {right();forward()}
    if (msg == "L")  {left();forward()}
    if (msg == "I") {left();left();forward()}
    if (msg == "A") forward()
    if (msg == "B") forward(2)
    if (msg == "C") forward(3)
    if (msg == "Z") backward()
    if (msg == "Y") backward(2)
    if (msg == "X") backward(3)
 
    
  } else if (!isOverMessage()) finished = true}