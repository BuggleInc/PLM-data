while(estSurMessage())
{
	else if(getIndication() == 'A')
		avance();
	else if(getIndication() == 'Z')
		recule();
	else if(getIndication() == 'L')
	{
		gauche();
		avance();
	}
	else if(getIndication() == 'B')
	{
		avance();
		avance();
	}
	else if(getIndication() == 'R')
	{
		droite();
		avance();
	}
	else if(getIndication() == 'X')
	{
		recule();
		recule();
		recule();
	}
	else if(getIndication() == 'I')
	{
		retourne();
		avance();
	}
}
