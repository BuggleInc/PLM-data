def cocktailSort2() {


	var tri:Boolean = false; 

	while (!tri)
	{
		tri = true
		for (i <- 0 to getNombreValeurs - 2)
		{
			if (!plusPetit(i , i+1))
			{
				echange(i, i+1);
				tri = false
			}
		}

		for (i <- getNombreValeurs - 2 to 0 by - 1)
		{
			if (plusPetit(i+1 , i))
			{
				echange(i, i +1);
				tri = false
			}
		}

		
	}

}
