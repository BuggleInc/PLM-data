def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var echangefait:Boolean=false
  do {
    var min=0
    echangefait=false
    for (i<- debut to fin){
      if (!plusPetit(i,i+1)){
        echange(i,i+1);
        echangefait=true;
        min=i
      }
    }
    
   fin-=1
   for (i<-min to debut){
     if (!plusPetit(i,i+1)){
       echange(i,i+1);
       echangefait=true;
    }
   }

  } while (echangefait==true)
}