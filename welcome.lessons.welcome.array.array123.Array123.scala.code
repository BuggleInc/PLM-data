def array123(nums:Array[Int]): Boolean = {
var isOne = false;
var isTwo = false;
var isThree = false;
var isTambul = false;
for(i <- 0 to nums.length - 1)
	{
		if(nums(i) == 1)
		{
			isOne = true;
		}else if(nums(i) == 2)
		{
			isTwo = true;
		}else if(nums(i) == 3)
		{
			isThree = true;
		}
	}
	if(isOne && isTwo && isThree)
	{
		isTambul = true;
	}
	
	return isTambul;

}