def gnomeSort() 
{
  var iSuiv = 2
  var jSuiv = 3
  var iPrece = 0

  while (jSuiv <= getNombreValeurs())
  {
    if (iPrece<1)
  {
    iPrece+=1
    iSuiv+=1
    jSuiv+=1
  }
    if (plusPetit(iSuiv,jSuiv))
    {
      iPrece+=1
      iSuiv+=1
      jSuiv+=1
    }

    else
    {
      echange(iSuiv,jSuiv)
      iPrece-=1
      iSuiv-=1
      jSuiv-=1

    }

  }
  
  if (jSuiv > getNombreValeurs())
  {
    return true
  }
}