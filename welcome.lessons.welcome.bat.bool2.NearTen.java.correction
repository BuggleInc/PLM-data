$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class nearTen extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( nearTen((Integer)t.getParameter(0)) ); }
	boolean nearTen(int num) {
		return (num % 10) <= 2 || (num % 10) >= 8; 
	}
 }