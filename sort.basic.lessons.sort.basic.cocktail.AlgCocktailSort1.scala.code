def cocktailSort() {
  var len = getValueCount()
  var continue = true
  
  while(continue){
    
  continue=false
  for (i <- 0 to len - 2){
    if (isSmaller(i+1, i)){
      swap(i+1, i)
      continue = true
    }
  }
    
  for (i <- len-2 to 0 by -1){
    if (isSmaller(i+1, i)){
      swap(i+1, i)
      continue = true
    }
  }
    
  }
}