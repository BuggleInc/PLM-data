def insertionSort() {
  
  for (i <- 1 to getNombreValeurs()-1) {
    
    var v:Int = getValeur(i)
    var w:Int = i
    for (j <- i to 1 by -1) {
      
      if ((plusPetitQue(j-1,v) == false) && (getValeur(j-1) != v)) {
        
        copie(j-1, j)
        w = j
      }
 
    }
    setValeur(w-1,v)
  }
  
  
}