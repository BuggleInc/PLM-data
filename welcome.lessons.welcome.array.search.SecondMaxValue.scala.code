def max2Value(nums:Array[Int]): Int =
{
  var max1:Int = nums(0)
  var max2:Int = Integer.MIN_VALUE
  var id:Int = 0
  
  for(i <- 0 to nums.length-1)
  {
    if(max1 <= nums(i))
    {
      max2 = max1
      max1 = nums(i)
      id=i
    }
  }
  
  max2 = nums(1)
  
 	if(id==0)
  {
    for(i <- 1 to nums.length-1)
    {
      if(max2 <= nums(i))
      {
        max2 = nums(i)
      }
    }
  }
  
  return max2
}