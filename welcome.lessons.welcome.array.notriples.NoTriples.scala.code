def noTriples(nums:Array[Int]): Boolean = {
  var L = nums.length
    
  for (i <- 0 to (L-1)) {
    
  if ((i+3)<=L) {
    if (nums(i)==nums(i)) {
      if (nums(i+1)==nums(i)) {
        if (nums(i+2)==nums(i)) {
          return false
          
        }
      }      
    }
  }
  
}
  return true
}
