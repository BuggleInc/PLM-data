boolean lastDigit(int a, int b, int c) {
	return twoOrMoreEqual(getLastDig(a),
					   getLastDig(b), 
					   getLastDig(c));
	
}

boolean twoOrMoreEqual(int a, int b, int c){
	int count = 0;
	if(a == b)
		count++;
	if(a == c)
		count++;
	if(b == c)
		count++;
	return count >= 1;
}

int getLastDig(int n){
	return n % 10;
}
