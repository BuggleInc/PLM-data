override def run() {
var hauteur = getMondeHauteur();
var largeur = getMondeLargeur();
var lar = largeur-1;
var tab = new Array[Color] (hauteur-1)
for (i<- 0 to hauteur-1 ) {tab(i) = getCouleurSol();avance()} 
for (i<-0 to lar) {gauche();avance();droite();avance()
for(h<- 0 to hauteur-1) {setCouleurBrosse(tab(h));baisseBrosse();leveBrosse();avance()}}
}
