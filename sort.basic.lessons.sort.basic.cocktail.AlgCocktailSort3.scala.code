def cocktailSort3() {
  var n = getNombreValeurs()
  var fin = n
  var debut = 0
  var swap = false
  do {
    swap = false
    for (i <- debut to (fin-2)) {
    	if (plusPetit(i+1,i)) {
     		echange(i,i+1);
     		swap = true
      }
    };
    fin = fin - 1
    for (i <- (fin-1) to debut+1 by -1) {
    	if (plusPetit(i,i-1)) {
     		echange(i,i-1);
     		swap = true
      }
    };
    debut = debut + 1
  } while (swap)
}