override def run() {
  rejoindrelenord()
  while (!estSurBiscuit())
  	keepHandOnSideWall()
  prendBiscuit()
}

def rejoindrelenord() {
  while (!estFaceMur() && getDirection()==Direction.NORTH) {
    if (getDirection()==Direction.NORTH) {
      if (!estFaceMur())
      	avance()
      else {
      	while (estFaceMur())
      		gauche()
      	avance
      }
    }
    else {
      if (getDirection()==Direction.WEST)
      	droite()
      if (getDirection()==Direction.EAST)
      	gauche()
      if (getDirection()==Direction.SOUTH) {
        droite()
        droite()
      }  	
    }
  }
  droite()
}

def keepHandOnSideWall() {
  gauche()
  if (estFaceMur()) {
  	droite()
    if (estFaceMur())
    	droite()
    else
    	avance()
  }
  else
  	avance()
}