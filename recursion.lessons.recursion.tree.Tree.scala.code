def tree(steps:Int, length:Double, angle:Double, shrink:Double)    
{

if (steps!=0)
{
	current(steps)
	avance(length)
	droite(angle)
	current(steps-1)
	tree(steps-1,length*shrink,angle,shrink)
	gauche(2*angle)
	tree(steps-1,length*shrink,angle,shrink)
	current(steps)
	droite(angle)
	recule(length)
	
	
	
	
}

}