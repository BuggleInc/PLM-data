def bubbleSort()  {
  
  var a:Boolean = true
  
  while(a) {
    
    var comptr:Int = 0
  
  	for (i <- 0 to getNombreValeurs()-2) {
    
    	if (!plusPetit(i,i+1)) {
        
    		echange(i,i+1)
        
      }
      
      else {
        
        comptr = comptr + 1
        
      }
        
    }
    
    if (comptr == getNombreValeurs()-1) {
      
      a = false
      
    }
      
  }

}