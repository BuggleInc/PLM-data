override def run() {
  var compteur=0
  for (lar <- 0 to getMondeLargeur()-1){
    if (lar%2==0){
    	for (lon <- 0 to getMondeHauteur()-1){
      setPos(lon,lar)
      ecritMessage(compteur)
      compteur += 1}
    }
    if (lar%2==1){
    	for (lon <- 0 to getMondeHauteur()-1){
      setPos(getMondeHauteur()-1-lon,lar)
      ecritMessage(compteur)
      compteur += 1}
    }
  }
}
