var biscuits = 0
var oranges = 0
var fini = false

while(!fini){
  
  if(estSurOrange()) {
    oranges+=1
    gauche()
    droite()
  }
  if(estSurBiscuit()) {
    prendBiscuit()
    biscuits += 1
  }
  
  if(2*biscuits != oranges +1){
    fini = true
    gauche()//test
  }
  else {
    avance()
  }
  
}