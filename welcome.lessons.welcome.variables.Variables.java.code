public void run() 
{ 
	int nbPas = 0;
	
	while(!estSurBiscuit()) //on avance tant que l'on n'est pas sur un biscuit
	{
		avance();
		nbPas++;
	}
	if(estSurBiscuit())//on arrive sur le biscuit
	{
		prendBiscuit();
		int pasRetour = 0;
		while(nbPas != pasRetour)//on revien en arrière
		{
			recule();
			pasRetour++;
		}
	}



}
