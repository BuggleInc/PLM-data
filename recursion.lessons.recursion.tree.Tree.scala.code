def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	if (steps!=0) {
		current(steps);
		avance(length);
		droite(angle);
		tree(steps-1, length*shrink, angle, shrink);
		leveCrayon();
		gauche(angle);
		recule(length);
		baisseCrayon();
		gauche(2*angle);
		avance(length);
		tree(steps-1, length*shrink, angle, shrink);
	}
	else return;
}
