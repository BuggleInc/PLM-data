def isFacingTrail():
    forward()
    getGroundColor()
    backward()
    if getGroundColor != Color.green:
        return False
    else:
        return True

while not isOverBaggle():
    if not isFacingWall():
        isFacingTrail()
    else:
        left() 
    return
    if isFacingTrail():
        forward()    
    
    
