def solve() {
  
  for(i<-0 to getTaillePile()-2)
  {
    var max=getRayonCrepe(i)
    var ind=i
    for(j<- 0 to getTaillePile()-i)
    {
      if(max<getRayonCrepe(i))
      {
        ind=i
        max=getRayonCrepe(i)
      }
    }
    retourne(ind+1)
    retourne(getTaillePile()-i)
    
  }
  
}