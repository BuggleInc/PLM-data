def fin(compteur:Int):Boolean =
{
  if(getMondeHauteur*getMondeLargeur=compteur+1)
  {
    ecritMessage(""+compteur)
    return true
  }
  else
  {
    return false
  }
}

def avance()
{
  var x:Int = 0
  var y:Int = 0
  
  if(getY==getHauteur()-1)
  {
    x=getX()+1
    y=0
  }
  else
  {
    x=getX()
    y=getY()+1
  }
  setPos(x,y)
}

override def run()
{
  var compteur:Int = 0
  
  do
  {
    ecritMessage(compteur)
    avance()
    compteur = compteur + 1
  }
  while(!fin(compteur))
}