def insertionSort() {
  var temp = 0
  var placement : Boolean = false
  for (i <- 1 to getNombreValeurs()) {
    temp = getValeur(i)
    while (placement == false) {
      var j = i
      if (getValeur(j-1) > temp) {
      	copie(j-1,j)
        j -= 1
      }
      else {
        setValeur(j,temp)
        placement = true
      }
    }
  }
}