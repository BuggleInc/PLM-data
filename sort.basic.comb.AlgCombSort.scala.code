def combSort() {
  var gap:Double=getValueCount()-2;
  var i =0;
  var sp=false;
  do {
    sp=false
    if (gap>1) {
      gap/=1.3
    }
    i=0;
    while (i+gap < getValueCount()) {
      if (!isSmaller(i,i+gap)) {
        swap(i,i+gap)
        i+=1
        sp=true
      }
    }
  } while (gap>1 || sp==true)
}