override def run() {
  while (isOverBaggle==false){
    keepHandOnSideWall()
  }
  pickupBaggle()
}


def keepHandOnSideWall(){
  left()
  if (isFacingWall==true){
    right()
    if (isFacingWall==false){
      forward()
    }
    else{
      right()
    }
  }
  else{
    while (isFacingWall==false){
    forward
    }
    right
  }
}