def cocktailSort() {
  var c = 1
  var lim = getNombreValeurs()-2
  do {
    c = 0
    for ( i <- 0 to lim){
      if (plusPetit(i+1, i)){
        echange(i+1, i)
        c += 1
      }
    }
    for (j <- 0 to lim){
      if (plusPetit(lim-j+1, lim-j)){
        echange(lim-j, lim-j+1)
        c += 1
      }
    }
  } while( c!=0)
}