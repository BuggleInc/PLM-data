public void run() {
	colByCol();
}

void colByCol(){
int width = getWorldWidth();
int height = getWorldHeight();
int count = 0;

	for(int x = 0; x < width; x++){
		for(int y = 0; y < height; y++){
			if(!finalPos()){
				setPos(x, y);
				count++;
				writeMessage(width);
			}			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth() && getY() == getWorldHeight()){
		return true;
	}

	return false;
}//end finalPos
