override def run() {
  while (!isOverBaggle()) {
    right()
    if (!isFacingWall()) {
      forward()
    }
    else {
      left();
      if (isFacingWall()) {
        left()
        if (isFacingWall()) {
          left()
          forward()
        }
        else {
          left(); forward()
        }
      }
      else {
        forward()
      }
    }
  }
}