public void run() {
	while (!isOverBaggle()) {
		findSideWall();
		keepHandOnSideWall();
	}
	pickupBaggle();
}

public void findSideWall(){
	while (!isFacingWall()){
		forward();
	}
	while (isFacingWall()){
		right();
	}
}

public void keepHandOnSideWall(){
	if (!isFacingWall()) {
		forward();
		left();
		if (isFacingWall()) {
			right();
		}
	}
}
