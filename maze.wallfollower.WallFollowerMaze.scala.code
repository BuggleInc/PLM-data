override def run() {
  while (!isOverBaggle()) {
    keepHandOnSideWall()
  }
  
  pickupBaggle();
}

def keepHandOnSideWall() {
  if (isFacingWall()) {
    right();
  }
	else {
    right();
    if (isFacingWall()) {
      left();
      forward();
    }
    else {
      forward();
      left();
      forward();
    }
    
  }
}