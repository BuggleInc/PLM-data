def shellSort()  {
  
  var ecart = getNombreValeurs()/2
  
  while (ecart>0) {
  
    for (i <- 1 to getNombreValeurs() by ecart) {
    
      var v = getValeur(i)
      var last = 0
      var c = 1

      while (i-(c+1)*ecart >= 0 && !plusPetitQue(i-c*ecart,v)) {

        copie(i-(c+1)*ecart,i-c*ecart)
        last = c
        c = c + 1

      }

      setValeur(i-last*ecart,v)

    }
    
  ecart = ecart/2
  
  }
  
}