def cocktailSort3()  {
  var sorted:Boolean = false
  var beg:Int = 0
  var end:Int = getValueCount() - 2
  while(!sorted){
    sorted = true
  	for(j <- beg to end)
      if(getValue(j) > getValue(j + 1)){
        swap(j, j + 1)
        sorted = false
      }
    if(sorted)
    	return
    sorted = true
    end -= 1
  	for(j <- end to beg by -1)
      if(getValue(j) > getValue(j + 1)){
        swap(j, j + 1)
        sorted = false
      }
    beg += 1
  }
}
