override def run() {
	retourne();
	var hauteur=getMondeHauteur();
	var largeur=getMondeLargeur();
	var num =0 ;
	var x=0 ;
	var y=0 ;
	while ( !(getX()==largeur) && !(getY()==hauteur))
{
	for ( i<- 0 to  48 )
	{
	
	ecritMessage(num)
	setPos(x,y+1)
	y=y+1
	num=num+1
	if (estFaceMur())
	{
	setPos(x+1,0)
	y==0
	}
	}
}
}