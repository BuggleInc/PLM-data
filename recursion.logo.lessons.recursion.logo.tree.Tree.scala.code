def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  var s:Int=steps
  var l:Double=length
  if (s==1){
      avance(l)
      recule(l)}
  else {
    current(s)
    avance(l)
    droite(angle)
    tree(s-1,l*shrink,angle,shrink)
    gauche(2*angle)
    tree(s-1,l*shrink,angle,shrink)
    droite(angle)
    current(s)
    recule(l)
  }}