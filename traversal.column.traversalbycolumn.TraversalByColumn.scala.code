override def run() {
  var x = 0
  var y = 0
  var num = 0
  do{
    ecritMessage(num)
    for(i<-0 to getMondeLargeur()-1){
      for(j <-0 to getMondeHauteur()-1){
        num = num+1
        setPos(i,j)
      }
    }
  }while(!posFinale(x,y))
  
}

def posFinale(x:Int,y:Int): Boolean ={
    return ((getX() == getMondeLargeur()) && (getY()== getMondeHauteur()))
}