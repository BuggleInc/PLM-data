def cocktailSort2() {
	var a=0
var b=0
var fin=getNombreValeurs()
var debut=0
var x=false
do{
	x=false
	
	for (n<-debut to fin){
	a= getValeur(n)
	b= getValeur(n+1)
	if(a>b){echange(n,n+1); x=true}
	}
	
	fin-=1
	
	for (n<-fin to debut by -1){
	a= getValeur(n)
	b= getValeur(n+1)
	if(a>b){echange(n,n+1); x=true}
	}
	
	debut+=1
	
}while(x==true)

}
