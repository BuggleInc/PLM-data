def cocktailSort2() {

	var modif:Boolean = true;
	var taille:Int = getNombreValeurs();
	var debut:Int = 0;
	var fin:Int = taille -2;

	do
	{
		modif = false;

	for (j <- debut to fin)
	{
		if (!plusPetit(j, j + 1)) 
		{
				echange(j, j + 1);
				modif = true;
		}
	}
	fin -= 1;

	for (j <- fin to debut by -1)
	{
		if (!plusPetit(j, j + 1)) 
		{
				echange(j, j + 1);
				modif = true;
		}
	}
	debut += 1;
		
	} while(modif);
}



