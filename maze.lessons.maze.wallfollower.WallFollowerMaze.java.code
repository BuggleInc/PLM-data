public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){
	do{
		while(isFacingWall() ){
			right();
		}

		forward();

		if(!isFacingWall() ){
			left();
		}

		if(isOverBaggle() ){
			pickUpBaggle();
		}//else{
			//left();
		//}
	}while(!isCarryingBaggle() );
}//end keepHandOnSideWall
