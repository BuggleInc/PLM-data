def cocktailSort2() {

  var a:Int=1;
  var b:Int=0;
  var c:Int=0
  while (a!=0) {
    a=0;
    for (i<- 0+c to getNombreValeurs-2-b) {
       if (plusPetit(i+1,i)) {
         echange(i,i+1);
         a=a+1
       }
    };
    b=b+1
    for (i<-getNombreValeurs-2-b to 0+c by -1) {
      if (plusPetit(i+1,i)) {
         echange(i,i+1);
         a=a+1
       }
       
    };
    c=c+1
  }}