def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {

	var longueur = length
	var etage = steps

	
	current(steps)

	avance(length)
	droite(angle)

	tree(etage,longueur,angle,shrink)

	gauche(angle)
	recule(length)
	gauche(2*angle)

	tree(etage,longueur,angle,shrink)

	longueur = longueur * shrink
	etage -= 1
			
}
