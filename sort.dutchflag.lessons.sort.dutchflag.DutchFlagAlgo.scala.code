def solve() {
  var afterRed:Int = getSize() - 1
  var beforeBlue:Int = 0
  var white:Int = getSize() - 1
  while(beforeBlue < white){
    if(getColor(beforeBlue) == BLUE)
      beforeBlue += 1
    if(getColor(beforeBlue) == RED){
      swap(white, afterRed)
      swap(beforeBlue, afterRed)
      afterRed -= 1
      white -= 1
    }
    if(getColor(beforeBlue) == WHITE){
      swap(white, beforeBlue)
      white -= 1
    }
  }
}