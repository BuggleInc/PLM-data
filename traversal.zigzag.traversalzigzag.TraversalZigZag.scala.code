override def run() {
  var cpt = 0;
  for(i <- 0 to getMondeLargeur-1){
    for (j <- 0 to getMondeHauteur-1){
      
      if(pair(i)) 				setPos(j, i);
      else 								setPos(getMondeLargeur-j-1, i)
      
      ecritMessage(cpt);
      cpt += 1;
    }
  }
}

def pair(n:Int):Boolean = {
  return n%2 == 0;
}