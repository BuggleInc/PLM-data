Compteur = 0
Do = False
while not sortieTrouvee():
    while Do == False or not croisement():
        forward()
        if estSurBiscuit():
            Compteur = Compteur + 1
        Do = True
    if Compteur >= 3:
        gauche()
        avance()
        Compteur = 0
        if estSurBiscuit():
            Compteur = Compteur + 1
    else:
        droite()
        avance()
        Compteur = 0
        if estSurBiscuit():
            Compteur = Compteur + 1