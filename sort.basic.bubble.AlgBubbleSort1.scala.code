def bubbleSort()
{
  var n = getNombreValeurs()-1;
  var i = 0;
  var isTermine = false;
  
  while (!isTermine)
  {
    isTermine = true;
    for (j <- 0 to i-1)
    {
      if (plusPetit(i,j))
      {
        echange(i,j);
        isTermine = false;
      }
    }
    i+=1;
  }
}




