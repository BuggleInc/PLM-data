def shellSort()  {
  var eca = getNombreValeurs()/2
  while (eca>0) {
    
    
    
    
    
        
        
    var v = getValeur(eca)
		var k = eca
   while (k>0 && !plusPetitQue(k,v)){
      
      copy(k-eca,k)
      k-=eca
   }
		setValeur(k,v)
        
        
        
        
      
        
    
    
        eca = eca/2
  }
}