def bubbleSort()  {
  var echanger =true
  var len = getNombreValeurs()
  while (echanger == true) {
    var not =0
    for (i <- 0 to len -2) {
    	if (plusPetit(i+1, i)) {
      	echange(i, i+1)
      };else { 
        not +=1
      }
      };if (not == len-2) {
      		echanger = false
    	}	
}
}

