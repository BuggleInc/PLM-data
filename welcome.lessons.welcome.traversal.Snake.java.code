public void run() {
	snakeBuggle();
}//end run

void snakeBuggle(){
int steps = 7;

	for(i = 0; i < steps; i++){
		for(int j = 1; j < steps; j++){
			forward();
		}

		if(isFacingWall() && getDirection == Direction.EAST){
			left();
			forward();
			left();
		}
	}	
}//end snakeBuggle

boolean finalPos(){
	return(isFacingWall());
}//end finalpos
