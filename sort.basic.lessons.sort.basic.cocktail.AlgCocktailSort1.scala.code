def cocktailSort() {
  var n = getNombreValeurs()
  var swap = false
  do {
    swap = false
    for (i <- 0 to (n-2)) {
    	if (plusPetit(i+1,i)) {
     		echange(i,i+1);
     		swap = true
      }
    };
    for (i <- (n-2) to 0) {
    	if (plusPetit(i,i+1)) {
     		echange(i+1,i);
     		swap = true
      }
    }
  } while (swap)
}
