override def run() {
  var k = 0
  var x=0
  var y = 0
  while (k!=48){
    setPos(x,y+1);y=y+1;setPos(x,y+1);y=y+1;setPos(x,y+1);y=y+1
    setPos(x,y+1);y=y+1;setPos(x,y+1);y=y+1;setPos(x,y+1);y=y+1
    setPos(x+1,y);x=x+1;setPos(x,y-1);y=y-1;setPos(x,y-1);y=y-1
    setPos(x,y-1);y=y-1;setPos(x,y-1);y=y-1;setPos(x,y-1);y=y-1
    setPos(x,y-1);y=y-1;setPos(x,y-1);y=y-1;setPos(x+1,y);x=x+1
  }
}

