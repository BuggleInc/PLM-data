override def run() {
var angle:Int = 0;

def Init(){
	while(!estSurBiscuit){
		if(!estFaceMur && angle == 0){
			setDirection(Direction.NORTH);
			avance();
		}
		else{
			garderMur();
		}
	}
}

def garderMur(){
	gauche();
	angle = angle + 1;
	if(estFaceMur){
		while(estFaceMur){
			gauche();
			angle = angle + 1;
		}
		avance();
	}
	else{
		avance();
	}
}

Init();

}

