def arrayFront9(nums:Array[Int]): Boolean = {
  
  var Neuf = false
  
 	if (nums.length < 4){
  	for (i <- 0 to nums.length-1){
    	if (nums(i) == 9){
        Neuf = true
      }
 	 	}
  } else if (nums.length >= 4){
    for (i <- 0 to 3){
      if (nums(i)==9){
        Neuf = true
      }
    }
  	}
  return Neuf
}