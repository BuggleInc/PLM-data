def cocktailSort3() {
  var n=getNombreValeurs()-2
	var t=0
  var ec=true
  var fg=false
  var gf=false
  do{
    ec=false
    fg=false
    for (i<- t to n){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        ec=true
      	fg=true}}
    if (!fg){return()}
    n-=1
    for (i<- n to t by -1){
      if (plusPetit(i+1,i)){
        echange(i+1,i);ec=true;fg=true
      }}
      
    if (!gf){return()}
    t=t+1
    
  }while (ec)
}
