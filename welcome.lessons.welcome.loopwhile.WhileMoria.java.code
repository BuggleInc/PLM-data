public void run() { 
while (true)
{
	right();
	right();
	forward();
	if (isOverBaggle())
	{
		pickupBaggle();
		right();
		right();
		forward();
		forward();
		forward();
		while(isOverBaggle())
		{
		back();
		dropBaggle();
	}}
}
}
/*isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle()
*/
