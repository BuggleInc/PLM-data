def cocktailSort2() {
	var debut:Int=0
	var fin:Int=getNombreValeurs()-2
	var c1 :Int =0
	var c2 :Int=0

	do{
	for(i<- debut to fin){
		c1=0
		if(!plusPetit(i,i+1)){
			echange(i,i+1)
			c1=c1+1
		}
		fin=fin-1
	}
	
	for(i<- fin to debut){
		c2=0
		if(!plusPetit(i,i+1)){
			echange(i,i+1)
			c2=c2+1
				}
		debut=debut+1
			}
	}	while(c1!=100)
}
