def array123(nums:Array[Int]): Boolean = {
  
  var ya1 = isValue(nums , 1)
  var ya2 = isValue(nums , 2)
  var ya3 = isValue(nums , 3)
  if ((ya1) && (ya2) && (ya3)) {return true}
  else {return false}
  
  
}

def isValue(nums:Array[Int] ,lookingFor:Int): Boolean = {
  var rep = false
  var last = nums.length -1
  for (i <- 0 to last){
    if (nums(i) == lookingFor){
      rep = true
      
    }
  } 
  return rep
}
