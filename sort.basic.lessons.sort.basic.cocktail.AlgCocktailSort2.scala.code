def cocktailSort2() 
{
	var hasSwap:Boolean = false;
	var end: Int = 1

	do{
		hasSwap = false;
		for(i <-1 to getNombreValeurs()-end by 1)
		{
			if (plusPetit(i, i-1))
				{
					echange(i,i-1);
					hasSwap = true;
				}
		}
		end-=1;
		for( i<- getNombreValeurs()-end to 1 by -1)
		{
			if (plusPetit(i, i-1))
				{
					echange(i-1,i);
					hasSwap = true;
				}
		}
		end+=1;
	}while(hasSwap)
	
}
	

