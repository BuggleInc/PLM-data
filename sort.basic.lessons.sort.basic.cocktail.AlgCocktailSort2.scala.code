def cocktailSort2() {
	var l = getNombreValeurs()-2
	var fin = false
	var debut = 0
	do {
		var a = 0
		for (i<- debut to l) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1); a=a+1
			}
			l -= 1
		}
		for ( i<- l to debut by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1);a=a+1
			}
			debut +=1
		}
		if (a==0) {
			fin = true
		}
	} while (!fin)
}
