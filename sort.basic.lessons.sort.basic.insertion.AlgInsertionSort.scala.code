def insertionSort() {
  var v = 0
  var j=0
  var changement=false
  for (i<-2 to getNombreValeurs()-1){
    changement = false
    v = getValeur(i)
    j=i-1
    while(j>=0){ 
      if( !plusPetitQue(j,v)&& j!=0 ){
        changement = true
        copie(j,j+1)
      } else if(j==0&&changement){
        changement=false
        copie(j,j+1)
        setValeur(j,v)
      }
      j=j-1
    }
  }
}