def max2Value(nums:Array[Int]): Int = {
  var taille = nums.length
  if (nums(0) >= nums(1))
  {var max1 = nums(0)
   var max2 = nums(1)}
  else 
  {
    var max1 = nums(1)
   var max2 = nums(0)
  }
  
  for ( i <- 0 to taille - 1)
  	{
      if (nums(i)>max1)
      {
        max2 = max1;
        max1 = nums(i)
        
      }
    }
  return max2 }
