override def run() {
	for (c<-1 to 4) {
		faireMotif();
	}
	avance(5);
	for (c<-1 to 4) {
		faireMotif();
	}
	avance(5);
	for (c<-1 to 4) {
		faireMotif();
	} // Motifs bas
	avance(4);
	gauche();
	avance(5)
	for (c<-1 to 4) {
		faireMotif();
	}
}

def faireMotif() {
	avance();
	baisseBrosse();
	leveBrosse();
	avance();
	gauche();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
	droite();
	avance();
	baisseBrosse();
	leveBrosse();
	avance();
	gauche();
}