int caughtSpeeding(int speed, boolean isBirthday) {
if (!isBirthday){
	if (speed<61){
		return 0;
	}
	if(estDans(61,80,speed)){
		return 1;
	}
	else {
		return 2;
	}
}
else {
	if (speed<66){
		return 0;
	}
	if(estDans(66,85,speed)){
		return 1;
	}
	else {
		return 2;
	}
}
}

boolean estDans(int i, int j, int a){
if(a>i-1 && a<j+1){
	return true;
}
else {
	return false;
}
}


