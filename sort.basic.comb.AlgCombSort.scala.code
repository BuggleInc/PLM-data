def combSort() {
  var ecart : Int = getNombreValeurs()
  var n : Int = getNombreValeurs()
  var i : Int  = 0  
  var aBouger : Boolean = true
  
  while (ecart > 1 ){
    aBouger = false
    if (ecart > 1){
      ecart = (ecart.toDouble/1.3).toInt
    }
    i = 0
    while (i + ecart < n){
      if(plusPetit(i, i + ecart) == false){
        echange(i,i+ecart)
        aBouger = true
      }
      i = i +1
    }
  }
}