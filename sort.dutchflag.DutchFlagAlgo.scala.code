def solve() {
		var afterBlue = 0;
		var beforeWhite = getTaille() - 1;
		var beforeRed = getTaille() - 1;
  
		while (afterBlue <= beforeWhite) {
			getCouleur(afterBlue) match {
			case BLUE => afterBlue += 1
			case WHITE => echange(afterBlue, beforeWhite); beforeWhite -= 1
			case RED => echange(afterBlue, beforeWhite); swap(beforeRed, beforeWhite); beforeWhite -= 1; beforeRed -= 1
			}
		}
	}