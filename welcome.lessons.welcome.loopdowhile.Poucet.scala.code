var n=0;
var m=0;
while(!sortieTrouvee()){
	avance();
	n=n+1;
	if(estSurBiscuit()){
		m=m+1;
	}
	if(n==5){
		if(m>=3){
			gauche();
			avance();
			m=0;
			if(estSurBiscuit()){
				m=m+1;
			}
			n=1;
		}else{ 
			droite();
			avance();
			m=0;
			if(estSurBiscuit()){
				m=m+1;
			}
			n=1;
		}
	}
}
avance();
