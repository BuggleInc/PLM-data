override def run() {
	var j:Int = 0;
	var flag:Boolean = true;
	while (!(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1)) {
		ecritMessage(j);
		if (getX() == getMondeLargeur()-1) {
			setPos(getX() , getY+1);
			flag = false
		}
		else if (flag == true) {
			setPos(getX() + 1, getY());
		}
		else setPos(getX() - 1, getY());
	j = j + 1;
	}
	ecritMessage("48");
}

