var diag = 0;

def fin(): Boolean = {
  if(getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1) {
    return true
  }
  else {
    return false
  }
}

override def run() {
  var compteur = 0
  ecritMessage(compteur)
  compteur = compteur + 1
  setPos(0,1)
  diag=1
  while(!fin() && diag<13) {
    if(getY()==0 && diag<7 && getX()<6) {
      diag = diag + 1
      ecritMessage(compteur)
      setPos(0,getX()+1)
      compteur = compteur + 1
    }
    else if(getY()==0 && getX()==6) {
      diag = diag + 1
      ecritMessage(compteur)
      setPos(1,6)
      compteur = compteur + 1
    }
    else if(getX()==getMondeLargeur()-1 && diag>=6) {
      diag = diag + 1
      ecritMessage(compteur)
      setPos(getY()+1,0)
      compteur = compteur + 1
    }
    else {
      ecritMessage(compteur)
      setPos(getX()+1,getY()-1)
      compteur = compteur + 1
    }
  }
}