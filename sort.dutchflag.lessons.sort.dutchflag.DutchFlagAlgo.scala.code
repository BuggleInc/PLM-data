def solve() {
  var ap_bleu:Int = 0;
  var en_blanc_rouge:Int = getTaille()-1;
  var av_rouge:Int = getTaille()-1;
  var av_blanc:Int = getTaille()-1;
  var coul:Int = 0
 	while(ap_bleu<=av_blanc){
  	coul = getCouleur(ap_bleu);
    if(coul == 0 ){
      ap_bleu+=1;
    }
    else if(coul == 1){
      echange(ap_bleu,av_blanc-1)
      av_blanc += -1;
    }
    else {
      echange(ap_bleu,av_rouge-1);
      av_rouge += -1;
    }
  } 
}