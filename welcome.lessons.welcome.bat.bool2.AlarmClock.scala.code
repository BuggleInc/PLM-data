def alarmClock(day:Int, vacation:Boolean): String = {
  (day,vacation) match {
    case (1,false) =>"7:00"
    case (2,false) =>"7:00"
    case (3,false) =>"7:00"
    case (4,false) =>"7:00"
    case (5,false) =>"7:00"
    case (6,false) =>"10:00"
    case (0,false) =>"10:00"
    case (1,true) =>"10:00"
    case (2,true) =>"10:00"
    case (3,true) =>"10:00"
    case (4,true) =>"10:00"
    case (5,true) =>"10:00"
    case (6,true) =>"off"
    case (0,true) =>"off"
  }
}