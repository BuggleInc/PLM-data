int golomb(int num) {
	if (num == 1) return 1;		
	int[] result = new int[num + 1];
	result[2] = 2;
	int pointer = 2;
	for (int i = 2; i <= num; i++) {
		int times = result[i];
		for (int j = pointer; j <= Math.min(num, (pointer + times - 1)); j++) {
			result[j] = i;
		}
		pointer += times;
	}
	return result[num];
}