/**
 * Avance, recupere le biscuit, revient au point de depert et pose le biscuit
 * 
 * Ne verifie pas la presence de murs.
 */
 void goAndGet()
 {
 	int compteur = 0;
 	while (!estSurBiscuit()){
		avance();
		compteur++;
 	
 		}
 	prendBiscuit();
 	while(compteur!=0){
 		recule();
 		compteur--;	
 	}
 	poseBiscuit();
 }