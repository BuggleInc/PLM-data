def shellSort()  {
  var i=0
  var v=0
  var ecart=getNombreValeurs()/2
  while(ecart>0){
    for(i <- 0 to getNombreValeurs()-1-ecart){
      v=getValeur(i)
      if(plusPetitQue(i+ecart,v)==true){
        for(j <- i+1 to ecart+1){
       		copie(i+1,i)
        }
      }
      setValeur(i+ecart,v)
    }
    ecart=ecart/2
  }
}
  
  
  
  
  
  
  