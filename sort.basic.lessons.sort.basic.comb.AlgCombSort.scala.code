def combSort() {
  var ecart = getNombreValeurs();
  var enOrdre = true ;
  while(ecart>1 || enOrdre){
    enOrdre =false ;
    if ( ecart > 1)
    ecart = (ecart.asInstanceOf[Double] / 			1.3).asInstanceOf[Int]
    var i = 0 ;
    while ( i+ecart<getNombreValeurs()){
      if ( plusPetit(i+1,i)){
        enOrdre = true ;
      }
      i+=1;
    }

  }
}