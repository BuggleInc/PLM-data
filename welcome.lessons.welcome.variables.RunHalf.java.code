public void run() {
int OrangeCount=0;
int BaggleCount=0;
//double BaggleCheck=(OrangeCount+1)\2;

while (isOverBaggle() == false && 2*BaggleCount != OrangeCount+1)
		{if (isOverOrange() == true) {OrangeCount = OrangeCount+1;}
		if (isOverBaggle() == true) {BaggleCount = BaggleCount+1;}
		forward();}
}



