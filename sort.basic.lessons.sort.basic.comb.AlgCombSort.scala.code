def combSort() 
{
  var ecart=getNombreValeurs()-2
  var swap=true
  do
  {
    swap=false
    if(ecart>1)
    {
      ecart=ecart/1.3
    }
    i=0
    while(i+ecart<getNombreValeurs)
    {
      if(plusPetit(i+ecart,i))
      {
        echange(i,i+ecart)
        swap=true
      }
      i+=1
    }
  }
  while(ecart>1 || swap)
}