def combSort() {
	var ecart = getNombreValeurs()
	var is_echange = false
	do {
		is_echange = false
		if (ecart > 1) {
			ecart = (ecart.toDouble /1.3).toInt
			}
		var i:Int = 0
		do {
			if (plusPetit(i+ecart,i)) {
				echange(i,i+ecart)
				i += 1
				is_echange = true
			}
		} while ((i+ecart) < getNombreValeurs())
	} while ((ecart > 1) || is_echange)
}
