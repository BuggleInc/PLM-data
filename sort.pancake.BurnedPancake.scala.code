def solve() {
  while(!isSorted()){
    for(i <- 0 to getStackSize()-2){
      if(getPancakeRadius(i) > getPancakeRadius(i+1)){
        flip(i+1)
        flip(i+2)
        }
      }
    
  	for(k <- 0 to getStackSize()-1){
      if (isPancakeUpsideDown(k)==false){
        flip(k+1)
        flip(1)
      }
    }
  }}