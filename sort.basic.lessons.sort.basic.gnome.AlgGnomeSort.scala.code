def gnomeSort() {
  var n:Int = getNombreValeurs()
  var position_gnome:Int = 0
  var condition_limite:Boolean = false
  while (!condition_limite){
    if (position_gnome + 2 > n) {conditon_limite=true}
    else {
    	(plusPetit(position_gnome + 2, position_gnome + 1))match{
      case true if (position_gnome == -1) => echange(position_gnome + 2, position_gnome + 1), position_gnome = position_gnome + 1
      case true => echange(position_gnome + 2, position_gnome + 1), position_gnome = position_gnome - 1
    	case false => position_gnome = position_gnome + 1
    	}
  	}
}