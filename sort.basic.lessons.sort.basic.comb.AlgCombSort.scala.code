def combSort() {
  var ecart=getNombreValeurs()
  var inverse=true
  do{
    if(ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i=0
    inverse=false
    while(i+ecart<getNombreValeurs()){
      i=i+ecart
      inverse=true
      i=i+1
    }
  }
  while(ecart > 1 || inverse == true)
  }