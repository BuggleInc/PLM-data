def cocktailSort2() {
  var continue = true
  var beg = 0
  var end = getValueCount()-2
  
  while(continue){
  continue = false
  for (i <- beg to end){
    if (isSmaller(i+1, i)){
      swap(i+1, i)
      continue = true
    }
  }
  end -= 1
    
  for (i <- end to beg by -1){
    if (isSmaller(i+1, i)){
      swap(i+1, i)
      continue = true
    }
  }
  beg += 1
    
  }
}