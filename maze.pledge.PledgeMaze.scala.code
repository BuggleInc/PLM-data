var c=0
override def run() {

  while(isOverBaggle==false){
    if((!murNord)&&(c==0)){forward();}
    else{
      right();
      c-=1;
      suivremur()
    }
  }
  pickupBaggle;
}


def suivremur()={
  while((isOverBaggle==false)&&(c!=0)){
    mur()
  }
}


def mur()={

  left()
  c+=1;
  while(isFacingWall==true){
    right();
    c-=1;
  }
  forward();
}

def murNord():Boolean={
  setDirection(Direction.NORTH)
  if(isFacingWall==true){return true;}
  else {return false;} 
}