override def run() {
  baisseBrosse()
  avance(6)
  while(!fini()){
    serpute()
  }
  droite()
}

def fini():Boolean = {
  if (estFaceMur()==true){
    gauche()
    if (estFaceMur()==true){
    return true
    }
    droite()
  }
  return false
}

def serpute(){
  if (getDirection()==Direction.EST){
    gauche()
    avance()
    gauche()
  }else if (getDirection()==Direction.OUEST){
    droite()
    avance()
    droite()
  }
  avance(6)
}
