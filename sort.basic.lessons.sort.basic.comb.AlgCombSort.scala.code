def combSort() {
  val n = getNombreValeurs()
  var eca = n
  var e = 0
  var i = 0
  do {e=0
      if (eca > 1) {eca = (eca.toDouble / 1.3).toInt}
      i = 0
      while (i + eca < n) {
        if (plusPetit(i+eca,i)) {echange(i+eca,i);e += 1}
        i+=1
      }
     } while (eca > 1 ||  e==0)
  if (e!=0) {i = 0
      while (i + eca < n) {
        if (plusPetit(i+eca,i)) {echange(i+eca,i);e += 1}
        i+=1}
}