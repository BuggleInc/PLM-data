def parotTrouble(talking:Boolean, hour:Int):Boolean = {
  if (talking == false) {
    return false
  }else {
    hour match {
      case (1) => true
      case (2) => true
      case (3) => true
      case (4) => true
      case (5) => true
      case (6) => true
      case (21) => true
      case (22) => true
      case (23) => true
      case _ => false
    }
  }
  }