var fin:Boolean = true


def cocktailSort() {
  
  do{
    fin = false;
    for (i <- 0 to getNombreValeurs() - 2){
      if (!plusPetit(i,i+1)){
        echange(i,i+1)
        fin = true;
      }
     
     for (i <-0 to getNombreValeurs()-2) {
       if (!plusPetit(i,i+1)){
         echange(i,i+1)
         fin = true;
       }
     }
    }
  }while (fin == true)
}