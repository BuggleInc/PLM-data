def cocktailSort3() {
var dbt = 0;
var fin = getNombreValeurs()-2;
var inv = true;
do {
	inv = false;
	for (i <- dbt to fin) {
		if (plusPetit(i+1,i)) {
			echange(i,i+1);
			inv = true;
		}
	}
	if (!inv) {return;}
	fin -= 1;
	for (i <- fin to dbt by -1) {
		if (plusPetit(i+1,i)) {
			echange(i,i+1);
			inv = true;
		} 
}
if (!inv) {return;}
dbt += 1;
} while (inv)
}
