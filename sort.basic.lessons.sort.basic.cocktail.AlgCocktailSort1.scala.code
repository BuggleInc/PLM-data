def cocktailSort() {
  var changement=true
  
  while (changement==true){
    changement=false
    for (compteur<-0 to getNombreValeurs()-2 ){
      if (plusPetit(compteur+1,compteur)==true){
        echange(compteur+1,compteur)
        changement=true
      } 
    }
    for (compteur<-getNombreValeurs()-1 to 1 by -1){
      if (plusPetit(compteur,compteur-1)==true){
        echange(compteur,compteur-1)
        changement=true
      }
    }
  }
}