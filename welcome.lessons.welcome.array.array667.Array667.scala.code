def array667(nums:Array[Int]): Int = {
	var j:Int = 0;
	var flag:Int = 0;
	for (i <- 0 to nums.length-1) {
		if (nums(i) == 6 || nums(i) == 7) {
		if (nums(i) == 6 && flag == 6) {
			flag = 6; j = j+1;
		}
			else if (nums(i) == 7 && flag == 6) {
			flag = 0; j = j+1;
		}
			else flag = 6;
	}
	}
	return j;
}
