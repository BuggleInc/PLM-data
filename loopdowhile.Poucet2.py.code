# while I did not find the exit, take the next corridor to decide whether I should turn left or right at the next intersection

firstTime = True
while firstTime or not exitReached():
  firstTime = False
  x=0
  secfirstTime = True
  while secfirstTime or not crossing():
    secfirstTime = False
    forward()
    if isOverBaggle():
      x=x+1
  if x>=3:
    left()
  elif x<=2:
    right()
forward()
