def nord() {
  setDirection(Direction.NORTH)
  while (!isFacingWall()){
    forward()
  }
  
  
}

override def run() {
	var state = 0
  var a = getDirection()
  do{
    if (state==0){
      nord()
      state = 1
    }
    else{
  		a = getDirection()
      keepHandOnSideWall()
      setDirection(Direction.NORTH)
      if (isFacingWall()){
        state=1
        setDirection(a)
      }
      else{
        state = 0
      }
    }
    
    
    
    
    
    
  }while (!isOverBaggle())
  pickupBaggle()
}