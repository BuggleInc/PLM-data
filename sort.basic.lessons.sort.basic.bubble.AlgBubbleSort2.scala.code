def bubbleSort2()  {
  var nbValeurs = getNombreValeurs()
	for (j <- 0 to nbValeurs-1){
    var compteur = 0
    for (i <- 0 to nbValeurs-j-2){
    	if (!plusPetit(i,i+1)){
        echange(i,i+1)
        compteur += 1
      }
    }
    if (compteur == 0){
      for (i <- 0 to 55){
        compteur += 1
      }
    	return
    }
  }
}

