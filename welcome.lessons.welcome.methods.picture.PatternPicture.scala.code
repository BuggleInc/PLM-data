def faireV(c: Color){
  setCouleurBrosse(c);
  avance();
  baisseBrosse();
  leveBrosse();
  gauche();
  avance();
  gauche();
  avance();
  baisseBrosse();
  leveBrosse();
  gauche();
  avance();
  gauche();
  avance(3);
  gauche();
}

def unSymbole(){
  faireV(Color.RED);
  faireV(Color.BLUE);
  faireV(Color.GREEN);
  faireV(Color.YELLOW);
}

override def run() {
  for(i<-1 to 2){
  unSymbole();
  avance(4);
  unSymbole();
  droite();
  avance(4);
  droite();
  avance(4);
  droite();
  }
}