void paintSquare() {
	brushDown();
	brushUp();
}
void makeV() {
	forward(2);
	paintSquare();
	forward();
	left();
	forward();
	paintSquare();
	backward();
	right();
	forward();
	paintSquare();
	forward(2);
}

void vSquare() {
	for(int i = 0; i < 4; i++) {
		makeV();
		left();
	}
}

void sRow() {
	for(int j = 0; j < 3; j++) {
		vSquare();
		forward(7);
	}
}
void rCol() {
	for(int i = 0; i < 3; i++) {
		sRow();
		left();
		forward(7);
		right();
	}
}

public void run() {
	rCol();
}
