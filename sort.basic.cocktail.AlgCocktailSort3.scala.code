def cocktailSort3() {
  var debut=0;var fin=getNombreValeurs()-2;var a=1
  var b=1
  while (b==1) { 
  	b=0
    a=0
    for (i<-debut to fin) {
      if (!plusPetit(i,i+1)) {echange(i,i+1);b=1;a=1}
    }
    fin-=1
    for (i <- (debut to fin).reverse) {
      if (!plusPetit(i,i+1)) {echange(i,i+1);b=1}
    }
    debut+=1
    if (a==0) {return}
}
}