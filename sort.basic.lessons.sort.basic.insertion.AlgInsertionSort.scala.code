def insertionSort() {
  var v = 0
  var j=0
  var changement=false
  var fini = false
  for (i<-2 to getNombreValeurs()-1){
    fini = false
    changement = false
    v = getValeur(i)
    j=i-1
    while(j>=0&& !fini){ 
      if( !plusPetitQue(j,v)&& j!=0 ){
        changement = true
        copie(j,j+1)
      } else if(changement){
        copie(j,j+1)
        setValeur(j,v)
        fini=true
      }
      j=j-1
    }
  }
}