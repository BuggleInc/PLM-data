def combSort() {
  var ecart = getNombreValeurs()
  var switch=true
  var i=0
  while (ecart>1 || switch) {
    switch=false
    if (ecart>1) {ecart = (ecart.toDouble / 1.3).toInt}
    i=0
    while (i+ecart<getNombreValeurs()) {
      if (plusPetit(i+ecart,i)) {echange(i,i+ecart);switch=true}
      i+=1
    }
  }
}
