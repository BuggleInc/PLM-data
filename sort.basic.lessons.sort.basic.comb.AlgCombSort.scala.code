def combSort() {
  var lgr=0
  var ecart=lgr
  var Inverse=true
  

  do {
      if (ecart>1) {
        ecart = (ecart.toDouble / 1.3).toInt
      }
      var i=0

      while (i+ecart<lgr) {
        if (!plusPetit(i, i+ecart)) {
          echange(i, i+ecart)
        }
        i=i+1
      }
    } while (Inverse || ecart>1)
  }

