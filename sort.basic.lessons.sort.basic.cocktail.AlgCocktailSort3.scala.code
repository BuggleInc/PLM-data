def cocktailSort3() {
  var c:Boolean = true
  var n:Int = getNombreValeurs()-1
  var f:Int = 0
  while (c){
    c=false
    for (i<- 0 to n-f){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        c = true
      }
    }
    if(!c){return}
    for (i <- 0 to n-f){
      if (plusPetit(n-f-i+1,n-f-i)){
        echange(n-f-i+1,n-f-i)
        c = true
    	}
     }
   }
  
}