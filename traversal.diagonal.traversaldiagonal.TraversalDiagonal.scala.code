var diag = 1

override def run() {
  var cpt=0
  ecritMessage(0)
  while(!testFin()){
    go()
    cpt+=1
    ecritMessage(cpt)
  }
}

def testFin():Boolean= {
  if(getX()+1==getMondeLargeur && getY()+1==getMondeHauteur){
    return true
  }else{
    return false
  }
}

def go(){
  if(getX()+1==getMondeLargeur || getY()==0){
    if(diag<6){
      setPos(0,diag)
    }else{
      setPos(getMondeHauteur-1,7-diag)
    }
    diag+=1
  }else{
    setPos(getX()+1,getY()-1)
  }
}