override def run() {
  var x = 0
  var y = 0
  var num = 0
  do{
    while(!estFaceMur()){
      ecritMessage(num)
      y = y+1
      setPos(x,y)
    	num = num +1
    }
    y=0
    x=x+1
    setPos(x,y) 
  }while(!posFinale(x,y))
  
}

def posFinale(x:Int,y:Int): Boolean ={
    return ((getX() == getMondeLargeur()) && (getY()== getMondeHauteur()))
}