override def run() {
  var i=0
  var x=1
  var y=2
  ecritMessage(i)
  avance()
  while (!posFinale()) {
    if (!estFaceMur()) {
    	ecritMessage(i)
    	if (y<=getMondeHauteur()) {setPos(x,y);y+=1}      
    }
		else if (!posFinale() && estFaceMur()) {x+=1;setPos(x,y)}
  }
  
  def posFinale() : Boolean = {
    if (estFaceMur()) {
      gauche()
      if (estFaceMur()) {droite; return true}
      else {return false}
  	}
    else {return false}
  }
}