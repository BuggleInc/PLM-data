override def run() {
  var etat = 0;
  etat match {
    case 0 => courirNord(); etat = 1
    case 1 => suiveurEst(); etat = 0
    
  }
}



def courirNord() {
  while (!estFaceMur) {
    forward();
  }
  if (estFaceMur) {
    right();
  }
}



def suiveurEst() {
  while (!estSurBiscuit) {
    	keepHandOnSideWall();
  }
  prendBiscuit();
}


def keepHandOnSideWall() {
  left();
  while (estFaceMur()) {
    right();
  }
  forward();
}