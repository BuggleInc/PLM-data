def insertionSort() {
  var len = getNombreValeurs()
  var continueboucle:Boolean = false
  var v = 0
  var rg = 0
  for (i <- 1 to len-1) {
    rg = i-1
    v = getValeur(i)
    continueboucle = getValeur(rg)>v
    while (rg>0) {
      if (getValeur(rg)>v)
      	copie(rg,rg+1)
      else
        rg=0
    	setValeur(rg+1,v)
      rg-=1
    }
	}
}