def gnomeSort() {
  var L = getNombreValeurs()
  var i=0
  var change= true
  while (change == true){
		 if (getValeur(i)> getValeur(i+1)) {
    	echange(i, i+1)
      change = true
      if (i!=0) {
      i = i-1
      }
  	}
    else if (getValeur(i)< getValeur(i+1) && i!=L) {
      i=i+1
      change = true
      
    }
    else if (i == L-1){
        change = false
      }
  }
  return
}