public void bubbleSort3() {
  
  int nb = getValueCount();
  boolean swap = true;
  for(int i = 0; i < nb -1 ; i++){
    swap = true;
    for( int j = 0 ; j < nb - i - 1 ; j++){
      if(!isSmaller(j,j++)){
        swap(j++,j);
        swap = false; 
        
      }
      
    }
    if(swap)
      break;
  }
}