var fini:Boolean=false
var m:String=""
while (fini==false){
  m=litMessage()
  if (m=="R"){
    droite();
    avance();
  }else if (m=="L"){
    gauche();
    avance();
  }else if (m=="I"){
    droite();
    droite();
    avance();
  }else if (m=="A"){
    avance();
  }else if (m=="B"){
    avance(2);
  }else if (m=="C"){
    avance(3);
  }else if (m=="Z"){
    recule();
  }else if (m=="Y"){
    recule(2);
  }else if (m=="X"){
    recule(3);
  }else {
    fini=true
  }
  
}
