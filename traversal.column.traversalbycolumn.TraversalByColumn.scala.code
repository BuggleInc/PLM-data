var compt:Int = 0

override def run() {
  
  
  var posFin:Boolean = false
  
  while(posFin != true){
    avancer()
    
    if(getX() == 6 && getY() == 6){
      posFin = true;
    }
  }

  }


def avancer() {
  for (x <- 0 to getMondeLargeur()-2){
    for (y <- 0 to getMondeHauteur()-2){
      ecritMessage(compt)
      compt = compt + 1;
      setPos(x, y)
    }
  }
}