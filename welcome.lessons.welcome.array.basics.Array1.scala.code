override def run() {
	var T = new Array[Color](getMondeHauteur())
	for (i<-0 to getMondeHauteur()-1) {
		avance();
		T(i)=getCouleurSol()
	}
	for (i<-0 to getMondeLargeur()-1) {
		avance();gauche();avance();droite();
		for (i<-0 to getMondeHauteur()) {
			avance();setCouleurBrosse(T(i));baisseBrosse();leveBrosse();
		}
	}
}
