override def run() {
  while (!estSurBiscuit())
  {
    keepHandOnSideWall()
  }
  prendBiscuit()
}

def keepHandOnSideWall()
{
  var murGauche = false
  if (estFaceMur())
  {
    gauche()
  }
  gauche()
  if (estFaceMur())
  {
    murGauche = true
  }
  droite()
  if (murGauche && !estFaceMur())
  {
    avance()
  }
  if (!murGauche)
  {
    gauche()
    avance()
  }
}