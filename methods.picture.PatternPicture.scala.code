override def run() {def makehalfpattern(c:Color, d:Color) {setBrushColor(c); left(); forward(); brushDown(); brushUp(); right(); forward(); right(); forward(); brushDown(); brushUp(); setBrushColor(d); left(); forward(); brushDown(); brushUp(); forward(); left(); forward(); brushDown(); brushUp()}
def makepattern() {makehalfpattern(Color.RED, Color.BLUE); forward(); makehalfpattern(Color.GREEN, Color.YELLOW)} 
makepattern()}