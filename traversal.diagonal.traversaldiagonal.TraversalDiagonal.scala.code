override def run() {
  var diag = 0
  var count = 0
  ecritMessage(count)
  setY(1)
  diag=1
  while (PositionFinale()==false){
    count=count+1
    ecritMessage(count)
    if (getX()==diag && getY()==0){
      diag=diag+1
      setX(0);setY(diag)
      
    }
    else{
      setX(getX()+1);setY(getY()-1)
    }
  }
}


def PositionFinale() : Boolean = {
  var H=getMondeHauteur()
  var L=getMondeLargeur()
  if (getX()+1==L && getY()+1==H){
    return true
  }
  return false
}