def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if (isBirthday==false)
	{
		(speed) match {
  case speed<60 => return 0
  case speed<=80 && speed>60  => return 1
  case speed>81 => return 2
	}
	else
	{
		(speed) match {
  case speed<65 => return 0
  case speed<=85 && speed>65  => return 1
  case speed>86 => return 2
	}


	
}
}
}