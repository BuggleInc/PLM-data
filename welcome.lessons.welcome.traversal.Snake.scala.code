override def run() {
	baisseBrosse()
	def positionFinale():Boolean= {
		return false
	}
	
	while (!positionFinale()) {
		if (estFaceMur()) {
			if (getDirection()==Direction.WEST) {
				droite();
				avance();
				droite();
			} else if (getDirection()==Direction.EAST) {
				gauche();
				avance();
				gauche();
			}
		} else {
			avance();
		}
	}
}