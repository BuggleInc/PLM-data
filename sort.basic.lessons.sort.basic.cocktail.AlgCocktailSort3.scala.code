def cocktailSort3() {
	
var swap : Boolean = true
var debut: Int = 0
var fin: Int = getNombreValeurs()-2

 
 while ( swap == true ) {
 	swap = false
	for (i <- debut to fin ) {
		
		if ( plusPetit(i , i+1) == false ) {
			echange (i , i+1 ) 
			swap = true
			
		}
	
		
	} else { return }
	fin = fin-1
	
	for (j <- fin to debut by -1) {
		
		if ( plusPetit(j , j+1) == false ) {
			echange (j , j+1 ) 
			swap = true
			
		}
		
	}
	debut = debut + 1 
	
}

}
