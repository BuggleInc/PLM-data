def cocktailSort() {
  var c:Boolean = true
  var n:Int = getNombreValeurs()-2
  while (c){
    c=false
    for (i<- 0 to n){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        c = true
      }
    }
    for (i <- n to 0){
      if (plusPetit(i+1,i)){
        echange(i-1,i)
        c = true
    	}
     }
   }
  
}
