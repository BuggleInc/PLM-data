def cocktailSort() {
  var x:Boolean = false
  while(!x){
    x = true
    for(i<- 0 to getNombresValeurs()-2){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        x = false
      }
    }
    for(j<- getNombresValeurs()-2 to 0){
      if(plusPetit(j,j+1)){
        echange(i,i+1)
        x = false
      }
    }
  }
}