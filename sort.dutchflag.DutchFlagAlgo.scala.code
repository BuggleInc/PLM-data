def solve() {
  var c=0
  while (c!=15){
   for (i <- 1 to getTaille-1){
     for (j <- 1 to getTaille-i-1){
       if (getCouleur(i)==0){
         if (getCouleur(getTaille-j)==2 || getCouleur(getTaille-j)==1){
           echange(i,getTaille-j)
         }
     	 } else if (getCouleur(i)==1) {
         if (getCouleur(getTaille-j)==2){
           echange(i,getTaille-j)
         }
       }
   		}
		}
    c=c+1
	}
}