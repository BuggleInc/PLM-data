def cocktailSort() {
	
  var swap:Boolean = true
  
  do {
    
  	swap = false
   
		for (i <- 0 to getNombreValeurs()-2) {
    
			if (!plusPetit(i,i+1)) {
      
      	echange(i,i+1)
      	swap = plusPetit(i,i+1)
      
    	}
    
  	}
  
  	for (i <- 1 to getNombreValeurs()-1 by -1) {
    
  		if (!plusPetit(i-1,i)) {
      
      	echange(i,i+1)
      	swap = plusPetit(i-1,i)
      
    	}
    
  	}

	} while (swap)
  
}