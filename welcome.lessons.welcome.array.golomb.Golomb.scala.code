def golomb(num:Int): Int = {
var compt = 0;
var T = new Array [Int] (num+1);
T(0) = 0;
T(1)=1;
for (i<- 2 to num) {
	for (j <- 1 to i-1) {
		if (T(i-1) == T(j)) {
			compt = compt+1;
		}
	if (T(i-1)==compt) {
		T(i) = T(i-1) +1;
		compt = 0;
	}
	else {
		T(i)=T(i-1);
		compt = 0;
}
}
}
return T(num);
}
