def combSort() {
	var ecart = getNombreValeur;
	var swap = false
while(ecart > 1 || swap = true){
	if (ecart > 1){
		swap = false
		ecart = ecart / 1.3
		var i = 0 
		while (i + ecart < getNombreValeur){
			if (plusPetit(i+ecart,i)){
				echange(i,i+ecart)
				i = i + 1
				swap = true
			}
		}
	}
}
}
