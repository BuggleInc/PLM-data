override def run() {
  while (!isOverBaggle()) {
    while (getDirection() != Direction.NORTH) {
      right()
    }
    
    while (! isFacingWall()) {
      forward()
    }
    
    keepWallOnLeft()
  }
  pickupBaggle()
}


def keepWallOnLeft() {
  var a:Boolean = false
  left()
  a = isFacingWall()
  
  if (a) {
    right()
    if (!isFacingWall())
    	forward()
    else
    	right()
  } else {
    forward()
  }
}