override def run() {
  exo()
}

def positionFinale():Boolean={
  if((getX()== getMondeLargeur() -1) && (getY()== getMondeHauteur -1)){
    return true
  }
  return false
}

def exo(){
  var n = 0
  while(!positionFinale){
    if(getX() == getMondeHauteur -1){
      ecritMessage(n)
      setPos(0,getX()+1)
      n=n+1
    }
    else{
      ecritMessage(n)
      setPos(getY()+1,getX())
      n=n+1
    }
  }
  ecritMessage(n)
}