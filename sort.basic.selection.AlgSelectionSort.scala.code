def selectionSort() {
  
  for (i <- 0 to getNombreValeurs()-1) {
    
    var min = i
    
    for (j <- i+1 to getNombreValeurs()-2) {
      
      if (plusPetit(j,min)) {
        
        min = j
        
      }
      
    }
    
    if (min != i) {
      
      echange(i,min)
      
    }
    
  }
  
}