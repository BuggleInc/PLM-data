def max2Value(nums:Array[Int]): Int = {
  var max= nums(0)
  var indiceMax= 0
  var max2=nums(0)
  for( i <- 0 to nums.length-1){
    if(max<nums(i)){
      max= nums(i)
      indiceMax=i
    }
  }
  for( i <- 0 to nums.length-1){
    if(max2<nums(i) && i!=indiceMax){max2=nums(i)}
  }
  return max2
}