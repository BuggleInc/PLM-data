override def run() {
  var T = new Array[Color](getMondeHauteur())
  for (i<- 1 to getMondeHauteur()){
    T(i-1)= getCouleurSol()
    avance()
  }
  for (i<- 1 to getMondeLargeur()){
    gauche()
    avance()
    droite()
    for (j <- 1 to getMondeHauteur()){
      setCouleurBrosse(T(j-1))
      baisseBrosse()
      leveBrosse()
      avance()
    }
  }
}