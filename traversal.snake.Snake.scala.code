override def run() {
  brushDown();
  while (!estFaceMur) {
    forward();
  }
  if (getDirection() == Direction.EST) {
    left(); 
    if (!estFaceMur) {
      forward(); left();
    }
    left();
  }
  if (getDirection() == Direction.OUEST) {
    right();
    if (!estFaceMur) {
      forward(); right();
    }
    right();
  }
}