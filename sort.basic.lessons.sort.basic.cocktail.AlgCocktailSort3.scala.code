def cocktailSort3() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var b = 0
  
  do {
    b=1
  	for(k<-debut to fin) {
    	if (!plusPetit(k,k+1)) {
      	echange(k,k+1)
      	
      	b=0
    	}     
  	}
    if (b==1) {
      return
    }
    fin-=1
  	
    b=1
    for (j<-fin to debut by -1) {
    	if (!plusPetit(j,j+1)) {
      	echange(j,j+1)
      	
      	b=0
    	}
  	}
    if (b==1) {
      return
    }
    debut+=1
  } while (b==0)
}
