var mem = 1
def cocktailSort() {
  do {
    mem=0
    for (i<- 0 to getNombreValeurs-2) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
        mem=1
      }
    }
    for (i<-0 to getNombreValeurs-3) {
      if (plusPetit((getNombreValeurs-2)-i,(getNombreValeurs-2)-i-1)) {
        echange((getNombreValeurs-2)-i,(getNombreValeurs-2)-i-1)
        mem=1
      }
    }
  } while (mem==1)
}