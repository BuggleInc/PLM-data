def cocktailSort() {

var trie : Boolean = false;
var nb : Int = 0;
var nb2 : Int = 0;

	while (!trie) {
		nb = 0;
		nb2 = 0;
		for (i <- 0 to getNombreValeurs() - 2) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				nb = nb + 1;
			}
		}


		for (j <- getNombreValeurs() - 2 to 0 by -1) {
			if (!plusPetit(j,j+1)) {
				echange(j,j+1);
				nb2 = nb2 + 1;
			}
		}
		
		if ((nb == 0) && (nb2 == 0)) {
			trie = true;
		}
		
	}
	
}