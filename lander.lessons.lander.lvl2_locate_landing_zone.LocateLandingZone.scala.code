def getLandingZone():Segment = {

	var c = 0

	var point1 = new Point(0,0)
	var point2 = new Point(0,0)
	

	while ( c <  getSol.length - 120)
	{
		if (getSol()(c).y == getSol()(c + 100).y )
		{
			if (getSol()(c).y == getSol()(c + 120).y)
			{
				point1 = getSol()(c)
				point2 = getSol()(c + 2)
				c = getSol.length - 2
			}
		}
		
		c = c + 1
	}

	var fina = new Segment(point1 , point2)

	
	return fina

}
