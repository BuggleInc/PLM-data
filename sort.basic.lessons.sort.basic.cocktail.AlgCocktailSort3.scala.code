def cocktailSort3() {

	var end = 0
	var debut = 0
	var fin = getNombreValeurs()-2

	do {
		end=0
		
		for (i<-debut to fin) {
			if (plusPetit(i+1, i)) {
				echange(i+1, i)
				end+=1
			}
		}
		if (end==0) {return}
		fin -= 1
		end=0
		
		for (i<-fin to debut by -1) {
			if (plusPetit(i+1, i)) {
				echange(i+1, i)
				end+=1
			}
		}
		if (end==0) {return}
		debut += 1
	} while (end!=0)

}
