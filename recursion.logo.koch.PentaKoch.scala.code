def pentaKoch(levels:Int, length:Double) {
    if (levels!=0){
      squareSide(levels-1, length*0.4)
      gauche(108)
      squareSide(levels-1, length*0.4)
      droite(108)
      squareSide(levels-1, length*0.4)
      droite(108)
      squareSide(levels-1, length*0.4)
      gauche(108)
      squareSide(levels-1, length*0.4)
      gauche(108)
  }else{
  	avance(length)
  }
}