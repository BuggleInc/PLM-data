def golomb(num):
    a=[]
    for b in range(num-1):
        a.append(0)
    for n in range(2,len(a)):
        j = 0
        for i in range(len(a)):
            if a[i] == a[n-1]:
                j +=1
        if j<a[n-1]:
            a[n] = a[n-1]
        elif j==a[n-1]:
            a[n]=a[n-1]+1
    return a[num]    