override def run() {
  
  
  while (!isFacingWall()){
    forward()
  }
  
  while (!isOverBaggle()){
    keepHandOnSideWall()
    
		forward()
  }
  
  
  pickupBaggle()
}



def keepHandOnSideWall(){
  
  if (getDirection() == Direction.NORTH){
    
	right()
  for (i <- 1 to 3){
  	if (isFacingWall()){
   	 left()
  	}
  }
    
  }else{
    
 	left()
  for (i <- 1 to 3){
  	if (isFacingWall()){
   	 right()
  	}
  }
  }
  
}
