def triangle(l:Int)={
  droite(30);avance(l);droite(120);avance(l);droite(120);avance(l);droite(180)
}




def maison(x:Int) {
  avance(x);triangle(x);avance(x);droite(90);avance(x);droite(90);avance(x);droite(90)
}



