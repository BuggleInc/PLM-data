var diag = 0;
override def run() {
  var diag=0
  var y=0
  var x=0
  var compteur=0
  while (diag!=getMondeHauteur()-1){
    for(i<-0 to diag){
      for(j<-0 to i){
        setPos(x,hauteur-1-y)
        ecritMessage(compteur)
        compteur+=1
        y=y+1
      }
      y=0
      x=x+1
    }
    
    diag+=1
  }
}