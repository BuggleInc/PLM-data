def combSort() {
  val taille=getNombreValeurs()
  var ecart=taille
  var fin=true
  do {
    fin=true
    if (ecart>1) {
      ecart=ecart/1.3
    }
    var i=0
    while (i+ecart<taille) {
      if (!plusPetit(i,i+ecart)){
        echange (i,i+ecart)
        fin=false
      }
      i=i+1
    }
  } while ((ecart>1)||(!fin))
}