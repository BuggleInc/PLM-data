def bubbleSort(tab: Array[int]) = {
  var tab_has_changed = false;
  var tmp_value = 0;
  
  if (tab.length < 2)
    return true;
  
  var i = 0;
    
  do{
  	for (i <- 0 until tab.length - 1){
    	if (tab(i+1) > tab(i){
        tmp_value = tab(i+1);
        tab(i+1) = tab(i);
        tab(i) = tmp_value;
        tab_has_changed = true;
      }
  	}
  }while (tab_has_changed);
  
}