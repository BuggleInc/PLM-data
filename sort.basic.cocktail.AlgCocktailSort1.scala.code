def cocktailSort() {
  
  var n:Int = getNombreValeurs()
  var ech:Boolean = true
  
  while (ech){
  ech = false
  for (i <- 0 to n-2){
    if (plusPetit(i+1, i)){
      echange(i, i+1)
      ech = true
    }
  }
  for (i <- 0 to n-2){
    if (plusPetit(n-2-i+1, n-2-i)){
      echange(n-2-i, n-2-i+1)
      ech = true
    }
  }
    
    
  }
}