def shellSort()  {
  var ecart = getNombreValeurs()/2
  while(ecart!=0){
    
    var v = 0
  	for(i<- ecart to getNombreValeurs()-1 ){
    	v = getValeur(i)
      var j = i
    	while(j>ecart-1  && getValeur(j-ecart)>v){
        copie(j-ecart,j)
        j=j-ecart
    		}
  		setValeur(j, v)
  
    }
    ecart = ecart/2
  }
  
}