def cocktailSort() {
  var a=true;
  var c=getValueCount()-2
  while(a){
  	a=false;
  	for (i<- 0 to c){
    	if (isSmaller(i+1, i)){
      	swap(i, i+1);
        a=true;
    	}
    }
    for (i<- 2 to c+2){
      if (isSmaller(c-i, c-i-1)){
        swap(c-i, c-i-1);
        a=true
      }
    }
  }
}