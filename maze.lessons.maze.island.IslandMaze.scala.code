override def run() {
  while (!estSurBiscuit()){
    while (!(getDirection()==Direction.NORTH)){
      droite()
    }
    if (estFaceMur){
      longerMur()
    } else {
      avance()
    }
  }
  prendBiscuit()
}


def longerMur(){
  droite()
  if (estFaceMur()){
    gauche()
    if (!estFaceMur()){
      avance()
    } else {
      gauche()
      if (estFaceMur()){
        gauche()
        avance()
      } else {
        avance()
      }
    }
  } else {
    avance()
  }
}