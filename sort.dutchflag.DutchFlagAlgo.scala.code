def solve() {
  insertionSort()
}



def insertionSort() {
  var n:Int=getTaille()
  var compteur:Int=0
  for ( k <- 0 to (n-1-compteur)){
    var stock:Int=getCouleur(k)
    var l:Int=0
    var h=n-1
    if (stock==BLEU){
      while(getCouleur(l)==BLEU){
        l+=1
      }
      echange(k,l)
    } else {
      if(stock==ROUGE){
        while(getCouleur(h)==ROUGE){
          h-=1
          compteur+=1
        }
        echange(k,h)
      }
    }

  }
}