def hunt() {
  var bis;Boolean = false
  while(bis==true){
    if(estSurBiscuit()){
      	bis=true
    	}
    if(bis==false){
  		while(isFacingTrail()){
        	baisseBrosse()
    			avance()
    			leveBrosse()
      }
      left()
  	}
  }
  prendBiscuit()
	}

def isFacingTrail():Boolean = {
