/**
Cette fonction permet ? la buggle d'aller chercher un donut et de le ramener ? son point de d?part

**/


void goAndGet(){
  int nbPas=0;
  int x;
	while (!estSurBiscuit()){
    avance();
    nbPas++;
  }
  
  prendBiscuit();
  
  for (x = 1; x <= nbPas; x++){
    recule();
  }
  
  poseBiscuit();
  nbPas = 0;
  x = 0;
  
}

