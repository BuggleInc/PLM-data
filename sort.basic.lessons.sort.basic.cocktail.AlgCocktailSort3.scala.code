def cocktailSort3() {
	var chgtfait : Boolean = true
	var debut : Int = 0
	var fin : Int = getNombreValeurs()-2
	while (chgtfait){
		chgtfait = false
		for (i<- 0 to getNombreValeurs()-2){
			if(plusPetit(i+1,i)){
				echange(i+1,i)
				chgtfait = true
			}
			fin-=1
		}
		if (chgtfait == false){
			return
		}
		for (i<- getNombreValeurs() -2 to 0 by -1){
			if (plusPetit(i+1,i)){
				echange(i+1,i)
				chgtfait = true	
			}
			debut+=1
		}
		if (chgtfait == false){
			return
		}
	}
}
