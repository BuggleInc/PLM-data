def insertionSortWllh(ecart:Int) {
  
  for (i <- 1 to getNombreValeurs()/ecart) {
    
    var c = 1
    var v = getValeur(i*ecart)
    var last = 0
    
    
    while (i-c*ecart >= 0 && !plusPetitQue(i-c*ecart,v)) {
      
      copie(i-c*ecart,i-(c+1)*ecart)
      last = c*ecart
      c = c + 1
      
    }
    
    setValeur(i-last,v)
    
  }
  
}

def shellSort()  {
  
  var ecart = getNombreValeurs/2
  while (ecart>0) {
    
    insertionSortWllh(ecart)
    
  }
  
}