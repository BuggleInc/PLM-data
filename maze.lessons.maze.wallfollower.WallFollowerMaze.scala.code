override def run() {
  while(!estSurBiscuit){
    keepHandOnSideWall()
  }
  prendBiscuit()
}

def keepHandOnSideWall(){
  while(murGauche==true && estFaceMur==false){
    avance
  }
    gauche()
}

def murGauche():Boolean={
  var p=false
  gauche()
  if (estFaceMur()){
    p=true
  }
  droite()
  return p
}