def cocktailSort3() {
	var stop : Boolean = false;
	var debut : Int = 0;
	var fin : Int = getNombreValeurs()-2;
	
	while(!stop){
		stop = true;
		
		for (i <- debut to fin){
			if (plusPetit(i+1, i)){
				echange(i+1, i);
				stop = false;
			}
		}
		fin-=1;

		if(stop) return;
		
		for(j <- fin to debut by -1){
			if (plusPetit(j+1, j)){
				echange(j+1, j);
				stop = false;
			}
		}
		debut+=1;	
		if(stop) return;
	}
}