def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {

	if (steps !=0){
		current(steps)
		avance(length)
		droite(angle)
		tree(steps-1, length*shrink,angle,shrink)
		gauche(angle)
		gauche(angle)
		tree(steps-1, length*shrink,angle,shrink)
		droite(angle)
		current(steps)
		recule(length)
	}

}
