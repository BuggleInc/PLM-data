def pair(x:Int): Boolean={
  var rep= false
  if(x== (0%2)){rep=true}
  return rep
}

def isWallR(): Boolean={
  var rep= false
  droite()
  if (estFaceMur){rep=true}
  gauche()
  return rep
}

def isWallL(): Boolean={
  var rep= false
  gauche()
  if (estFaceMur){rep=true}
  droite()
  return rep
}


