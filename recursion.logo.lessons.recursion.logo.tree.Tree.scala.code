def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if(steps<1){
    return

  }else{
    current(steps)
    avance(length)
    droite(angle)
    (tree(steps-1,shrink*length,angle,shrink))
    gauche(angle*2)
    (tree(steps-1,shrink*length,angle,shrink))
    droite(angle)
    current(steps)
    recule(length)
    
  }
}
