$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class lastDigit extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( lastDigit((Integer)t.getParameter(0), (Integer)t.getParameter(1), (Integer)t.getParameter(2)) ); }
	boolean lastDigit(int a, int b, int c) {
		int da = a % 10;
		int db = b % 10;
		int dc = c % 10;
		return da == db || da == dc || dc == db;
	}
 }