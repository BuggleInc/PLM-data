

def max1020(a:Int, b:Int):Int = {
  var c:Int=0
  if ((10<=a && a<=20)&&(10<=b && b<=20)){
    return Math.max(a,b)
  }else if((10<=a && a<=20)&&(10>b && b>20)){
    return a
  }else if((10<=b && b<=20)&&(10>a && a>20)){
    return b
  }else if((10>b && b>20)&&(10>a && a>20)){
    return 0
  }

  
}