def combSort() {
  var lgr:Int=getNombreValeurs()
  var ecart:Int= lgr
  var c:Int=0
  var i:Int=0
  do {
    c=0
    if (ecart>1) {
      ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]   
    }
    i=0
    while ((i+ecart)< lgr){
      if (plusPetit(i+ecart:Int, i:Int)) {
        echange(i:Int, i+ecart:Int)
        i=i+1
        c=1
      }
    }
  } while ( ecart>1 )
}