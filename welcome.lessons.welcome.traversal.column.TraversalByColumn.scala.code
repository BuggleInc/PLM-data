override def run() {
  retourne()
  var compteur:Int=0
  ecritMessage(compteur)
  while (getX() !=(getMondeLargeur()-1) && getY() != getMondeHauteur()-1) {
    setPos(getX(),getY()+1)
    compteur+=1
    ecritMessage(compteur)
    if (estFaceMur()){
      setPos(getX()+1,0)
    }    
  }
  compteur+=1
  ecritMessage(compteur)
}