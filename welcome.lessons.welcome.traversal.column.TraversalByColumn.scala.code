override def run() {
var compteur = 1
ecritMessage("0")
while (!fini){
	if(getY!=getMondeHauteur-1&&getX!=getMondeLargeur){
		setPos(getX,getY+1)
	}
	else if(getY==getMondeHauteur-1&&getX!=getMondeLargeur){
		setPos(getX+1,0)
	}
	ecritMessage(compteur)
	compteur=compteur+1
}

}

def fini():Boolean={
	if(getX== getMondeLargeur()-1&&getY == getMondeHauteur()-1){
		return true
	}
	else return false
}


