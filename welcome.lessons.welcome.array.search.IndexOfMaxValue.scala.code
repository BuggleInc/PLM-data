def indexOfMaxValue(nums:Array[Int]):Int = {
  var a=nums(0)
  var b=0
  for (cpt1<-0 to nums.length-1) {
    if (a<nums(cpt1)) {
      a=nums(cpt1)
      b=cpt1
    }
    
  }
  return b
}


def maxValue(nums:Array[Int]): Int = {
  var a=nums(0)
  for (cpt1<-0 to nums.length-1) {
    if(a<nums(cpt1)) {
      a=nums(cpt1)
    }
  }
  return a
}