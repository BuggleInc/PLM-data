def shellSort()  {
  
  var ecart=getNombreValeurs()/2
  while(ecart>0)
  {
    for(i<-ecart-1 to getNombreValeurs()-1)
    {
      var v=i;
      var valeur=getValeur(i);
      while(v>=ecart && getValeur(v-ecart)>valeur)
      {

        copie(v-ecart,v);
        v-=ecart;
      }
      setValeur(v,valeur)

    }
    
  	ecart=ecart/2;
  }
}
