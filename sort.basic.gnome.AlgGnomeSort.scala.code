def gnomeSort() {
  var lgr = getNombreValeurs()
  var pasNain = 0
  while(pasNain<lgr){
    if(pasNain>=0){
      for(i <- 0 to lgr-3){
    if(plusPetit(i+1,i+2)){
      pasNain = pasNain +1
    }else{
      echange(i+1,i+2)
      pasNain = pasNain-1
    }
  	}
    }else{
      pasNain = pasNain+1
    }
    
  } 
}