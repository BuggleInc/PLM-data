def combSort() {
  var ecart = getNombreValeurs()
  var echanges = true
  do{
    if(ecart>1){
      ecart = (ecart.toDouble/1.3).toInt
    }
    var i=0
    echanges = false
    while(i + ecart< getNombreValeurs()){
      if(!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        echanges = true
      }
      i = i+1
    }
  }while((ecart > 1) || (!echanges))
}