 public void hunt(Color c) {
 	while(!isOverBaggle()){
 		if(isFacingTrail()){
 			forward();
 		}else{
 			left();
 		}
 	}

 	pickUpBaggle();
     
 }

boolean isFacingTrail() {
boolean pinkCell = false;

	if(isFacingWall()){
		return false;
	}

	forward();
	pinkCell = getGroundColor().equals(Color.pink); 
	backward();
	
	return pinkCell;
}//end isFacingTrail()

