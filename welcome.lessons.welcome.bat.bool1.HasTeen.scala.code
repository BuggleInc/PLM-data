def hasTeen(a:Int, b:Int, c:Int): Boolean = {
  if (a<=19) {
    if (a>=13) {
      return true
    } else return false
  } else if (b<=19) {
    if (b>=13) {
      return true
    } else return false
  } else if (c<=19) {
    if (c>=13) {
      return true
    } else return false
  } else return false
}