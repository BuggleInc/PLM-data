def brush(){
  brushDown()
  brushUp()
}

def makeV(){
  forward(); forward()
  brush()
  forward(); left(); forward()
  brush()
  backward(); right(); forward()
  brush()
  forward(); forward(); left()
}

def makePattern(){
  for(j <- 1 to 3){
    for(k <- 1 to 3){
      for(i <- 1 to 4){
        makeV()
      }
      for(i <- 1 to 7){
        forward()
      }
    }
    left()
    for(i <- 1 to 7){
      forward()
    }
    right()
  }
}

override def run() {
  makePattern()
}