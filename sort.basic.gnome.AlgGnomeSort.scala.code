def gnomeSort() {
  var i=0;
  while(i!=getNombreValeurs()-1){
		var stop=false;
    while(stop!=true){
      if(!plusPetit(i,i+1)){
        echange(i,i+1);
        stop=true;
        i=i-1;
      }
      else{i=i+1}
    }
    var stop2=false
    while(stop2!=true){
      if(!plusPetit(i-1,i)){
        echange(i-1,i);
        stop2=true;
        i=i+1;
      }
      else{i=i-1}
    }
  }
}