def isFacingTrail():        
    if isFacingWall():
        return False
        
    else:
        forward()
    if getGroundColor()==Color.green:
        brushDown()
        brushUp()
        backward()
        return True
    else:
        backward()
        return False

def Hunt():
    if isFacingTrail()==True:
        forward()
    else:
        left()
    while not isOverBaggle()
        return False
Hunt()
        