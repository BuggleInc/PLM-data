def cocktailSort() {

  var swapped:Boolean =true
  var doneDroit:Int=0
  var done:Int=0
  while (swapped) {
    swapped=false
    for (i <- done/2 to getValueCount()-2-doneDroit/2) {
    	if ( ! isSmaller(i,i+1)) {
      	swap(i,i+1)
      	swapped=true
        
      }
      
    }
    doneDroit+=1
    for (i <- (getValueCount()-2-doneDroit/2) to done/2) {
    	if ( ! isSmaller(i,i-1)) {
      	swap(i,i-1)
      	swapped=true
      }
    }
    done+=1
  } 
}