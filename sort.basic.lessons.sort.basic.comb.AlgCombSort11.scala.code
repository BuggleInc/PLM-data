def combSort11() {
  var ecart:Int = getNombreValeurs()
  var aInverse:Boolean = false
  do {
    aInverse = false
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    if (ecart==9 || ecart==10) {
      ecart = 11
    }
    var i:Int = 0
    while (i+ecart < getNombreValeurs()) {
      if (getValeur(i)>getValeur(i+ecart)) {
        echange(i,i+ecart)
        aInverse = true
      }
      i += 1
    }
  } while (ecart>1 || aInverse == true)
}
