def max2Value(nums:Array[Int]): Int = {
  var tab = Array(nums(0), -9999999)
  var longueur:Int = nums.length
  for (i <- 0 to longueur-1) {
    if (nums(i) > tab(0)) {
      tab(1) = tab(0)
      tab(0) = nums(i)
    } else if (nums(i) <= tab(0) && nums(i) > tab(1) && i != 0) {
      tab(1) = nums(i)
    }
  }
  return tab(1)
}
