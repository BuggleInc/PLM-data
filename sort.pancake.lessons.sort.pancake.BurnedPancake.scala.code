def solve() {
  val lgr:Int = getTaillePile()
  var crepesTriees:Int = 0
  while (!estTriee()) {
    var posPlusLarge:Int = 0
    for (i <- 1 to lgr-1-crepesTriees) {
      if (getRayonCrepe(i)>getRayonCrepe(posPlusLarge)) {
        posPlusLarge = i
      }
    }
    retourne(posPlusLarge+1)
    retourne(lgr-crepesTriees)
    crepesTriees += 1
  }
}