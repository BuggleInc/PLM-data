def cocktailSort3() {
  var debut=0;
  var fin= getNombreValeurs()-2
  var c = 1
  do {
    c = 0
    for (i <- debut to fin){
      if (plusPetit( i+1, i)){
        echange( i+1, i)
        c +=1
      }
    }
    if (c==0){
      return
    }
    c=0
    fin -= 1
    for (j <- debut to fin){
      if (plusPetit(getNombreValeurs-2-j, getNombreValeurs-3-j)){
        echange(getNombreValeurs-j-2, getNombreValeurs-j-3)
        c += 1
      }
      }
    debut +=1
    } while (c != 0)
  }