def shellSort()  {
var ecart = getNombreValeurs()/2
while (ecart>0) {
  var v = 0
  var k = 1
    for (i <- ecart to getNombreValeurs()-1) {
      v = getValeur(i);
      k = i
      while ( (k>ecart-1)&&(!plusPetitQue((k-ecart), v)) ) {
        setValeur(k, getValeur(k-ecart));
        k=k-ecart
      }
      setValeur(k,v)
    }
  ecart = ecart/2
}
}

