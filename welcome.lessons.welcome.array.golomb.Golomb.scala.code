def golomb(num:Int): Int = {

println("golomb("+num+")");
	
var suite = new Array[Int](num + 1);
var sol = 0;
suite(1) = 1;
for(i <-2 to num ){

	if(suite(i-1) == nbOccurs(suite,i-1))
	{	//Si i-1 apparait a(i-1) fois, on ajoute 1 à a(i) 
		suite(i) = suite(i-1) + 1;

		println(i+"=>"+suite(i));
	}
	else
	{
		suite(i) = suite(i-1);
		println(i+"=>"+suite(i));
	}
	


	
}
sol = suite(num);
return sol;

}
def nbOccurs(table : Array[Int], n:Int):Int ={

	var count = 0;
	for(i <- 0 to table.length - 1){

		if(table(i)==n){
			count = count + 1;
		}
		
	}
	return count ;
}
