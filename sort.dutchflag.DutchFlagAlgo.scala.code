def solve() {
  
  var term = false
  
  do{
    term = true
    
    for (i <- 0 to getSize()-1){
      
       if((getColor(i) == Color.RED) && (getColor(i+1) != Color.RED)){
    		 swap(i,i+1)
         term = false
   		 }
      
      else if((getColor(i) == Color.WHITE) && (getColor(i+1) != Color.WHITE)){
        swap(i,i+1)
        term = false
      }
    
   	 }
   
    } while (term == false)
}