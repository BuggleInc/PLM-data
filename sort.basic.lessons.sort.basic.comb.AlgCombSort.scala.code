def combSort() {
  
  var ecart = getNombreValeurs();
  var n = getNombreValeurs();
  var trie = false;
  
  do
  {
    trie = false;
  	if(ecart > 1)
    {
      ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int];
    }
    var i=0;
    while((i+ecart)<n)
    {
      if(!plusPetit(i,i+ecart))
      {
        echange(i,i+ecart);
        trie = true;
      }
      i+=1;
    }
  }while((ecart>1)||(trie))
  
}
