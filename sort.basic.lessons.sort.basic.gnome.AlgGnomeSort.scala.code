def gnomeSort() {
  var swapped = true;
  var i :Int = 0;
  while(swapped){
    swapped = false;
    for(i<- 0 to getNombreValeurs){
      if(i == 0){
        if(!plusPetit(0,1)){
        	echange(0,1);
        }
      }
      if(!plusPetit(i-1,i+1)){
        echange(i-1,i+1);
        swapped = true;
      }
    }
  }
}