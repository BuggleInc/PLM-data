public void run() {
	for (int i = 0; i < 4; i++) {
		makeV();
	}
}

public void makeV() {
	forward();
	forward();
	mark();
	corner();
	mark();
	corner();
	mark();
	left();
	forward();
	forward();
	left();
}

public void mark() {
	brushDown();
	brushUp();
}

public void corner() {
	left();
	forward();
	right();
	forward();
}
