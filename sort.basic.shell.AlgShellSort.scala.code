def shellSort()  {
  var gap=getValueCount()/2
  while (gap>0) {
    for (i <- getValueCount() to 1 by -1) {
      var j=1
      var v=getValue(i)
      while (j*gap<=i && ! isSmallerThan(i-j*gap,v)) {
        copy(i-j*gap,i-j*gap+1)
        j=j+1
      }
      setValue(i-j*gap+1,v)
    }
    gap=gap/2
  }
}