def cocktailSort3() {
  var n=getNombreValeurs() ; var a :Int = 1 
  var debut=1; var fin=n-1
  while(a==1){
      a==0     
    	for(j<-debut to fin){       
    			if(plusPetit(j-1, j)==false ){
      			echange(j-1, j)
        		a=1
      		}}
       fin-=1
    if (a==1) {    
    	for(j<-(debut to fin).reverse){ 
    		if (plusPetit(j-1, j)==false ){
      		echange(j-1, j)
        	a=1
      		}}
     
      
      debut+=1}
      
  }	
      }