def golomb(num:Int): Int = {
  var avance=0
  var test =0
  var nombre=1
  var T= new Array[Int](1000)
  T(0)=nombre
  if (num == 1){
    return 1
  }
  if (num==2 ){
    return 2
  }
  while (avance<num){
    nombre =nombre+1
    test =T(nombre-2)
    while (test !=0 && avance < num){
      test=test-1
      avance=avance+1
      T(avance)=nombre
    }
  }
  return T(num-2)
}