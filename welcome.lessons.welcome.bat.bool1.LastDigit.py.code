def unite1(x):
    if x>=100:
        unite1=x%100
    if x>=10:
        unite1=x%10
    if x<10:
        unite1=x    

def unite2(x):
    if x>=100:
        unite2=x%100
    if x>=10:
        unite2=x%10
    if x<10:
        unite2=x  

def lastDigit(a, b):
    unite1(a)
    unite2(b)
    if unite1==unite2:
        return True
    else:
        return False    