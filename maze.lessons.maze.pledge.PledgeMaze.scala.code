override def run() {
var Virage = 4

def Left_wall() {
	gauche()
	Virage += 1
	if (estFaceMur) {
		droite()
		Virage -= 1
		if (estFaceMur) {
			droite()
			Virage -= 1
		}
		else avance()
	}
	else avance()
}
def keepHandOnSideWall() {
