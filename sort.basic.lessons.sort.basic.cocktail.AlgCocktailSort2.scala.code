def cocktailSort2() {
  var debut = 0;
  var fin = getNombreValeurs()-2;
  var trie = false;
  
  do
  {    
  	for(i <- 0 to fin)
  	{
      trie = false;
			if(!plusPetit(i,i+1))
   	 	{
   			echange(i,i+1);
        trie = true;
     	}
      fin-=1;
  	}
  
  	for(i <- getNombreValeurs()-2 to debut by -1)
  	{
      trie = false;
    	if(!plusPetit(i,i+1))
    	{
      	echange(i,i+1);
 
        trie = true;
    	}
      debut+=1;
  	}
    
  }while(trie)
  
}