go = 1

while (go):
   m = readMessage()
   if m=='A': forward()
   elif  m=='B': forward(2)
   elif m=='C': forward(3)
   elif m=='I': back(); forward()
   elif m=='L': left(); forward()
   elif m=='R': right(); forward()
   elif m=='X': backward(3)
   elif  m=='Y': backward(2)
   elif m=='Z': backward()
   else: go = false
