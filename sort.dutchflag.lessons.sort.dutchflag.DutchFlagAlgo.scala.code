def solve() {
  for (i<- 1 to getTaille()-1){
    var v = getCouleur(i)
    var j = i-1
    if (v == BLEU){
      while(j>=0 && getColor(j) != BLEU ){
        echange(j,j+1)
        j-=1
      }
    }else if(v==BLANC){
    	while(j>=0 && getColor(j) == ROUGE ){
      	echange(j,j+1)
      	j-=1
    	}

  }
}
}