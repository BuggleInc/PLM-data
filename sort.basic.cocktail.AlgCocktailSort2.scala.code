def cocktailSort2() {

  var a:Int=1;
  var b:Int=0
  while (a!=0) {
    a=0;
    b=b+1;
    for (i<- 0 to getNombreValeurs-1-b) {
       if (plusPetit(i+1,i)) {
         echange(i,i+1);
         a=a+1
       }
    }
    for (i<-getNombreValeurs-2 to -1+b by -1) {
      if (plusPetit(i+1,i)) {
         echange(i,i+1);
         a=a+1
       }
       
    }
  }}