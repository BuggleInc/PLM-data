public void bubbleSort2() {
    int length = getValueCount();
    for(int i = length-1; i>-1; i--) {
        for(int j = 0; j<i; j++) {
            if(!isSmaller(j,j+1)) {
                int tmp = getValue(j);
                setValue(j, getValue(j+1));
                setValue(j+1, tmp);
            }
        }
    }
}
