def cocktailSort3() {
	var test:Boolean=false;
	var debut:Int=0;
	var fin:Int=getNombreValeurs()-2;
	do{
		test=false;
		for(i<-debut to fin){
			if(plusPetit(i+1,i)){
				echange(i+1,i);
				test=true;
			}
		}
		if(test==false)return;
		fin=fin-1;
		test=false;
		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i+1,i);
				test=true;
			}
		}
		debut=debut+1;
	}while(test)
}
