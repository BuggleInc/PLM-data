override def run() {
var T = new Array[Color](getMondeHauteur())
for (i <- 0 to (getMondeHauteur()-1))
	{T(i) = getCouleurSol();
	avance()}
for (j <- 1 to getMondeLargeur)
	{gauche()
	avance()
	droite()
	avance()
	for (k <- 0 to (getMondeHauteur()-1))
		{setCouleurBrosse(T(k))
		baisseBrosse()
		leveBrosse()
		avance()}
}
