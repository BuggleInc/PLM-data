def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
  if (!negative) {
    if (a>=0) {
      if (b<=0) {
        return true
      }
    }
    if (a<=0) {
      if (b>=0) {
        return true
      }
    }
  }
  if (negative) {
    if (a <= 0) {
      if (b <= 0) {
        return true
      }
    }
  }
  else {
    return false
  }
}