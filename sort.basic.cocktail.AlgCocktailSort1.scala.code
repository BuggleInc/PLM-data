def cocktailSort() {
  var echange1:Int = 1
  var echange2:Int = 1
  while ((echange1>0)||(echange2>0)){
    echange1 = 0
  	echange2 = 0
    for (i <- 0 to getNombreValeurs-2){
      if (getValeur(i)>getValeur(i+1)){
        echange(i,i+1)
        echange1 = echange1+1
      }
    }
    for (i <- getNombreValeurs-2 to 0 by -1){
      if(getValeur(i)>getValeur(i+1))
      echange(i,i+1)
      echange2 = echange2+1
    }
  }
}