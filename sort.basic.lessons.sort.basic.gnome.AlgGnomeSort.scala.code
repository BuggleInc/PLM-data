def gnomeSort() {
	
	var fin=false
	var i=1
	do{
		
		if (plusPetit(i,i+1) & plusPetit(i-1,i)){
			if (i!=getNombreValeurs()-1){
			i==i+1
			}
			else fin==true
		}
		else { echange(i-1,i+1)
		       if (i!=0){
		       i==i-1	
		       }
		       else i==i+1
		       
	     }
	    
	
       }while(!fin)
}
