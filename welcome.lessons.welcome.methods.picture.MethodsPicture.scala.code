override def run() {
  
  for (i <- 1 to 3){
    makeV(Color.YELLOW)
    makeV(Color.RED)
    makeV(Color.BLUE)
    makeV(Color.GREEN)
  }
 
  left()
  forward(5)
}



def makeV(c: Color){
  setBrushColor(c)
  forward()
  brushDown()
  brushUp()
  forward()
  left()
  forward()
  brushDown()
  brushUp()
  backward()
  right()
  forward()
  brushDown()
  brushUp()
  forward()
  left()
}