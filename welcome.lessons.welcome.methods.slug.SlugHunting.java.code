public void hunt() {
	while(!isOverBaggle()){
		
		if(isFacingTrail()){
			brushDown();
			forward();
			brushUp();
		}else{
			left();
		}
	}

	pickUpBaggle();
}//end hunt()

boolean isFacingTrail() {
boolean greenCell = false;

	if(isFacingWall()){
		return false;
	}

	forward();
	greenCell = getGroundColor().equals(Color.green); 
	backward();
	
	return greenCell;
}//end isFacingTrail()


