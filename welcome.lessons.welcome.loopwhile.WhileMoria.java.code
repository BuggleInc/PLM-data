public void run() {
	
	retourne();
while( getCouleurSol() != Color.green )
{
	if( estSurBiscuit() == true)
	{
		prendBiscuit();
		retourne();
		while (estSurBiscuit() == false)
		{
			avance();		
		}
		
		recule();		
		poseBiscuit();
		retourne();
		if(estFaceMur() == false)
		{
			avance();
		}		
	}
	else
	{
		if(estFaceMur() == false)
		{
			avance();
		}
		else
		{
			droite();
			avance();
			gauche(); 
			avance();
			break;
		}
	}


	
}

}



