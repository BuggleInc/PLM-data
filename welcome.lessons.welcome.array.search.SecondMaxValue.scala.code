def max2Value(nums:Array[Int]): Int = {
  var max:Int= nums(0)
  for (i<-1 to nums.length-1){
    if (max<nums(i)){
      if (max<maxValue(nums)){
        max=nums(i)
      }
    }
  }else{ return nums(0)}
}
  
  
def maxValue(nums:Array[Int]): Int = {
  var comparateur:Int=nums(0)
  for (i<- 1 to nums.length-1){
    if (comparateur<nums(i)){
      comparateur=nums(i)
    }
  }
  return comparateur
}