def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
	if(!negative && ( a<0 && b>0 || a>0 && b<0)){
		return true
	}
	if (negative && a<0 && b<0){
		return true
	}
		return false
}
/**CORRECTION  if (negative) {
    return (a < 0 && b < 0);
  }
  else {
    return ((a < 0 && b > 0) || (a > 0 && b < 0));
  }
}