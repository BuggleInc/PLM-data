def golomb(num:Int): Int = 
{
	var a: Int=1
	var b: Int=0
	var T = new Array[Int](50)
	T(0)=0
	T(1)=1
	for (i <- 0 to 49)
	{
		a=T(i)
		for (n <- 0 to a)
		{
			if (T(n)==a)
			{
				b=b+1
			}
		}
		if (b<a)
		{
			T(i)=a
		}
		else
		{
			T(i)=a
		}
	}
	return  T(a)
}

