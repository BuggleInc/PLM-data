def shellSort()  {
  var n = getNombreValeurs()
  var e = n/2
  while (e > 0) {
    var v = 0
  var k = 0
  var m = 0
  for (i<- 1 to n-1) {
    k = i
    v = getValeur(i)
    var b = false
    var j = i-e
    while (j>= 0 && !b) {
      if (getValeur(j) > v) {
        setValeur(j+e,getValeur(j))
        k = j
      } 
      else {
        b = true
      }
      j-=e
  }
   setValeur(k,v)
  }
    e=e/2
    
  }
}