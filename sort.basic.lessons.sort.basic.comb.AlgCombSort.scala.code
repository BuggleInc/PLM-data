def combSort() {
  var lng = getValueCount();
  var fin = false;
  
  while(lng > 1 && !fin) {
    fin = true;
    if(lng > 1){
      lng = (lng.toDouble / 1.3).toInt;
    }
    var i = 0;
    while(i+lng > lng){
      if(getValue(i+lng) < getValue(i)){
        swap(i, i+lng);
        fin = false;
      }
      i += 1;
    }
      
  }
}
