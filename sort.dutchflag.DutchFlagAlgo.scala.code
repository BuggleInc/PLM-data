def solve() {
  var i=0
  var debut=0
  var fin = getTaille()-1
	var avantBlanc= fin
  while (debut< avantBlanc){
    if (i !debut){
      if (getCouleur(i)==BLEU){
        echange(i, debut)
        debut +=1
      }
    }
    if (getCouleur()==ROUGE){
      echange(i, fin)
      fin -=1
      echange(i, avantBlanc)
      avantBlanc-=1
    }else if (getCouleur()==BLANC){
      echange(avantBlanc, i)
      avantBlanc-=1
    }
    i+=1
  }
}