override def run() {
  exo()
}

def positionFinale():Boolean={
  if((getX()== getMondeLargeur() -1) && (getY()== getMondeHauteur -1)){
    return true
  }
  return false
}

def exo(){
  var n = 0
  while(!positionFinale){
    if(getY() == getMondeHauteur -1){
      ecritMessage(n)
      setPos(getX()+1,0)
      n=n+1
    }
    else{
      ecritMessage(n)
      setPos(getX(),getY()+1)
      n=n+1
    }
  }
  ecritMessage(n)
}
