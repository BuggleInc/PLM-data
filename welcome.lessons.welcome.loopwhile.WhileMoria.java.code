public void run() { 
	while(!isOverBaggle()){
		backward();
		while(isOverBaggle()){
			pickUpBaggle();
			while(isCarryingBaggle()){
				while(!isOverBaggle()){
					forward();
				}
				
			}
			
		}else {
			backward();
		}
	}
}
//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().
