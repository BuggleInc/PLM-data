def spiral(steps:Int, angle:Int, length:Int, increment:Int)    {
	var n=0;
	for(i<-99 to 0 by -1){
		 if(n==0){
		 	for(k<-0 to 5 by 0,01){
		 	setCouleur(Color.orange)
		 	baisseCrayon()
		 	cercle(k)
		 	leveCrayon()
		 	}
		 	setCouleur(Color.black)
		 	baisseCrayon()
		 }
	avance(n);
	gauche(90);
	n=n+3;
	}
	
}
