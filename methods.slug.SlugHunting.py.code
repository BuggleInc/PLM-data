# Copy your isFacingTrail, and write your code below
def isFacingTrail():
  brushUp()
  if isFacingWall():
    isFacingTrail=False
  else :
    forward()
    if getGroundColor()==Color.green:
      isFacingTrail=True
    else :
      isFacingTrail=False
    backward()
  return isFacingTrail

while not isCarryingBaggle():
  if isFacingTrail():
    brushDown()
    forward()
  else:
    left()
  if isOverBaggle():
    pickupBaggle()