def selectionSort() {
  var lgr : Int = getNombreValeurs()
  var m:Int = 0
  
  for (N<-0 to lgr-2) {
    m = minim(N)
    echange(N,m)
  }
}

def minim(N:Int):Int={
  var lgr : Int = getNombreValeurs()
  var ind:Int = N
  
  for (k<-N-1 to lgr) {
    if (plusPetit(k,ind)) {ind=k}
  }
  return ind
  
}
