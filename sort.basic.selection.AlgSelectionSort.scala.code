def selectionSort() {
  var n:Int = getValueCount();
  var min:Int = getValue(0);
  var placeMin:Int = 0;
  var testMin:Int = 0;
  
  for (i<-0 to n-1) {
    min = getValue(i);
    for (j<-i+1 to n-1) {
      testMin = getValue(j);
      if (testMin<min) {
        min=testMin;
        placeMin = j;
      }
    }
    swap(i,placeMin);
  }
}