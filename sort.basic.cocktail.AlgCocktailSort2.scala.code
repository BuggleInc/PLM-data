def cocktailSort2() {
  val n:Int = getNombreValeurs()
  var nbEchange:Int = 0
  var debut:Int = 0
  var fin:Int = n-2
  do {
    nbEchange=0
    for (i <- debut to fin) {
      if plusPetit(i+1, i) {
        echange(i, i+1)
        nbEchange = nbEchange+1
      }
    }
    fin = fin-1
    for (j <- fin to debut) {
      if (plusPetit(j+1, j)) {
        echange(j, j+1)
        nbEchange = nbEchange+1
      }
    }
    debut = debut+1
  } while (nbEchange!=0)
}