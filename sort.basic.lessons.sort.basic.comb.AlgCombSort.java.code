public void combSort() {
  var a : Int = getNombreValeurs()
  var trie =false
  var ecart = a
  while (ecart>1 || trie==false){
    if (ecart>1){
      ecart =(ecart.toDouble/1.3).toInt
    }
    i=0
    trie = true
    while(i+ecart < a){
      if (plusPetit(i+ecart,i)==true){
        echange(i+ecart,i)
        trie = false
      }
      i=i+1
    }
  }
}