def array123(nums:Array[Int]): Boolean = {
  var e1=false
  var e2=false
  var e3=false
  for (i<-0 to nums.length) {
    if (nums(i)==1) {
      e1=true}
    if (nums(i)==2) {
      e2=true}
    if (nums(i)==3) {
      e3=true}
    }
  if (e1 && e2 && e3) {
    return true}
  else {return false}
  }
}