def cocktailSort2() {
  
  var test=true;
  var debut=0;
  var fin=getNombreValeurs()-2;
  
  while(test)
  {
    test=false;
    for(i<-debut to fin)
    {
        if(plusPetit(i+1,i))
        {
          echange(i+1,i);
          test=true;
        }

    }
    fin-=1;
    for(i<-fin to debut by -1)
    {
      if(plusPetit(i+1,i))
      {
        echange(i+1,i);
        test=true;
      }
    }
    debut+=1;
  }
}