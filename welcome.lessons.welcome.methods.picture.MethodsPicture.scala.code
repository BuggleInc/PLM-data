override def run() {
	for (i<- 1 to 3){
		faireV(Color.yellow)
		faireV(Color.red)
		faireV(Color.blue)
		faireV(Color.green)
	}
	
}
def faireV(c:Color) {
	avance()
	colorierSol(c:Color)
	avance()
	gauche()
	avance()
	colorierSol(c:Color)
	retourne()
	avance()
	gauche()
	avance()
	colorierSol(c:Color)
	avance()
	gauche()
	
}
def colorierSol(c:Color){
	setCouleurBrosse(c:Color)
	baisseBrosse()
	leveBrosse()
}
