def bubbleSort()  {
  var i : Int = 0
  var a : Int = getNombreValeurs()
  var j : Int = 0
  var trie : Boolean = false
  while (trie==false){
    for (i<- 0 to a-2){
      if (plusPetit(i+1,i)== true) {
        echange(i+1,i)
        j=j+1
      }
      if (j==0){
        trie==true
      }
     
    }
  }
}
