def shellSort()  {
	var ecart = (getNombreValeurs()/2).toInt
	var temp=0
	var i=1
	var v=0
	var last=0
	var k=0
	while(ecart>0)
	{
		i=0
		while(i<getNombreValeurs()-1)
		{
		last=i
		v=getValeur(i)
		k=ecart
		while(k<=i)
		{
			if(getValeur(i-k)>v)
			{
				copie(i-k,i-k+1)
				last=i-k
			}
			else
				k=i
			k=k+ecart
		}
		setValeur(last,v)
		i=i+1
		ecart = (ecart.asInstanceOf[Double] / 2).asInstanceOf[Int]
		}
	}
	
}
