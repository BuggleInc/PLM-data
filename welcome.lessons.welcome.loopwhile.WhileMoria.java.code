public void run() { 
	boolean hasBaggle = false;
	while(!isFacingWall()) {
		if(hasBaggle) {
			forward();
			if(isOverBaggle()) {
				backward();
				dropBaggle();
				hasBaggle = direction(hasBaggle);
			}
		}
		else {
			forward();
			if(isOverBaggle()){
				pickUpBaggle();
				direction();
				hasBaggle = direction(hasBaggle);
			}
		}
	}
}

public boolean direction(boolean currentDirection) {
	left();
	left();
	return !currentDirection;
}
