def combSort() {
  var i=0
  var c=1
  var ecart=getNombreValeurs()
  while (ecart>1 ||c==1) {
    c=0
  	if (ecart>1) {
    	ecart= (ecart.toDouble/1.3).toInt
  	}
  	i=0
    while (i+ecart<getNombreValeurs()){
      if (plusPetit(i+1:Int,i:Int)) {
        echange(i+1:Int,i:Int)
        i+=1
        c=1
      }
    }
  }
}