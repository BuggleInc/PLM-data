def combSort() {
  var ecart:Int=getNombreValeurs()
  var x:Int=getNombreValeurs()
  var t=0
  do {
    t=0
    if(ecart > 1) ecart=(ecart.toDouble/1.3).toInt
  var i:Int=0
  while (i+ecart < x) {
    if (!plusPetit(i,i+ecart)) {
      echange(i,i+ecart)
      t=1
    }
    i=i+1
  }
  } while (t==1 || ecart>=1) 
}