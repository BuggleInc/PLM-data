def max1020(a:Int, b:Int):Int = {
	if (a<=20 & a>=10 & b>20 || a<=20 & a>=10 & b<10) {
    return a
  }
  if (b<=20 & b>=10 & a>20 || b<=20 & b>=10 & a<10 ) {
  	return b
  }
  if (a<=20 & a>=10 & b<=20 & b>=10 ) {
    if (a>b) {
      return a
    }
  }
    else {
      return b
    }
  if (!(a<=20 & a>=10) & !(b<=20 & b>=10)) {
    return 0
  }
}
  