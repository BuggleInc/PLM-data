var diag = 0;
override def run() {
var c:Int=0


			
	do {
		for{k<-0 to (getMondeHauteur()-1)}
		
		for (y<-(0 to k).reverse)
		{
			setPos(k-y,y)
			ecritMessage(c)
			c=c+1
		}
		for{k<-1 to (getMondeHauteur()-1)}
		
		for (y<-(0 to k).reverse)
		{
			setPos(y-5,k-y+6)
			ecritMessage(c)
			c=c+1
		}
		}

	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-1
		)
}
