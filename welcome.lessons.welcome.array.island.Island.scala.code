def island(num:Array[Int]): Int = {
  var cpt = 0
  for(i<-0 to num.length-1)
  {
    if(isPresent(num:Array[Int] ,i:Int))
    {cpt = i}
  }
  return cpt
}

def isPresent(nums:Array[Int] ,lookingFor:Int): Int = {
  for (i<- 0 to nums.length-1)
  {
    if (nums(i) == lookingFor)
    	return true
  }
  return false
}