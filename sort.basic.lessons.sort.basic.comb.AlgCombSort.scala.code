def combSort() {
  var swapped=false
  var gap:Int=getValueCount()
  do {
    if (gap>1) {
      
      gap = (gap.toDouble / 1.3).toInt
      }
    
    var i:Int=0
    while ((i+gap)<getValueCount()+1) {
      if (!isSmaller(i,i+gap)) {
        swap(i+gap,i)
        swapped=true
        
        
      }
       i+=1
    }
  } while ((gap>1)||(swapped))
  
}