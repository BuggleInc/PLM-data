var mem = 0
var cpt = 2
def solve() {
  while (!estTriee) {
    for (i<-0 to getTaillePile-cpt+1) {
      if (getRayonCrepe(i)>=getRayonCrepe(mem)) {
				mem=i
      }
    }
    retourne(mem+1)
    if (getTaillePile-cpt+2!=0) {
      if (!estCrepeRetournee(0)) {
        retourne(1)
      }
    	retourne(getTaillePile-cpt+2)
    }
    cpt+=1
    mem=0
  }
}