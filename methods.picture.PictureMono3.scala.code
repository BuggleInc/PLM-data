def paintTile(){
  brushDown()
  brushUp()
}


def makeV(){
  forward();forward()
  paintTile()
  forward()
  left()
  forward()
  paintTile()
  backward()
  right()
  forward()
  paintTile()
  forward();forward();left()
}

def goUp(){
  left()
  forward(7)
  right()
}

def makePattern(){
 for(i <- 1 to 4){
    makeV()
  } 
  forward(7)
}


override def run() {
 for(i <- 1 to 9*9){
   if(i%10 == 0) goUp()
   makePattern()
 } 
}