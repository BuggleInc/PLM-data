def combSort() {
	var len = getValueCount();
	var gap = len;
  var swapped = false;

  do {
    swapped = false;
    
		if(gap>1)
    	gap = (gap.toDouble / 1.3).toInt;
    var i = 0;

    while(i+gap < len){
      if(isSmaller(i+gap,i)){
        swap(i, i+gap);
        i+=1;
        swapped = true;
      }
    }

  } while(gap >= 1 || swapped)


}