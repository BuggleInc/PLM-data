def combSort() {
  var n = getNombreValeurs()
  var ecart = n
  var end = false
  
  while(ecart!=1 && !end)
  {
    end = true
    if(ecart>1)
    {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    for(i <- ecart to n-ecart-1 )
    {
      if(plusPetit(i,i+ecart))
      {
        echange(i,i+ecart)
        end = false
      }
    }
  }
  
}