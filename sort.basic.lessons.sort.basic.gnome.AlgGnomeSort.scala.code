def gnomeSort() {
  var position = 0
  while (position != getNombreValeurs()-1) {
    if (position == 0) {
      position = position + 1
    } else if (plusPetit(position, position+1)) {
      position = position + 1
    } else {
      echange(position, position+1)
      position = position - 1
    }
  }
}
}