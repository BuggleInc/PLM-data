override def run() {
  var hasard=0
  while (!estSurBiscuit){
    setDirection(Direction.NORTH)
    if (estFaceMur){
    	hasard=random3()
    	hasard match {
      	case 0 => if (!estFaceMur){
        	avance()
      	}
      	case 1 =>droite();if (!estFaceMur){
        	avance()
      	}
      	case 2 =>gauche;if (!estFaceMur){
        	avance()
      	}
    	}
  	}else{
      avance()
    }
  }
  prendBiscuit()
}