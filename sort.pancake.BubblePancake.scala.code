def solve() {
  var n:Int = getTaillePile()
  var rayon:Int = 0
  var j:Int = 0
  retourne(3)
  for (i <- 0 to n-2) {
    j = i+1
    rayon = getRayonCrepe(i)
    while (j<n && rayon>getRayonCrepe(j)) {
      echange(j)
      j = j+1
    }
  }
}

def echange(rg:Int) { //Echange les crÃªtes de rangs rg et rg-1
  if (rg==1) {
    retourne (2)
  }
  else {
    retourne(rg)
    retourne(2)
    retourne(rg)
  }
}