def cocktailSort2() {
  var debut = 0
  var cpt=0
  var j = 0
  var fin = getNombreValeurs-2
  do {cpt=0
    for (i<- debut to fin){
      if (plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt=1
      }
    fin-=1
    }
    for (i <- debut to fin){
      j=getNombreValeurs-i-2
      if (plusPetit(j,j+1)==false){
       	echange(j,j+1)
        cpt=1
      }
    debut+=1
    }
  }while (cpt==1)
}