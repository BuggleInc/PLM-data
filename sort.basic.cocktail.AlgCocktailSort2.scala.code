def cocktailSort2() {
  var debut: Int = 0
  var fin: Int = getNombreValeurs()-2
  var permutation: Int = 0
  do {
    permutation=0
    for (i <-debut to fin){
      if (plusPetitQue(i,i+1)==false){
        echange(i,i+1)
        fin-=1
        permutation= 1
      }
    }
     for (i <-fin to debut){
       if (plusPetitQue(i,i+1)==false){
         echange(i,i+1)
         debut+=1
         permutation=1
       } 
     }    
  } while (permutation==1)
}