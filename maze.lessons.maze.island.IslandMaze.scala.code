override def run() {
  
  while(!estSurBiscuit()){
    if (getDirection() == Direction.NORD && !estFaceMur()){
      avance()
    }
    keepHandOnSideWall()
  }
  prendBiscuit()
}



def keepHandOnSideWall(){
  if (murLeft() && !estFaceMur()){
    avance()
  }else if (murLeft() && estFaceMur()){
    droite()
  }else {
    gauche()
    avance()
  }
}




def murLeft(): Boolean = {
  gauche()
  if (estFaceMur()){
    droite()
    return true
  }else{
    droite()
    return false
  }
  
}