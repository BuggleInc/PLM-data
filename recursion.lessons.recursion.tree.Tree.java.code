public void tree(int steps, double length, double angle, double shrink){
	
	if(steps != 0){
		
		current(steps);
		avance(length);
		droite(angle);
		tree(steps-1, length*shrink, angle, shrink);

		gauche(angle);
		leveCrayon();
		recule(length);

		BaisseCrayon();
		gauche(2*angle);
		tree(steps-1, length*shrink, angle, shrink);
		
		//droite(angle);
	}
	
}
