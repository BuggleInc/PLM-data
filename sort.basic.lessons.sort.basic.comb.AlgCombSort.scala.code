def combSort() {
  var longueur = getValueCount()
	var ecart = longueur
  var ech = false
  var i = 0
  
  do{
    ech = false
  	if(ecart > 1)
    {
      ecart = ((ecart.toDouble)/1.3).toInt
    }
    
    i = 0
    
    while ((i+ecart) < longueur)
    {
      if (isSmaller(i+ecart,i))
      {
        swap(i,i+ecart)
        ech = true
      }
      i+=1
    }
  }while((ecart > 1)||ech)
}
