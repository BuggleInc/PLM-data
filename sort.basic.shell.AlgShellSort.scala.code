def shellSort()  {

  var i = 0;
  var k = 0;
  var j = 0;
  var v = 0;
  var ecart = getNombreValeurs()/2;
  
  while (ecart > 0) {

    for (i <- ecart to getNombreValeurs()-1) {
      v = getValeur(i);
      j = i;

      while ((j >= ecart) && plusPetitQue(j-ecart,v) == false) {

        copie(j-ecart, j);
        j -= ecart

      }

      setValeur(j, v);

    }
    ecart = ecart/ 2

  }
  
}
