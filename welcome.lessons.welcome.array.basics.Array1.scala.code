override def run() {
  var T= new Array[Color](getMondeHauteur())
	for (compteur <-0 to getMondeHauteur()-1){
  	T(compteur)=getCouleurSol()
  	avance()
  }
  
  gauche()
  avance()
  droite()
  for (compteur<-0 to getMondeHauteur()-1 ){
    setCouleurBrosse(T(compteur))
    leveBrosse()
    baisseBrosse()
    avance()
  }

}




