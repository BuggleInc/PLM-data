var c:Int=0
var i:Int=0
override def run() {
  def makeV(){
  	brushDown();
    brushUp();
    forward();
    left();
    forward();brushDown();brushUp();backward();
    right();forward();brushDown();left();
    right();brushUp();forward();forward();
    left();forward();forward()
  }
  def makePattern(){
    forward();
    forward();
    for(c<-1 to 4){
      makeV();
    }
    forward();forward();forward();forward();forward();
  }
  for(i<-1 to 3){
    makePattern();
  }
}