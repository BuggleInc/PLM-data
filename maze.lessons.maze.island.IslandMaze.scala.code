override def run() {
    def keepHandOnRightWall() = {
    	gauche();
    	if (estFaceMur) {droite(); if (!(estFaceMur)) {avance()} else droite()}
    	else if (!(estFaceMur())) {avance()}
    		else {gauche()}
    }
    while (!(estSurBiscuit)) {
    		if (!(getDirection==Direction.NORD)) {
    			keepHandOnRightWall();
    	
    		} else
    			if ((getDirection==Direction.NORD) && !(estFaceMur)) {
    	 		avance()	
    			} else {keepHandOnRightWall()}
	}
}