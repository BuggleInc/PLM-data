biscuits =0
caseOrange=0
while 2*biscuits!=caseOrange + 1:
    avance()
    if estSurOrange() is True:
        caseOrange= caseOrange+1
    elif estSurBiscuit() is True:
        biscuits=biscuits+1
    elif estSurOrange() and estSurBiscuit() is True:
        caseOrange= caseOrange+1
        biscuits=biscuits+1
if 2*biscuits==caseOrange+1:
    recule()
