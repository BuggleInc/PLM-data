def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if (speed<=60) {
		return 0
	}else if (speed>60 && speed <=65) {
		if (isBirthday == true) {
			return 0
		}else return 1
	}else if (speed>65 && speed <=80) {
		return 1
	}else if (speed>80 && speed <=85) {
		if (isBirthday == true) {
			return 1
		} else return 2
	} else return 2
}
