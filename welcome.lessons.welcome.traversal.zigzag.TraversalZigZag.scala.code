override def run() {
  
  var x = 0
  var y = 0
  var cpt = 0
  
  ecritMessage(cpt)
  cpt = cpt+1
  
  while(x!=getMondeLargeur()-1 && y!= getMondeHauteur()-1)
  {
    
    
    
    if (y%2==0)
    	x = x+1
    else
    	x = x-1
    
    if (x<0)
    {
      x=0
      y=x+1
    }
    if (x==getMondeLargeur()-1)
    {
      x=getMondeLargeur()-1
      y=y+1
    }
    
    setPos(x,y)
    
    ecritMessage(cpt)
    cpt=cpt+1
    
  }
}