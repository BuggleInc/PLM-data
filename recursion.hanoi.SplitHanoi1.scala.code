def hanoi(height:Int, src:Int, other1:Int, other2:Int, dst:Int) {
	if (height>0)
  {
  	hanoi(height-2, src, other1, dst, other2)
  	move(src, other1)
    move(src, dst)
    move(other1, dst)
    hanoi(height-2, other2, src, other1, dst)
  }
}

def splitHanoi(height:Int, src:Int, other:Int, dst1:Int, dst2:Int) {
  if(height > 0)
  {
   	hanoi(height-2, src, dst1, dst2, other)
    move(src, dst1)
    move(src, dst2)
    splitHanoi(height-1, other, src, dst1, dst2)
  }
}