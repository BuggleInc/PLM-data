var diag:Int = 1;
override def run() {
  var x:Int = 0
		ecritMessage(x)
   while (getMondeHauteur()-1 !=getY() || getMondeLargeur()-1 != getX()){
     if (diag<getMondeHauteur()){
for (i <- 0 to diag){
  x+=1
  setPos(i,diag-i)
  ecritMessage(getMondeHauteur())
}
     }
     else{
       for (i <- getMondeHauteur() to diag){
  x+=1
  setPos(getMondeHauteur()-i,i)
  ecritMessage(x)
     }
     }
     diag+=1
      
}
}