def bubbleSort2() {
 	var swapped = true
	var i : Int = 0;
  var count : Int = getNombreValeurs;
  while (swapped){
    swapped = false;
    for(j<- 0 to count - 2)
    for(i<- 0 to j){
      if(!plusPetit(i,i+1)){
        echange(i,i+1);
        swapped = true;
      }
 		count = count - 1;
    }
  }
}