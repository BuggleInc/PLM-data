override def run() {
  while(!posFin){
    tp()
  }
  
  
}

var large = getMondeLargeur()-1
var hauteur = getMondeHauteur()-1

def posFin(): Boolean = {
  if( getX() = large && getY() = hauteur){
    return true
  }else{
    return false
  }
}

def tp(){
  if(getY() != hauteur){
    setPos(getX(),getY()+1)
  }else{
    setPos(getX()+1,0)
  }
}