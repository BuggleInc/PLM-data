override def run() {
var msg = 1
ecritMessage(""+msg)
msg += 1
var x = 0
var y = 1
while (!positionFinale())
	{if (y!=getMondeHauteur()-1)
			{setPos(x,y)
			ecritMessage(""+msg)
			msg += 1
			y += 1}
		else {y=0
			x=x+1
			setPos(x,y)
			ecritMessage(""+msg)
			msg += 1
			y += 1}
	}
}

def positionFinale():Boolean= {
		(getX(),getY()) match {
			case (x,y) if (x==getMondeLargeur()-1 && y ==getMondeHauteur()-1) =>
						return true
			case _	=>	return false
		}
}
