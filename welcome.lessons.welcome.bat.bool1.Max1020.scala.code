def max1020(a:Int, b:Int):Int = {
if ((a<10 || a>20) & (b<10 ||b>20)){
	return 0
}
if ((a>=20 & a<=10) & (b>20 || b<10)){
	return a
	
}
if ((b>=20 & b<=10) & (a>20 || a<10)){
	return b
	
}
if ((a>=20 & a<=10) & (b>=20 & b<=10) & (a>=b)){
	return a
}
if ((a>=20 & a<=10) & (b>=20 & b<=10) & (b>=a)){
	return b
}
}