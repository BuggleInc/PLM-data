var finale = 0


def deplaceRocher() {
  prendBiscuit()
  if (!estSurBiscuit) {
    recule()
  } else {
    avance()
    poseBiscuit()
  }
}

retourne()
while(finale==0) {
  if (!estSurBiscuit) {
    avance()
  } else {
    deplaceRocher()
  }
  if (getCouleurSol==Color.GREEN) {
    finale=1
  }
}

droite()
avance()
gauche()