def combSort() {
  var y=getNombreValeurs()
  var i=0
  var ecart=y-3
  var nombreechanges=1
  while (ecart!=0){
    for (i <- 0 to y-(ecart+1)){
      if (plusPetit(i,i+ecart)==false){
        echange(i,i+ecart)
      }
    }
    if (ecart==1){
      ecart=0
    }
    ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
  }
}