def array667(nums:Array[Int]): Int = {
	var j:Int = 0
	for (k<-0 to nums.length-2){
		if(nums(k)==6 && nums(k+1)==nums(k) || nums(k)==6 && nums(k+1)==1+nums(k)  ){
		j = j + 1
		}
		if(nums(k)==6 && nums(k+1)==nums(k) && nums(k)==6 && nums(k+1)==1+nums(k)  ){
		j = j + 2
		}else{
			j = j
		}
}
return j
}