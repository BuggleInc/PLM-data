var fini=false
var x:String=litMessage()

while (fini==false) {
  if (x=="R") {
    droite();
    avance(1);
	} else if (x=="L") {
    gauche();
    avance(1);
	} else if (x=="I") {
    retourne();
    avance();
  } else if (x=="A") {
    avance(1);
  } else if (x=="B") {
    avance(2);
  } else if (x=="C") {
    avance(3);
  } else if (x=="Z") {
    recule(1);
  } else if (x=="Y") {
    recule(2);
  } else if (x=="X") {
    recule(3);
  } else {
    fini=true;
  }
}