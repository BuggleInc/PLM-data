def bubbleSort3()  {
  
  var trier:Boolean=true
  
  while(trier){
  	trier=false
    for(i<- 0 to getNombreValeurs()-2){
      if(plusPetit(i+1, i)){
        echange(i,i+1)
        trier=true
    	}
  	}
    return
  }
}