def allDifferent(l:List[Int]): Boolean = {
  def present(l: List[Int], v:Int): Boolean {
    l match {
      case Nil => false
      case h::q if (h==v) => true
      case _::q => present(q,v)
    }
  }
  def aux(l:List[Int]):Boolean = {
    l match {
  		case Nil => true
  	 	case h::q => (!present(q,h))&&allDifferent(q)
    }
  }
  aux(l)
}
