var diag = 0;
override def run() {
  var cpt: Int =0
  ecritMessage(cpt);
  while (getMondeHauteur()-1 != getY() || getMondeLargeur()-1 != getX()) {
    cpt += 1;
    if ((getX()+getY()) == diag) {
      setPos(getX()+1,getY()-1); ecritMessage(cpt);
    } else {
      if (getX() == getMondeLargeur()-1 || getY() == getMondeHauteur()-1) {
        setPos(0,getY()+1); ecritMessage(cpt);
      }
    }
  }
}
