override def run() {
  for(i <- 1 to 4) {
    forward();
    forward();
    makeV();
    forward();
    forward();
  }
}

def makeV() {
  colorize();
  left();
  forward();
  right();
  colorize();
  forward();
  right();
  forward();
  colorize();
  left();
}

def colorize() {
  brushDown();
  brushUp();
}