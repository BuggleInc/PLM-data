override def run() {
  makePattern(9, 9);
}

def  mark() {
  brushDown();
  brushUp();
}

def makeV(c:Color) {
  setBrushColor(c);
  forward();
  mark();
  
  forward(); left(); forward();
  mark();
  
  backward(); right(); forward();
  mark();
  
  forward(); left();
}

def makeSquare() {
  makeV(Color.yellow);
  makeV(Color.red);
  makeV(Color.blue);
  makeV(Color.green);
}

def makePattern(x:Int, y:Int) {
  for (stepper1 <- 1 to y) {
    for (stepper2 <- 1 to x) {
      makeSquare();
      forward(5);
    }
    left(); forward(5); right();
  }
}
