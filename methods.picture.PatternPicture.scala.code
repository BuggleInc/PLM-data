  		forward()
  		forward()
  		forward()
    	faireV(r,b)
      faireV(b,g)
      faireV(g,y)
      faireV(y,r)
      left()
      forward()
  		forward()
 		 forward()
  		forward()
      right()
      faireV(r,b)
      faireV(b,g)
      faireV(g,y)
      faireV(y,r)
