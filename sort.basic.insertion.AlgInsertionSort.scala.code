def insertionSort() {
  var n:Int = getValueCount();
  var v:Int = 0;
  var j:Int = 0;
  var lastOne:Int = 0;
  
  for (i<-1 to n-1) {}
  
    v = getValue(i);
    j = i;
    
    while (j!=0) {
      
      if (isSmaller(v,j-1)) {
      	setValue(j,getValue(j-1));
        lastOne = j;
      }
      
      j =  j-1;
    }
  
    setValue(lastOne,v);
  }
}