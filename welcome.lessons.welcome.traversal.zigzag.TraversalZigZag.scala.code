override def run() {

def fonctionTest():Boolean = {
	(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1)
}

var i = 0
var j = 0
var cpt = 0


while (!fonctionTest) {
	setPos(i,j)
	ecritMessage(cpt)
	cpt += 1
	if (j%2 == 0) {
		i += 1		
		if (i == getMondeLargeur()) {
			j += 1
			i -= 1			
		}
	} else {
		i = i - 1		
		if (i == 0) {
			j += 1
		}
	}
	
}

}
