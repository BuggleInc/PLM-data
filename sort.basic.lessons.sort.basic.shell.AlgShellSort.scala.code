def shellSort()  {
  var lgr:Int= getNombreValeurs()
  var ecart = lgr
  while (ecart>0){
    ecart = (ecart.toDouble / 2).toInt
    for (i<-1 to lgr-1) {
      var v=getValeur(i:Int)
      var j:Int=1
      while (i-j*ecart>=0 && v<getValeur(i-j*ecart)){
        setValeur(i-(j+1)*ecart, getValeur(i-j*ecart))
        j=j+1
      }
      setValeur(i-(j+1)*ecart,v)
    }
  }
}