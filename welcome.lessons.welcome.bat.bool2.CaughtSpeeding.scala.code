def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (isBirthday==false){
    if (speed<=60){
      return 0
    }
    else if (speed>=61 && speed<=80){
      return 1
    }
    else {
      return 2
    }
  }
  else {
    if (speed<=65){
      return 0
    }
    else if (speed>=66 && speed<=85){
      return 1
    }
    else {
      return 2
    }
  }
}