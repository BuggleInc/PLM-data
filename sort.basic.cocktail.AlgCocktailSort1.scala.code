def cocktailSort() {
  val lgr = getNombreValeurs()
  var echangeFait : Boolean = true
  do {
    echangeFait = false
    for (i <- 0 to (lgr - 2)) {
      if (!plusPetit(i, i+1)) {
        echange(i, i+1)
        echangeFait = true
      }
    }
   	for (i <- (lgr - 2) to 0) {
      if (plusPetit(i, i+1)) {
        echange(i, i+1)
        echangeFait = true
      }
    }
  } while (echangeFait = true)
}
