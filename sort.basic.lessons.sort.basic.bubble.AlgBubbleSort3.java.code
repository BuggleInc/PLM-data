public void bubbleSort3(){
  boolean enOrdre = true ;
	for(int j = getNombreValeurs()-1;j>=0;j--){  
    enOrdre = false ;
    for(int i = 0; i < j;i++){
      if(plusPetit(i+1,i)){
        echange(i,i+1); 
        enOrdre = true ;
      	}      
      
    } 
    if (!enOrdre) 
        break ;
  }
}
