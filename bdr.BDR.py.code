while isOverMessage():
  x=readMessage()
  if x==A:
      forward()
    elif x==B:
      forward(2)
    elif x==C:
      forward(3)
    elif x==I:
      right(); right(); forward()
    elif x==R:
      right();forward()
    elif x==L:
      left(); forward()
    elif x==X:
      backward(3)
    elif x==Y:
      backward(2)
    elif x==Z:
      backward()