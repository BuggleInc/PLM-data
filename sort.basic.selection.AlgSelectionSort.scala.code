def selectionSort() {
  var n:Int = getValueCount();
  var min:Int = getValue(0);
  var placeMin:Int = 0;
  var testMin:Int = 0;
  
  for (i<-0 to n-2) {
    min = getValue(i);
    placeMin = i;
    for (j<-i+1 to n-1) {

      if (getValue(j)<min) {
        min = getValue(j);
        placeMin = j;
      }
    }
    swap(i,placeMin);
  }
}