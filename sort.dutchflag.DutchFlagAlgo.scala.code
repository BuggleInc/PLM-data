def solve() {
		var afterBlue = 0;
		var beforeWhite = getSize() - 1;
		var beforeRed = getSize() - 1;
		while (afterBlue <= beforeWhite) {
			getColor(afterBlue) match {
			case BLUE =>
				afterBlue += 1
			case WHITE =>
				swap(afterBlue, beforeWhite)
				beforeWhite -= 1
			case RED =>
				swap(afterBlue, beforeWhite)
				swap(beforeRed, beforeWhite)
				beforeWhite -= 1
				beforeRed -= 1
			}
		}
	}