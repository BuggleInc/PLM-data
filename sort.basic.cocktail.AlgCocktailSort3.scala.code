def cocktailSort3() {
  var debut=0;var fin=getNombreValeurs()-2;
  var b=1;var a=1;var c=1
  while (b==1) { 
    a=0
  	b=0
    c=0
    for (i<-debut to fin) {
      if (!plusPetit(i,i+1)) {echange(i,i+1);b=1;a=1}
    }
    fin-=1
    for (i <- (debut to fin).reverse) {
      if (!plusPetit(i,i+1)) {echange(i,i+1);b=1;c=1}
    }
    debut+=1
    if (b==0 || (a==1&&c==0)) {return}
}
}