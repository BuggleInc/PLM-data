def bubbleSort2()  {
  var tri : Boolean = true
  var taille=getNombreValeurs()
  var avancement = 0
  while (tri && (avancement!=taille))
  {
    tri=false
    for (i <- 0 to (taille-2))
    {
      if (!plusPetit(i,i+1))
      {
        echange(i,i+1)
        tri=true
      }
    }
    avancement=avancement+1
  }
}