def estSortie(): Boolean = {
  return getCouleurSol() == Color.GREEN
}

def deplaceRocher() {
  prendBiscuit()
  
  retourne()
  
  while (!estSurBiscuit()) {
    avance()
  }
  
  recule()
  
  poseBiscuit()
  
  retourne()
}

def run() {
  retourne()
  
  while(!estSortie()) {
    avance()
    
    if (estSurBiscuit())
    	deplaceRocher()
  }
}

run()