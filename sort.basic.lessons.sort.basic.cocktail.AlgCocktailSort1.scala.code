def cocktailSort() {
  var inverse:Boolean = false
  var fini:Boolean = false
  while (fini!=true){
    fini=true
    for (i<- 0 to getNombreValeurs()-2){
      if (plusPetit(i+1,i)==true){
        echange(i,i+1)
        fini=false
      }
    }
    for (j<- getNombreValeurs()-2 to 0 by -1){
      if (plusPetit(j+1,j)==true){
        echange(j,j+1)
        fini=false
      }
    }
  }
}