right(); right()
while(getGroundColor() != Color.GREEN){
  if(isFacingWall()){
    right(); forward(); left(); forward()
  }
  else if(isCarryingBaggle()){
    forward()
    if(isOverBaggle())
    	right(); right(); forward(); dropBaggle()
  }
  else{
    forward()
    if(isOverBaggle())
    	pickupBaggle(); right(); right()
  }
}