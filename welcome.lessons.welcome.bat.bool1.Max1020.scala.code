def estDansIntervalle(n:Int, a:Int, b:Int): Boolean =
{
  return n >= a && n <= b
}

def max1020(a:Int, b:Int):Int =
{
	if (Math.max(a, b) == a)
  {
    if (estDansIntervalle(a, 10, 20))
    	return a
    else if (estDansIntervalle(b, 10, 20))
    	return b
    return 0
  }
  else
  {
    if (estDansIntervalle(b, 10, 20))
    	return b
    else if (estDansIntervalle(a, 10, 20))
    	return a
    return 0
  }
}