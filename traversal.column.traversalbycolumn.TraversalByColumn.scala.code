override def run() {
  var cpt = 0
  do
  {
    ecritMessage(cpt)
    move()
    cpt = cpt+1
  }while(!finalPos())
}

def finalPos():Boolean = 
{
  return (getMondeHauteur()==getY()+1)&&(getMondeLargeur()==getX()+1)
}
def move()
{
  if(getMondeHauteur()==getY()+1)
    setPos(0,getX()+1)
	else
  	setY(getY()+1)
}