def close10(a:Int, b:Int): Int = {
  if ( Math.abs(10-a)< Math.abs(10-b)){
    return a
  }
  else {
    if (Math.abs(10-a)> Math.abs(10-b)){
      return b
    }
    else (Math.abs(10-a)== Math.abs(10-b)){
      if (Math.abs(a)< Math.abs(b)){
        return a
      }
      else {
        return b
      }
    }
  }
}