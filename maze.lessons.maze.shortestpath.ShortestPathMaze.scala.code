override def run() {
var wl=0;
var wh=0;
    wl=getMondeLargeur();
    wh=getMondeHauteur();
    def findBiscuit() {
    		for (i <- 1 to wl) {
    			for (l <- 1 to wh) {
    				if (aBiscuit(i, l)) {
    					setIndication(i, l, 0);
    					return true;
    				}
    			}
    		}
    	return false;
    }
   findBiscuit();
}
