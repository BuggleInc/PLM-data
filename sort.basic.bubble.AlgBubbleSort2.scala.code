var cpt=0
def bubbleSort2(){
  var i = 0
  while(i<getNombreValeurs()-1){
    if (plusPetit(i,i+1)==false){
      echange(i,i+1)
      i=i+1
      cpt=1
    }else{
      i=i+1
    }
  }
  var j = 1
  while(cpt==1){
    cpt=0
    i=0
    while(i<getNombreValeurs()-1-j){
    	if (plusPetit(i,i+1)==false){
      	echange(i,i+1)
      	i=i+1
      	cpt=1
    	}else{
      	i=i+1
    	}	
  	}
		j=j+1 
  }
}