def cocktailSort() {
  var taille=getNombreValeurs()
  var trier=false
  var cnt=0
  while (trier==false) {
    cnt=cnt+1
    for (i<-0 to taille-2-cnt) {
      if (plusPetit(i,i+1)==false) {
          echange(i,i+1)
        trier=false 
      }
    }
    for (i<-taille-2-cnt to 0 by -1) {
      if (plusPetit(i,i+1)==false) {
          echange(i,i+1)
          trier=false
      }
    }   
  }
}