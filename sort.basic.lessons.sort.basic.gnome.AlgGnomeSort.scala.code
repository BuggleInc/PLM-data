def gnomeSort()
{
  var idCaseTraitee = 0
  
  while (idCaseTraitee < getNombreValeurs() - 1)
  {
    if (plusPetit(idCaseTraitee + 1, idCaseTraitee))
    {
      echange(idCaseTraitee, idCaseTraitee + 1)
      
      if (idCaseTraitee == 0)
      	idCaseTraitee += 1
      else
      	idCaseTraitee -= 1
    }
    else
    idCaseTraitee += 1
  }
}