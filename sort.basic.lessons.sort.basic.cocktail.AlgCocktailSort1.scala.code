def cocktailSort() {
  var Tri=true
  var nbValeurs=getNombreValeurs()
  while(Tri=true) {
    Tri=false
  for (i<-0 to nbValeurs-2) {
    if (!plusPetit(i,i+1)) {
      echange (i, i+1)
      Tri=true
    }
  }
  for (i<-nbValeurs-2 to 0 by -1)
  if (plusPetit(i,i+1)) {
    echange (i,i+1)
    Tri=true
  }
}
}