def gnomeSort() {
  val lgr = getNombreValeurs()
  var position = 0
  while (position <= lgr - 2) {
    if (!plusPetit(position + 1, position + 2)) {
      echange(position+1,position+2)
      if (position > -1) {
      	position = position - 1
      }
    }
    else {
      position = position + 1
    }
  }
}