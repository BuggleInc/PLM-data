def golomb(num:Int): Int = {

	var tab = new Array[Int](num);
	var i:Int = 2;
	var j:Int = 1;
	var k:Int = 0;
	var nbre:Int = 0;
	tab(0) = 1;

	while (j < num)
	{
		k = 0;
		
		while (k < i && j < num)
		{
			tab(j) = i;
			k = k + 1;
			j = j + 1;
			nbre = nbre + 1;	
		}
		
		if (num > 2) i = tab(i);
		if (nbre == tab(i))
		{
			i = i + 1;
			nbre = 0;
		}
	}

	return tab(num - 1);
}
