def positionFinale():Boolean = {
	if ((getX() == getMondeLargeur()-1) && (getY() == getMondeHauteur())){
		return true
	}
	return false
}
override def run() {
	var cpt=0
	for (i <- 1 to getMondeHauteur){
		for (j <- 1 to getMondeLargeur){
			ecritMessage(cpt)
			cpt=cpt+1
			if (getY() == 0){
				setPos(getX()+1, getY())
			}
			if ((getY() == 6) && (!positionFinale())){
				setPos(getX()-1, getY()) 
			}
		}
	}
}
