def array123(nums:Array[Int]): Boolean = {
  var x:Boolean = false
  var y:Boolean = false
  var z:Boolean = false
  for(i<- 0 to nums.length-1){
    if(nums(i) == 1){x = true}
    if(nums(i) == 2){y = true}
    if(nums(i) == 3){z = true}
  }
  if(x == true && y == true && z == true){return true}
  else(return false)
}
