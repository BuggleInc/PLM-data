def selectionSort() {
  var lgr=getNombreValeurs
  var indice=0
  
  var min=0
  var posMin=0
  
  while (indice!=lgr-1){
    min=getValeur(indice)
    for (i<- (indice+1) to lgr-1){
      if (plusPetitQue(i,min)){
        posMin=i
        min=getValeur(i)
      }
    }
    echange(indice,posMin)
    indice+=1
  }
}