def max1020(a:Int, b:Int):Int =  {
	var A:Int = a
	var B:Int = b
	var temp:Int = 0
	if (b > a) {
	    	temp = A;
    		A = B;
   		B = temp
	    
	  }
	  
	  // Knowing a is bigger, just check a first
	  if (A >= 10 && A <= 20) {
	  	return A;
	  }
	  if (B >= 10 && B <= 20) {
	  	return B;
	  }
	  	return 0
}	