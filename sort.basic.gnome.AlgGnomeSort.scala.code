def gnomeSort() {
  var length:Int = getNombreValeurs()
  var continuer:Boolean = true
  var i:Int = 0
  while (continuer){
    print(i)
    if (i == length){
      continuer = false
    } else if (plusPetit(i,i+1) && (i < length)){
      i += 1
    } else if (i < length){
      echange(i,i+1)
      if (i > 0){
      	i -= 1
      }
    }
  }
}