def insertionSort() {
  var position:Int = 0
  var indexSorted:Int = 0
  var v:Int = 0
  for(i <- 1 to getValueCount() - 1){
    v = getValue(i)
    position = i
    while(position != 0 && getValue(position - 1) > getValue(position)){
    	copy(position - 1, position)
      position -= 1
    }
    setValue(position, v)
  }
}