def cocktailSort() {
  var a: Boolean=true;
  var n: Int=getNombreValeurs();
  while(a==true)
  {
    a=false;
    for(i <- 0 to n-1)
    {
      if(getValeur(i)>getValeur(i+1))
      {
        echange(i,i+1);
        a=true;
      }
    }
    for(i <- n-1 to 0)
    {
      if(getValeur(i)<getValeur(i+1))
      {
        echange(i,i+1);
        a=true;
      }
    }
  }
}