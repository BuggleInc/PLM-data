def bubbleSort()  {
  
 	var isSort:Boolean = false
	var sorted:Boolean = false;
  
  while(isSort == false){
    
    sorted = false

    for(i <- 0 to (getNombreValeurs()-2)){

      if(plusPetitQue(i+1, i)){

        echange(i, i+1);
        
        if(sorted == false){
          
          sorted = true
          
        }

      }

    }
    
  }
  
}