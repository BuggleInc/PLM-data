boolean haveBaggle() {
	boolean vuBiscuit=false;
	if (estSurBiscuit()==true) {
		vuBiscuit=true;
	} else {
		for (int casesParcourues=0; casesParcourues<6; casesParcourues++) {
		avance();
		if (estSurBiscuit()==true) {
			vuBiscuit=true;
		}
	} for (int casesDeRetour=0; casesDeRetour<6; casesDeRetour++) {
		recule();
	}
	} if (vuBiscuit==true) {
		return true;
	} else {
		return false;
	}
}
