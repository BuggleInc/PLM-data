override def run() {

  def keepHandOnSideWall() = {
    gauche()
    if (!estFaceMur()){
      avance()
    }
    else {
      droite()
      if (!estFaceMur()){
        avance()
      }
      else {
        droite()
        if (!estFaceMur()){
          avance()
        }
        else {
          droite()
          avance()
        }
      }
    }
  }
  
  while (!estSurBiscuit()){
    keepHandOnSideWall()
  }
  prendBiscuit()
}
