def max1020(a:Int, b:Int):Int = {
	if (in1020(a) && in1020(b)){
		return max(a,b) ;
	}else{
		return 0;
	}
}





def in1020(c:Int):Boolean = {
	if (c == 10){
		return true;
	}else{
		if (c == 20){
			return true;
		}else{
			if( c<20 && c>20){
				return true;
			}else{
				return false;
			}
		}
	}
}


def max (a:Int , b:Int): Int = {
	return   (  (a + b + Math.abs(b - a))/2);
}
