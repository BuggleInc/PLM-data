def insertionSort() {
  var i=0
  var v=0
  var c=0
  var test=false
  for(i <- 1 to getNombreValeurs()-1){
    v=getValeur(i)
    test=plusPetit(i-1,i)
    c=0
    while(test==false && i-1-c>=0){
      copie(i-1-c,i-c)
      c+=1
      if(i-1-c>=0){
      	test=plusPetitQue(getValeur(i-1-c),v)
      }
    }
    setValeur(i-c+1,v)
  }
}