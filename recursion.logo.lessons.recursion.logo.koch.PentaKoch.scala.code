def pentaKoch(levels:Int, length:Double) {
  if (levels < 1){
    avance(length)
    return
  }
  pentaKoch(levels - 1 ,length * 0.4)
  left(180-360/5)
  pentaKoch(levels - 1 ,length * 0.4)
  right(360/5)
  pentaKoch(levels - 1 ,length * 0.4)
  right(360/5)
  pentaKoch(levels - 1 ,length * 0.4)
  right(360/5)
  pentaKoch(levels - 1 ,length * 0.4)
  left(180-360/5)
  pentaKoch(levels - 1 ,length * 0.4)
}
