def bubbleSort3() {
  var n:Int = getNombreValeurs()
  var echangeCount:Int = -1
  var i:Int = 0
  
  while (echangeCount != 0) {
    echangeCount = 0
    
    for (j <- 0 to n - i - 2) {
      if (plusPetit(j + 1, j)) {
        echange(j + 1, j)
        
        echangeCount += 1
      }
    }
    
    i += 1
  }
}
