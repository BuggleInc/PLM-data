def gnomeSort()  {
  var n = getNombreValeurs();
  var parcours = true;
  var i = 0
  while (i<n-1) {
      if (i>0 & plusPetit(i+1,i)) {
        echange(i,i+1)
        i=i-1
      }
    	else if (i==0 & plusPetit(i+1,i)) {
        echange(i,i+1)
        i=i+1
      }
      else 
        i=i+1
      
      
  }
}