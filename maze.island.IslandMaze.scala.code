override def run() {
  while (!isOverBaggle){
    while(!isFacingWall) {
      avance();
    }
    left();
    while(!isOverBaggle && getDirection!=Direction.NORTH) {
      keepHandOnWall();
    }
  }
  pickupBaggle();
}

def keepHandOnWall() {
  left();
  while(isFacingWall) {
    right();
  }
  forward();
}