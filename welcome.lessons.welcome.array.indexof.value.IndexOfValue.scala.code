def indexOfValue(nums:Array[Int] ,value:Int): Int = {
	var p:Int =nums.length
	if (nums.length == 0){
		return -1
	}
	if (nums.length == 1){
		if(nums(0)==value){
			return 0
		}else{
			return -1
		}
	}
	if (nums.length == 2){
		if(nums(0)==value){
			return 0
		}
		if(nums(1)==value){
			return 1
		}
		if(nums(2)==value){
			return 2
		}else{
			return -1
		}
	}else{
		return 666
	}
	

}
