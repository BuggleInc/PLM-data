def selectionSort() {
	var length = getNombreValeurs();
	var i =0;
	for(i <- 0 to length){
		var j = i;
		var bonnePlace = 0;
		var min = 100000;
		var valeur = 0;
		while(j < length){
			valeur = getValeur(j);
			if(valeur < min){
				bonnePlace = j;
				min = valeur;		
			}
			j += 1;
		}
		echange(i, bonnePlace);
	}
}
