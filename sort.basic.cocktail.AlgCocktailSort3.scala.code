def cocktailSort3() {
  var n = getValueCount();
  var haveBeenSorted = true;
  var j = 0;
  while (haveBeenSorted) {
    haveBeenSorted = false;
    for (i <- j to n-2-j) {
      if (isSmaller(i+1, i)) {
        swap(i, i+1);
        haveBeenSorted = true;
      }
    }
    if (!haveBeenSorted) {
      return ();
    } else {
    j += 1;
    for (i <- (n-2-j) to j by -1) {
      if (isSmaller(i, i-1)) {
        swap(i, i-1);
        haveBeenSorted = true;
      	}	
   	 }
  	}
	}
}