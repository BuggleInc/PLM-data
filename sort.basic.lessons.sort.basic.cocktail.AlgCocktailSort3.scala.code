def cocktailSort3() {
	var start = 0;
	
	var end = getNombreValeurs-2;
	var swap = true;
	while (swap) {
		swap = false;
		for (i <- start to end) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				swap = true
			} 
		}
		end = end - 1
		if (!swap) { return }
		swap = false
		for (i<- end to start by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				swap = true
			} 
		}
		start = start + 1
	}
	return;
}
