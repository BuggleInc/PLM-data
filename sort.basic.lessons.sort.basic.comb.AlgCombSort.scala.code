def combSort() {
  var ecart = getNombreValeurs();
  var n = getNombreValeurs();
  var i = 0
		  
  while (ecart>1) {
 		
   	ecart=(ecart.toDouble/1.2).toInt 
    
 		i=0
  	
 		while (i+ecart<n) {
     	 i=i+1 
 			 if (getValeur(i)>getValeur(i+ecart)) {
 	  		 echange(i,i+ecart)
         
 	  	 }
     
    }
  }
}
