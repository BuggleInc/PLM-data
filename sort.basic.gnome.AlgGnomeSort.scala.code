def gnomeSort() {
  var length:Int = getNombreValeurs()
  var continuer:Boolean = true
  var i:Int = 0
  while (continuer){
    if (i == length){
      continuer = false
    } else if (plusPetit(i,i+1) && (i < length)){
      i += 1
    } else if (i > 0){
      echange(i,i+1)
      i -= 1
    }
  }
}