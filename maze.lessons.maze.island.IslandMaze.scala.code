override def run() {
  while (!estSurBiscuit())
  {
    if (!estFaceMur())
    {
      if (getDirection()==Direction.NORTH)
    		avance()
    }
    else
    {
      droite()
    	keepHandOnSideWall()
    }
  }
  prendBiscuit()
}

def keepHandOnSideWall()
{
  gauche()
  if (estFaceMur())
  {
    droite()
    if (estFaceMur())
    	droite()
    else
    	avance()
  }
  else
  	avance()
}