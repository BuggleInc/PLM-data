def shellSort()  {
	var v = 0
	var ecart = getNombreValeurs()/2

	while (ecart > 0) {
		for (i <- ecart-1 to getNombreValeurs()-1) {
			var k = i
			v = getValeur(i)
			while (k > ecart-1 && v < getValeur(k-ecart)) {
				copie(k-ecart,k)
				k=k-ecart
			}
			setValeur(k,v)
		}
		ecart = ecart /2
	}
}
