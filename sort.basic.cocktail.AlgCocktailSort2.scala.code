def cocktailSort2() {

  var fini = false;
  var n = getNombreValeurs() -2 ;
  var p = 0;
  while(!fini) {
 
    fini = true ;
    for (i<- p to n){
      if (!plusPetit(i,i+1)){
        echange(i,i+1);
        fini = false
      }
    }
    if(n!=p){
    for (i<- p to n){
      if (!plusPetit(n - i,n - i + 1)){
        echange(n - i,n - i + 1);
        fini = false
      }
    }
    p += 1;
    n -= 1;   
  }
  }

}