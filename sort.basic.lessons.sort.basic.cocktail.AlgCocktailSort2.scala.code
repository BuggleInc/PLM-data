def cocktailSort2() {
var length = getNombreValeurs(); 
var debut=0; var fin=length-2;
var echangé:Boolean=false;
do
{
	echangé=false
	
	for(i<-debut to fin){
		if(!plusPetit(i,i+1)){
			echange(i,i+1);
			
			fin-=1;
		}
		echangé=true;
	}
	for(i<-fin to debut by -1){
		if(!plusPetit(i,i+1)){
			echange(i,i+1);
			
			debut += 1;
		}
		echangé=true;
	}
}
while(echangé)
}
