def isFacingTrail():Boolean = {
    while(!isOverBaggle){
      if(!isFacingWall()){
        forward();
      }else{
        left();
      }
      var x= getGroundColor()
      if(x!=Color.green){
        backward();
        left();
        
        
      }
      if(x==Color.green){
        brushDown();
        brushUp();
      }
    }
  if(isOverBaggle()){
    pickupBaggle();
  }
  return isOverBaggle()
}