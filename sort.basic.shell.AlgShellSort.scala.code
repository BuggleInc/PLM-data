def shellSort()  {
  var ecart = getNombreValeurs()/2
  while(ecart>0.5){
    var v = 0
  	for(i<- 1 to getNombreValeurs()-1){
    	v = getValeur(i)
      if(i-ecart > 0){
    		while(i>0 && getValeur(i-ecart)>v){
        	copie(i-ecart,i)
    		}
  			setValeur(i-ecart, v)
  	}
    }
    ecart = ecart/2
  }
  
}