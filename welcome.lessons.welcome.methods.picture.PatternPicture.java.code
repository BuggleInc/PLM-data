public void run() {
	makeSquare();
	
	
}
public void makeSquare()
{
	
		for(int j =0;j<2;j++)
		{forward();
		  brush();
		  backward();
		  left();
		}
	back();
	
}
public void brush()
{
	brushDown();
	brushUp();
}
public void horizontal()
{
	for(int j=0;j<4;j++)
	{
		forward();
	}
}
public void vertical()
{
	left();
	horizontal();
	right();
}
