def bubbleSort3() {
  var n= getNombreValeurs()
  var dejaTrie=0 
  var count=0
   for (nbTour <- 0 to n-1){
     count=0
     for (parcours <- 0 to n-2-nbTour){
       if (getValeur(parcours)>getValeur(parcours+1)){
         echange(parcours	,parcours+1)
         count=count+1
      
       }
      if (count==0){ return }
     }
   }    
}
  
  
