def goBiscuit() {
  for(i<- 0 to (getMondeLargeur()-1)){
    for(j<- 0 to (getMondeHauteur()-1)){
    	if(aBiscuit(i,j)){
        tracechemin(i,j,0);
        return
      }
  	}
  }
}

def tracechemin(x:Int,y:Int,nb:Int):Boolean = {
  
  setIndication(x,y,nb);

  
  if(getX()==x && getY()==y){
    println("ok");
    return true
  }

  
  if(y+1<getMondeHauteur()){
    if(!aMurNord(x,y+1) && getIndication(x,y+1)==9999)	{
      if(tracechemin(x,y+1,nb+1)){
        return true
      }
    }
  }
  if(x-1>=0){
    if(!aMurOuest(x,y) && getIndication(x-1,y)==9999){
      if(tracechemin(x-1,y,nb+1)){
        return true
      }
    }
  }
  if(y-1>=0){
    if(!aMurNord(x,y) && getIndication(x,y-1)==9999)		{
      if(tracechemin(x,y-1,nb+1)){
        return true
      }
    }
  }
  if(x+1<getMondeLargeur() && getIndication(x+1,y)==9999){
    if(!aMurOuest(x+1,y)){
      if(tracechemin(x+1,y,nb+1)){
        return true
      }
    }
  }
  
  return false
  
}

def yaller(){
    
  var x:Int = getX();
  var y:Int = getY();
  
  var mini:Int = getIndication(x,y);
  
  println(mini);
  
  
  if(y+1<=getMondeHauteur()-1){
    if(getIndication(x,y+1)<mini)			{
      setDirection(Direction.SUD);
      if(!estFaceMur()){
        mini = getIndication(x,y+1);
      }
    }
  }
  if(x-1>=0){
    if(getIndication(x-1,y)<mini){
      setDirection(Direction.OUEST);
      if(!estFaceMur()){
        mini = getIndication(x-1,y);
      }
    }
  }
  if(y-1>=0){
    if(getIndication(x,y-1)<mini)		{
      setDirection(Direction.NORD);
      if(!estFaceMur()){
        mini = getIndication(x,y-1);
      }
  }
  }
  if(x+1<=getMondeLargeur()-1){
    if(getIndication(x+1,y)<=mini){
      setDirection(Direction.EST);
    }
  }
  
  gauche();
  while(estFaceMur()){
    droite();
  }
  avance();
  
  ecritMessage();
  
  
}

override def run() {
  goBiscuit();
  while(!estSurBiscuit()){
    yaller();
  }
  prendBiscuit();
}