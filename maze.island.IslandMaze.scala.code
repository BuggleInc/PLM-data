override def run() {
  while (!estSurBiscuit()){
    if (allerAuNord){
      setDirection(Direction.NORTH)
      avance()
    }
    else if (keepHandOnSideWall()==true){
      if (!estFaceMur()){
        avance()
      }else{
        droite()
        if (estFaceMur()){
          droite()
        }
        avance()
      }
    }else{
      gauche()
      avance()
    }
  }
  prendBiscuit()
}

def keepHandOnSideWall():Boolean={
  gauche()
  if (estFaceMur()){
    droite()
    return true;
  }else{
    droite()
    return false;
  }
}

def allerAuNord():Boolean={
  var direction=getDirection()
  setDirection(Direction.NORTH)
  if (estFaceMur){
    setDirection(direction)
    return false
  }
  setDirection(direction)
	return true
}