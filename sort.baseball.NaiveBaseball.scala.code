override def run() {
  var n = getNombreBases()
  var trou = getTrouBase()
  var posJoueur = 1
  var candidat1 = 0
  var candidat2 = 0
  var candidat3 = 0
  var candidat4 = 0
  while (!estTrie()) {
    trou = getTrouBase()
    if (trou == n-1) {
      n = 0
    } else {
      n += 1
    }
    
  }
}