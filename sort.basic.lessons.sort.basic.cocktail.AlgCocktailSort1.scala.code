def cocktailSort() {
var u=1
do {
      
      u=0
    
       for( i<- 0 to getNombreValeurs()-2) {
              if( plusPetit(i,i+1)==false) {
                  echange(i,i+1) 
                  u=u+1 
              }
       }
       for ( i<- getNombreValeurs()-2 to 0) {
              if( plusPetit(i,i+1)==false) {
                  echange(i,i+1) 
                  u=u+1
              }
       }
              
       }
while ( u!=0)
}
