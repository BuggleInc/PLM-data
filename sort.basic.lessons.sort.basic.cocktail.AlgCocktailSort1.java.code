public void cocktailSort() {

	boolean fini=false;
	while(!fini){
		fini=true;
		
		for(int i=0; i<getNombreValeurs()-1; i++){
			if(plusPetit(i+1, i)){
				fini=false;
				int temp = getValeur(i);
				copie(i+1, i);
				setValeur(i+1, temp);
			}
		}

		for(int i=getNombreValeurs()-2; i>=0; i--){
			if(plusPetit(i+1, i)){
				fini=false;
				int temp = getValeur(i);
				copie(i+1, i);
				setValeur(i+1, temp);
			}
		}
	}
}
