override def run() {
  var finalpos : Boolean = false
  var counter : Int = 0
  while (finalpos==false){
    for (a <- 0 to 6){
      for (b <- 0 to 6){
        if (a==6 && b==6){
          setPos(6-a,b)
          writeMessage(counter)
          finalpos=true
        }
        else {
          setPos(6-a,b)
          writeMessage(counter)
          counter+=1
        }
      }
    }
  }
}