public void run() {
makeSquare();
	
}

public void makeSquare()
{
	for(int p=0;p<2;p++)
	{
	paint();
	turn();
	}

}
public void square()
{
	brush();
	forward();
	left();
	forward();
	brush();
}

public void brush()
{
	brushDown();
	brushUp();
}

public void paint(){
	forward();
	brush();
}
public void turn()
{
	left();
	forward();
	left();
}
