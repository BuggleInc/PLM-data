override def run() {
  
	faireMotif()
  
}

def faireMotif(){
  faireV(Color.YELLOW)
  faireV(Color.RED)
  faireV(Color.BLUE)
  faireV(Color.GREEN)
  
}


//marque le sol
def marquerSol(){
  baisseBrosse()
  leveBrosse()
}

//fait un seul V => une seule couleur
def faireV (c:Color){
  setCouleurBrosse(c:Color)
  avance()
  marquerSol()
  avance()
  gauche()
  avance()
  marquerSol()
  recule()
  droite()
  avance()
  marquerSol()
  avance()
  gauche()  
  
}