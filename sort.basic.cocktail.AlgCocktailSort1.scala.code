def cocktailSort() {
  val n = getNombreValeurs()
  var aInverse = true
  while (!aInverse) {
    aInverse = false
    for (i <- 0 to n - 2) {
      if (getValeur(i+1) < getValeur(i)) {
        echange(i,i+1)
        aInverse = true
      }
    }
    for (j <- n-2 to 0 by -1) {
      if (getValeur(j) < getValeur(j+1)) {
        echange(j+1,j)
        aInverse = true
			}
    }
  }
}