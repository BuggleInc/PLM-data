def max2Value(nums:Array[Int]): Int = {
  if (nums(0) < nums(1)) {
    var max:Int = nums(1)
    var min:Int = nums(0)
  } else {
    var max:Int = nums(0)
    var min:Int = nums(1)
  }
  for (i <- 2 to nums.length-1) {
    if (nums(i) > max) {
      min = max
      max = nums(i)
    }
    if (nums(i) > min && nums(i) < max) {
      min = nums(i)  
    }
  }
  return min
}