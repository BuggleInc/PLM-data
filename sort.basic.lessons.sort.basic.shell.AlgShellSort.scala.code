def shellSort()  {
  var ecart = getNombreValeurs() / 2
  var temp = 0
  while (ecart > 0) {
    for (i <- 0 to getNombreValeurs() - 1) {
      temp = getValeur(i)
      var j = i
      while (j > 0 && getValeur(j-ecart) > temp) {
        copie(j-ecart,j)
        j = j - ecart
      }
      setValeur(j,temp)   
      ecart = ecart / 2
    }
	}
}