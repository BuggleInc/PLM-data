def cocktailSort() {
  var swapped=false
  for (j<-0 to getValueCount()-1){
  for (i <-0 to getValueCount()-2) {
    if (!isSmaller(i,i+1)) {
      swap(i,i+1);swapped=true
    }
  }
  for (k<-0 to getValueCount()-2 by -1) {
    if (!isSmaller(k,k+1)) {
      swap(k,k+1);swapped=true
    }
   }
  if (!swapped) {return}
  }
}