def bubbleSort2() {
  
  for (i<-0 to getNombreValeurs()-1){
    for (j<-1 to getNombreValeurs()-i-1){
      if (!plusPetit(i,j)){
        echange(i,j)
      }
    }
  }
  for (i<-getNombreValeurs()-1 to 0 by -1){
    for (j<-0 to i-1){
      if (!plusPetit(j,i)){
        echange(j,i)
      }
    }
  }
}