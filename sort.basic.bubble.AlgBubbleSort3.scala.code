def bubbleSort3()  {
  val dim : Int = getNombreValeurs()
  var echangeFait : Boolean = true
  var compteur : Int = 0
  while (true) {
    echangeFait = false
    for (i <- 0 to (dim - 2 - compteur)) {
      if (!plusPetit(i,i+1)) {
        echange(i, i + 1)
        echangeFait = true
      }
    }
  	compteur = compteur + 1
    if (!echangeFait)
    	return
  }
}