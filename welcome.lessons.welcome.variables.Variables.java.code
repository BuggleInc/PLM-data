public void run() { 

	//Move forward until over a baggle, counting steps as you go
	int stepAmount = 0;
	while (!isOverBaggle()) {
		forward();
		stepAmount = stepAmount + 1;
	}

	//When over baggle, pick it up
	pickupBaggle();
	while (stepAmount>0) {
		backward();
		stepAmount = stepAmount - 1;
	}

	dropBaggle();
}
