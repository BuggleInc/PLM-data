def combSort() {
  var i = 0
  var a = getNombreValeurs()
  var trie = false
  var ecart = a
  while (ecart>1 || trie ==false){
    ecart = ecart/1.3
    i= 0
    trie= true
    while (i+ecart < a){
      if (plusPetit(i+ecart,i)==true){
        echange(i+ecart,i)
        trie =false
      }
      i=i+1
    }
  }
}