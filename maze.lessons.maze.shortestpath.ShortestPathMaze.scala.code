override def run() {
	
	var posX = 0
	var posY = 0
	var compteur = 0
	
	def biscuit() {
		for (i<- 0 to getMondeLargeur -1) {
			for (j<- 0 to getMondeHauteur -1) {
				if (aBiscuit(i,j)) {
					setIndication(i,j,compteur)
					compteur = compteur +1
					posX = i
					posY = j
				}
			}
		}
    }

    biscuit()

    
}
