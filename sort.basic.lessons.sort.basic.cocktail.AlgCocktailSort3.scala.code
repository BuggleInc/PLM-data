def cocktailSort3() {

var b=true
var debut=0
var fin=0
	var a=getNombreValeurs()
	while (b==true) {
		
		b=false	
		
		for (i<-debut to a-2-fin) {if (plusPetit(i+1,i)) 
		{echange(i+1,i); b=true }}
		fin=fin+1;

		b=false
		
		for (j<-fin to a-2-debut) {if (plusPetit(a-1-j,a-2-j)) 
		{echange(a-1-j,a-2-j); b=true }}
		debut=debut+1
		
	}	
	}


