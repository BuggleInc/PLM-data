def array667(nums:Array[Int]): Int = {
  var cumule:Boolean =false
  var nbRepet:Int=0
  for (i<- 0 to nums.length-1){
    if (nums(i)==6 && cumule==false){cumule=true}
    else if (nums(i)==6 && cumule==true){nbRepet+=1}
    else if (nums(i)==7 && cumule==true){nbRepet+=1;cumule=false}
    else {cumule=false}
  }
  return nbRepet
}