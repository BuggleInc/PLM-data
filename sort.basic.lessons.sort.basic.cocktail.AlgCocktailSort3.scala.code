def cocktailSort3() {
  var debut=0
  var fin=getNombreValeurs()-2
  var fini= true
  var fini2=true
  do {
    fini=true
    for (i<- debut to fin){
      if (!plusPetit(i,i+1)){
        echange(i,i+1)
        fini=false
      }
    }
    fini2=true
    	fin=fin-1
    	for (i<- fin to debut by -1){
      	if (!plusPetit(i,i+1)){
        	echange(i,i+1)
        	fini2=false
      	}
    	}
    	debut=debut+1
  } while ((!fini)&&(!fini2))
}