override def run() {
  while (getY()!=0){
    if (!estFaceMur()){
      setDirection(Direction.NORTH)
      avance()
    }else{
      KeepHandOnSideWall()
  }
  }
  while(!estSurBiscuit()){
    KeepHandOnSideWall()
  }
  prendBiscuit() 
}

def KeepHandOnSideWall(){
  if (!estFaceMur()){
    avance()
    droite()
  } else{
    gauche()
  }
}