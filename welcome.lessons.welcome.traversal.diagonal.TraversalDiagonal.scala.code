var diag = 0;
override def run() {
	var x=0
	var y=0
	var cpt=0
	ecritMessage(cpt);
	
	for (i<- 1 to getMondeHauteur()) {
		x=0
		y=i
		setPos(x,y);
		diag=x+y
		for (c<- 1 to diag+1) {
			y-=1
			x+=1
			cpt+=1
			ecritMessage(cpt);
			setPos(x,y);
		}
	}
	for(i<-1 to getMondeLargeur()) {
		x=i
		y=getMondeHauteur()
		setPos(x,y);
		diag=x+y
		for (c<- getMondeHauteur+1 to diag+1) {
		y-=1
			x+=1
			cpt+=1
			ecritMessage(cpt);
			setPos(x,y);
		}
	}
	
}
