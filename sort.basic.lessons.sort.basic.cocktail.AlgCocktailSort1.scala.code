def cocktailSort() {
	var inverse:Boolean = true 
	do {
		inverse = false
		for ( i <- 0 to getNombreValeurs()-2) {
			if ( plusPetit(i+1,i) ) {
			echange(i,i+1)
			inverse = true
			}
		}
		for ( j <- getNombreValeurs()-2 to 0 by -1) {
			if (plusPetit(j+1,j)) {
				echange(j,j+1)
				inverse = true
			}		
		}
	}
	while ( inverse )	
}

