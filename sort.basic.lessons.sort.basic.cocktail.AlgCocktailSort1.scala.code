def cocktailSort() {
var fini:Boolean=false
while (fini == false) {
	fini=true
	for (i <- 0 to getNombreValeurs-2) {
		if (getValeur(i) > getValeur(i+1)) {
			echange(i,i+1)
			fini=false
		}
	}
	for (i <- getNombreValeurs-2 to 0 by -1) {
		if (getValeur(i) < getValeur(i-1)) {
			echange(i,i-1)
			fini=false
	}
}
}

