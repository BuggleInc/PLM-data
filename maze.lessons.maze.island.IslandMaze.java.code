public void run() {
	searchWall();
	boolean northFollower  = false;
	while(!isOverBaggle()) {
		keepHandOnSideWall(northFollower);
	}
	pickUpBaggle();
}

void keepHandOnSideWall(boolean northFollower){
	if(northFollower){
		
	}
	else{
		if(!isFacingWall()) {
			forward();
			left();
		}
		if(isFacingWall()){
			right();
		}
	}
}

void searchWall() {
	while(!isFacingWall()){
		left();
	}
	right();
}
