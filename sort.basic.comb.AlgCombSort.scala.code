def combSort() {
  var lgr = getNombreValeurs();
  var ecart = lgr;
  var fini = false; 
  do {
    fini = true;
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt;
    }
    var i = 0;
    while(i+ecart < lgr ){
      i += 1;
      if(plusPetit(i+ecart,i)){
        echange(i+ecart,i);
      }
      
    }
  } while(ecart>1 ||  !fini)
}