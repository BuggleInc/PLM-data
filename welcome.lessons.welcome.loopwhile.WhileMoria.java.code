public void run() {	
	while (isOverBaggle() == false) { 
		setDirection(Direction.EAST);
		forward(); 
		(isOverBaggle() == true) 
		pickupBaggle();
		setDirection(Direction.WEST);
		forward(); 
		(isCarryingBaggle() == true);
		setDirection(Direction.EAST);
		forward(1); 
		dropBaggle();					
	}
						}