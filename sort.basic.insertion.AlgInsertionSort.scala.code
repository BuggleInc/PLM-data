def insertionSort() {
  val lgr=getNombreValeurs()
  var dernierecase:Boolean=true
  var i:Int=0
  var v:Int=0
  var j:Int=0
  var best:Int=0
 	for (i <- 1 to lgr-1){
    dernierecase=true
    v = getValeur(i)
    j = 0
    while (i-j>0||dernierecase){
      if (getValeur(i-j)>=v){
        copie(i-j,i+1-j)
        j+=1
      }
      else{
        dernierecase=false
      }
      
     	}
    setValeur(i-j+1, v)
  
	}
}