def max2Value(nums:Array[Int]): Int = {
  var max:Int=nums(0)
  var secmax:Int=nums(1)
  var ech:Int=0
  var temp:Int=0
  val taille=nums.length
  
  
  for (i<-1 to (taille-1)){
    if (nums(i)>=max){
      ech=max
      max=nums(i)
      secmax=ech
    }
    if (secmax<nums(i)){
      secmax=nums(i)
    }
   
  }
  
  return secmax
  
}

