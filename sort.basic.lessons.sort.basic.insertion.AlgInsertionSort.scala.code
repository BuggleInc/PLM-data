def insertionSort() {
  var n = getNombreValeurs()
  var v = 8
  var marqueur = false
  var j=0
  for (i <- 1 to (n-1)) {
    v = getValeur(i)
    j=1
    while (j<=i) {
       if (getValeur(i-j)>v) {
         echange(i-j,i-j+1)
         j=j+1
       } else {
         setValeur(j-1,v)
         j=i+1
       }    
    }
  }
}