def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs() -2
  var len = getNombreValeurs()
  var echanger : Boolean = true
  while(echanger == true) {
    echanger = false
    for (i<- debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i+1,i)
        echanger = true
      }
    };debut +=1
    for (i<- debut to fin) {
      	if (plusPetit(len-1-i, len-2-i)) {
        	echange(len -1 -i, len-2-i)
        	echanger = true
      }
      
      
    };fin -= 1
    
  }
}