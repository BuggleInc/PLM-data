def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps!=0){
      current(steps)
      avance(length:Double)
      droite(angle)
      tree(steps-1,length*shrink,angle,shrink)
      gauche(length)
      tree(steps-1,length*shrink,angle,shrink)
      droite(angle:Double)
      recule(length)
  }
}