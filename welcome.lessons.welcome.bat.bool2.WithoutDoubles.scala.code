def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
  if (noDoubles==true & die1==die2){
    if (die1==6)
    	return 1
    else if (die1==5)
    	return 2
   	else if (die1==4)
    	return 3
    else if (die1==3)
    	return 4
    else if (die1==2)
    	return 5
    else
    	return 6
  }
  else
  	return die1+die2
}