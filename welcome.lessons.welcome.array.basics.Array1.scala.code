override def run() {
  remplirTableau();
  seReplacer();
}

var tableauCouleur = new Array[Color](6)

def remplirTableau(){
  for (i<- 1 to 5){
    tableauCouleur(i)=getCouleurSol();
    avance();
  }
  tableauCouleur(5)=getCouleurSol();
}

def seReplacer(){
  gauche();avance();droite();recule(5)
}

def modifTab(t:Array[Color]){
  for(i<-1 to 5 ){
    var x = t[i];
    t[i] = t[i-1];
    t[i-1]=x;
  }
  x = t[5]
  t[0]=t[5]
  t[5]=x
}
