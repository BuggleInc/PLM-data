def combSort() {
  var n:Int=getNombreValeurs
  var ecart:Int=n
  var i:Int=0
  var compteur:Boolean=true
  while (ecart>1 || compteur){
    compteur=false
    if (ecart>1) {
      ecart = (ecart.toDouble/1.3).toInt
    }
    i=0
    while ((i+ecart)<n){
      if (!plusPetit(i,(i+ecart))){
        echange(i,(i+ecart))
        compteur=true
      }
      i=i+1
    }
  }
}