def solve() {
  val bleu = 0
  val rouge = 2
  val blanc = 1
  val n = getTaille()
  for (i <- 0 to n-1) {
    for(j <- i to n-1) {
      if(getCouleur(i) != bleu) {
        if(getCouleur(j) == bleu) {
          echange(i,j)
        }
      }
      if(getCouleur(i) == rouge) {
        echange(i,j)
      }
    }
  }
}