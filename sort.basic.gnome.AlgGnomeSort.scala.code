def gnomeSort() {
  var lgr=getNombreValeurs()
  var i=0
  
  while (i!=lgr){
    for (i<- 0 to lgr-2){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        var k=0
        while (i!=0){
          for (k<-i to 1 by -1){
            if (plusPetit(k,k-1)){
              echange(k,k-1)
            }
            i=i-1
          }
        }
      }
    }
  }
}