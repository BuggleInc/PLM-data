def cocktailSort() {
  var tri:Boolean = false
  while (!tri) {
    tri=true
  for (i<-0 to getNombreValeurs()-2) {
    if (getValeur(i+1)<getValeur(i)) {
      echange(i,i+1)
      tri=false
    }
  }
  for (i<- getNombreValeurs()-2 to 0 by -1) {
    if (getValeur(i)<getValeur(i-1)) {
      echange(i,i-1)
      tri=false
    }
  }
 }   
}