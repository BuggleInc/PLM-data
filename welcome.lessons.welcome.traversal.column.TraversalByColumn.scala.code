override def run() {
  while(!posFinale()){posSuivante()}
}

def posFinale():Boolean ={
  var z:Boolean = false
  if(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1){z = true}
  return(z)
}

def posSuivante(){
  var i:Int = 0
  val y:Int = getMondeHauteur()
  ecritMessage(i)
  if(getY() < y-1){setY(getY()+1);i += 1;ecritMessage()}
  else if(getY() == y-1){setPos(getX()+1,0); i += 1;ecritMessage()}
}