def solve() {
  
  var term = false
  
  do{
    term = true

    for (i <- 0 to getSize()-2){

       if((getColor(i) == RED) && (getColor(i+1) != RED)){
    		 swap(i,i+1)
         term = false
   		 }
    
      else if((getColor(i+1) == BLUE) && getColor(i) != BLUE){
        swap(i,i+1)
        term = false
      }
    
   	 }
   
    } while (term == false)
}