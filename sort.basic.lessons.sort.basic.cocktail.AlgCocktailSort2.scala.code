def cocktailSort2() {
	
	var debut=0;
	var fin = getNombreValeurs()-2;
	var needSwap = true; 
	while(needSwap)
	{
		needSwap=false;
		for( i<- debut to fin)
		{
			if(plusPetit(i+1,i))
					{
						echange(i,i+1);
						needSwap=true;
					}
			
		}
		fin -= 1;
		for(i<- fin to debut by -1)
		{
			if(plusPetit(i+1,i))
					{
						echange(i,i+1);
						needSwap=true;
					}
			
		}
		debut +=1;
	}
}