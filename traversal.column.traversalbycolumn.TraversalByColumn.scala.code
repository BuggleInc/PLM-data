override def run() {
  ecritMessage(cpt)
  do {
    var cpt=0
    for(x<- 0 to getMondeLargeur()){
      for (y<-0 to getMondeHauteur())
				setPos(x,y)
        ecritMessage(cpt)
      	cpt += 1
      
      	
    }

  } while (positionFinale()) 
  
}

def positionFinale(): Boolean = {
  	return ((getX()==getMondeLargeur()) && (getY()==getMondeHauteur()))
    
  }

