override def run() {
  def FaireMachin(c:Color) {
  	setCouleurBrosse(c)
    baisseBrosse()
 	 	leveBrosse()
  	gauche()
    avance()
    gauche()
    avance()
  	baisseBrosse()
  	leveBrosse()  
	}
  
  def couleurMotif(i:Int): Color= {
    i match {
      case 4 => return Color.yellow
      case 1 => return Color.red
      case 2 => return Color.blue
      case 3 => return Color.green
    }
  }
  
  def faireMotif() {
    for (i <- 1 to 4) {
  		avance()
  		FaireMachin(couleurMotif(i))
  		gauche()
      avance()
      gauche()
      avance(3)
      gauche()
		}
  }
	
  def Dessin() {
    faireMotif()
    avance(4)
    faireMotif()
    recule(4)
    gauche()
    avance(4)
    droite()
    faireMotif()
    avance(4)
    faireMotif()
  }
  Dessin()
}
