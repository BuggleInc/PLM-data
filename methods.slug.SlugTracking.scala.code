def isFacingTrail():Boolean = {
  	var vuVert: Boolean = false
  	forward();
    if (getCouleurSol()==Color.green) {
      vuVert=true
    } else {
      vuVert=false
    }
  	backward();
  	return vuVert
}

while (estSurBiscuit==false) {
    if (estFaceMur()==true) {
      left();
    }
  	if (isFacingTrail()==true) {
      brushDown(); forward(); brushUp();
    } else {
      left();
    }
}