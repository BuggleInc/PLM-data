int blueTicket(int a, int b, int c) {
	if(any(10,sum(a,b),sum(b,c),sum(a,c)) ){
		return 10;
	} else if(any(10, a - c,b - c)){
		return 5; 
	} else {
		return 0;
	}
}

int sum(int a,int b){
	return a + b;
}

boolean any(int isValue, int... ofThese){
	for (int i : ofThese){
		if(i == isValue)
			return true ;
	}
	return false;
}
