def cocktailSort() {

  var swapped:Boolean =true
  
  while (swapped) {
    swapped=false
    for (i <- 0 to getValueCount()-2) {
    	if (  isSmaller(i+1,i)) {
      	swap(i,i+1)
      	swapped=true
        
      }
      
    }
    
    for (i <- (getValueCount()-2) to 0) {
    	if (  isSmaller(i-1,i)) {
      	swap(i,i-1)
      	swapped=true
      }
    }
    
  } 
}