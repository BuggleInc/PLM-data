override def run() {
	var t= new Array[Color](getMondeHauteur());
	var w:Int=0;

	for (y<-0 to getMondeHauteur()-1) {
	
		t(y)=getCouleurSol();
		avance();
	}
		
	for (x<-0 to getMondeLargeur()-1) {
		for (z<-0 to getMondeLargeur()-1) { 
			gauche();
			avance();
			droite();
			avance();
			baisseBrosse();
			setCouleurBrosse(t(w));
			leveBrosse();
		
		}
		w=w+1
	}
}
