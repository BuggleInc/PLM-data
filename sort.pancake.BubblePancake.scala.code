def solve() {
  var n = getTaillePile()
  for (i<-0 to n-2) {
    var x=plusGrand(i,i+1)
    var j=i
    while (j!=(-1) && plusGrand(j,j+1)){
      echange(j+1)
      j = j-1
    }
  }
}


def plusGrand(i : Int,j : Int) : Boolean = {
	var x = getRayonCrepe(i)
  var y = getRayonCrepe(j)
  return (x>y)
}


