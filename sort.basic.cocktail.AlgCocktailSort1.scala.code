def cocktailSort() {
  var nb=false
  var n=getNombreValeurs-2
  
  do{
    for( i<- 0 to getNombreValeurs()-2){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        nb=true
      }
      
    }
    for(i<- 0 to n){
      
      if(plusPetit(n-i+1,n-i)){
        echange(n-i+1,n-i)
        nb=true
      }
    }
    
  }
  while(nb)
}