def cocktailSort() {
  
  var n = getValueCount();
	var swapped = false
  
  do
  {
    swapped = false
    for( i <- 0 to n-1)
    {
      if (!isSmaller(i,i+1))
      {
        swap(i,i+1);
        swapped = true;        
      }
    }
      
    for( i <- n-1 to 1 by -1)
    {
      if (isSmaller(i,i-1))
      {
        swap(i-1,i);
        swapped = true;        
      }
    }
  }while(!swapped)

}