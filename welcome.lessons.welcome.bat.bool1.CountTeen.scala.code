def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
var NbB=0

	if (a>=13 && a<=19) {
		NbB = NbB+1
	} else { NbB=NbB }
	if (b>=13 && b<=19) {
		NbB = NbB+1
	} else { NbB=NbB }
	if (c>=13 && c<=19) {
		NbB = NbB+1
	} else { NbB=NbB }
	if (d>=13 && d<=19) {
		NbB = NbB+1
	} else { NbB=NbB }
	return NbB
}
