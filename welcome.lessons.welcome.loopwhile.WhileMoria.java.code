public void run() { 
	right();
	right();

	while(!isFacingWall()) {
		while(!isOverBaggle()) {
			forward();
		}
		pickupBaggle();

		while(!isOverBaggle()) {
			backward();
		}
		if(hasBaggle()) {
		forward();
		dropBaggle();
		forward();
		}
	}
	escape();
}

public void escape() {
	right();
	forward();
	left();
	forward();
}


