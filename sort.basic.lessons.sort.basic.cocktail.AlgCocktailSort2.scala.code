def cocktailSort2() {
  var lgr=getNombreValeurs()
  var debut=0
  var fin=lgr-2
  var parcours=0
  do{
    parcours=0
    for (i<-0 to lgr-2){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        parcours=parcours+1
        fin=lgr-1
      }
    }
    for (k<-lgr-2 to 0 by -1){
      if (plusPetit(k+1,k)){
        echange(k+1,k)
        parcours=parcours+1
        debut=debut+1
      }
    }
  }while (parcours!=0)
}