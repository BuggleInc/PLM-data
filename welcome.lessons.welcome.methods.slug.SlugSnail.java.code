public void hunt(Color c) {
	while(!isOverBaggle()){
		if (!isFacingTrail(c)) {
			right();
		}
		if (isFacingTrail(c)){
			brushDown();
			forward();
			brushUp();
		}
	}
    	pickupBaggle();
} 

boolean isFacingTrail(Color c) {
	boolean stillOnTrail = false;
	if (!isFacingWall()){
	forward();
	if (getGroundColor().equals(c)) {
		stillOnTrail = true;
	}
	backward();}
    	return stillOnTrail;
}