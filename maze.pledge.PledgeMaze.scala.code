override def run() {

  var c=0
  while(isOverBaggle==false){
    if(!murNord){forward();}
    else{
      right();
      suivremur()
    }
  }
  pickupBaggle;
}


def suivremur()={
  while(isOverBaggle==false){
    mur()
  }
}


def mur()={

  left()
  while((isFacingWall==true)||(getDirection!=Direction.NORTH)){
    right();
  }
  forward();
}

def murNord():Boolean={
  setDirection(Direction.NORTH)
  if(isFacingWall==true){return true;}
  else {return false;} 
}