def max2Value(nums:Array[Int]): Int = {
  var m : Int = nums(0)
  var n  = nums.length
  var m2 = Integer.MIN_VALUE
  for (k<-1 to n-1){
    if (m < nums(k)){
      m=nums(k)
    }
    else m=m
  }
  for (k<-0 to n-1){
    if (nums(k)<=m && nums(k)>m2){
      m2=nums(k)
    }else{
      m2=m2
    }
  }
  return m2
}