def selectionSort() {
  var lgr:Int = getNombreValeurs()
  var min:Int = 0
  var position:Int = 0
  var minimum:Int = 0
  for (i <- 0 to lgr-1) {
    min = i
    minimum = getValeur(i)
    for (j <- i to lgr-1) {
      if (getValeur(j) < minimum) {min = j; minimum = getValeur(j)}
    }
    echange(i,min)
  }
}