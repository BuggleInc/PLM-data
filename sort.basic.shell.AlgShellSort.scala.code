def shellSort()  {
  var lgr = getNombreValeurs()
  var ecart = lgr/2
  var casei = 0
  var j =0
  while(ecart>0){
    for(i<-ecart to lgr-1){
    casei = getValeur(i)
    j = i
    while(j-ecart>0){
      if(getValeur(j-1)>casei){
        echange(j-1,j)
      }
      j = j-1      
    }
      ecart = ecart/2
  	}
  } 
}
