def cocktailSort() {
  var fin=false
  var reali=0
  for (i<-0 to getValueCount()-2){
    if (isSmaller(i+1,i)==true) {
            swap(i,i+1);
            fin=false;
    }
  }
  for (i <-0 to getValueCount()-2){
    reali=getValueCount()-2-i
    if (isSmaller(reali-1,reali)==true) {
            swap(reali,reali-1);
            fin=false;
  }
    
  
}
}