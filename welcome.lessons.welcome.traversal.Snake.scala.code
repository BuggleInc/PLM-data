def fin():Boolean =
{
  if(estFaceMur())
  {
    if(getDirection()==Direction.EST)
    {
			gauche()
      if(estFaceMur())
      {
        droite()
        return true
      }
      droite()
    }
    if(getDirection()==Direction.OUEST)
    {
      droite()
      if(estFaceMur())
      {
        gauche
        return true
      }
      gauche()
    }
  }
  return false
}

def serpent()
{
  if(!estFaceMur)
  {
    avance()
  }
  else if(getDirection()==Direction.EST)
  {
    gauche()
    avance()
    gauche()
  }
  else if(getDirection()==Direction.OUEST)
  {
    droite()
    avance()
    droite()
  }
}

override def run()
{
  baisseBrosse
  while(!fin())
  {
    serpent()
  }
}