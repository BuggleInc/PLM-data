def hunt() {
    while (isFacingTrail==true) {
    	avance();
    }
    while (!isFacingTrail==true) {
    	gauche();
    }
}

def isFacingTrail():Boolean = {
    if (estFaceMur) {
    	return false;
    }
    avance();
    var trace:Boolean=getCouleurSol()==Color.green;
    recule();
    return trace
}        
