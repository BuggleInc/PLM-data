def cocktailSort2() {
	var debut:Int = 0
	var fin:Int = getNombreValeurs()-2
	do {

		for ( i <- debut to fin) {
			if ( plusPetit(i+1,i) ) {
			echange(i,i+1)
			inverse = true
			}
		}
		fin -= 1
		for ( i <- fin to debut by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				inverse = true
			}		
		}
		debut += 1
	}
	while ( inverse )	
}



