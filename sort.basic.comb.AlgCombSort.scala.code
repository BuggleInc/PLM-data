def combSort() {
  var ecart=getNombreValeurs()
  var nb=false
  do{
    nb=false
    if(ecart>1){
      ecart=ecart/1.3
    }
    i=0
    while(i+ecart<getNombreValeurs()){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        i=i+1
        nb=true
      }
    }
  }
  while(((ecart>1))||(nb))
}