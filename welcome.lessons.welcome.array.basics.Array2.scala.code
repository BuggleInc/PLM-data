override def run() {
	
	var colorTable = new Array[Color](getMondeHauteur());
	var inc = 0;
	for(i <- 0 to getMondeHauteur() -1)
	{
		colorTable(i) = getCouleurSol();
		avance();
	}
	for(i <- 0 to getMondeHauteur() - 2)
	{
		while(!estDosMur()){
			recule();
		}
		gauche();
		avance();
		droite();
		
	
	for(j <- 0 to getMondeHauteur() - 1){
	setCouleurBrosse(colorTable(j));
	baisseBrosse();
	leveBrosse();
	avance();
	}
	inc = inc + 1;
	
	}
}
