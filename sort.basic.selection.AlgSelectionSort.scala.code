def selectionSort() {
  var min = 0
  val longueur = getNombreValeurs()
  var valeur = 0
  var j = 0
  for (i <- 0 to longueur) {
    valeur = getValeur(i)
    j = i
    while (j < longueur) {
      if (getValeur(j) < min) {
        min = getValeur(j)
      }
      j += 1
    }
    echange(i,j)
  }
}