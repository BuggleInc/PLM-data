$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; import lessons.recursion.cons.universe.*;import java.util.Vector;public class nfirst extends BatEntity { public RecList cons(int head, RecList tail){  return new RecList(head, tail);} @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( nfirst( (RecList)t.getParameter(0), (Integer)t.getParameter(1) ) ); }
	RecList nfirst(RecList seq, int n) {
		if (n == 0) return null;
		return cons( seq.head, nfirst(seq.tail, n-1));
	}
 }