override def run(){
  for (i <- 1 to 3){
    faireMotif()
		if (i%2!=0){
      avance(4)
    }else{
      recule(4)
      gauche()
      avance(4)
      droite()
    }  
    }
  faireMotif()
}



def faireV(c:Color){
  avance(2)
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  gauche()
  recule()
}

def faireMotif(){
  	faireV(Color.BLUE)
    faireV(Color.GREEN)
    faireV(Color.YELLOW)
    faireV(Color.RED)
}