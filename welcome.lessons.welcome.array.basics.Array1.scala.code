def printMotif(c:Array[Color])
{
	for(k<-1 to getMondeHauteur())
	{
		setCouleurBrosse(c(k-1))
		baisseBrosse()
		leveBrosse()
		avance()
	}
}
override def run()
{
	var Motif=new Array[Color](getMondeHauteur())
	for (k<-1 to getMondeHauteur())
	{
		Motif(k-1)=getCouleurSol()
		avance()
	}
	
for(k<-1 to getMondeLargeur()-1)
	{
		avance()
		gauche()
		avance()
		droite()
		printMotif(Motif)
	}
}
