void solve() {
  
  int bleu = getTaille();
  int rouge  = 0;
  int blanc = 0;
  
  while(blanc<=bleu)
  {
  	if(getCouleur(bleu)==BLANC)
      {
      blanc++;
    }
    else if(getCouleur(bleu)==ROUGE)
      {
      echange(rouge,blanc);
      rouge++; blanc++;
    }
    else{
     echange(blanc,bleu);
     bleu--;
  
    }

    
	}
}