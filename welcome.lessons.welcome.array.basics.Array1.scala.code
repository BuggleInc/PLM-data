override def run() {
	var T =new Array[Color](getMondeHauteur())
	for (i <- 1 to getMondeHauteur()){
		T(i)=getCouleurSol()
		avance()
	}
	for (i <- 1 to getMondeLargeur()){
		gauche()
		avance()
		droite()
		avance()
		for (i <- 1 to getMondeHauteur()){
			setCouleurBrosse(T(i))
			baisseBrosse()
			leveBrosse()
			avance() 
		}
	}
}
