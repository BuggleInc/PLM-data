def cocktailSort2() {
  var debut =0
  var fin=getNombreValeurs()-1
  var permutation=0
  do {
    permutation=0
    for (i<- debut to fin){
      if (plusPetit(i,i+1)==false){
        echange(i,i+1);
        permutation=1;
        fin=fin-1;
      }
    }
   for (i<-fin to debut){
     if (plusPetit(i,i+1)==false){
       echange(i,i+1);
       permutation=1;
       debut+=1;
     }
   }
  } while (permutation!=0)
}