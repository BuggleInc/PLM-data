override def run() {
  var finalpos : Boolean = false
  var b : Int = 0
  var counter : Int = 0
  while (finalpos==false){
  	for ( a <- 0 to 6){
       if  ( a== 6 && b ==6){
         setPos(a,b)
         counter+=1
         writeMessage(counter)
         finalpos=true
       }
    setPos(a,b)
   	 writeMessage(counter)
    	while (isFacingWall()==false){
    	  setPos(a,b)
    	  b+=1
    	  counter+=1
        writeMessage(counter)}
      b=0
   	 counter+=1
   }
  }
}