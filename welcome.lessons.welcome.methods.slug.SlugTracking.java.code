boolean isFacingTrail() {
boolean greenCell = true;
	do{
    		if(isFacingWall()){
    			greenCell = false;
    			backward();
    		}

    		if(getGroundColor().equals(Color.green)){
    			greenCell = true;
    			forward();
    		}else{
    			forward();
    		}



    		if(isOverBaggle()){
    			greenCell = true;
    		}
    		
	}while(!greenCell);

    return greenCell;
}        
