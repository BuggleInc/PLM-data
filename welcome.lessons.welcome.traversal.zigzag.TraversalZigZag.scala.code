override def run() {

def fonctionTest():Boolean = {
	(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1)
}

var i = 0
var j = 0
var cpt = 0


while (!fonctionTest) {
	if (j%0 == 0) {
		setPos(i,j)
		ecritMessage(cpt)
		i += 1
		cpt += 1
	} else {
		setPos(i,j)
		i -= 1
		cpt += 1	
	}
	if (i == getMondeLargeur()) {
		i = 0
		j += 1
	}
}

}

