def combSort() {
  var len=getValueCount()
  var gap=getValueCount()
  var bool=true
  while(gap>1 && bool==true){
    bool=false
    gap = (gap.toDouble / 1.3).toInt
  	var i=0
      while(i+gap<len){
    		if(isSmaller(i+gap,i)){
          swap(i,i+gap)
          bool==true
          i+=1
        }
  }
  }
}