def cocktailSort3() {
	var debut = 0
	var fin = getNombreValeurs()-2
	var sorted = false
	while (!sorted){
		if (sorted = false){
		sorted = true
		for (i <- debut to fin){
			if (plusPetit(i,i+1) == false){
				echange(i,i+1)
				sorted = false
			}
		}
		}
		fin = fin - 1
		if (sorted == false){
		sorted = true
		for (i <- fin to debut by -1){
			if (plusPetit(i,i+1) == false){
				echange(i,i+1)
				sorted = false
			}
		}
		}
		debut = debut + 1
	}	
}
