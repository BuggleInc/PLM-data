public void run() { 
int stepFwd = 0;

    while(!isOverBaggle()){
        forward();
        stepFwd ++;

        if(isOverBaggle()){
            pickupBaggle();
            
            for(int i=0; i != stepFwd; i++){
                backward();
            }
            
            dropBaggle();
        }
    }
}
