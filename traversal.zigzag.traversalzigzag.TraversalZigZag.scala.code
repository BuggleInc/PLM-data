override def run() {
  var counter:Int = 0;

  var i = 0;
  for(i <- 0 to getWorldWidth() - 1 by +2) {
    for(j <- 0 to getWorldHeight() - 1) {
      setPos(j, i);
      writeMessage(counter);
      counter += 1;
    }
    
    var c = i + 1;
    
    for(j <- getWorldHeight() - 1 to 0 by -1) {
      setPos(j, c);
      writeMessage(counter);
      counter += 1;
    }
  }
}