def combSort() {
  var ecart:Int=getNombreValeurs()-1
  var c:Int =1
  while (ecart>1 && c==1){
    var c:Int =0
    ecart= (ecart.asInstanceOf[Double]/ 1.3).asInstanceOf[Int]
    var i:Int =0
    while (i+ecart<getNombreValeurs()){
      if( plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        i+=1
        c=1
      }
    }
  }
}

