}

def keepHandOnSideWall() 
{
  if (murADroite())
  {
    if (estFaceMur)
    {
     gauche() 
      if (estFaceMur()) 
      {
        gauche()
      }
    }
    avance()
  } else 
  {
    droite()
    avance()
  }
}

def murADroite() : Boolean =
{
  var murDroite = false
  droite()
  murDroite = estFaceMur()
  gauche()
  return murDroite
}