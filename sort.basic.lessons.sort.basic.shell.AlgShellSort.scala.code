def shellSort()  {
  var ecart:Int = getNombreValeurs()  // largeur du peigne //
  var v:Int = 0  // pour m?moriser la valeur //
  var j:Int = 0  // i - k * ecart //
  while (ecart > 0) {
    ecart = (ecart.toDouble / 2).toInt
    for (i <- ecart to getNombreValeurs() - 1) {
      v = getValeur(i)
      j = i - ecart
      while (j >= 0 && getValeur(j) > v) {
        setValeur(j + ecart, getValeur(j))
        j -= ecart
      }
      setValeur(j + ecart,v)
    }
  }
}
