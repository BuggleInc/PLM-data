override def run() {
  setCouleurBrosse(Color.RED);
  baisseBrosse;
  while (!finTest){
    if (!estFaceMur){
      avance;
    }else{
      if (getDirection==Direction.EST && estFaceMur){
        gauche;avance;gauche
      }
      if (getDirection==Direction.OUEST && estFaceMur){
        droite;avance;droite
      }
    }
  }
}


def finTest () :Boolean ={
  var a:Boolean=false
  if (getDirection==Direction.EST && estFaceMur){
    gauche()
    if(estFaceMur){
      droite()
      return true
    }else{
      droite()
      return false
    }


  }else{
    return false
  }
  return false
}