def cocktailSort2() {
  var trie:Boolean = false
  var debut:Int=0
  var fin:Int=getNombreValeurs()-2
  while(!trie){
    trie = true
    for(i<-debut to fin){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        trie = false
      }
    }
    fin = fin-1
    for(i<-fin to debut by-1 ){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        trie = false
      }
    }
    debut=debut+1
  }
}
