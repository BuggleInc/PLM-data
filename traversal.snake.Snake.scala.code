override def run() {
  while(!arrivee) {
    avance()
  }
}


def arrivee():Boolean = {
  if(estFaceMur){
    if (getDirection==Direction.OUEST){
      droite()
      if (estFaceMur){
        gauche()
        return true
      }
      gauche()
      return false
    }
    if (getDirection==Direction.EST){
      gauche()
      if (estFaceMur){
        droite()
        return true
      }
      droite()
      return false
    }
  }
  return false
}