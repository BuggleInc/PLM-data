void faitFleur(Color c) {
	setCouleurBrosse(c);
	baisseBrosse();
	leveBrosse();
	avance(2);
	baisseBrosse();
	leveBrosse();
	recule();
	droite();
	avance();
	baisseBrosse();
	leveBrosse();
	recule(2);
	baisseBrosse();
	leveBrosse();
	avance();
	setCouleurBrosse(Color.YELLOW);
	baisseBrosse();
	leveBrosse();
	gauche();
	recule();
	
}

void growFlowers() {
	faitFleur(Color.RED);
}
