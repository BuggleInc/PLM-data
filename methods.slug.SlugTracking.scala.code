override def run() {
  
def isFacingTrail():Boolean = false{
  	if (isFacingWall()) {
      return false
    }
    forward()
  	if (getGroundColor()==Color.green) {
      return true
    } else {
      return false
    }
  	backward()
}
  
while (!isOverBaggle()) {
	while (!isFacingTrail()) {
  	left()
  }
  forward()
}
pickupBaggle()
}