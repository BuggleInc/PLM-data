def hunt() {
  
  do{
  	if (isFacingTrail()==true){
    	avance()
    	} 
  	; {
    	droite()
    	if (isFacingTrail()==true){
      	avance()
    	}
  	} 
  	; {
    	gauche()
    	if (isFacingTrail()==true){
      	avance()
    	}
      
  	}
  } while (estSurBiscuit()!=true) 
  prendBiscuit()
}

def isFacingTrail():Boolean = {
  var green: Boolean=false
  
  if (estFaceMur()==false){
    avance()
    if (getCouleurSol()==Color.green){
      
      green= true
    }
    recule()
  }
  
  return green

}