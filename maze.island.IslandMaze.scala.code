override def run() {
  murExterne()
   while(!estFaceMur){
    gauche()
  }
  droite()
   while(!estSurBiscuit){
    KeepHandOnSideWall()
    
  }
  prendBiscuit()
}

def KeepHandOnSideWall(){
  	while(!estFaceMur){
      avance()
      gauche()
    }
  droite()
}

def murExterne(){
  while(getDirection()!=Direction.NORD){
    gauche()
  }
  while(getY()!=0){
    if(!estFaceMur){
      avance()
    }
    else{
      var n=1
      if(n==1){
        droite()
        if(estFaceMur){
          gauche()
          
          n=2
        } else {
          avance()
          gauche()
        }
      } else {
        gauche()
        if(estFaceMur){
          droite()
          n=1
        } else {
          avance()
          droite()
        }
      }
      
    }
  }
}