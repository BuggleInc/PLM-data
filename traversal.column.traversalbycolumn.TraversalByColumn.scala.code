override def run() {
  var a = getMondeHauteur()
  var b = getMondeLargeur()
  var x = getX()
  var y = getY()
  var cpt = 0
  var lin = 0
  var col = 0
 do {
 ecritMessage(cpt)
 setPos(lin, col)  
 x +=1  
 cpt += 1
 
   
   if (y == 6){
     y = 0
     x +=1
   }
 }while (a!=x && b!=y)
 ecritMessage(cpt+1)
}