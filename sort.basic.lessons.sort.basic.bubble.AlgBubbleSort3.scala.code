def bubbleSort3(){	
	var i:Int = getNombreValeurs() ;
	var hasSwap:Boolean = false ;
	var nBoucle:Int = i;
	
	while(nBoucle != 0)
	{	
		hasSwap = false;
		for (j <- 1 to nBoucle by 1)
			{
				if (plusPetit(j,j-1))
				{
					echange(j-1,j);
					hasSwap = true;
				}
			}
		nBoucle = nBoucle - 1;
		if (!hasSwap)
			return;
		
		
	}
	
}