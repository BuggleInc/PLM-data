def shellSort()  {
  val lgr:Int = getNombreValeurs()
  var ecart:Int = lgr/2
  while (ecart>0) {
    for (i <- ecart-1 to lgr-1) {
      var valeur:Int = getValeur(i)
      var k:Int = i
      while (k>ecart-1 && getValeur(k-ecart)>valeur) {
        copie(k-ecart,k)
        k -= ecart
      }
      setValeur(k, valeur)
    }
    ecart /= 2
  }
}
