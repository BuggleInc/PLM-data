def cocktailSort2() {
  var incr:Int=0;
  var debut:Int=0;
  var fin:Int=getNombreValeurs();
  
  do{
    incr=0;
  	for(i<- 0 to getNombreValeurs()-(2+fin)){
  	  if(plusPetit(i+1,i)){
        echange(i,i+1);
        incr=incr+1;
        fin=fin-1;
      }
  	}
  	for(i<- getNombreValeurs()-2 to (0+debut) by -1){
      if(plusPetit(i+1,i)){
        echange(i,i+1);
        incr=incr+1;
        debut=debut+1;
      }
    }
  }while(incr!=0);
}