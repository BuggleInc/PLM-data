def combSort() {
  var ecart=getNombreValeurs() 
  var s=true
  do{
    s=true
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i=0
    while (i+ecart <getNombreValeurs()){
      if (!plusPetit(i, i+1) ){
        echange(i,i+1)
        s=false
      }
      i=i+1
    }
  }while(ecart>1 || !s)
}