def combSort() {
	var i=0
	var trié=true
	var ecart=getNombreValeurs()
	while (trié==true || ecart>1) {
		trié==false
		if (ecart>1)
			{ecart = (ecart.toDouble / 1.3).toInt}
			
		while (i+ecart<getNombreValeurs()) {
		if (plusPetit(i:Int, i+ecart:Int)==false) {
				echange(i:Int, i+ecart:Int) }
				i=i+1
				trié==true
	}
}
}
