override def run() {
  var x=0
  var y=0
  val x2=getMondeLargeur()
  val y2=getMondeHauteur()
  var cmpt=0
  while (!arrive()) {
    ecritMessage(cmpt)
    if (y%2==0) {x=x+1} else {x=x-1}
    if (x==x2) {y=y+1}
    if (x==0) {y=y+1}
    setPos(x,y)
    cmpt=cmpt+1
  }
  ecritMessage(cmpt)
}

def arrive ():Boolean ={
  val x=getX()
  val y=getY()
  val x2=getMondeLargeur()
  val y2=getMondeHauteur()
  return ((x==x2-1)&&(y==y2-1))
}