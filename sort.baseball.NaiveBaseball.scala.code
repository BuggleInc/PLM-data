override def run() {
  var n = getNombreBases()
  var trou = getTrouBase()
  var candidat = trou
  while (!estTrie()) {
    trou = getTrouBase()
    if (trou >= n-1) {
      candidat = 0
    } else {
      candidat += 1
    }
    if (((getCouleurJoueur(candidat,0))-candidat)*((getCouleurJoueur(candidat,0))-candidat) > ((getCouleurJoueur(candidat,0))-candidat)*((getCouleurJoueur(candidat,0))-candidat)) {
      deplace(candidat,0)
    } else {
      deplace(candidat,1)
    }
  }
}