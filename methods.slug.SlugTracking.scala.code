override def run() {
  
def isFacingTrail():Boolean = {
  	if (isFacingWall()) {
      return false
    }
    forward()
  	if (getGroundColor()==Color.green) {
      return true
    } else {
      return false
    }
  	backward()
}
while (!isOverBaggle()) {
	while (isFacingTrail()!=true) {
  	left()
  }
  forward()
}
}