}

def isFacingTrail():Boolean = {
		if (estFaceMur()) {
			return false;
		}
		avance();
		if (getCouleurSol()==Color.orange) {
			recule();
			return true;
		}
		else if (getCouleurSol()==Color.pink) {
			recule();
			return true;
		}
		else {
			recule();
			return false
		}
		
}



