$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class shareDigit extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( shareDigit((Integer)t.getParameter(0), (Integer)t.getParameter(1)) );  }
	boolean shareDigit(int a, int b) {
		return (a/10 == b/10 || a/10 == b%10 || a%10 == b/10 || a%10 == b%10);
	}
 }