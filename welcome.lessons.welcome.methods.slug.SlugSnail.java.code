 public void hunt(Color c) {
 	while(!isOverBaggle()){
 		
 		if(isFacingTrail(c)){
 			brushDown();
 			forward();
 			brushUp();
 		}else{
 			left();
 		}
 	}

 	pickUpBaggle();
     
 }

boolean isFacingTrail(Color c) {
boolean pinkCell = false;

	if(isFacingWall()){
		return false;
	}

	forward();
	pinkCell = getGroundColor().equals(c); 
	backward();
	
	return pinkCell;
}//end isFacingTrail()

