def solve() {
val lengthstack=getTaillePile()

while(estSelectionne()==false){
	//faire (chercher la position de la crepe la plus grande non triée,retourne (de là où elle est )
	//retourne (de la position où elle devrait être)) jusqu'à ce que estselectionne renvoie vrai
var	j=1	
var s=0
	for(rang<-1 to lengthstack-j){
		if(crepelapluslarge<getRayonCrepe(rang))
		s+=1
		crepelapluslarge=rang
	}
	retourne(rang) 
	retourne(lengthstack+1-s)
}
}
