def selectionSort() {
  var len:Int = getNombreValeurs()
  var mini:Int = 0
  var ind:Int = 0
  
  for (i <- 0 to (len - 2)) {
    mini = getValeur(i)
    for (j <- i to (len - 1)) {
      if (getValeur(j) < mini) {
        mini = getValeur(j)
        ind = j
      }
    }
    if (mini != getValeur(i)) {
      echange(i,ind)
    }
  }
}