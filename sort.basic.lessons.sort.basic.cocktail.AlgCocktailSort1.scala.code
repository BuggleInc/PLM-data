def cocktailSort() {
	var compteur:Int=1
	var etape:Int=0
	while (compteur!=0) {
		compteur=0
		for (x<-0 to getNombreValeurs()-2-etape) {
			if (plusPetit(x,x+1)) {}
			else {
				echange(x,x+1)
				compteur=compteur+1
			}
		}
		for (x<- getNombreValeurs()-2-etape to 0) {
			if (plusPetit(x+1,x)) {}
			else {
				echange(x+1,x)
			}
		}	
		etape=etape+1
	}
}
