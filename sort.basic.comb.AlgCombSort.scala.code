def combSort() {
  var ecart=0.0
  var i=0
  var k=0
  var c=0
  var c2=0
  ecart=getNombreValeurs()
  
do{
  c2=0
  if (ecart>1){
    ecart = ecart/1.3
  }
  while(i + ecart< getNombreValeurs){
    c=c+1
    if(plusPetit(k+1,k)){
      echange(k,k+1)
    }else{
      c2=c2+1
    }
    i=i+1
  }
}while(ecart>1 || c!=c2)
}