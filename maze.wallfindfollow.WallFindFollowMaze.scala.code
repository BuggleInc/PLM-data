override def run() {
  gauche()
  while(!estFaceMur())
  	avance()
  while(!estSurBiscuit())
  	keepHandOnSideWall()
  prendBiscuit()
}

def keepHandOnSideWall()
{
  while(estFaceMur())
  	droite()
  avance()
  gauche()
  
  if(!estFaceMur())
  {
    avance()
    gauche()
    if(!estFaceMur())
    	avance()
    else
    	droite()
  }
  else
  	droite()
}