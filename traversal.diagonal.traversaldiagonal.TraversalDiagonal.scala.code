var diag = 0;
override def run() {
  var nbCase = 0
  var x = 0
  var y = 0
  while (!positionFinale()) {
    ecritMessage(nbCase)
    diag += 1
    if (diag <= 6) {
      for (compteur <- 1 to diag + 1) {
        setPos(compteur, (diag + 1) - compteur)
      }
    }
  }
}

def positionFinale():Boolean = {
  return(getX() == 6 && getY() == 6)
}
