override def run() {
  
  for(i<-0 to getMondeLargeur()-1)
  {
    for(j<- 0 to i)
    {
      setPos(j,Math.abs(j-i))
      ecritMessage(i*(i+1)/2+j)
    }
  }
  
  for(i<-getMondeLargeur()-1 to 1 by -1)
  {
    for(j<- 0 to i)
    {
      setPos(6-Math.abs(j-i),6-j)
      ecritMessage(27+(i*(i+1)/2)+j)
    }
  }
  
  
  
}