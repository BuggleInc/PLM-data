override def run() {
  
  // part du principe qu'on a un mur ? gauche
  def keepHandOnSideWall() {
    droite()
    if (!estFaceMur()) {avance()} // pas mur ? droite? go
    else {gauche() ; if (!estFaceMur()) {avance()} // sinon pas mur en face ? go
        						else {gauche() ; if (!estFaceMur()) {avance()}
                          						// sinon pas mur ? gauche ? go
           													  else {droite() ; avance()} // sinon va en arri?re
   }}}
  
  //---
  
def murAGauche():Boolean =
  {gauche(); if (!estFaceMur()) {droite(); return true}
  					 else {droite(); return false}}
  //---  
  
  while (!estSurBiscuit) {
    while (!estFaceMur() && !estSurBiscuit) 
    			{avance()}
  	while (estFaceMur() && getDirection()!=Direction.NORTH && !estSurBiscuit)
    			{keepHandOnSideWall()}
  }
  prendBiscuit()
}