def cocktailSort() {
  var test=false
  while (!test){
    test = true
    for (i <- 1 to getNombreValeurs()-2){
      if(plusPetit(i+1,i)){
        test = false
        echange(i+1,i)
      }
    }
    var i=getNombreValeurs()
    while(i!=2){
      i-=1
      if (plusPetit(i-1,i)){
        test = false
        echange(i-1,i)
    }
    }
  }
}