def cigarParty(cigars:Int, isWeekend:Boolean):Boolean = {
  if (isWeekend==false){
    cigars match{
      case cigars if cigars < 40 => return false
      case cigars if cigars < 61 => return true
      case _ => return false      
    }
  } else {
    cigars match{
      case cigars if cigars < 40 => return false
      case _ => return true
    }}}