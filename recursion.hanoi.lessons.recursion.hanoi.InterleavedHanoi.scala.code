def interleavedHanoi(height:Int, src1:Int, src2:Int, other:Int, dst:Int) {
  if (height == 1){
    move(src1,dst)
    move(src2,dst)
  }
  else{
    interleavedHanoi(height-1,src1,src2,dst,other)
    interleavedHanoi(height-1,src1,src2,dst,other)
    move(src1,dst)
    move(src2,dst)
    interleavedHanoi(height-1,other,src1,src2,dst)
    interleavedHanoi(height-1,other,src1,src2,dst)
  }
  }
