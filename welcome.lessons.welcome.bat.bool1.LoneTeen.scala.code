def loneTeen(a:Int, b:Int):Boolean = {
  def ado(a:Int):Boolean={
    return (a>=13 && a<=19)
  }
  return ((ado(a) || ado(b))&& ado(a)!=ado(b))
  
  
  
  
  
def hasTeen(a:Int, b:Int, c:Int): Boolean = {
  def int1319(a:Int):Boolean={
    if (a>=13 && a<=19){
      return true
    }else{
      return false
    }
  }
  if (int1319(a) || int1319(b) || int1319(c)){
    return true
  }else{
