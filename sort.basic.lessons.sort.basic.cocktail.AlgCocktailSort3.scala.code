def cocktailSort3() {
	var swapC=false
	var swapD=false
	var debut=0
	var fin=getNombreValeurs()-2
	do{
		swapC=false
		swapD=false
		for(i<-debut to fin){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swapC=true
			}
		}
		if(!swapC) return
		fin-=1
		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				swapD=true
			}
		}
		if(swapC && !swapD) return
		debut+=1
	}while(swapC || swapD)
}
