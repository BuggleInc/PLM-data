def deplaceRocher() {
  prendBiscuit()
  
  retourne()
  
  while (!estSurBiscuit()) {
    avance()
  }
  
  recule()
  
  poseBiscuit()
  
  retourne()
}

def run() {
  retourne()
  
  while(!estFaceMur()) {
    avance()
    
    if (estSurBiscuit())
    	deplaceRocher()
  }
  
  droite(); avance(); gauche(); avance()
}

run()
