fini=estSurMessage()
while fini==True:
    if litMessage()=="R":
        droite()
        avance()
    elif litMessage()=="L":
        gauche()
        avance()
    elif litMessage()=="A":
        avance()
    elif litMessage()=="B":
        avance(2)
    elif litMessage()=="Z":
        recule()
    elif litMessage()=="X":
       recule(3)
    elif litMessage()=="I":
       droite(2)
       avance()
    else:
        fini==False
       
