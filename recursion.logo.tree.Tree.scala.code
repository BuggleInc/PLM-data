def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps==0) {
    gauche(angle)
    avance(length)
    recule(length)
    droite(2*angle)
    avance(length)
    recule(length)
  }
  else {
    current(steps)
    avance(length)
    droite(angle)
    tree(steps-1, length*shrink, angle, shrink)
    //gauche(angle*2)
    //tree(steps-1, length*shrink, angle, shrink)
  }
  
}