def cocktailSort() {
  var c:Int=0
  while( c==0){
    c=1
  for (i <-0 to getNombreValeurs()-2 ){
    if(plusPetit(i+1,i)){
      echange(i,i+1)
      c=0
    }
  }
  for (i <-0 to getNombreValeurs()-2 ){
    if(plusPetit(getNombreValeurs()-2 -i,getNombreValeurs()-1 -i)){
      echange(getNombreValeurs()-2+i+1,getNombreValeurs()-2+i)
      c=0
}
       }
       }
}