def shellSort()  {
  var ecart:Int= getValueCount()/2
  while(ecart>0){
    insertionSort();
    ecart=ecart/2;
  }

def insertionSort(){
  for(i<-0 to getValueCount()-1){
    var x:Int=getValue(i);
    var j:Int=i-ecart;
    while(j>=0 && getValue(j)>x){
      copy(j,j+ecart);
      j=j-ecart;
    }
    setValue(j+ecart,x)
  }
}
}