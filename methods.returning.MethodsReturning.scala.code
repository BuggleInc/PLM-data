def haveBaggle():Boolean = {
  
  var vuBagg:Boolean = false
  
  while (!vuBagg) {
  
  	for (i <- 1 to 6) {
    
    	avance();
    
    	if (estSurBiscuit()) {
      
      	vuBaggle = true
      
    	}
    
 	 }
  
  	for (i <- 1 to 6) {
    
    	recule();
    
  	}
  
  	droite();
  	avance();
  	gauche();
    
  }
  
  return vuBagg
  
}