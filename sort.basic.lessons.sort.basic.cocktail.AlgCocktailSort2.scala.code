def cocktailSort2() {

  var debut=0;  var fin=getNombreValeurs()-2
  var k=getNombreValeurs()-2
  var a=true

  do {
    a=false
    for (i <- debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        fin=fin-1
        a=true
      }
    }
     for (i <- fin to debut){
      if (plusPetit(k+1,k)){
        echange(k,k+1)
        debut=debut+1
        a=true
      }
      k=k-1

    }
  }
  while (a)
}