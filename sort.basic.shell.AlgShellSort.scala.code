def shellSort() {
  val lgr=getNombreValeurs()
  var dernierecase:Boolean=true
  var i:Int=0
  var v:Int=0
  var j:Int=0
  var ecart:Int = (lgr.toDouble/2).toInt
 	while (ecart>0){
  for (i <- 1 to lgr-1){
    v = getValeur(i) 
    j = 1
    while (i-j*ecart>=0 && getValeur(i-j*ecart)>v){
      copie(i-j*ecart,i-(j-1)*ecart)
      j+=1
      }
    setValeur(i-(j-1)*ecart, v)
  }
    ecart = (ecart.toDouble/2).toInt
}
}