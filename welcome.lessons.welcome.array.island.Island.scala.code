def maxValue(nums:Array[Int]): Int = {
	var k:Int = nums(0)
	for (i<-1 to nums.length-1) {
		if (nums(i)>k) {k=nums(i)}
		}
		return k

def occurrences(nums:Array[Int],lookingFor:Int):Int = {
	var m: Int = 0
	var sh : Int =0
	while (m<nums.length) {
		if (lookingFor == nums(m)) {sh=sh +1}
		m=m+1 
	}
	return sh

