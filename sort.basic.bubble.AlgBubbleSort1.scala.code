def bubbleSort()  {
  var B : Boolean = false
  var n = getNombreValeurs()
  var rg = 0
	for (i <- 0 to n-2 ){
      rg=i
    	if (rg>=0)
    		B= true
      while (B) {
        if (plusPetit(rg+1,rg)) {
      		echange(rg,rg+1)
          if (rg>0)
          	rg-=1
        }
        else
          B=false
        
   
      }
   }
}