while estSurMessage():
    if litMessage()="A":
        avance(1)
    elif litMessage()="R":
        droite()
        avance(1)
    elif litMessage()="L":
        gauche()
        avance(1)
    elif litMessage()="I":
        gauche(2)
        avance(1)
    elif litMessage()="B":
        avance(2)
    elif litMessage()="C":
        avance(3)
    elif litMessage()="Z":
        recule(1)
    elif litMessage()="Y":
        recule(2)
    elif litMessage()="X":
        recule(3)
