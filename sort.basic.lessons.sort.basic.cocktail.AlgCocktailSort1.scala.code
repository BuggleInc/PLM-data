def cocktailSort() {
  var echanges = true
  for(i<- 0 to getNombreValeurs()-2) {
    if(!plusPetit(i,i+1)) {
      echange(i,i+1)
      echanges = false
    }
  }
  return
  for(i<- getNombreValeurs()-2 to 0 by -1) {
    if(!plusPetit(i,i+1)) {
      echange(i,i+1)
      echanges = false
    }
  }
  return
}