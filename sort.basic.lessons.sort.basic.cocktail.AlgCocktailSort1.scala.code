def cocktailSort() {
  
  var n = getValueCount();
	var swapped = false
  
  do
  {
    swapped = false    
    for( i <- 0 to n-2)
    {
      if (!isSmaller(i,i+1))
      {
        swap(i,i+1);
        swapped = true;        
      }
    }
      
    for( i <- n-3 to 0 by -1)
    {
      if (!isSmaller(i,i+1))
      {
        swap(i,i+1);
        swapped = true;        
      }
    }
  }while(!swapped)

}