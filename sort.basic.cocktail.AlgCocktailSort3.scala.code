def cocktailSort3() {
  var beg = 0
  var end = getValueCount() - 2
  var swaped = false
  
  do {
    swaped = false
    
    for(i <- beg to end) {
      if(isSmaller(i + 1, i)) {
        swap(i, i + 1)
        
        swaped = true
      }
    }
    
    end -= 1
    
    for(i <- end to beg by -1) {
      if(isSmaller(i + 1, i)) {
        swap(i, i + 1)
        
        swaped = true
      }
    }
    
    beg += 1
    
  } while(swaped)
}