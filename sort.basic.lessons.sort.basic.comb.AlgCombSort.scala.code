def combSort() {

var ecart = getNombreValeurs
var x = true 
var i = 0
do {
	x = false
	if ( ecart > 1 ) {
		ecart = (ecart.toDouble / 1.3).toInt
	}
	i=0
	while (i + ecart < getNombreValeurs()) {
		if (plusPetit(i+1,i)) {
				echange(i,i+1)
				x = true
				}
		i=i+1		
	}
} 
while (ecart > 1 || x == true )

}
