override def run() {
  while(porteBiscuit()==false) {
      if (estFaceMur()==false&&getDirection()==Direction.NORTH) {
      	avance()
      }
      else {
        keepHandOnSideWall()
    }
    setDirection(Direction.NORTH)

    if (estSurBiscuit()==true) {
      prendBiscuit()
    }  
  }
}  

def keepHandOnSideWall() {
  droite()
  if (estFaceMur()==true) {
    gauche()
    if (estFaceMur()==false) {
    avance()
    }
    else {
      gauche()
    }
  }
  else {
    avance()
  }  
}