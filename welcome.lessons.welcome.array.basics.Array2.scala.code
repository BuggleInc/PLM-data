override def run() {

var T = new Array[Color](getMondeHauteur())
for (i <- 1 to T.length-1){
   T(i)=getCouleurSol();
   avance();
}
T(T.length-1)=getCouleurSol()
for (i <- 1 to getMondeHauteur-1){recule()}
for (i <- 1 to getMondeLargeur-1){
  gauche();
  avance();
  droite();
  var j =0
  do {avance();
     setCouleurBrosse(T((j+i%getMondeHauteur));
                        baisseBrosse();
                        leveBrosse()
     j=j+1} 
  while (estFaceMur==false);
  do {recule()} while(!estDosMur())
  setCouleurBrosse(T(j+i));
  baisseBrosse();
  leveBrosse();
  for (k <- 1 to i){avance()};
  for (l <- 1 to i){recule()};
}
}