def cocktailSort3() {
	var debut:Int =0
	var fin:Int = getNombreValeurs()-2
	var isechange = false
	do {
		for (i<- debut to fin) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i)
				isechange = true
			}
		}
		fin -= 1
		if (!isechange) {
			for (i<-fin to debut by -1) {
				if (plusPetit(i+1,i)) {
					echange(i,i+1)
					isechange = true
				}
			}
		 	debut += 1
		}
		isechange = false
	} while(isechange)
}
