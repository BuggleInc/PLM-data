override def run() {
  
  def avanceVersNord (etat : Int) {
    
    setDirection(Direction.NORTH);
    while (estFaceMur != true) {
      avance();
    }
    etat = 1;
    return etat;
  }
  
  def suitAGauche (etat : Int) {
    
    gauche();
    while (estFaceMur() == true ) {
      droite();
    }
    avance();
    etat = 0;
    return etat;
    
  }
  
  def keepHandOnSideWall()  {
    
    var etat = 0;
    
    etat match {
      
      case 0 => avanceVersNord(etat);
      
      case 1 => suitAGauche(etat);
      
      case_ => println("Ce cas ne devrait pas arriver. Corrigez le probl√®me SVP. ");
         
    }
    
  }
    
  
  while (estSurBiscuit == false) {
    keepHandOnSideWall();
  }
  
	prendBiscuit();

  
}