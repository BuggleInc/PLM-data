override def run() {
  var T=remplir 
  gauche()
  avance()
  droite()
  for (i<-0 to getWorldHeight-1){
    setCouleurBrosse(T((i+1)% T.length))
    baisseBrosse
    leveBrosse
    avance()
  }
}

def remplir():Array[Color]={
  var X:Int=getWorldHeight()
  var T = new Array[Color](X)
  T(0) = getCouleurSol()
  for (i<-1 to X-1){
    avance()
    T(i)=getCouleurSol()
  }
  for (i<-0 to X-2){
    recule
  }
  return T
}