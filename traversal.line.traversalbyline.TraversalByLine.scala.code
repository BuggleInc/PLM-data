override def run() {
  var n = 0
  val x = getWorldWidth()
  val y = getWorldHeight()
  while(! isInPosition(x,y)) {
    writeMessage(n)
    n = n+1
    if (x-1 != getX()) {
      setPos(getX() + 1, getY())
    }
    else {
      setPos(0, getY() +1)
    }
  }
}


def isInPosition(x: Int, y: Int):Boolean = {
  if ( (x -1 == getX()) && (y -1 == getY())) {
    writeMessage(x * y -1)
    return true
  }
  else {return false}
}