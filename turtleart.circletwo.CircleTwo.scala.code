override def run()
{  
  cercleTroue(2);
  cercleTroue(1);
}

def cercleTroue(cote: Int)
{
  val angle = cote.toDouble/2;
  
  for (i <- 1 to 174)
  {
    avance(cote);
    droite(angle);
	}
  
  leveCrayon();
  for (i <- 1 to 7)
  {
    avance(cote);
    droite(angle);
	}
  baisseCrayon();
  
  for (i <- 1 to 179)
  {
    avance(cote);
    droite(angle);
	}
}