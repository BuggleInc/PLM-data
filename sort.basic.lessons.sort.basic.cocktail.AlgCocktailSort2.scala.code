def cocktailSort2() {
  var i : Int = 0
  var a : Int = getNombreValeurs()
  var j : Int = 0
  var trie : Boolean = false
  while (trie==false){
    trie = true
    for (i<- 0+i to a-2-i){
      if (plusPetit(i+1,i)== true) {
        echange(i+1,i)
        trie=false
      }
    }
    for (i<- a-2-i to 0+i by -1){
      if(plusPetit(i+1,i)==true){
        echange(i+1,i)
        trie=false
      }
    }
  }
}