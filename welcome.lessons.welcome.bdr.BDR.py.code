while True:
    x = readMessage()
    if x == "Z":
        backward()
    if x == "L":
        setBrushColor(Color.blue)
        brushDown()
    if x == "A":
        forward()
    if x == "B":
        forward(2)
    if x == "X":
        backward(3)
    if x == "R":
        setBrushColor(Color.blue)
        brushDown()
    if x == "Z":
        setBrushColor(Color.blue)
        brushDown()
    if x == "I":
        setBrushColor(Color.blue)
        brushDown()        

