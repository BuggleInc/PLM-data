def haveBaggle():Boolean = {
	var vuBiscuit=false;
	var n=0;
	while (vuBiscuit==false) {
		if (estSurBiscuit()) {
			vuBiscuit=true;
			return vuBiscuit;
		}
		avance();
		n=n+1;
			if (estSurBiscuit()) {
			vuBiscuit=true;
			for (f <- 1 to n) {
				recule();
			}
		}
		else if (n==6) {
			for (f <- 1 to 6) {
				recule();
			}
			n=0;
			droite();
			avance();
			gauche();
		}
	}
	n=0;
	return vuBiscuit;
}
