def keepHandOnSideWall() {
  var efm:Boolean = false
  gauche()
  if (estFaceMur()) {
    efm = true
  } else {
    avance()
  }
  droite()
  if (estFaceMur()) {
    if (efm) {
      droite()
    } else {
      gauche()
      avance()
    }
  }
  if (!estFaceMur()) {
    avance()
  }
  
}

override def run {
  while (!estSurBiscuit()) {
    keepHandOnSideWall()
  }
  prendBiscuit()
}