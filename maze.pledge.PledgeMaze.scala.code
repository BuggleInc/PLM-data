def hand(tour:Int)={
  if (!estFaceMur()) {avance();tour -=1}  
  else {
    droite()   
    if (estFaceMur()) {droite();tour+=1;if (estFaceMur()) {droite()}}
    avance()
  }
}




override def run() {
