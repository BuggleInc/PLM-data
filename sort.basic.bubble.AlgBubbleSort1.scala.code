def bubbleSort()  {
  var n = getNombreValeurs()-1
  for(i <- 1 to n)
  {
    if(plusPetit(i,i-1))
    	remonte(i)
  }
}
def plusPetitSafe(j:Int,i:Int):Boolean=
{
  if(i>=0&&j>=0)
  	return plusPetit(j,i)
  else
  	return false
}
def remonte(pos:Int)
{
  var i = pos-1
  var j = pos
  while(plusPetitSafe(j,i)&&i>0)
  {
    echange(j,i)
    j = i
    i = i-1
  }
}