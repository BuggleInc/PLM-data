override def run() {
  var dir:Direction = Direction.NORTH
  
  while(!testGauche())
  	gauche()
  
  while(!estSurBiscuit())
  {
    if(murNord() && dir!=Direction.SOUTH)
    	keepHandOnSideWall()
    else
    	goNorth()
  }
  prendBiscuit()
}

def keepHandOnSideWall() {
  var gauch = testGauche()
  if(gauch && !estFaceMur)
  	{
      avance()
    }
  else if(estFaceMur())
  {
    droite()
  }
  else if(!gauch)
  {
    gauche()
    avance()
  }
  
}

def testGauche():Boolean = {
  var test = false
  gauche()
  test = estFaceMur()
  droite()
  return test
}

def goNorth() {
  while(getDirection()!=Direction.NORTH)
  {
    droite()
  }
  while(!estFaceMur())
  {
    avance()
  }
}

def murNord():Boolean = {
  var res = false
  var save:Direction = getDirection()
  while(getDirection()!=Direction.NORTH)
  {
    droite()
  }
  res = estFaceMur()
  while(getDirection()!=save)
  {
    droite()
  }
  return res
}