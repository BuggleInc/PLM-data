override def run() {
  while (!estSurBiscuit())
  {
    if (!estFaceMur())
    	avance()
    else
    {
      droite()
    	keepHandOnSideWall()
    }
  }
  prendBiscuit()
}

def keepHandOnSideWall()
{
  if (estFaceMur()) //si il y a un mur à gauche ira en face ou à droite
  {
    droite()
    if (estFaceMur())
    	droite()
    else
    	avance()
  }
  else  //si pas de mur à gauche va à gauche
  	avance()
}