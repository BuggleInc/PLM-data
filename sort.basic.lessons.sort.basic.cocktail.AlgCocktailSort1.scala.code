def cocktailSort() {
  var fin=false;
  while(fin==false){
    fin=true
    for(i<- 0 to getNombreValeurs()-2){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        fin=false
      }
    }
    for(i <- getNombreValeurs()-2 to 1 by -1){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
      }
    }
  }
}
