override def run() {
var c:Int=0
ecritMessage(c)
	do {
		for (x<-0 to  getMondeHauteur()-1)
		{
			for (y<-0 to getMondeHauteur()-1)
			{
				ecritMessage(c)
				setPos(x,y)
				c=c+1
			}
			setPos(x,6)
		}
		setPos(6,0)
	}
	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-1
		)
	
}
