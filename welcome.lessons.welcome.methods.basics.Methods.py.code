def goAndGet():
 """Go, retrieves the baggle in front of the buggle, and brings it back.

  Does not check for walls, so be careful to not call it when walls are present."""
stepper = 0
while isOverBaggle() != True:
    stepper = stepper + 1
    forward()
if (isOverBaggle()):
    pickupBaggle()
for stepper in range(stepper):
    backward()
dropBaggle()
stepper = 0

for i in range(7):
  goAndGet()
  right()
  forward()
  left()
