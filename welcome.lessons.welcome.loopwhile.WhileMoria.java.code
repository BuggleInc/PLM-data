public void run() {

	boolean mur = false;
	while (!mur) {
		retourne();
		while (!estSurBiscuit() && !estFaceMur())
			avance();

		if (estFaceMur() && !estSurBiscuit()) {	
			mur = true;
			droite();
			avance();
			gauche();
			avance();
		} else {
			prendBiscuit();
			retourne();
			
			while (!estSurBiscuit()) {
				avance();
			}
			recule();
			poseBiscuit();
			recule();
		}
	}

}
