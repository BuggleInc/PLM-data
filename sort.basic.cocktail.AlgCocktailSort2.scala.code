def cocktailSort2() {
  var e = true
  var f = getNombreValeurs-2
  var d = 0
  while (e)
      {e=false
      for (i<- d to f)
          {if (plusPetit(i+1,i))
              {e=true ;echange(i, i+1) ;};
          }
       f-=1
       for (i<- f to d by -1 )
          {if (plusPetit(i+1,i))
              {e=true ;echange(i, i+1) ;}}
       d+=1
      }
  
       ;}
