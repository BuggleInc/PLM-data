override def run() {def makehalfpattern(c:Color, d:Color) {setBrushColor(c); left(); forward(); brushDown(); brushUp(); right(); forward(); right(); forward(); brushDown(); brushUp(); setBrushColor(d); left(); forward(); brushDown(); brushUp(); forward(); left(); forward(); brushDown(); brushUp(); forward()}
def makepattern() {makehalfpattern(Color.RED, Color.BLUE); forward(); left(); makehalfpattern(Color.GREEN, Color.YELLOW); forward(); left()} 
for (stepper1<-1 to 2) {for (stepper2<-1 to 2) {makepattern()}; left(); for (stepper3<-1 to 4) {forward()}; right() }}