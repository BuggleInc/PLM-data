var biscuit = 0
do { for (i<-0 to 4) {avance();
                      if (estSurBiscuit()) {biscuit = biscuit +1}};
                     biscuit match{
                       case i if i<3 => droite(); biscuit=0
                       case _ => gauche(); biscuit=0
                     }}
while (!sortieTrouvee)