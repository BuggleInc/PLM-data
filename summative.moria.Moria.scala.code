    }
  }
right();forward();left();forward();


def finished(): Boolean = {
  var width:Int = getWorldWidth;
  var x:Int = getX;
  if (isOverBaggle()) {
    	return false
    }
  else if (x==width-3 && !isCarryingBaggle){
    return true
  }
  return false
}