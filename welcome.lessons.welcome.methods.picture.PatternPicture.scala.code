override def run() {
	faireMotif()
	avance(4)
}

def faireMotif() {
	var j = Color.YELLOW
 	var r = Color.RED
 	var b = Color.BLUE
 	var v = Color.GREEN
	
	faireC(r);
	faireC(b);
	faireC(v);
	faireC(j);
}

def faireC(c:Color) {
 	avance();
 	setCouleurBrosse(c);
 	baisseBrosse();
 	leveBrosse();
 	gauche();
 	avance();
 	gauche();
 	avance();
 	baisseBrosse();
 	leveBrosse();
 	gauche();
 	avance();
 	gauche();
 	avance(3);
 	gauche()
}