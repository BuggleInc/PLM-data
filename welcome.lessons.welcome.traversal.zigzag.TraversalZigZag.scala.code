override def run() {
  var cpt=0
  var a=0
  var b=0
  
  while(cpt!=42) {
    for (cpt1<-1 to 3) {
      for (cpt2<- 1 to 6) { // premi?re ligne
        ecritMessage(cpt)
        a+=1
        setPos(a,b)
        cpt+=1
      }
      ecritMessage(cpt)
      b+=1
      setPos(a,b)
      cpt+=1
      for (cpt3<-1 to 6) { // deuxi?me ligne
        ecritMessage(cpt)
        a-=1
        setPos(a,b)
        cpt+=1
      }
      ecritMessage(cpt)
      cpt+=1
      b+=1
      setPos(a,b)
    }
  }
  for (cpt4<-1 to 6) {
    ecritMessage(cpt)
    a+=1
    setPos(a,b)
    cpt+=1
  }
  ecritMessage(48)
}