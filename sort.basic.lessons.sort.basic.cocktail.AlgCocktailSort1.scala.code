def cocktailSort() {
  var chgt1 = true
  var chgt2 = true
  do{
    chgt1 = true
    for (i <- 0 to getNombreValeurs()-2){
      if (!plusPetit(i,i+1)){
        echange(i,i+1)
        chgt1 = false
      }
    chgt2 = true
    for (i <- getNombreValeurs()-2 to 0 by -1){
      if (!plusPetit(i+1,i)){
        echange(i+1,i)
        chgt2 = false
      }
    }
    }
  }
  while (!chgt1 || !chgt2)
}
  