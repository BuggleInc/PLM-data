def gnomeSort() {
  var gnomePos = 0
  var longueur = getValueCount()
  
  while(gnomePos < longueur-2)
  {
    if((isSmaller(gnomePos+2,gnomePos+1))&&(gnomePos>0))
    {
      swap(gnomePos+2,gnomePos+1)
    	gnomePos-=1
    }
    else if(isSmaller(gnomePos+2,gnomePos+1))
    {
      swap(gnomePos+2,gnomePos+1)
      gnomePos+=1
    }
    else
    {
      gnomePos+=1
    }
  }
}