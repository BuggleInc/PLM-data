override def run() {
  for (i <- 1 to 4) {
    faireMaison();leveCrayon()
    droite(180);avance(50)
    baisseCrayon();gauche(90)
  }
}


def faireMaison() {
  avance(30);droite(30)
  for (i <- 1 to 3) {
    avance(30);droite(120)
  }
  droite(60);avance(30)
  for (j <- 1 to 2) {
    droite(90);avance(30)
  }
}