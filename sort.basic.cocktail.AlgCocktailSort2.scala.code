def cocktailSort2() {
	
  var swap:Boolean = true
  var debut = 0
  var fin = getNombreValeurs() - 2
  
  do {
    
   swap = false
   
   for (i <- debut to fin) {
    
    if (!plusPetit(i,i+1)) {
      
      echange(i,i+1)
      swap = true
      fin = fin - 1
    }
    
  }
  
  for (i <- debut to fin by -1) {
    
    if (plusPetit(i,i+1)) {
      
      echange(i,i+1)
      swap = true
      debut = debut + 1
      
    }
    
  }

   
 } while (swap)
  
}