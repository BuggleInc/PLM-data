public void run() {
	colByCol();
}

void colByCol(){
int x = getWorldWidth();
int y = getWorldHeight();


	for(int i = 0; i < x; i++){
		for(int j = 0; j < y; j++){
			if(!finalPos()){
				setPos(i, j);
			}			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth()){
		return true;
	}

	return false;
}//end finalPos
