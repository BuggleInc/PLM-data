override def run() {
def peindre(){
  baisseBrosse();
  leveBrosse();
}

def motif1(c:Color){
  setCouleurBrosse(c);
  avance();
  gauche();
  peindre();
  avance();
  gauche();
  avance();
  peindre();
  gauche();
  avance();
  gauche();
}
def motif2(c:Color){
  setCouleurBrosse(c);
  peindre();
  avance();
  gauche();
  avance();
  gauche();
  peindre();
  avance();
  gauche();
