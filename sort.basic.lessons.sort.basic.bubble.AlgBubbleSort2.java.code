public void bubbleSort2() {
	boolean swapped = false;
	int passes = 1;
	System.out.println("Number of elements: " + getValueCount());
	do {
		swapped = false;
		System.out.println("Iterations next round: " + (getValueCount() - passes));
		for (int i = 0; i < getValueCount() - passes; i++) {
			if (!isSmaller(i, i+1)) {
				swap(i, i+1);
				swapped = true;
			}
		}
		passes++;
	} while(swapped);
}

