public void cocktailSort() {
	boolean swapped;
	do {
		swapped = false;
		int i = 0;
		while(i < getValueCount() - 2) {
			if (!isSmaller(i, i+1)) {
				swap(i, i+1);
				swapped = true;
			}
			i++;
		}
		while(i > 0) {
			if (!isSmaller(i, i + 1)) {
				swap(i, i + 1);
				swapped = true;
			}
			i--;
		}
	} while(swapped);
}

