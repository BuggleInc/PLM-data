def rechercheMin(N: Int): Int = {
  
  var indMin = N
  
  for (i <- N to getNombreValeurs()-2) {
    
		if (getValeur(i) <= getValeur(i+1)) {
      
			indMin = i
      
    }
    
  }
  
  return indMin
  
}


def selectionSort() {
  
  for (i <- 1 to getNombreValeurs()-1) {
    
    echange(i,rechercheMin(i))
    
  }
  
}