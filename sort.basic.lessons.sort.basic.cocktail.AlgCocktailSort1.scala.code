def cocktailSort() {

  var incr:Int=0;
  do{
    incr=0;
  	for(i<- 0 to getNombreValeurs()-2){
  	  if(plusPetit(i+1,i)){
        echange(i,i+1);
        incr=incr+1;
      }
  	}
  	for(i<- getNombreValeurs()-2 to 0 by -1){
      if(plusPetit(i+1,i)){
        echange(i,i+1);
        incr=incr+1;
      }
    }
  }while(incr!=0);
  
  
  
}