def combSort() {
  var ecart = getNombreValeurs()
  var echanges = true
  while (ecart >= 1 || echanges){
    if (ecart > 1){
      ecart = (ecart.toDouble / 1.3).toInt;
    }
    var i = 0;
    while (i + ecart < getNombreValeurs()){
      if (getValeur(i) > getValeur(i + ecart)){
        echange(i, i + ecart);
        i += 1;
      }
    }
  }
}