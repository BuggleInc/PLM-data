n=getNombreValeurs()-1

debut=0
fin=n
trie=False

while trie==False:
  trie=True
  for i in range(fin):
    if plusPetit(i,i+1)==False:
      echange(i,i+1)
      trie=False
    fin=fin-1
  for j in range(fin,debut,-1):
    if plusPetit(j-1,i)==False:
      echange(i-1,j)
      trie=False
    debut=debut+1
  if trie==True:
    break