var beg : Int = 0
var isOver : Boolean = false
var end : Int = getValueCount()-2
def cocktailSort2() {
  
  while (isOver == false){
    isOver = true
    for (i <- beg to end){
      if (isSmaller(i,i+1)== false){
        swap(i,i+1)
        end = end - 1
        isOver = false
      }
    }
    for ( i <- end to beg){
      if (isSmaller(i,i+1)== false){
        swap(i,i+1)
        beg = beg +1
        isOver = false
    }
  }
}
}