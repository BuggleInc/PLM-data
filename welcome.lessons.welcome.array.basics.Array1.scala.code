override def run() {
  
  var tab=new Array[Color](getMondeHauteur());
  
  leveBrosse();
  
  for(i <- 0 to getMondeLargeur()-1)
  {
    for(j <- 0 to getMondeHauteur()-1)
    {
      if(i==0)
      {
        tab(j)=getCouleurSol();
      }else{
        setCouleurBrosse(tab(j));
      	baisseBrosse();
        leveBrosse();
      }
      
      avance();
    }
    if(i!= getMondeLargeur()-1)
    {
      gauche()
      avance()
      droite()
      avance() 
    }
  }
  
}
