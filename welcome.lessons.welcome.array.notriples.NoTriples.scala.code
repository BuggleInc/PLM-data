def noTriples(nums:Array[Int]): Boolean = {
	for (i<- 0 to nums.length -1){
		if (nums(i)==1){
			for (i<- 0 to nums.length -1){
				if (nums(i)==1){
					for (i<- 0 to nums.length -1){
						if (nums(i)==1){
							return true
						}
					}
				}
			}
		}
		if (nums(i)==2){
			for (i<- 0 to nums.length -1){
				if (nums(i)==2){
					for (i<- 0 to nums.length -1){
						if (nums(i)==2){
							return true
						}
					}
				}
			}
		}
	}
	return false
}