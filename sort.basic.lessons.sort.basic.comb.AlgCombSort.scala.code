def combSort() {
	var ecart = getNombreValeurs()
     var i = 0
     var échangé = true
	
	while ( (ecart > 1 ) || ( échangé == true  ) ) {
		if (ecart>1) {
			ecart = (ecart.toDouble / 1.3).toInt
		}
		échangé = false
		while ( i+ecart < getNombreValeurs() ) {
			if (plusPetit(i,i+ecart)==false) {
				echange(i,i+ecart)
				échangé = true
			}
			i=i+1
		}
	}
}
