override def run() {
  faireMotif(Color.RED)
  faireMotif(Color.BLUE)
  faireMotif(Color.GREEN)
  faireMotif(Color.YELLOW)
}

def faireMotif(c:Color){
  avance()
  setCouleurBrosse(c:Color)
  baisseBrosse()
  leveBrosse()
  for (l<-1 to 2){
    gauche()
  	avance()
  }
  baisseBrosse()
  leveBrosse()
  gauche()
  avance()
  gauche()
  for(k<-1 to 2){
    avance()
  }
}