override def run() {
  var color:Color = Color.BLUE
  for (i <- 0 to 1){
    for(j <- 0 to 1){
      setPos(i*4,j*4)
      for (k <- 1 to 4)
    		faireV(k)
    }
  }
  setPos(4,3)
  
  def faireV(k:Int){
    k match{
      case 1 => color = Color.YELLOW
      case 2 => color = Color.GREEN
    	case 3 => color = Color.BLUE
    	case 4 => color = Color.RED
    }
    setCouleurBrosse(color)
    avance()
    baisseBrosse()
    avance()
    leveBrosse()
    avance()
    droite()
  }
}