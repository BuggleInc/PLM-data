def goAndGet() {
  while (!estSurBiscuit()) {
    avance()
  }
  if (estSurBiscuit()) {
    prendBiscuit()
  }
  while (porteBiscuit() && !estSurBiscuit()) {
    recule()
  }
  if (estSurBiscuit() && porteBiscuit()) {
    avance() ; poseBiscuit()
  }
}

def sortir() {
  while (!estFaceMur()) {
    goAndGet()
  }
  if (estFaceMur()) {
    droite() ;avance() ; gauche() ; avance()
  }
}

sortir()