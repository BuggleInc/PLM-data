def array123(nums:Array[Int]): Boolean = {

var I:Boolean = false
var II:Boolean = false
var III:Boolean = false

	for (i <- 0 to nums.length - 1) {
		if (nums(i) == 1) {
			I = true
		}
		if (nums(i) == 2) {
			II = true
		}
		if (nums(i) == 3) {
			III = true
		}
	}
	if (I == true && II == true && III == true) {
		return true
	} else {
		return false
	}
}