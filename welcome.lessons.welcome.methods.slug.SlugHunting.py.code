def isFacingTrail():
    if estFaceMur() == True :
        return False
    forward()
    if getCouleurSol() == Color.green :
        backward()
        return True
    else :
        backward()
        return False

def hunt():    
    while estSurBiscuit() == False :
        leveBrosse()
        if isFacingTrail() == True :
            baisseBrosse()
            forward()
            leveBrosse()
        else :
            gauche()
    prendBiscuit()

hunt()
        
    
