def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = { 
  if (!isBirthday) {
  if (speed<60) {
    return 0
  }
  if (speed>60 & speed<81) {
    return 1
  }
  if (speed>81) {
    return 2
  }
}
if (isBirthday) {
  if (speed<65){
    return 0
  }
  if (speed>61 &speed<86){
    return 1
  }
  if (speed>86) {
    return 2
  }
}
}