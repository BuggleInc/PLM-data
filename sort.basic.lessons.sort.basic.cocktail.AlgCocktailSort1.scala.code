def cocktailSort() {
  var ech = true
  do{
    ech = false
    for (i<-0 to getNombreValeurs()-2){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        ech = true
      }
    }
    for (i<-getNombreValeurs()-2 to 0 by -1){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        ech = true
      }
    }
  }while(ech)
}
