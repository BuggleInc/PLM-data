def haveBaggle():Boolean = 
{
  var vuBiscuit = false
  var nbPas = 0
  while (!estSurBiscuit() && nbPas < 6)
  {
    if (estSurBiscuit())
    {
      vuBiscuit = true
      for (i <- 1 to nbPas)
      {
        recule()
      }
      return true
    }
    else
    {
      avance()
      nbPas = nbPas + 1
    }
  } 
  for (i<- 1 to nbPas)
  {
    recule()
  }
  return false
}