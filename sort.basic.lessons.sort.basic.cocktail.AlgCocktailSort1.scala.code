def cocktailSort() {
	var swapped = false
	do{
		var swapped = false
		for(i<-0 to getNombreValeurs()-2){
			if(plusPetit(i+1, i)){
				echange(i, i+1)
				swapped = true
			}
		}
		for(i<-getNombreValeurs()-2 to 0 by -1){
			if(plusPetit(i, i+1)){
				echange(i, i+1)
				swapped = true
			}
		}
	}while(swapped)
}
