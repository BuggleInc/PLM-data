def solve() {
  insertionSort()
}

def insertionSort() {
  var n:Int=getTaille()
  for ( k <- 1 to (n-1)){
    var stock:Int=getCouleur(k)
    var l:Int=k-1
    if (stock==BLEU){
      while(l>=0 && getCouleur(l)!=BLEU ){
        echange(l,l+1)
        l-=1
      }
    } else {
      if(stock==BLANC){
        while(l>=0 && getCouleur(l)==ROUGE ){
          echange(l,l+1)
          l-=1
        }
      }
    }
  }
}