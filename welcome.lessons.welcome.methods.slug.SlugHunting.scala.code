def hunt() {
    while (!estSurBiscuit()){
    		while (isFacingTrail()){
    			baisseBrosse()
    			avance()
    			leveBrosse()
    			if (!isFacingTrail()){
    				recule()
    				gauche()
    			}
    		}
    }
}
def isFacingTrail():Boolean = {	
 	if (!estFaceMur()){
    		avance()
    		if (getCouleurSol() == Color.green){
    			
    			return true
    		}else{
    			recule()
    			return false
    		}
    		return true
 	}else{
 		return false	
 	}
    	
}        
