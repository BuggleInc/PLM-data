def shellSort()  {

	var taille:Int = getNombreValeurs();
	var v:Int = 0;
	var j:Int = 1;
	var gap:Int = taille / 2;

	while (gap > 0)
	{
		for (i <- 1 to taille - 1)
		{
			v = getValeur(i);
			j = 1;
			while (i - j * gap >= 0 && getValeur(i - j * gap) > v)
			{
				setValeur(i - j * gap + gap, getValeur(i - j * gap));
				j += 1;
			}
	
			if (i - j * gap + gap >= 0) setValeur(i - j * gap + gap, v);
		}

		gap /= 2;
	}
}
