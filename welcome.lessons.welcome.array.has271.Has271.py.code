import math
def has271(nums):
    tab = nums
    for i in range(len(nums)-2):
        if 269<=tab[i]*100+tab[1+i]*10+tab[i+2]<=273:
            return True
        else:
            min = tab[i]*100+tab[i+1]*50+tab[i+2]-3
            max = tab[i]*100+tab[i+1]*50+tab[i+2]+1
            if min<=tab[i]*100+tab[i+1]*50+tab[i+2]+1<=max:
                return True
    return False