def greenTicket(a:Int, b:Int, c:Int):Int = {(a,b,c) match {
  																							case (_,a,a) => return 20
  																							case (a,_,a) => return 10
                                                case (a,a,_) => return 10
  																							case (_,b,b) => return 10
  (_,_,_) => 0}
}