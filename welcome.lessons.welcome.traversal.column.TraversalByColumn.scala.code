override def run() {
  def positionFinal() : Boolean = {
    if (getMondeLargeur()==getX() & getMondeHauteur()==getY()){
      return true
    }else return false
  }
  
  def positionSuivante() = {
    if (getY() != getMondeHauteur()){
     setPos(getX(),getY()+1)
   }
    else setPos(getX()+1,0)
  }
  
  for (i <-1 to 10){
    positionSuivante()
  }
  
}