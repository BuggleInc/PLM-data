override def run() {
  var hauteur = getMondeHauteur()
  var T=new Array[Color](hauteur)
  for ( i<=1 to hauteur ){
    T(i)=getCouleurSol()
    avance()
  }// code cr?er le tableau avec les couleurs
  for (i<=1 to hauteur){
    leveBrosse()
    gauche()
    avance()
    droite()
    for (i<-1 to T.lenght-){
      baisseBrosse()
      setCouleurBrosse(T.(i))
      avance()
    }
  }
}