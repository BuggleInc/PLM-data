def isFacingTrail():
	v=True
	if estFaceMur():
		v=False
	if v==True:
		avance()
		if getCouleurSol()!=Color.green:
			v=False
		recule()
		return(v)

def hunt():
	while estSurBisuit()==False:
		if isFacingTrail()==True:
			avance()
		else:
			gauche()
			if isFacingTrail()==True:
				avance()
			else:
				gauche()
				gauche()
				if isFacingTrail()==True:
					avance()
          
hunt()