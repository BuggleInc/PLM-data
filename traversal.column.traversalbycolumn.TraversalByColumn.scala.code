override def run() {
  var cpt=0
  ecritMessage(cpt)
  do {
    
    for(x<- 0 to getMondeLargeur()-1){
      for (y<-0 to getMondeHauteur()-1)
				setPos(x,y)
      	cpt = cpt + 1
        ecritMessage(cpt)
      	
      
      	
    }

  } while (positionFinale()) 
  
}

def positionFinale(): Boolean = {
  	return ((getX()==getMondeLargeur()-1) && (getY()==getMondeHauteur()-1))
    
  }

