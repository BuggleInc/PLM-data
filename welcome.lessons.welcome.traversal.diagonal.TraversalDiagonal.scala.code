var diag = 0;
override def run() {
  var diag = 0
  var compt:Int=0
  for(a<-0 to 6){
    for (b<-0 to a){
      setPos(b,a-b)
      ecritMessage(""+compt)
      compt=compt+1
    }
  }
  compt=0
  for (a<-0 to 5){
    for (b<-0 to a){
      setPos(getMondeLargeur()-b-1,getMondeHauteur()-(a-b)-1)
      ecritMessage(""+(48-compt))
      compt=compt+1
    }
  }
}
