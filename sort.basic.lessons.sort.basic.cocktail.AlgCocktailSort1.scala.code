def cocktailSort() {

	var needSwap=true
	while(needSwap)
	{
		for (i<- 0 to getNombreValeurs()-2)
		{
			needSwap=false
				if( plusPetit(i+1,i))
				{
					echange(i,i+1);
					needSwap=true;
				}
			
		}
		for (i<- getNombreValeurs()-2 to 0 by -1)
		{
			
				needSwap=false;
				if(plusPetit(i-1,i))
				{
					echange(i,i-1);
					needSwap=true;
				}
			
		}
	}
	
}

