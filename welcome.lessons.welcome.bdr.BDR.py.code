Message = ""
if isOverMessage():
  Message = readMessage()
  if Message == "R":
    right()
    forward()
  elif Message == "L":
    left()
    forward()
  elif Message == "I":
    left()
    left()
    forward()
  elif Message == "A":
    forward()
  elif Message == "B":
    forward(2)
  elif Message == "C":
    forward(3)
  elif Message == "Z":
    backward()
  elif Message == "Y":
    backward(2)
  elif Message == "X":
    backward(3)
  
  