def cocktailSort3() {
var Debut :Int=0
var Fin:Int=getNombreValeurs()-2
var swap : Boolean = true
do{
	swap =false
	for(i<-Debut to Fin){
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap= true
		}
	}
	if(swap==false){
		return
	}
		Fin-=1
	
	
	for(i<-Fin to Debut by -1){
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap= true
		}
	}
	if(swap==false){
		return
	}
		Debut+=1

}while(swap)
}
