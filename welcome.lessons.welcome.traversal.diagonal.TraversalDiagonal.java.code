public void run() {
	for (int z = 0; z < getWorldWidth() * getWorldHeight() - 1; z++) {		
		int y = (int) (((Math.floor((Math.sqrt(8 * z + 1)+1)/2)*Math.floor((Math.sqrt(8 * z + 1)+3)/2))/2) - z - 1);
		int x = (int) (z - ((Math.floor((Math.sqrt(8 * z + 1)+1)/2)*Math.floor((Math.sqrt(8 * z + 1)-1)/2))/2));
		setPos(x,y);
		writeMessage(z);
	}
}