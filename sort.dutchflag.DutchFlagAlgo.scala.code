def solve() {
  var cpt=0
  var color = 0
  for(j<-0 to getTaille-1){
    color=getCouleur(j)
    if(color==0){
      echange(cpt,j)
      cpt+=1
    }
  }
  for(j<-0 to getTaille-1){
    color=getCouleur(j)
    if(color==1){
      echange(cpt,j)
      cpt+=1
    }
  }
  for(j<-0 to getTaille-1){
    color=getCouleur(j)
    if(color==2){
      echange(cpt,j)
      cpt+=1
    }
  }
}