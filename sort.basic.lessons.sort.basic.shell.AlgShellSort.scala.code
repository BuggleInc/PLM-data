def shellSort()  {
	var ecart: Int = getNombreValeurs() / 2;
	while(ecart > 0){
		insertionSort(ecart)
		ecart = ecart / 2;
	}
}

def insertionSort(ecart: Int) {
	for(i <- 1 to getNombreValeurs() - 1){
		var v: Int = getValeur(i);
		var last: Int = 0;;
		var j: Int = i - ecart;
		while(j >= 0 && getValeur(j) > v){
			setValeur(j, getValeur(j - ecart));
			j = j - ecart;
			}
		setValeur(j, v);
	}
}

