boolean haveBaggle() {
	boolean aux=false;
	int compteur=0;
	while(!aux){
		for (int i=0; i<6;i++){
			if(estSurBiscuit()){
				aux=true;
			}
			else{
				avance();
				compteur=compteur+1;
			}
		}
		if(!aux){
			for (int i=0; i<6;i++){
				recule();
			}
			droite();
			avance();
			gauche();
			compteur=0;
		}
	}
	for (int i=0; i<compteur;i++){
		recule();
	}
	return aux;
}
