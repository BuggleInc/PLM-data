def cocktailSort2() {
  
  var switch:Boolean=true
  var debut = 0
  var fin = getNombreValeurs()-2
  do{
    
    switch=false
    
    for(i<- debut to fin){
      if (plusPetit(i,i+1)){echange(i,i+1);switch=true}
      fin = fin-1}
    
    for(j<- fin to debut by -1){
      if(plusPetit(j,j+1)){echange(j,j+1);switch=true}
      debut=debut+1}
    
  }while(switch)
}