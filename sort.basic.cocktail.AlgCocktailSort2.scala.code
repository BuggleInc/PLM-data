def cocktailSort2() {
  var test=false
  var debut=0
  var fin=getNombreValeurs()-2
  do{
    test=false
    for(i <- debut to fin){
      if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        test=true
        fin-=1
      }
    }
    for(j <- debut to fin){
      if(plusPetit(getNombreValeurs()-1-j,getNombreValeurs()-2-j)==true){
        echange(getNombreValeurs()-1-j,getNombreValeurs()-2-j)
        test=true
        debut+=1
      }
    }
  }
  while(test==true)
}