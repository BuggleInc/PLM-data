def combSort() {
	var ecart = getNombreValeurs()
	var x:Boolean = false
	do {
		if (ecart>1) {
			ecart = (ecart.toDouble / 1.3).toInt 
		}
		var i:Int=0
		x=false
		while (i+ecart<getNombreValeurs()) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				x=true}
			i=i+1;
		}
	} while (ecart>1 || x==false)
}
