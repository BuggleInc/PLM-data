/**
 *  Fait des V
 */
void faireV() {
  avance(2);
	marqueCase();
	avance();
	gauche();
	avance();
  marqueCase();
	retourne();
	avance();
  gauche();
  avance();
	marqueCase();
  avance(2);
  gauche();
}

/**
 * Marque 1 seule case
 */
void marqueCase(){
	baisseBrosse();
	leveBrosse();
}
public void run() {
  for (int j = 0; j < 9; j++){
		for (int i = 0; i < 9; i++){
			for(int k = 0; k < 4; k++){
        faireV();
      }
      avance(7);
		}
    gauche();
    avance(7);
    droite();
  }
}