
override def run() {
	def finale():Boolean = {
		if(getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur - 1) {
			return true
		}
		else return false
	}
	var c:Int = 0
	while(!finale) {
		var diag = 0
		while(diag != getY()-2) {
			ecritMessage(c)
			c = c + 1
			setPos(getX(),getY() + 1)
			diag = diag + 1
			}
		var X:Int = getX()
		while(X != getY()){
			ecritMessage(c)
			c = c + 1
			setPos(getX() + 1,getY() + 1)
		}
	}
}
