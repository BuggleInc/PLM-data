override def run() {
  
  var h:Int =getMondeHauteur()
  var l:Int =getMondeLargeur()
  
  var T = new Array[Color](h)
  
  for (g<- 0 to h-1){
    T(g)=getCouleurSol()
    avance()
  }
  for (i<- 0 to l-1){
    gauche();avance();droite()
    for(j<- 0 to h-1{
      avance()
      setCouleurBrosse(T(j));baisseBrosse();leveBrosse()
    }
        avance()
  }
}