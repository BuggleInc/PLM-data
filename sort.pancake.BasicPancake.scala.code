def solve() {
  while (isSorted()==false){
    var i =1
    var max=0
    var n =0
    for (i <- 0 to getStackSize()-n) {
      if (getPancakeRadius(i)>getPancakeRadius(max)){
      max=i
      }
    }
    if (i==0){
      flip(getStackSize()-n)
    }
    else {
    flip(i+1)
    flip(getStackSize()-n)
    }
    n=n+1
  }
}