def est4050(x):
    if x<=50 and x>=40:
        return True
    else:
        return False    

def est3040(x):
    if x<=40 and x>=40:
        return True
    else:
        return False            

def in3050(a, b):
    if (est3040(a)==True and est3040(b)==True) or (est4050(a)==True and est4050(b)==True):
        return True
    else:
        return False    
    