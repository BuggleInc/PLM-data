var mem = 1
def cocktailSort() {
  do {
    mem=0
    for (i<- 0 to getNombreValeurs-2) {
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
        mem=1
      }
    }
    for (i<-0 to getNombreValeurs-2) {
      if (plusPetit((getNombreValeurs-2)-i+1,(getNombreValeurs-2)-i)) {
        echange((getNombreValeurs-2)-i+1,(getNombreValeurs-2)-i)
        mem=1
      }
    }
  } while (mem==1)
}