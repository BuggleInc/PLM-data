public void interleavedHanoi(int height, int src1,int src2, int other, int dst) {
  if (height==1){
    deplace(src1,dst);
  	deplace(src2,dst);
  } else {
    interleavedHanoi(height-1,src1,src2,dst,other);
    deplace(src1,dst);
    deplace(src2,dst);
    interleavedHanoi(height-1,other,other,src1,dst);
  }
  
}