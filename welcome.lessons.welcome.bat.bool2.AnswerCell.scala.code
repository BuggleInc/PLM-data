def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
(isMorning,isMom,isAsleep) match {
	case (x,y,z) if (isAsleep==true) => return false
	case (x,y,z) if (isMorning==true && isMom==true) => return true
	case (x,y,z) if (isMorning==false && isAsleep==false) => return true
	case (_,_,_) => return false
}
}
