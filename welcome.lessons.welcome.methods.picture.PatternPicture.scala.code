override def run() {
	def marquer() {
		baisseBrosse();
		leveBrosse();
	}
	def faireI(c:Color) {
		setCouleurBrosse(c);
		avance();
		marquer();
		recule();
		gauche();
		avance();
		marquer();
		recule();
		droite();
		avance();
		avance();
		avance();
	}
	def place1() {
		for (k<- 1 to 4) {
			avance();
		}
	}
	def place2() {
		gauche();
		for (k<- 1 to 4) {
			avance();
		}
		gauche();
		for (k<- 1 to 4) {
			avance();
		}
		retourne();
	}
	for (i<-1 to 2) {
		for (j<- 1 to 2) {
			for (k<- 1 to 4) {
				if (k==4)
					faireI(Color.YELLOW);
				if (k==1)
					faireI(Color.RED);
				if (k==2)
					faireI(Color.BLUE);
				if (k==3)
					faireI(Color.GREEN);
			}
			if (j!=2){
				place1();
			} else if (i!=2) {
				place2();
			}
		}
	}
}
