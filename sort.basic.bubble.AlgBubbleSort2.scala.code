def bubbleSort2() {
  var echanger =true
 	var j = 1
  var len = getNombreValeurs()
  while (echanger == true) {
    j += 1
    var oui =0
    for (i <- 0 to len -j) {
      if (plusPetit(i+1, i)) {
      	echange(i, i+1)
        oui += 1
      };if (oui == 0) {
      		echanger = false
    	}	
}
}
}