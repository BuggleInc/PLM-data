def combSort() {
  var l = getNombreValeurs()
  var ecart = l
  var ech=false
  do
  {
    if (ecart>1)
    {
      l/=1.3
    }
    i=0
    while(i+ecart<l)
    {
      if(plusPetit(i+ecart,i))
      {
        echange(i,i+ecart)
        ech=true
      }
      i+=1
    }
  }while((ecart>1)||(ech))
}