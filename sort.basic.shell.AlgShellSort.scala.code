def shellSort()  {
	var ecart: Int=getNombreValeurs()/2
  var n: Int=getNombreValeurs
  for (i<- 1 to n-1){
    while (ecart>0){
      ecart/=2
      var v: Int=getValeur(i)
      var l: Int=i
      for (j<- i-ecart to 0 by -ecart){
        if (!plusPetitQue(j,v)){
          echange(j,j+1)
          l=j
        }
      }
    }
    setValeur(l,v)
  }
}