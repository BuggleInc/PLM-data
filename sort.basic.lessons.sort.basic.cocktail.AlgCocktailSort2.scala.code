def cocktailSort2() {
	var d:Int=0;
	var f:Int=getNombreValeurs-2;
	var test:Boolean=false;

	while (test== true) {
		test=false;
		for (i<-d to f) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				f=f-1;
				test=true;
			}
		}
		for (i<-f to d by -1) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				d=d+1;
				test=true;
			}
		}
	}
}
