void solve() {
  
  int b = 1;
  int bl = 1;
  int r = getTaille();
  int i = 0;
  
  while(bl<r)
  {
  	if(getCouleur(i)==1)
      {
      echange(b,bl);
      b++;
      r++;
    }
    else if(getCouleur(i)==0)
      {
     b++;
    }
    else
      {
      echange(bl,r);
      r--;
    }

    i++;
	}
}