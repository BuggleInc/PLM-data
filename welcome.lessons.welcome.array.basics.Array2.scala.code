override def run() {
  var T =new Array[Color] (getMondeHauteur())
  T(0)=getCouleurSol
  var p:Int =0
  while ( !estFaceMur ){
    avance()
    T(p)=getCouleurSol
    p=p+1
  }
  recule(p)
  for (i<-0 to getMondeHauteur()-2){
    gauche()
    avance()
    droite()
    var m:String=litMessage()
    var me:Int=m.toInt
    setCouleurBrosse(T((me) % 6))
    baisseBrosse()
    leveBrosse()
    for (j<-  0 to getMondeHauteur()-2){
      avance()
      setCouleurBrosse(T((j+me) % 6))
      baisseBrosse()
      leveBrosse()
    }
    recule(p)
  }
}