def combSort() {
   var x=getNombreValeurs(); var y =true;
   while ( x>1 || y) {
   	y=false
   	if (x>1) {
   		x = (x.toDouble / 1.3).toInt;
   	}
   	var i=0;
   	while (i+x < getNombreValeurs()) {
   		if (plusPetit(i+1,i)) {
   			echange(i,i+1);
   			y=true
   		}
   		i +=1;
   	}
   }
   
}
