def arrayFront9(nums:Array[Int]): Boolean = {
  var n  = nums.length
  var ya9:Boolean = false
  if (n>=4){
    for (k<-0 to 3){
      if (nums(k)==9){
        ya9=true
      }
      else ya9=ya9
    }
  }else{
    for (k<-0 to n-1){
      if (nums(k)==9){
        ya9=true
      }
      else ya9=ya9
    }
  }
  return ya9
}