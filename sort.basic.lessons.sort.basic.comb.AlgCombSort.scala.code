def combSort() {
  var lgr = getNombreValeurs()
  var ecart = lgr
  var NbEch = 1
  while (NbEch!=0 || ecart>1)
  NbEch=0
  if (ecart>1){
    ecart=(ecart.toDouble / 1.3).toInt
  }
  var i = 0
  while (i+ecart < lgr){
    if (plusPetit(i+ecart,i)){
      echange(i,i+ecart)
      NbEch=NbEch+1
    }
    i=i+1
  }
}