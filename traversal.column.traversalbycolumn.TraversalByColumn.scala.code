override def run() {
  var cpt=0
  ecritMessage(cpt)
  while (PositionFinale()==false){
    PositionSuivante()
    cpt+=1
    ecritMessage(cpt)
  }
}

def PositionFinale():Boolean={
  if (getMondeLargeur()==getX() && getMondeHauteur()==getY()){
    return true 
  }
  return false
}

def PositionSuivante(){
  if (getY()==6){
    setPos(getX()+1,0)
  }else if (getX()==6 && getY()==6){
    return
  } else {
    setPos(getX(),getY()+1)
  }
}