override def run() {
  var positionFinal=false
  while (!positionFinal){
    if(estFaceMur!=true){
      avance()
    }else{
      if(getDirection() == Direction.EST && estFaceMur()){
        positionFinal=true
      }else{
        if(getDirection() == Direction.EST){
          gauche()
        	avance()
        	gauche()
        }else{
          droite()
          avance()
          droite()
        }
      }
    }
  }
}