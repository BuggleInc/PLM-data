def solve() {
  val n = getTaille()
  var Bleu = 0
  var Rouge = n-1
  var Blanc = n-1
  var c = BLEU
  while (Bleu < Blanc) {
    c = getCouleur(Bleu)
    if (c == ROUGE) {echange(Bleu,Rouge);Rouge -= 1;Blanc -= 1}
    else {if (c == BLANC) {echange(Blanc,Bleu);Blanc -=1}
          else {Bleu +=1}}
  }
}