override def run() {
  
  faireDoubleMotif()
  
  gauche()
  for(i<- 1 to 4){
    avance()
  }
  droite()
  
  faireDoubleMotif()

}


def faireRed() {
  setCouleurBrosse(Color.red)
  avance()
  baisseBrosse()
  leveBrosse()
  recule();gauche();avance()
  baisseBrosse()
  leveBrosse()
  recule();droite()
}


