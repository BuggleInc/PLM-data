var suce = 0;
override def run() {
	for(i<- 0 to getMondeHauteur*getMondeLargeur/2){
		for(j<- 0 to i){
			setPos(j,i-j)
			ecritMessage(suce)
			suce+=1
			
		}
			
	}
		for(i<- 0 to getMondeHauteur*getMondeLargeur/2){
		for(j<- 0 to i){
			setPos(getMondeHauteur-j,i-j)
			ecritMessage(suce)
			suce+=1
			
		}
			
	}
}
