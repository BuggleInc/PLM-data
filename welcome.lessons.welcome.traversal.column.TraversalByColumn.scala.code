override def run() {
  retourne()
  var compteur:Int=0
  while (!(getX() ==(getMondeLargeur()-1) && getY() == getMondeHauteur()-1)) {
    ecritMessage(compteur)
    setPos(getX(),getY()+1)
    compteur+=1
    if (estFaceMur() && !(getX()==6) ){
      ecritMessage(compteur)
      compteur+=1
      setPos(getX()+1,0)
    }   
  }
  ecritMessage(compteur)
  retourne()
}