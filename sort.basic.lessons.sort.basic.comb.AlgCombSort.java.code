public void combSort() {
  double gap = getValueCount();
  boolean swap;
  do{
    swap=false;
    if(gap>1){
      gap = gap / 1.3;
    }
    int i = 0;
    int ecart = (int)gap;
    while(i+gap < getValueCount()-2){
      if(isSmaller(i+ecart,i)){
        swap(i,i+ecart);
        swap=true;
      }
      i++;
    }
  }while(gap>1 || swap);
}