override def run() {
  var diag = 0
  var count = 0
  while (diag!=12){
    ecritMessage(count)
        count=count+1
    if(getX()==getMondeLargeur || getY()==0 ) {
      diag=diag+1
      if (diag<getMondeHauteur){
      setPos(0,diag)
      }
      else{
        setPos(12-diag,getMondeHauteur)
      }
    }
    else {
      setPos(getX()+1,getY()-1)
    }
  }
}

