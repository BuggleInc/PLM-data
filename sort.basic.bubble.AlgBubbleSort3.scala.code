def bubbleSort3() {
  
  var c:Int = 0;
  
    for(i <- getNombreValeurs()-2 to 0 by -1){
      
      c = 0;
  
      for(j <- 0 to i){

        if(plusPetit(j+1, j)){

          echange(j, j+1);
          c += 1;

        }

      }
      
      if(c == 0){
        
        return
        
      }
    
  }
  
  
}