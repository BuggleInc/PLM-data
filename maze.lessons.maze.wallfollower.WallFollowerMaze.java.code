public void run() {
	keepHandOnsidewall();
}

public void keepHandOnsidewall(){
int count = 0;

	do{
		if(count > 1){
			back();
		}
		
		left();
		
		if(isFacingWall() ){
			count++;
			right();
		}else{
			count = 0;
			forward();
		}
/*		
		if(!isFacingWall()){
			count = 0;
			forward();
		}
*/	
		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall
