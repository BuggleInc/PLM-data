}

def vasChercherBiscuitDucon():Boolean ={
  retourne
  while(!estSurBiscuit()&& !estFaceMur()){
      avance()
  }
  if(estSurBiscuit){
    prendBiscuit()
    return true
  }else{
    return false
  }
  
  
}

def rapporteBiscuitEnfoireDeNain(){
  retourne()
  while(!estSurBiscuit){
    avance()
  }
  recule()
  if(porteBiscuit()){poseBiscuit()}
  recule()
}

moria()