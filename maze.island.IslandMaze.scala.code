}

def keepHandOnSideWall(){

  etat match{
    case 0 => while (!estFaceMur() && getDirection == Direction.NORD){
      avance()
    }
    gauche()
    if(estFaceMur()){
      etat = 1;
    }
    else {
      avance()
      droite()
      
    }


    case 1 => while (!estFaceMur() && getDirection != Direction.NORD){ 
       
    gauche()
    while (estFaceMur()){
      droite()

    }
    avance()

  }
