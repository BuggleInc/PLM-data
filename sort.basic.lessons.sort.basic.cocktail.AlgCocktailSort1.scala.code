def cocktailSort() {
  var x:Boolean = false
  var y:Boolean = false
  while(!x && !y){
    x = true
    y = true
    for(i<- 0 to getNombreValeurs()-2){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        x = false
      }
    }
    for(j<- getNombreValeurs()-2 to 0 by -1){
      if(plusPetit(j,j+1)){
        echange(j,j+1)
        y = false
      }
    }
  }
}