def gnomeSort() {
  var fini:Boolean=false
  var i:Int=0
  var lgr:Int=getNombreValeurs()
  while (!fini){
    if (plusPetit(i+1:Int, i:Int)){
      echange(i:Int, i+1:Int)
      if (i==0){
        i=i+1
      } else {
        i=i-1
      }
    } else {
      i=i+1
    }
    if (i==lgr-1){
      fini=true
    }
  }
}
