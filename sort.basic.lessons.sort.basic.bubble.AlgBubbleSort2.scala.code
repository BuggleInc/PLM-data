def bubbleSort2() {
  var nontri:Boolean= true 
  var indice:Int=2
  while (nontri) {
    var ech:Int=0
    for (i<-0 to (getNombreValeurs()-indice){
      if (plusPetit( i+1, i)) {
        echange(i, (i+1))
        ech = (ech +1) }
      indice=indice+1}    
    if (ech==0) {nontri=false}
  }  
}



