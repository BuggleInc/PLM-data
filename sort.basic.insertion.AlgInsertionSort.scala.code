def insertionSort() {
  
  var i = 0;
  var j = 0;
  var v = 0;
  
  for (i <- 1 to getNombreValeurs()-1) {
    v = getValeur(i);
    j = i;
    
    while (plusPetitQue(j,v) == false) {

      copie(j-1, j)
      if (j <= 1) {
         j += 1

      } else {
         j -= 1
      }



    } 

    setValeur(j+1, v);

    
  }
  
  
}