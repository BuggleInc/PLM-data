def cocktailSort3() {



	var x = true
	var debut = 0
	var fin = getNombreValeurs()-2
	do {
 		x = false

		for (i<- debut to fin ) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				x = true
			}
		}
		fin = fin-1
		if ( x == false ) {
			return
		}
		
		x = false
		for (i<- fin to debut by -1) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				x = true
			}
		}
		debut=debut+1
	}
	while (x)

}
