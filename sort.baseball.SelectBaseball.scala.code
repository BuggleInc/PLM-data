override def run() {
  var nbBases: Int =getBasesAmount()
  var hole : Int = 0
  while (!isSorted()) {
    hole=getHoleBase()+1
    if (hole==nbBases) {
      hole=0
    } 
    if (getPlayerColor(hole,0)>getPlayerColor(hole,1)) {
      move(hole,0)
    } else {
      move(hole,1)
    }
  }
  move(getHoleBase(),1)
}