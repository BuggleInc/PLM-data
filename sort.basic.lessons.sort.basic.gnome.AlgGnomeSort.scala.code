def gnomeSort() {
	var i=0
	while(i!=getNombreValeurs()-1)
	if(!plusPetit(i,i+1)){
		echange(i,i+1)
		if(i!=0){
			i=i-1
		}else{
			i=i+1
		}
		if(i==getNombreValeurs()-2){
			return
		}
	}else{
				if(i==getNombreValeurs()-2){
			return
		}
		if(i<getNombreValeurs()-1){
		i=i+1
		}else{
			return
		}
				if(i==getNombreValeurs()-1){
			return
		}
	}
}
