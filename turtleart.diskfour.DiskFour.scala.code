override def run() {
  setCouleur(Color.black)
  quartDeCercle()
  
  setCouleur(Color.red)
  quartDeCercle()
  
  setCouleur(Color.orange)
  quartDeCercle()
  
  setCouleur(Color.magenta)
  quartDeCercle()
}

def quartDeCercle(){
  for (i<- 0 to 89){
    avance(100)
    droite(180)
    avance(100)
    droite(180)
    droite(1)
  }
}