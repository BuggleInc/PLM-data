def combSort() {
  
  var ecart:Int=getNombreValeurs()
  var i:Int=0
  var cpt:Int=0
  
  
  do{
    cpt=0
    if(ecart>1){
      ecart=(ecart.toDouble/1.3).toInt
    }
    i=0
    while(i+ecart< getNombreValeurs()){
      if(plusPetit(i, i+ecart)==false){
        echange(i,i+ecart)
        cpt+=1
      }
      i+=1
    }
    
    
    
    
  }while(ecart>1 || cpt!=0)
  
  
}