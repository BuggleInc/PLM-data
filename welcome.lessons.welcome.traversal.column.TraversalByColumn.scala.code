override def run() {
  var i=0
  var x=0
  var y=1
  retourne()
  ecritMessage(i)
  i+=1
  setPos(x,y)
  while (i!=getMondeHauteur()*getMondeLargeur()) {
    if (i%6==0) {
      x+=1
      y=0
      ecritMessage(i)
      setPos(x,y)
      i+=1
    }
    else {ecritMessage(i);y+=1;setPos(x,y);i+=1}
  }
  
  def posFinale() : Boolean = {
    if (estFaceMur()) {
      gauche()
      if (estFaceMur()) {droite; return true}
      else {droite();return false}
  	}
    else {return false}
  }
}