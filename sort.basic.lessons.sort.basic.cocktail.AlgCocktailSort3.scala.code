def cocktailSort3()  {
  var n = getNombreValeurs()
  var c = 1
  var debut = 0
  var fin = n-2
  while (c!=0) {
    c = 0
    for (i<-debut to (fin)) {
      if ((getValeur(i))>getValeur(i+1)) {
        echange(i,(i+1))
        c=c+1
      }
    }
    
    if (c!=0) {
    fin=fin-1
    for (i<-(fin) to debut by -1) {
      if ((getValeur(i))>getValeur(i+1)) {
        echange(i,(i+1))
        c=c+1
        
      }
    }
     debut=debut+1
  }
}
}