def estPositionFinale:Boolean = {
  if (getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur() - 1)
  	return true
  
  return false
}

def vaPositionSuivante(oldX: Int, oldY: Int) {
	var newX:Int = 0
  var newY:Int = (oldY + 1) % getMondeHauteur()
}

override def run() {
  while (!estPositionFinale()) {
    vaPositionSuivante(getX(), getY())
  }
}