def combSort11() 
{
	var i = 0
	var fini = false
	var ecart = getNombreValeurs()

	do
	{
		if(ecart>1)
			ecart = (ecart.toDouble / 1.3).toInt
		if(ecart==9 || ecart==10)
			ecart = 11
		i = 0
		fini = true

		while(i+ecart < getNombreValeurs())
		{
			if(plusPetit(i+ecart,i))
			{
				echange(i,i+ecart)
				fini = false
			}
			i += 1
		}
		
	}while(ecart>1 || !fini)
}
