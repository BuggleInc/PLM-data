def combSort() {
	var ecart: Int = getNombreValeurs();
	var lgr: Int = getNombreValeurs();
	var inverse: Boolean = false;
	do{
		inverse = false;
		if(ecart > 1){
     		ecart = (ecart.toDouble / 1.3).toInt;
		}
   		var i: Int = 0;
   		while(i + ecart < lgr){
     		if(!plusPetit(i, + ecart)){
				echange(i, i + ecart);
				i = i + 1;
				inverse = true;
     		}
		}
	}while(ecart > 1 | inverse)
}
