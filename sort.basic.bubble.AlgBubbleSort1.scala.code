var nbValeurs=0
var fin = 0
var test = false

def bubbleSort()  {
  do {
      fin = 0 
      nbValeurs=getNombreValeurs()
      for (i <- 1 to nbValeurs-1){
         for (j<- 2 to nbValeurs){
            test = plusPetit(i,j)
            if(test){
               echange(i,j)
               test = false
               fin = fin +1
            }
         }
      }
  } while (fin != 0) 
}