




String mes;
boolean estFait=False;

while(!estFait){
  
  mes=litMessage();
  if(mes='R'){droite();avance();}
  else if(mes='L'){gauche();avance();}
  else if(mes='I'){droite();droite();avance();}
  else if(mes='A'){avance();}
  else if(mes='B'){avance(); avance();}
  else if(mes='C'){avance(); avance();}
  else if(mes='Z'){recule();}
  else if(mes='Y'){recule();recule();}
  else if(mes='X'){recule();recule();recule();}
  
  
}