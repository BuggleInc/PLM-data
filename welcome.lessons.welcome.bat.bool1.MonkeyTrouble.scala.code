var probleme = false

def monkeyTrouble(aSmile:Boolean, bSmile:Boolean): Boolean = {
  if (aSmile==true && bSmile==true) {
    probleme = true
    return probleme
  }
  if (aSmile==false && bSmile==false) {
    probleme = true
    return probleme
  }
  else {
    probleme = false
    return probleme
  }
}
  