override def run() {
  def pattern() {
    def makeV(c: Color) {
      setBrushColor(c)
  		forward()
  		brushDown()
  		brushUp()
  		forward()
  		left()
  		forward()
  		brushDown()
  		brushUp()
  		backward()
  		right()
  		forward()
  		brushDown()
  		brushUp()
  		forward()
  		left()
    }
		makeV(Color.YELLOW)
    makeV(Color.RED)
    makeV(Color.BLUE)
    makeV(Color.GREEN)
  }
  for (i <- 1 to 9) {
    pattern() ; forward(7)
    if (i==3 || i==6) {left() ; forward(7) ; right()}
  }
  right() ; forward(14) ; left()
}