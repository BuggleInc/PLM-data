def cocktailSort2() {
	var debut:Int = 0
	var fin:Int = getNombreValeurs() - 2 
	var swapped:Boolean = true
	while(swapped){
		swapped = false
		for(i <- debut to fin){
			if(plusPetit(i+1,i)){
				echange(i+1,i)
				swapped = true
			}
			fin -= 1
			
		}
		for(i <-fin to debut by -1){
			if(plusPetit(i+1,i)){
				echange(i+1,i)
				swapped = true
			}
			debut += 1
		}
	}
}
	
	
	
