def cocktailSort() {
  var modif = false;
  do {
    modif = false
    for(i <- 0 to (getValueCount() - 2) by -1) 
          if(isSmaller(i+1,i)){
          	swap(i,i+1);
            modif = true;
          }
    
    for (j <- 0 to (getValueCount() - 2))  {
				if(isSmaller(j+1,j)){
          swap(j,j+1);
    			modif = true;
        }
    }
  } while(modif)
      
}