def selectionSort() {
  var N:Int=0
  var lgr:Int = getNombreValeurs()
  var M:Int=0
	var min:Int = 0
  var valmin:Int=0
  for (N <- 0 to lgr-2){
    min = N
    valmin = getValeur(N)
    for (M <- N+1 to lgr-1){
      if (getValeur(M)<getValeur(min)){
        min = M
      }
    }
    echange(N,min)
  }
  
}