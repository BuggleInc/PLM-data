public void hunt(Color c) {
		
		/* BEGIN SOLUTION */
		while (! isOverBaggle()) {
			if (isFacingTrail(c)) {
				brushDown();
				forward();
				brushUp();
			} else {
				left();
			}
		}
		pickupBaggle();
		/* END SOLUTION */
	}
boolean isFacingTrail(Color c) {
		if (isFacingWall())
			return false;

		forward();
		boolean res = getGroundColor().equals(c);
		backward();
		return res;

	}
    

