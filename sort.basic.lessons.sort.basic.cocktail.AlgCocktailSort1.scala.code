def cocktailSort() {
	var faitechange:Boolean=true
	while(faitechange==true){
		faitechange=false
	for(i<-0 to getNombreValeurs()-2){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
			faitechange=true
				}
			}
		
		
		for(i<-getNombreValeurs()-2 to 0){
			if(plusPetit(i+1,i)){
				echange(i,i+1)
				faitechange=true
		}
	}
	
}
}

