def cocktailSort() {
  
  var n = getValueCount();
	var swapped = false
  
  do
  { 
    swapped = true
    for( i <- 0 to n-2)
    {
      if (!isSmaller(i,i+1))
      {
        swap(i,i+1);
        swapped = false;        
      }
    }
      
    for( i <- n-2 to 1 by -1)
    {
      if (!isSmaller(i,i-1))
      {
        swap(i,i-1);
        swapped = false;        
      }
    }
    
    
  }while(!swapped)

}