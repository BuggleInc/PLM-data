def array123(nums:Array[Int]): Boolean = {
  var c1 = false ;
  var c2 = false ;
  var c3 = false ;
  for (i<- 0 to nums.length-1){
    if ( nums(i) == 1) {
      c1=true;
    }
    if ( nums(i) == 2) {
      c2=true;
    }
    if ( nums(i) == 3) {
      c3=true
    }
  }
  if (c1 && c2 && c3){
    return true
  }
  return false
}
