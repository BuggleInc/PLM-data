override def run() {
	def positionFinale(x:Int,y:Int):Boolean = {
		if(y==getMondeLargeur()-1 && x==getMondeHauteur()-1) {
			return true
		}
		else{return false}
	}

	var x=0
	var y=0
	ecritMessage(0);
	var cpt=0
	while(!positionFinale(x,y)) {
		x=x
		y=y+1;
		if(y==getMondeHauteur()){
			y=0
			x=x+1
		}
		setPos(x,y)
		cpt= cpt+ 1
		ecritMessage(cpt)
	}
}


	

