override def run(){repMotif()}

def repMotif(){
  for (i <- 1 to 2){
    faitMotif()
  }
  
} 

def faitMotif(){
  Motif(Color.RED)
  Motif(Color.BLUE)
  Motif(Color.GREEN)
  Motif(Color.YELLOW)
  if (estFaceMur()==false){
    avance(4)
  }
}
  
def Motif(c:Color){
