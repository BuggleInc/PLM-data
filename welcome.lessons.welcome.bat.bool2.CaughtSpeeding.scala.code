def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (isBirthday)
  	{
      if (speed<=65)
      	return 0
      if (speed>=86)
      	return 2
      else
      	return 1
    }
  else
  	{
      if (speed<=60)
      	return 0
      if (speed>=81)
      	return 2
      else
      	return 1
    }
}  