override def run() {
	def finale():Boolean = {
		if(getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur - 1) {
			return true
		}
		else return false
	}
	var c:Int = 0
	while(!finale) {
		ecritMessage(c)
		c = c + 1
		setPos(getX(),getY() + 1)
		if(getY() == getMondeHauteur - 1) {
			ecritMessage(c)
			c = c + 1
			if(getX() < getMondeLargeur - 1) {
			setPos(getX() + 1,0)
			}
		}
		
	}
}

