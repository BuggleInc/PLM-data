def shellSort()  {

	var ecart=getNombreValeurs()/2
	while(ecart>0)
	{	
	 	for (i <- ecart - 1 to getNombreValeurs()-1)
	 	{ 
		     var j=i;
		     var valeur =getValeur(i)
		     while (j>= ecart&& (getValeur(j-ecart)>valeur))
		     { 
		     	copie(j-ecart,j)
		        	j=j-ecart
		     }
			 	setValeur(j,valeur);
	    }
	    ecart = ecart/2;
	}
}