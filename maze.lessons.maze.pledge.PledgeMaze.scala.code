override def run(){
  while(!estFaceMur()){
    avance()
  }
  gauche()
  
  def keepHandOnSideWall(): Boolean={
    droite()
    if(estFaceMur()){ 
      gauche()
      return true}
    else{
      gauche()
      return false}
  }
  
  while(!estSurBiscuit()){
    var pos=(0,0)
    if(pos==(getX(),getY())){
       while(!estFaceMur()){
         avance()
  			}
    }
    if(keepHandOnSideWall()){
      if(!estFaceMur()){avance()}
      else{gauche()}
    }
    else{droite()
         avance()
         pos=(getX(),getY())
        }
    
  }
  prendBiscuit()
}
