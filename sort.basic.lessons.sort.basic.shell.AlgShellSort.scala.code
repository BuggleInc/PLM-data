def shellSort()  {
	val l=getNombreValeurs();
	var v=0;
	var j=0;
	var b=true;
	var ecart=l/2;
	for (i<-1 to l-1) {
		v=getValeur(i);
		b=true;
		j=i;
		while ((b)||(j>ecart)) {
			b=false;
			if (j==0) {
				setValeur(0,v);
			} else if (plusPetitQue(j-ecart,v)) {
				setValeur(j,v);
			} else {
				copie(j-ecart,j);
				j-=ecart;
				b=true;
			}
		}
		
	}
}
