override def run() {
  var cpt=0
  var x=0
  var y=0
  while (fonctionTest==false) {
    y+=1
    setPos(x,y)
    cpt+=1
    if (y==6) {
      x+=1
      y=0
    }
    ecritMessage(cpt)
  }
}

def fonctionTest() {
  var res:Boolean=false
  if (getMondeHauteur()==6 & getMondeLargeur()==6) {
    res=true
  }
}