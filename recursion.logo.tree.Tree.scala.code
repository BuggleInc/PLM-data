def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
 	if(steps!=0)
  {
    current(steps);
  	avance(length);
  	droite(angle);
    tree(steps-1,length*shrink,angle,shrink);
    gauche(angle*2);
    current(steps);
    tree(steps-1,length*shrink,angle,shrink);
    gauche(180-angle);
    avance(length);
    droite(180);

  }
}