def combSort() {
var ecart : Int = getNombreValeurs()
var swap: Boolean =false
do{
	if(ecart>1){
		ecart=(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
	}
	var i: Int =0
	while(i+ecart<getNombreValeurs()){
	
		if(plusPetit(i+1,i)){
			echange(i,i+1)
			swap=true
		}
		i+=1
	}
	
}while( swap || ecart>1)
}
