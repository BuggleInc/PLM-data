def cocktailSort() {
  var cpt = 0
  do{
    for (i<- 0 to getNombreValeurs-2){
      if (plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt=1
      }
    }
    for (i<- 0 to getNombreValeurs-2){
      for (j <- 0 to getNombreValeurs-i-1){
        if (plusPetit(j,j+1)==false){
          echange(j,j+1)
          cpt=1
        }
      }
    }
  } while (cpt==1)
}