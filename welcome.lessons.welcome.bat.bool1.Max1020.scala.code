def max(a:Int, b:Int){
  if (a<b){
    return b
  }
  return a
}

def max1020(a:Int, b:Int):Int = {
  if ((((10<=a) && (a<=20))) && ((10<=b) && (b<=20))){
    return max(a,b)
  } else if (((10<=a) && (a<=20)) && ((b<10)||(b>20))){
    return a
  } else if (((10<=b) && (b<=20)) && ((a<10)||(a>20))){
    return b
  }
  return a
}