public void run() { 
	while(!isOverBaggle()){ // going backward for baggles
		backward();
		if(isOverBaggle()){ // pick up baggle 
			pickUpBaggle();
			forward();
			if(isOverBaggle()){
				backward();
				dropBaggle();
			}else {
				run();
			}
		}else {
			run();
		}
	}
}
//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().
