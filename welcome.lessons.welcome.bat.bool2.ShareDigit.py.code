def shareDigit(a, b):
    if a>=10:
        dizaine_a=int(a/10)
    else:
        dizaine_a=0
    if b>=10:
        dizaine_b=int(b/10)
    else:
        dizaine_b=0 
    if a<10:
        unite_a=a
    else:
        unite_a=a%(dizaine_a*10)
    if b<10:
        unite_b=b
    else:
        unite_b=b%(dizaine_b*10)   

    if unite_a==unite_b or unite_a==dizaine_b or unite_b==dizaine_a or dizaine_a==dizaine_b:
        return True
    else:
        return False             
                 