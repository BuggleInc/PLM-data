void goAndGet(){
int stepFwd = 0;
int stepBwd = 0;

	while(!isOverBaggle()){
		forward();
		stepFwd++;
		
		if(isOverBaggle()){
			pickupBaggle();
			
			while(isCarryingBaggle()){
				backward();
				stepFwd--+;
			
				if(stepFwd == 0){
					dropBaggle();
					stepFwd = 0;
					stepBwd = 0;
				}
			}
		}
	}
}
