biscuits=0
caseOrange=0
while 2*biscuits != caseOrange + 1:
    avance()
    if estSurOrange() is True:
        caseOrange=caseOrange+1
    elif estSurBiscuit() is True:
        biscuits=biscuits+1 
    elif estSurOrange() is True and estSurBiscuit() is True:
        biscuits=biscuits+1 
        caseOrange=caseOrange+1