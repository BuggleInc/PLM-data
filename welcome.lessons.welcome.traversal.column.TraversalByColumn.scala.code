override def run() {
  var compteur = 0
  var X=0
  var Y=0
  while (((getY,getX())!=(6,6))) {
    
    if (Y!=6) {
      Y=Y+1
      ecritMessage(compteur)
      setPos(X,Y)
      compteur=compteur+1
    } else {
      Y=0
      X=X+1
      
      ecritMessage(compteur)
      setPos(X,Y)
      compteur=compteur+1
    }
  }
  ecritMessage(compteur)
}