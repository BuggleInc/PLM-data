public void run() {
int laps = 10;
int steps = 8;
int sides = 4;

	//loop for laps
	for(int x = 1; x <= laps; x++){
		//loop all four sides
		for(int count = 1; count <= sides; count++){
			//loop for one side
			for(int y = 1; y <= steps; y++){
				forward();
				if(y == 8){left();}
			}
		}

	}
}
