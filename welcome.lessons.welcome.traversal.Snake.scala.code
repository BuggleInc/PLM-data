override def run() {
  var fin=false
  while (!fin){
    if(!estFaceMur()){
      avance()
    }else{
      if (getDirection==Direction.EST){
        gauche()
        if(estFaceMur()){
          gauche()
          if (estFaceMur()){
            fin=true
          }
        }
      }else{
        if(getDirection() == Direction.EST){
          gauche()
          avance()
       		gauche()
      	}else{
        	droite()
        	avance()
        	droite()
        }
      }
    }
  }
}

