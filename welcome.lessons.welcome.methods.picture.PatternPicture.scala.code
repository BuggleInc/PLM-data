override def run() {
  faireCarre()
}

  

def faireL(c: Color, d: Color) {
  avance()
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
  avance()
  setCouleurBrosse(d)
  baisseBrosse()
  leveBrosse()
  avance()
}

faireCarre() {
  faireL(Color.RED, Color.BLUE)
  gauche()
  faireL(Color.BLUE, Color.GREEN)
  gauche()
  faireL(Color.GREEN, Color.YELLOW)
  gauche()
  faireL(Color.YELLOW, Color.RED)
}