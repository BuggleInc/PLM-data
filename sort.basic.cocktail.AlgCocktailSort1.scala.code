def cocktailSort() {
  var x=0
  var ok:Boolean=false
  x=getNombreValeurs
  do {
    for (i <- 0 to x-2) {
      if (plusPetit(i+1,i)) {
        echange(i+1,i)
        ok=true
      }
    }
    for (i <- x-2 to 0 by -1) {
      if (plusPetit(i+1,i)) {
        echange(i+1,i)
        ok=true
      }
    }
  } while (ok)
}