def cocktailSort2() {
var debut:Int=0
var fin:Int=getNombreValeurs-2
var fini:Boolean=false
while (fini == false) {
	fini=true
	for (i <- debut to fin) {
		if (getValeur(i) > getValeur(i+1)) {
			echange(i,i+1)
			fini=false
		}	
	}
	fin+=(-1)
	for (i <- fin to debut by -1) {
		if (getValeur(i) < getValeur(i-1)) {
			echange(i,i-1)
			fini=false
		}
	}
	debut+=1
}
}
