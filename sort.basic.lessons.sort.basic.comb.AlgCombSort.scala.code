def combSort() {
var ecart=getNombreValeurs()
var exchange=true
while (ecart > 1 || exchange == true) {
	exchange = false
	if (ecart > 1) {
		ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
	}
	for(i<-0 to getNombreValeurs()-ecart-1){
		if (plusPetit(i+ecart,i)==true) {
			echange(i,i+ecart)
			exchange=true
	}
}
}
}
