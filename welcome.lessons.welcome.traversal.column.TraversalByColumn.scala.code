override def run() {
	var x:Int = 0
	def posFin():Boolean = {
		var b = false 
		if (getX() == getMondeHauteur()-1 && getY() == getMondeLargeur()-1) {b = true}
		return b
	}
	while (!posFin) {
		while (!estDosMur()) {
			ecritMessage(x);
			setPos(getX(),getY()+1);
			x=x+1;
		}
		if (estDosMur) {setPos(getX()+1,0)}
	}
}
