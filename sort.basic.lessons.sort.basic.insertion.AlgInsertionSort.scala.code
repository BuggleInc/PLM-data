def insertionSort() {
  var lgr : Int = getNombreValeurs()
  var position : Int = 0
  var v : Int = 0
  var temp : Int = 0
  
  for (i<-1 to lgr-1) {
    v = getValeur(i)
    temp = 0
    while ((i-temp>0) && (!plusPetitQue(i-temp-1,v))) {
      setValeur(i-temp,getValeur(i-temp-1))
      temp+=1
    }
    setValeur(i-temp,v)
  }
}
