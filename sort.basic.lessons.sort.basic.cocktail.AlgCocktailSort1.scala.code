def cocktailSort() {
  var longueur = getNombreValeurs()
  var trier = true
  
  while(trier){
    trier = false
    
    //1ere boucle
    for (i <- 0 to longueur -2){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        trier = true
      }
    }
    
    //2e boucle
    for (j <- longueur -2 to 0 by -1){
      if (plusPetit(j+1,j)){
        echange(j,j+1)
        trier = true
      }
    }
      
  }
  
}