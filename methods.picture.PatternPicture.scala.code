override def run() {
  for(a <- 1 to 4) {
    round(Color.RED)
  	forward()
    round(Color.YELLOW)
  	forward()
 	  forward()
  	round(Color.GREEN)
  }
}  
  
  
  
def round(c:Color) {
  setBrushColor(c)
  left()
  forward()
  brushDown
  brushUp
  left()
  forward()
  backward()
  right()
  forward()
  brushDown()
  brushUp()
  forward(2)
  left()
}