override def run() {
  baisseBrosse()
  var dir = Direction.EST
  while (((estFaceMur==false)&&(MurNord==false))) {
    avance()
    dir=getDirection()
    if ((estFaceMur)&&(dir==Direction.EST)) {
       gauche()
       avance()
       gauche()
    }
    if ((estFaceMur)&&(dir==Direction.OUEST)) {
       droite()
       avance()
       droite()
    }
  }
  
}


def MurNord(): Boolean= {
  setDirection(Direction.NORD)
  if (estFaceMur()) {
    setDirection(Direction.EST)
    return true
  } else {droite(); return false;}
}