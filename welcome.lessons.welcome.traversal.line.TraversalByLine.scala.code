override def run() {
  var x=getX()
  var y=getY()
  var pas=0
  while (!posFinale(x,y)) {
    ecritMessage(pas.toString)
    if (x==getMondeHauteur-1) {
      setPos(0, y+1)
    } else setPos(x+1, y)   
    x=getX()
    y=getY() 
    pas=pas+1  
  }
  ecritMessage(pas.toString)
}

def posFinale(x:Int,y:Int):Boolean = {
  if (y==getMondeHauteur()-1 & x==getMondeHauteur()-1) {
    return true
  } else return false
}