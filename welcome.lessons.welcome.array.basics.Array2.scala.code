override def run() {
	val H = getWorldHeight();
	var decalage = litMessage().toInt;
	var T = new Array[Color](H+1)
	for (i <- 1 to H) {
		T(i) = getCouleurSol() ;
		avance();
	}
	for (i <- 1 to H-1) {
	   gauche(); avance(); droite();
	   for (x <- 1 to decalage) { avance(); }
		for (tmp <- 1 to H) {
			avance();
			setCouleurBrosse(T(tmp));
			baisseBrosse();
			leveBrosse();
		}
		avance();
	}
	
}
