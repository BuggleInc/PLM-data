override def run() {


var steps:Int = 0;


	while(!estArrive())
	{

		
		setPos(getX(), getY() + 1);
		ecritMessage(steps);
		steps = steps + 1;
		if(getY() == 6)
		{
			setPos(getX() + 1, 0);
		}

	
	}
	
		

def estArrive() : Boolean = {

	var result = false;

	if(getX() == getMondeLargeur() - 1 && getY() == getMondeHauteur() - 1)
	{
		result = true;
	}

		return result;

	
}


}


