def cocktailSort() {
  var nbOps:Int = 1;
  while (nbOps > 0) {
    nbOps = 0;
    for (i<- 0 to getNombreValeurs()-2) {
      if (plusPetit(i+1, i)) {
        echange(i, i+1);
        nbOps = nbOps + 1;
      }
    }
    for (j<- getNombreValeurs()-1 to 1 by -1) {
      if (plusPetit(j, j-1)) {
        echange(j, j-1);
        nbOps = nbOps + 1;
      }
    }
  }
}