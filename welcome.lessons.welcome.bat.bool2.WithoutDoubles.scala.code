def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
	if (noDoubles) {
		if ( die1 == die2 ) { 
			if (die1 != 6) { return ( die1 + die2) }
			else { return (die2 + 1) }
	                                      }
                                                 }
 return (die1 + die2) 
}
