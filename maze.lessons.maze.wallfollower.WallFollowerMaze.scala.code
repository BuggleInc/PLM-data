    	}
    	return false
}
def keepHandOnSideWall(){
	if (!estFaceMur()){
		avance()
		droite()
	} else {
		gauche()
		if (!estFaceMur()){
		avance()
		}
	}
}
override def run() {
	while (!sortie()){
