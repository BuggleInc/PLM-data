def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  
  if(steps==0){
    return
  }
  else{
  avance(length);
  current(steps);
  
  droite(angle);
  tree(steps-1,length*shrink,angle,shrink);
	recule(1);
  
  avance(length);
  gauche(2*angle);
  tree(steps-1,length*shrink,angle,shrink);
  recule(length);
  
  }
}