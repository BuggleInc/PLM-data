def cocktailSort2() {
var a:Int=0
var b:Int=getNombreValeurs()-2
var faitechange:Boolean=true
	while(faitechange==true){
		faitechange=false
     for(i<-b to a by -1){
	      if(plusPetit(i+1,i)){
	             echange(i,i+1)
	             b-=1
	             faitechange=true
}
}
     for (i<-a to b){
     	if(plusPetit(i+1,i)){
     		echange(i,i+1)
     		a=a+1
     		faitechange=true
     	}
     }
	}
}
