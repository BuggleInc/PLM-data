override def run() {
  var cpt:Int = 0
  ecritMessage(cpt)
  cpt+=1
  var d:Boolean = true
	for (i <- 0 to 47) {
    if (d) {
      if (getX()<6) {
        setPos(getX()+1,getY())
      }
      else {
        setPos(getX(),getY()+1)
        d=false
      }
    }
    else {
      if (getX()>0) {
        setPos(getX()-1,getY())
      }
      else {
        setPos(getX(),getY()+1)
        d=true
      }
    }
    ecritMessage(cpt)
    cpt=cpt+1
  }
}