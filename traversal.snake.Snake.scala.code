override def run() {
  while(!estPosFinale()){
  	if(!estFaceMur()){
    	avance()
  	}
  	else{
    	if(getDirection==Direction.EST){
      	left()
      	avance()
      	left()
    	}
    	if(getDirection==Direction.OUEST){
      	droite()
      	avance()
      	droite()
    	}
  	}
  }
}

def estPosFinale():Boolean = {
  if(estFaceMur()){
    if(getDirection()==Direction.EST){
      left()
      if(estFaceMur()){
        return true
      }
      else{
        return false
      }
    }
    if(getDirection()==Direction.OUEST){
      droite()
      if(estFaceMur()){
        return true
      }
      else{
        return false
      }
    }
    else{
      return false
    }
  }
  else{
    return false
  }
}