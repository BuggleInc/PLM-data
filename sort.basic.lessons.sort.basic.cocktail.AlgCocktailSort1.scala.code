def cocktailSort() {
	var flag = true;
	do {
		flag = true;
		for (i <- (0 to getNombreValeurs()-2)) {
				if (!plusPetit(i, i+1)) {
					echange(i,i+1);
					flag = false;
				}
		}
		for (i <- (getNombreValeurs()-2 to 0 by -1)) {
			if (!plusPetit(i, i+1)) {
					echange(i,i+1);
					flag = false;
				}
		}
	}
	while(!flag);
}
