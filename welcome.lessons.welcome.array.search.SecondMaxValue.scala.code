def max2Value(nums:Array[Int]): Int = {
  var maximum=nums(0)
  for (i<-0 to nums.length-1){
    if (maximum < nums(i)){
      maximum=nums(i)
    }
  }
  var i=0
  while (nums(i)>=maximum){
    i=i+1//on va chercher la premi?re valeur inf?rieur au max
  }
  var maximum2=nums(i)
  for ( j <-i to nums.length-1){
    if (nums(j)<maximum && nums(j)>maximum2){
      maximum2=nums(j)
    }
  }
  return maximum2
}