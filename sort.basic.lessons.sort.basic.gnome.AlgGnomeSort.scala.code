def gnomeSort() {
	var lgr = getNombreValeurs();
	var i = 0;
	var fin = false;
	
	while(!fin){
		if(i == 0)
			i += 1;
		else{
			if(i == lgr)
				fin = true;
			else{
				if(plusPetit(i-1, i+1))
					i += 1;
				else{
					echange(i-1, i+1);
					i -= 1;
				}
			}
		}
	}
}

