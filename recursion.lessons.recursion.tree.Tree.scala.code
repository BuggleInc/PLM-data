def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	while(steps!=0){
	avance(length:Double)
gauche(angle:Double)
avance(length*shrink)
recule(length*shrink:Double)
droite(2*angle:Double)
avance(length*shrink)
getX():Double
getY():Double
recule(length*shrink)
gauche(2*angle:Double)
avance(length*shrink)
tree(steps-1,length*shrink,angle/2,shrink*shrink)
}
}

