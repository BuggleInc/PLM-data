def cocktailSort() {
  
  var i:Int=0
  var cpt:Int=0
  var test:Boolean=false
  
	do{
    cpt=0
    for(i<-0 to getNombreValeurs()-2){
      if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt+=1
      }
    }
    for(i<- getNombreValeurs() to 0 by -1){
       if(plusPetit(i,i+1)==false){
        echange(i,i+1)
        cpt+=1
    }
    }
    
    if(cpt==1){
      test=true
    }

  }while(test==true)
  
  
}