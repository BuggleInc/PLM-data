def combSort() {
  var ecart=getNombreValeurs() 
  var s=false
  do{
    s=false
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    for (j <- ecart to getNombreValeurs()-1){
      if (!plusPetit(j, j+1) ){
        echange(j,j+1)
        s=true
      }
    }
  }while(ecart>1 || s)
}