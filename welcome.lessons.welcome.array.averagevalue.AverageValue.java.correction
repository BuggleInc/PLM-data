$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class averageValue extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( averageValue((int[])t.getParameter(0)) ); }
	int averageValue(int[] nums) {
		int total = 0;
		for (int i=0; i < nums.length; i++) 
			total += nums[i];
		return total / nums.length;
	}
 }