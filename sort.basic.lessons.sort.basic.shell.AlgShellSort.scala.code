def shellSort()  {

  val taille=getNombreValeurs()
  var temp=0
  var j=0
  var ecart=taille/2
  while (ecart>0) {
  for (i<-1 to taille-1){
    temp=getValeur(i)
    j=i
    while ((j>0)&&(getValeur(j-ecart)>temp)){
      setValeur(j,getValeur(j-ecart))
      j=j-ecart
      }
    setValeur(j,temp) }}}