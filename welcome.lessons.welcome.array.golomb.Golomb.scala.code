def compteur(nums:Array[Int], nb:Int): Int = {
  var compteur = 0
  for (k <- 0 to nums.length-1) {
    if (nums(k) == nb) {
      compteur = compteur + 1
    }
  }
  return compteur
}

def golomb(num:Int): Int = {
  var tab = new Array[Int](num)
  var repere = 0
  for (k <- 0 to num-1) {
    for (j <- repere to repere+tab(k)) {
      tab(j) = k
    }
    repere = k
  }
  return tab(num)
}