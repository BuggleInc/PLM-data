def combSort() {
  var ecart = getNombreValeurs()
  var est_triee = false
	do{
    est_triee = false
    if (ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i=0
    while(i+ecart<getNombreValeurs()){
      if (!plusPetitQue(ecart,ecart+i)){
        echange(i+ecart,ecart)
        est_triee = true
      }
      i=i+1
    }
  }while(ecart>1 || est_triee)
}