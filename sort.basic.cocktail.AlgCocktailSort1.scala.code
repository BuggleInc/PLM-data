def cocktailSort() {
  var c=1
  while(c==1) {
    c=0
  	for (i<-0 to getNombreValeurs()-2) {
    	if (plusPetit(i+1:Int,i:Int)) {
      	echange(i+1:Int,i:Int)
      	c=1
    	}
  	}
    for (i<-0 to getNombreValeurs()-2) {
    	if (plusPetit(getNombreValeurs()-1-i:Int,getNombreValeurs()-2-i:Int)) {
      	echange(i+1:Int,i:Int)
      	c=1
    	}
  	}
  }
}
