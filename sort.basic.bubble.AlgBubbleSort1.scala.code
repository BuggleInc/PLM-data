var nbValeurs=0
var fin = 0
var test = false

def bubbleSort()  {
  do {
      fin = 0 
      nbValeurs=getNombreValeurs()
      for (i <- 2 to nbValeurs-1){
            test = plusPetit(i-1,i)
            if(!test){
               echange(i-1,i)
               test = false
               fin = fin +1
            }
      }
  } while (fin != 0) 
}