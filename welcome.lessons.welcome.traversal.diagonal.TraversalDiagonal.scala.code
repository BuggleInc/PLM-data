var diag = 0;
override def run() {
	var x=0
	val gML=getMondeLargeur-1
	val gMH=getMondeHauteur-1	
	var i=0
	var j=0
	var y=1
	while (diag!=2*gML){
		do {
			setPos(i,j)
			ecritMessage(x)
			x+=1
			j-=1
			i+=1
		}while (i<=diag)
		if (diag<gML) {i=0;j=diag+1}
		else {i=diag-gML+1;j=gMH}
		diag+=1
	}
}
