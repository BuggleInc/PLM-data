def cocktailSort3() {
val n :Int = getNombreValeurs()
var debut:Int = 0
var fin : Int = n-2
var swapped=true
while(swapped) {
	
	swapped=false
for (i <- debut to fin) {
 	if (plusPetit(i+1,i)) {
 		echange(i+1,i)
 		swapped =true}}
 		fin -= 1
 		if (!swapped) {return}
for (k <- fin to debut by -1) {
 	if (plusPetit(k+1,k)) {
 		echange(k+1,k)
 		swapped=true}  
}
	debut +=1
	if (!swapped) {return}
}
}
