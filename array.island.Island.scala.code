def island(num:Array[Int]): Int = {
  var Etage=0      //variable augmentant a chaque mont?e ou descente
  for (k <- 1 to num.size){   //parcours de la liste
    if (num(k) <= num(k+1)){    //comparaison des elements de la liste deux a deux
      
      Etage = Etage + 1 
      
    }
    
  }
        return Etage
}