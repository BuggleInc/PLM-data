def bubbleSort()  
{
  for(i<-0 to (getNombreValeurs()-2)) 
  //on prend chaque élément de la liste pour le remonter
  	{
      var case=getValeur(i)
      var indice=i+1 //ce sera l'indice de l'élément auquel on va comparer i
      while(getValeur(i)==case)
      	{
          if (plusPetit(getValeur(indice),getValeur(i)))
      			{
          		echange(i,indice)
        		}
          indice+=1
        }
		}
}