def stringX(str:String):String =
{
  var s=""
  var  suppression=false;
  for(i <- 0 to str.length)
  {
    
    if((str.drop(i).take(1)!="x" && suppression) || !suppression)
    {
      s+=str.drop(i).take(1)
    }
    
    if(str.drop(i).take(1)!="x" && !suppression)
    {
      suppression=true
    }
  }

  return s
}