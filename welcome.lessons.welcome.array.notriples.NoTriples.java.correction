$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class noTriples extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( noTriples((int[])t.getParameter(0)) ); }
	boolean noTriples(int[] nums) {
		
		
		for (int i=0; i < (nums.length-2); i++) {
			int first = nums[i];
			if (nums[i+1]==first && nums[i+2]==first) return false;
		}

		
		return true;
	}
 }