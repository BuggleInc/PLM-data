def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	var a =60 
	var b = 80
	var c = isBirthday
	var v = speed
	if( c == false){
		if( v <= a){
			return 0
		}else{
			if(v>a && v<= b){
				return 1
			}else{
				return 2
			}
		}
	}else{
		if( v <= (a +5) ){
			return 0
		}else{
			if(v>(a+5) && v<= (b+5)){
				return 1
			}else{
				return 2
			}
		}
	}
}
