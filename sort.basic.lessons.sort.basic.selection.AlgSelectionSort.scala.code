def selectionSort() {
  var smallestValueIndex:Int = 0
  var i:Int = 0
  while(i < getValueCount() - 2){
    smallestValueIndex = i
    for(j <- i to getValueCount() - 2)
    	if(getValue(smallestValueIndex) > getValue(j))
    		smallestValueIndex = j
    if(smallestValueIndex != i)
    	swap(i, smallestValueIndex)
    i += 1
  }
}