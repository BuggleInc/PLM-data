  override def initialize() {
  	getLandingZone()
  }

  override def step() {
  }

def getLandingZone():Segment = {

var sol=getSol()
var pos:Double= 0
var premier = Point(0,0)
var deuxieme = Point(0,0)

	for(i<-0 to getSol.length-2){
		premier = sol(i)
		deuxieme = sol(i+1)
		if(premier.y==deuxieme.y){
			return Segment(premier,deuxieme)
		}
	}
	return Segment(premier,deuxieme)
}
