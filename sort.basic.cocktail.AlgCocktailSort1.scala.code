var isOver : Boolean = true
var isOver2 : Boolean = true

def cocktailSort() {
  while (isOver == true || isOver2 == true){
    isOver = false
    isOver2=false
  	for (i <- 0 to getValueCount()-2){
   	 if (isSmaller(i,i+1)==false){
    	  swap(i,i+1)
       isOver = true
   	 }
   for (i <- getValueCount()-2 to 0){
     if (isSmaller(i,i+1)==false){
       swap(i,i+1)
       isOver2 = true
     }
           
     }
   }
            }
            }
