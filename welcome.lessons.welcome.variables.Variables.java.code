public void run() {
int stepAmount=0;
int doneSteps=0;
	while (isOverBaggle() == false && isCarryingBaggle() == false) {
		forward();
		stepAmount = stepAmount + 1;
		if (isOverBaggle() == true) {pickupBaggle();}
		}
		while (isCarryingBaggle() == true && stepAmount != doneSteps){backward();}
		dropBaggle(); 
}

/* isFacingWall() isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().*/
