def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps ==0) {return}
  
  current(steps)
  avance(length)
  droite(angle)
  tree(steps-1,length*shrink,angle,shrink)
  gauche(angle)
  recule(length)
  gauche(angle)
  tree(steps-1,length*shrink,angle,shrink)
  droite(angle)
  recule(length)
}