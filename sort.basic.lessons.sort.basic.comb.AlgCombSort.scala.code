def combSort() {
	var ecart = getNombreValeurs()-1
	val l = getNombreValeurs()-1
	var a = l
	do {
		a = 0
		if (ecart >= 1) {
			ecart = (ecart.toDouble / 1.3).toInt
		}
		var i = 0
		while ( (i + ecart) < l) {
			if (plusPetit(i+ecart,i)) {
				echange(i,i+ecart)
				a +=1
			}
			i += 1
		}
	} while ( (ecart > l) || (a != 0))
}
