override def run() {
  while(!posFinale()){posSuivante()}
}

def posFinale():Boolean ={
  var z:Boolean = false
  if(getX() == getMondeLargeur()-1 && getY() == getMondeHauteur()-1){z = true}
  return(z)
}

def posSuivante(){
  var i:Int = 0
  val x:Int = getMondeLargeur()
  val y:Int = getMondeHauteur()
  ecritMessage(i)
  while(getX() <= x-1){
    i += 1
    if(getY() < y-1){setY(getY()+1)}
    else if(getY() == y-1 && getX()! = x-1){setPos(getX()+1,0)}
    ecritMessage(i)
  }
}