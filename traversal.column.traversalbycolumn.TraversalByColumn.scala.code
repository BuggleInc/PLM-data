override def run() {
  var x = 0
  var y = 0
  var num = 0
  do{
   	ecritMessage(num)
    num = num+1
    if(y<6){
      y = y +1
      setPos(x,y)
    }else{
      y=0
      x = x+1
      setPos(x,y)
    }
  }while(!posFinale(x,y)) 
}

def posFinale(x:Int,y:Int): Boolean ={
    return ((getX() == 6) && (getY()== 6))
}

