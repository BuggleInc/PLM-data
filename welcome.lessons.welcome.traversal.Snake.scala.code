  }
  right()
}

def snake(){
  	
  	while(!estFaceMur()){
    	avance()
  	}
  	if (getDirection()== Direction.EST){
    	left()
    	if(!estFaceMur())
    	avance()
  	}
  	if (getDirection()== Direction.OUEST){
    	right()
    	if (!estFaceMur())
    	avance()
  	}
  right()
  if(estFaceMur()){
    right()
    right()
  }  
}

def posf():Boolean ={
