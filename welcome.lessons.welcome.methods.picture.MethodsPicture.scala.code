def faireV(c:Color) {
	setCouleurBrosse(c);
	avance();
	baisseBrosse();
	leveBrosse();
	avance();
	gauche();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
	droite();
	avance();
	baisseBrosse();
	leveBrosse();
	avance();
}

override def run() {
	for (x<-0 to 2) {
		for (x<-0 to 2) {
			faireV(Color.yellow);
			gauche();
			faireV(Color.red);
			gauche();
			faireV(Color.blue);
			gauche();
			faireV(Color.green);
			gauche();

			avance(5);
		}
		gauche();
		avance(5);
		gauche();
	}
}
