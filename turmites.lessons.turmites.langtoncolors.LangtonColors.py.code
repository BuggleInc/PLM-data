def setCouleurSol(couleur):
    setCouleurBrosse(couleur)
    baisseBrosse()
    leveBrosse()
def step(rule, colors):
    a=getCouleurSol()
    i=0
    while a != colors[i]:
        i=i+1
    j=i+1
    if j>len(colors):
        j=0
    if rule[i]=='L':
        setCouleurSol(colors[j+1])
        gauche()
        avance()
        
    elif  rule[i]=='R':
        setCouleurSol(colors[j+1])
        droite()
        avance()

