def cocktailSort() {
  var len:Int = getNombreValeurs()
  var fin:Boolean = false
  do {
    fin = true
    for (i <- 0 to (len - 2)) {
      if (plusPetit((i+1),i)) {
        echange(i,(i+1))
        fin = false
      }
    }
    for (i <- (len - 2) to 0 by -1) {
      if (plusPetit((i+1),i)) {
        echange(i,(i+1))
        fin = false
      }
    }
  } while (!fin)
}