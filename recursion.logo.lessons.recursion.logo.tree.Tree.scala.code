def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if (steps < 1){return}
  current(steps)
  avance(length)
  droite(angle)
  subtree(steps-1,length*shrink,angle,shrink)
  current(steps)
  recule(length*shrink)
  gauche(angle*2)
  subtree(steps-1,length*shrink,angle,shrink)
  current(steps)
  recule(length*shrink)
  droite(angle)
  
}