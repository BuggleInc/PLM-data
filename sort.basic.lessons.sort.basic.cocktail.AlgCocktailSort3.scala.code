def cocktailSort3() {

var debut = 0
var fin = getNombreValeurs()-2
var f = false
while(f == false){
	f = true

	for(i <- debut to fin){
		if(getValeur(i)>getValeur(i+1)){echange(i,i+1); f = false}
	}
	if (f == true){return()}
	fin -= 1
	f = true
	for(i <- fin to debut by -1){
		if(getValeur(i)>getValeur(i+1)){echange(i,i+1); f = false}
	}
	debut += 1
	if(f == true){return()}
}

	
}
