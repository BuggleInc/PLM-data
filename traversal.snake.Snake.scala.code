override def run() {
  brushDown()
  while (! isInPosition() ) {
    if (isFacingWall() ) {
      doTheMove()
    }
    else {forward()}
  }
}


def isInPosition():Boolean = {
  if ((getDirection() == Direction.EAST) && isFacingWall() ) {
    left()
    if (isFacingWall()) {right; return true}
    else return false
  }
  else return false
}

def doTheMove() {
  if (getDirection() == Direction.EAST) {
    left()
    forward()
    left()
  }
  else {
    right()
    forward()
    right()
  }
}