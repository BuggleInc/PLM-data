override def run() {
var c:Int=0


	do {
		for (x<-0 to getMondeHauteur()-1)
		{
			setPos(x,0)
			ecritMessage(c)
			c=c+1
		}
		setPos(6,1)
		for (y<-0 to 2)
		{
			for (x<-(0 to getMondeHauteur()-1).reverse)
			{
				setPos(x,y)
				ecritMessage(c)
				c=c+1
			}
			
			for (x<-0 to getMondeHauteur()-1)
			{
				setPos(x,y+1)
				ecritMessage(c)
				c=c+1
				
			}
	
		}
		}
	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-2
		)
	
}
