public void run() {
	mouseMaze();
}

public void mouseMaze(){
	
	do{
		switch(random3()) {
			case '0' :
				left();
				break;
			case '1' :
				right();
				break;
			case 2 :
				if(!isFacingWall() ){
					forward();
				}
				break;
		}
		
		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());

}//end mouseMaze
