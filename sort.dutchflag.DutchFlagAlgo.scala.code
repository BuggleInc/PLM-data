def solve() {
  var lgr = getTaille();
  var fini = false
  while(!fini){
    var bleu = 0;
  	var blanc = 0;
  	var rouge = 0;
    fini = true;
    for(i<-bleu to lgr-rouge-1){
     
    if (getCouleur(i) == BLEU){
	  echange(i,bleu+1);
  	bleu += 1;
	} else {
  	if(getCouleur(i) == ROUGE){
    	echange(i,lgr-rouge-1);
    	rouge += 1;
  	} else {
    	echange(i,bleu+blanc+1);
    	blanc += 1;
    }
  }
}
  }
}





