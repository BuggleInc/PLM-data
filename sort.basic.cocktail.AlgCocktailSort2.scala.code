def cocktailSort2() {
  var n=2
  var a=0
  var m=1
  while(n!=getNombreValeurs && m!=0){
    m=0
  for (i <- a to getNombreValeurs()-n){
    if (plusPetit(i+1,i)==true){
      echange(i+1,i)
      m=m+1
    }
  }
    n=n+1
  for (i <- getNombreValeurs()-n to a by -1){
    if (plusPetit(i+1,i)==true){
      echange(i+1,i)
      m=m+1
    }
  }
    a=a+1
}
}