def combSort() {
  
  var len:Int = getNombreValeurs()
  var end:Boolean = false
  var ecart:Int = len
  var i:Int = 0
  
  do {
    end = true
    if (ecart > 1) {
      ecart = (ecart.toDouble / 1.3).toInt 
    }
    i = 0
    while ((i + ecart) < len) {
      if (plusPetit((i + ecart),i)) {
        echange(i,(i + ecart))
        end = false
      i += 1
      }
    }
  } while ((ecart > 1) || (!end))
}