def combSort() {
	val n:Int = getNombreValeurs()
	var yaw:Int = n
	var swapped:Boolean = true

	do {
		if (yaw>1)
			yaw = (yaw.toDouble/1.3).toInt
		else
			return
		swapped = false
		for (i <- 0 to n-yaw) {
			if (plusPetit(i+yaw, i)) {echange(i, i+yaw); swapped = true}
		}
	} while (swapped)
}
