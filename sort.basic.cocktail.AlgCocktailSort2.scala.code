def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs() -2
  var echanger : Boolean = true
  while(echanger == true) {
    echanger = false
    for (i<- debut to fin) {
      if (plusPetit(i+1,i)) {
        echange(i+1,i)
        echanger = true
      };fin -= 1
    };for (i<- debut to fin) {
      if (plusPetit(fin+1-i, fin -i)) {
        echange(fin+1-i, fin -i)
        echanger = true
      };debut +=1
      
      
    }
    
  }
}