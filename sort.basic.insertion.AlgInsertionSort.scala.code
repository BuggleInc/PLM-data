def insertionSort() {
  var n:Int = getValueCount();
  var v:Int = 0;
  var j:Int = 0;
  var lastOne:Int = 0;
  
  for (i<-1 to n-1) {
  
    v = getValue(i);
    j = i;
    
    while (j!=0) {
      
      if (isSmaller(v,getValue(j-1))) {
      	copie(j-1,j);
        lastOne = j-1;
      }
      
      j =  j-1;
    }
  
    setValue(lastOne,v);
  }
}

