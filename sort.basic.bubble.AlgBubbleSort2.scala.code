def bubbleSort()  {
  val dim : Int = getNombreValeurs()
  var echangeFait : Boolean = true
  var compteur : Int = 0
  while (echangeFait) {
    echangeFait = false
    for (i <- 0 to (dim - 2 - compteur)) {
      var a = getValeur(i)
      var b = getValeur(i+1)
      if (a >= b) {
        echange(i, i + 1)
        echangeFait = true
      }
    }
  	compteur = compteur + 1
  }
}