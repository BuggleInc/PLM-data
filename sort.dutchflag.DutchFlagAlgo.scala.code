def solve() {
  while(!estTrie()){
    insertionSort()
  }
}

def insertionSort() {
  var n:Int=getTaille()
  for ( k <- 1 to (n-1)){
    var stock:Int=getCouleur(k)
    var l:Int=k-1
    if (stock==BLEU){
      while((l>=0) && getCouleur(l)!=BLEU){
        l-=1
      }
      echange(k,l+1)
    } else {
      if(stock==ROUGE){
        while((l>=0) && getCouleur(l)!=ROUGE){
          l-=1
        }
        echange(k,l+1)
      }
    }

  }
}