def combSort() {
  var ecart = getNombreValeurs()
  var change=true
  while (ecart >1 || change){
    if (ecart>1){
      ecart =(ecart.asInstanceOf[Double]/1.3).asInstanceOf[Int]
    }
    var i=0
    change=false
    while (i+ecart<getNombreValeurs()){
      if (getValeur(i)>getValeur(i+ecart)){
        echange(i,i+ecart)
        change=true
      }
      i=i+1
    }
  }
}
