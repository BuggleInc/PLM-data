var NbB : Int=0


while( !sortieTrouvee() ) {
	
		do {
			avance()
		if (estSurBiscuit) {
			NbB= NbB+1
		}
		} while (!croisement())
	
		if (estSurBiscuit && croisement) {
			NbB = NbB+1
		}
		
		if (croisement && NbB <= 2) { 
		droite()
		NbB =0
		}
		
		if (croisement && NbB >= 3) {
		gauche()
		NbB =0
		}
	}
		
		
