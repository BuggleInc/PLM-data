def array123(nums:Array[Int]): Boolean = {
	
	if(indexOfValue(nums,2)!=(-1) && nums(indexOfValue(nums,2)-1)==indexOfValue(nums,1) && nums(indexOfValue(nums,2)+1)==indexOfValue(nums,3)){
			return true
	}else{
		return false
	}
}




def indexOfValue(nums:Array[Int] ,value:Int): Int = {
	var i = 0
	while(i < nums.length && nums(i) != value){
  	i = i + 1
  }
  if(i < nums.length){
  	return i 
  }else{
  	return -1
  }
}



