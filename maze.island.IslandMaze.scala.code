override def run() {
  var dir:Direction=Direction.NORTH
  while (getY()!=0) {
  	while(porteBiscuit()==false) {
      dir=getDirection()
      setDirection(Direction.NORTH:Direction)
      while (estFaceMur()==false) {
        avance()
      }
    	keepHandOnSideWall()
    	if (estSurBiscuit()==true) {
      	prendBiscuit()
    	}
      
  	}
  }
  while(porteBiscuit()==false) {
    	keepHandOnSideWall()
    if (estSurBiscuit()==true) {
      	prendBiscuit()
      }
  } 	
}


def keepHandOnSideWall() {
  gauche()
  if (estFaceMur()==true) {
    droite()
    if (estFaceMur()==false) {
    avance()
    }
    else {
      droite()
    }
  }
  else {
    avance()
  }  
}