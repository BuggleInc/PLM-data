def golomb(num:Int): Int = {
  if ( num == 1 ){
    return 1;
  }
  var c:Int = 0;
  var T1 = new Array[Int](num);
  var T2 = new Array[Int](num);
  for ( i <- 1 to num ){
    T1(i-1) = i;
  }
  for ( i <- 1 to num ){
    if ( c >= num ){
      return T2(num);
    }
    while( T1(i-1) != 0 ){
      T2(c) = i;
      c += 1
      T1(i-1) = T1(i-1)-1;
    }
  }
  return T2(num);
}
  