var isOver : Boolean = true

def cocktailSort() {
  while (isOver == true){
    isOver = false
  	for (i <- 0 to getValueCount()-2){
   	 if (isSmaller(i,i+1)==false){
    	  swap(i,i+1)
       isOver = true
   	 }
   for (i <- 0 to getValueCount()-2){
     if (isSmaller(getValueCount()-i-1,getValueCount()-i)==false){
       swap(isSmaller(getValueCount()-2-i,getValueCount()-2-i+1))
       isOver = true
     }
           
   }
            }
            }
}

