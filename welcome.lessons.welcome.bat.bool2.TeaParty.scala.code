def teaParty(tea:Int, candy:Int): Int = {
  var outcome=0
  if (tea>=5 & candy>=5 & tea<2*candy & candy<2*tea) {
    outcome=1
  } else {
    if (tea>= 2* candy & candy>=5 & tea >=5) {
      outcome=2
    } else {
      if (candy>= 2*tea & candy>=5 & tea>=5) {
        outcome=2
      } else {
        outcome=0
      } 
    }
  }
  return outcome
}
