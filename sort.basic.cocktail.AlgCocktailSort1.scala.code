def cocktailSort()
{
  var n = getNombreValeurs()-2;
  var isEchange = false;
  var j = n;
  
  do
  {
		isEchange = false;
    
    for (i <- 0 to n)
    {
      if (plusPetit(i+1, i))
      {
        echange(i, i+1);
        isEchange = true;
      }
    }

    j = n;
    
    while (j>=0)
    {
      if (plusPetit(j+1, j))
      {
        echange(j, j+1);
        isEchange = true;
      }
      j-=1;
    }
  }
  while(isEchange)
}