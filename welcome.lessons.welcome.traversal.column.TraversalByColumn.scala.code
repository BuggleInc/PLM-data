override def run() {

def posFinale():Boolean={
	if((getX()==getMondeLargeur()-1) && (getY() == getMondeHauteur()-1)){true}else{false}
}
var x = 0
while(!posFinale()){
	for(c<-0 to getMondeLargeur() - 1){
		setPos(c,0)
		for (l <- 1 to getMondeHauteur()-1){
			ecritMessage(""+x)
			x = x+1
			setPos(c,l)
		}
		ecritMessage(""+x)
		x = x+1
		
	}
}

}
