def gnomeSort() {
	var sorted : Boolean = false;
	var x=1;
	do {
		if (x==0) {
			x=x+1;
		}
		if (plusPetit(x-1,x)) {
			x=x+1;
		}
		else {
			echange(x-1,x);
			x=x-1;
			sorted = false;
		}
		if (x==getNombreValeurs()) {
			 x=x-1;
		}
		if (x==getNombreValeurs()+1) {
			return;
		}
	} while (!sorted);
}
