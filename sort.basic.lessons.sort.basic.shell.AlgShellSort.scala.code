def shellSort() {
  var len = getValueCount();
  var ecart = len/2;
  
  while(ecart > 0){
   
    for(i <- ecart-1 to len-1) {
   
      var v = getValue(i);
 			var place = i;
      
      while(place > 0 && place-ecart >= 0 && v < getValue(place - ecart)) {
        copy((place-ecart), place);
        place -= ecart;
        
      }

      setValue(place , v);
    }   
    ecart = ecart/2;
  }
}