override def run() {
var n =  (getMondeHauteur()) ;
var T = new Array[Color](n);
for (i <- 0 to n-1) {
	T(i) = getCouleurSol();
	avance();
}
for (j <- 0 to n-2) {
	gauche();
	avance();
	droite();
	var aux = T(n-1);
	for (c <- 1 to n-2) {
		var a = T(c) ;
		var b = T(c+1);
		T(c) = T(c-1);
		T(c+1) = a;
		T(c+2) = b;
	}
	T(0) = aux ;
		for (k <- 0 to n-1) {
		setCouleurBrosse(T(k))
		baisseBrosse();
		leveBrosse();
		avance();
	}
	
}
}
