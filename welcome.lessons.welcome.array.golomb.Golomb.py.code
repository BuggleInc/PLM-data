def golomb(num):
    a=[0,1]
    nRang = 1
    for b in range(num-1):
        a.append(0)
    for n in range(2,len(a)):
       nValeurs=0
       for i in a:
           if i == n-nRang:
               nValeurs +=1
       if nValeurs < a[n-nRang]:
           a[n]=n-nRang
           nRang +=1
       elif nValeurs == a[n-nRang]:
           a[n] =n
           nRang=1
    return a[num]