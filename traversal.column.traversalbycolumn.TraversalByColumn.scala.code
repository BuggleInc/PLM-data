override def run() {
  var finalpos : Boolean = false
  var counter : Int = 0
  while (finalpos==false){
    for (a <- 0 to 7){
      for (b <- 0 to 7){
        if (a==7 && b==7){
          setPos(a,7-b)
          writeMessage(counter)
          finalpos=true
        }
        else {
          setPos(a,7-b)
          writeMessage(counter)
          counter+=1
        }
      }
    }
  }
}