public void run() {
		hunt((Color) getParam(0)); 
	}

	/* BEGIN TEMPLATE */
	public void hunt(Color c) {
		
		/* BEGIN SOLUTION */
		while (! isOverBaggle()) {
			if (isFacingTrail(c)) {
				brushDown();
				forward();
				brushUp();
			} else {
				left();
			}
		}
		pickupBaggle();
		/* END SOLUTION */
	}

    

