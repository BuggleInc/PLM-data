def cocktailSort() {
	var isechange = false
	do {
		isechange = false
		for (i<- 0 to getNombreValeurs() - 2) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i)
				isechange = true
			}
		}
		for (i<-getNombreValeurs()-2 to 0 by -1) {
			if (plusPetit(i,i-1)) {
				echange(i,i-1)
				isechange = true
			}
		}
	} while(isechange)
}
