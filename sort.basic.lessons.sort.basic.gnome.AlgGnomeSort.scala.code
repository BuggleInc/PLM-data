def gnomeSort() {
  var position:Int=0
  while(position!=8) {
    if (position ==0) {position+=1}
    else if (plusPetit(position+1,position+2)) {position+=1}
    else {echange(position+2,position+1)
          position-=1}}
  if ( plusPetit(position,position+1)) {position+=1}
  else {echange()
       position+=1}
}