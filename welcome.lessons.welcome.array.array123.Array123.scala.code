def array123(nums:Array[Int]): Boolean = {
  var L = nums.length
  var possible = (L-3)
  
  for (i <- 0 to (L-1)) {
  if (possible>=0) {
    if (nums(i)==1) {
      if (nums(i+1)==2) {
        if (nums(i+2)==3) {
          return true
          
        }
      }      
    }
  }
  
}
  return false
}