override def run() {
  motif()
}

def dessine(){
  baisseBrosse()
  leveBrosse()
}

def faireDess(c:Color){
  avance()
  setCouleurBrosse(c)
  dessine()
  avance()
  left()
  avance()
  dessine()
}

def motif(){
  avance()
  faireDess(Color.BLUE)
  faireDess(Color.GREEN)
  faireDess(Color.YELLOW)
  faireDess(Color.RED)
  avance(2)
}
