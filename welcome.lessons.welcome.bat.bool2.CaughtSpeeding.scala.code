def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if(isBirthday==true){if(speed>60&&speed<81){return 1}
	                     if(speed>80){return 2}
	                     else{return 0}
	}else{if(speed>65&&speed<86){return 1}
	                     if(speed>85){return 2}
	                     if(speed<66){return 0}
}
}