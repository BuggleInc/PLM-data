def combSort() {
  var switch = false
  var ecart = getNombreValeurs()
  do {
    switch = false
    if (ecart > 1) {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    var i = 0
    while (i + ecart < getNombreValeurs()) {
      if (!plusPetit(i, i+ecart)) {
        echange(i, i+ecart)
        switch = true
      }
      i = i + 1
    }
  } while (ecart > 1 || switch)
}
