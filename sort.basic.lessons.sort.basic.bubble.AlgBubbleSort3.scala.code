def bubbleSort3() {
val p=getNombreValeurs()
var swap=true
var parc=true
do {
	parc=false
	swap=false
	for (i<-p-2 to 0 by -1){
	for (j<-0 to i){
			if (plusPetit(j,j+1)==false){
				echange(j,j+1)	
				swap=true	
				parc=true 
	}else parc = false
	if (parc==false){
		return
	}
		
	}
}
}while (swap==true)
}

