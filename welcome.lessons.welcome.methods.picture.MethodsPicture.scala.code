def makeV() {
	drawCase()
  forward()
  left()
  forward()
	drawCase()
  backward()
  right()
  forward
	drawCase()
}

def drawCase(){
  brushDown()
  brushUp()
}


def makePattern() {
  
  for (i <- 1 to 4){
    forward(1)
    makeV()
    forward()
    left()
  }
  
  forward(5)
}


override def run(){
 
  for (i <- 1 to 3){
    makePattern()
    left()
    forward(5)
    right()
  }
  
}