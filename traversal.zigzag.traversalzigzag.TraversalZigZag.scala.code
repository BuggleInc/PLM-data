override def run() {
  var w = getWorldWidth();
  var h = getWorldHeight();
  
  var count:Int = 0;
  var direction:Int = 1;
  
  var x:Byte = 0;
  var y:Byte = 0;
  
  
  do {
    setPos(x,y);
    writeMessage(count);
    
    count += 1;
    x = byte(int(i) + direction);
    
    if (x < 0) {
      y += 1;
      x = 0;
      direction *= -1;
    } else if (x >= w) {
      y + = 1;
      x = w - 1;
      direction *= -1;
    }
    
  } while (!isOnFinalDestination());
}

def isOnFinalDestination() : Boolean = {
  return getX() == getWorldWidth() && getY() == getWorldHeight();
}