def bubbleSort3() {
  var x:Int =getNombreValeurs()
  var t:Int =0
  while (t==0) {
  for (i<-x-2 to 0 by -1) {
    for (j<-0 to i) {
      if (plusPetit(j,j+1)==false) {
        echange(j,j+1)
        t=t+1
      }
    }
    if (t==0) {
      t=1
    }
    else t=0
  }
  }
}
    
    
        
      