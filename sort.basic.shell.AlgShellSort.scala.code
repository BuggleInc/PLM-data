def shellSort()  {
  var i=0
  var v=0
  var ecart=getNombreValeurs()/2
  while(ecart>0){
    for(i <- 0 to getNombreValeurs()-ecart){
      if(plusPetit(i,i+ecart)==false){
        v=getValeur(i)
        for(j <- i to i+ecart-1){
          copie(j+1,j)
        }
        setValeur(i+ecart,v)
      }
    }
    ecart=ecart/2
  }
}
    
    
    
