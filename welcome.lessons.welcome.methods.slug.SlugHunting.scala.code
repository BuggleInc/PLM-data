def hunt() {
    // Write your code here
  while(!estSurBiscuit()) {
    if (isFacingTrail()) {
      baisseBrosse()
      avance()
      leveBrosse
    }
    else {
      while(!isFacingTrail()) {
        gauche()
      }
    }
  }
}

// Copy your isFacingTrail() here
def isFacingTrail():Boolean = {
  if (estFaceMur()) return false
  else {
    var vert=false
    avance()
    if (getCouleurSol()==Color.green) vert= true
    recule()
    return vert
  }
}