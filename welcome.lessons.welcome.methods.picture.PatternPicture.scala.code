override def run() {
	def mark() {
		baisseBrosse()
		leveBrosse()
	}
	def slash(c:Color) {
		setCouleurBrosse(c)
		mark()
		gauche()
		avance()
		gauche()
		avance()
		mark()
		gauche()
		avance()
		gauche()
		avance()
		avance()
	}
	def carre() {
		avance()
		slash(Color.red)
		slash(Color.blue)
		for (i <- 1 to 4) {
			recule()
		}
		gauche()
		avance()
		avance()
		droite()
		slash(Color.yellow)
		slash(Color.green)
		}
	carre()
}
