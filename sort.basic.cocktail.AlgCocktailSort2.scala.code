def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var ok:Boolean=false
  
  do{
    ok=true
    for(i<-debut to fin){
      if(getValeur(i)>getValeur(i+1)){
        echange(i,i+1)
        ok=false
      }
      
    }
    fin=fin-1
  
  	for(i<- debut to fin-1){
      if (getValeur(fin-i)<getValeur(fin-i-1)){
        echange(fin-i,fin-i-1)
        ok=false
      	} 
    	}
  	debut=debut+1
		} while(!ok)
}
