def island(num:Array[Int]): Int = {
  var l = num.length
  var niveauActuel = num(0)
  var nbNiveau = 0
  
  // En supposant que la difference de niveau est de 1 au max
  
  for (i <- 1 to l-1){
    if (num(i) > niveauActuel){
    	nbNiveau = nbNiveau + 1
      niveauActuel = niveauActuel + 1
    }
    
    else{
      if (num(i) < niveauActuel){
        niveauActuel = niveauActuel - 1
      }
      	
    }
  }
  
  
  return nbNiveau
}
