public void run() {
	int counter = 0;
	for (int x = 0; x < getWorldWidth() - 1; x++) {
		for (int y = 0; y < getWorldHeight() - 1; y++){
			setPos(x,y);
			writeMessage(x*7+y);
			counter++;
		}
	}
}




void boolean endPositionreached(){
	return (getX() == getWorldWidth() - 1 && getY() == getWorldHeight() -1);
}
