def golomb(num):
    a=[]
    for b in range(num):
        a.append(0)
    for n in range(1,len(a)):
        nValeursDeAn =0
        for i in a:
            if i == a[n-1]:
                nValeursDeAn +=1
        if nValeursDeAn < a[n-1]:
            a[n]=a[n-1]
        elif nValeursDeAn == a[n-1]:
            a[n] = a[n-1]+1
    return a[num]