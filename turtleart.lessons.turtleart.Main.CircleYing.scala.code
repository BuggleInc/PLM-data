override def run() {
	def polygone(taille:Int,cote:Int) ={
		for (i<-1 to cote) {
			avance(taille)
			droite(360/cote)
		}
	}
	def polygoneMoitie(taille:Int,cote:Int) ={
		for (i<-1 to cote) {
			avance(taille)
			droite(180/cote)
		}
	}
		polygone(2,360)
		polygoneMoitie(1,180)
		polygoneMoitie(-1,180)
}
