def bubbleSort()  {
  var i: Int = 0
  var j: Int = 0
  var lastEchange: Boolean = false
  do {
    for (compteur <- 1 to getNombreValeurs()-2) {
      i = getValeur(compteur)
      j = getValeur(compteur + 1)
      if (plusPetit(j,i)) {
        echange(i,j)
        lastEchange = true
      }
    }
  }
  while (lastEchange==true)
}