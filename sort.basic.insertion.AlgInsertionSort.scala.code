def insertionSort()
{
  var n = getNombreValeurs()-1;
  var v = 0;
  var x = 0;
  var j = 0;
  var dernier = 0;
  
  for (i <- 1 to n)
  {
    v = getValeur(i);
    x = 0;
    dernier = i;
    j = i;
    
    while (j>0 && x<v)
    {
      x = getValeur(j-1);
      if (x > v)
      {
        setValeur(j, x);
        dernier = j-1;
      }
      j-=1;
    }
    setValeur(dernier, v);
  }
}