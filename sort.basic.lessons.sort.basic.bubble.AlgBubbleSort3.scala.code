def bubbleSort3() {
  var change = true
  var L = getNombreValeurs()
  
  for (i<- L-2 to 0 by -1){
    while (change == true) {
    	L= L-1
      change = false
    	for (j<- 0 to i) {
        change = true
      	if (getValeur(j) > getValeur(j+1)) {
        	echange(j,j+1)
         
          
      	}
 	   	}
  	}
    return
	}
}
