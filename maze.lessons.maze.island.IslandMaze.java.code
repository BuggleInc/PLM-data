int dir = 0;

public void run() {
	searchWall();
	dir = 1;
	while(!isOverBaggle()) {
		keepHandOnSideWall();
	}
	pickUpBaggle();
}

void keepHandOnSideWall(){
	switch(dir){
		case 0:
			if(!isFacingWall()) {
				forward();
				left();
			}
			if(isFacingWall()){
				right();
			}
		break;
		case 1:
			while(!isFacingWall()){
				forward();
			}
			right();
			dir = 0;
		break;
	}
}

void searchWall() {
	while(!isFacingWall()){
		left();
	}
	//right();
}
