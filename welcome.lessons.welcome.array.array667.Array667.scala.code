def array667(nums:Array[Int]): Int = {
		var c = 0
	var r = 0
	
	while(nums.length > c)
	{
		
		if(nums(c) == 6 && c < nums.length)
		{
			 if (nums(c+1) == 6)
			 {
			 	r = r + 1
			 }
			 else if (nums(c+1) == 7)
			 {
			 	r = r + 1
			 }
			 else 
			 {
			 	
			 }
		}
		c = c+1
	}

	return r 
}
