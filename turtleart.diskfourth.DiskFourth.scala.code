override def run() {
  for(i<-1 to 360){
    
    i match{
      case 90 => qcircle(Color.red);
      case 180=> qcircle(Color.yellow);
      case 270=> qcircle(Color.magenta);
      case _ => qcircle(Color.black);
    }
  }
}
  def qcircle(c:Color){
    	setColor(c)
    	for(j<-1 to 90){
    		forward(100);
    		backward(100);
    		right(1);
      }
  }