public void run() { 
	while(getGroundColor()!=Color.green) {
		while(!isFacingWall()) {
			left();
			left();
			forward();
			while(!isOverBaggle()) {
				forward();
			}
			pickUpBaggle();
			left();
			left();
			while(!isOverBaggle()) {
				forward();
			}
			backward();
			dropBaggle();
		}
		if(isFacingWall()) {
			right();
			forward();
			left();
			forward();
		}
	}
}
