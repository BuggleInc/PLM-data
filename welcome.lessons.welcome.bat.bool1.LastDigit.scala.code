def lastDigit(a:Int, b:Int):Boolean = {
var rep = false
if( a >= 10 & a < 100 )
{
	if( b >= 10 & b < 100 )
	{
		if( a%10 == b%10 )
		{
			rep = true
			return rep 
		}
	}

	if( b < 10 )
	{
		if( a%10 == b )
		{
			rep = true
			return rep
		}
	}
}

if( b >= 10 & b < 100 )
{
	if( a >= 10 & a < 100 )
	{
		if( a%10 == b%10 )
		{
			rep = true
			return rep 
		}
	}

	if( a < 10 )
	{
		if( b%10 == a )
		{
			rep = true
			return rep
		}
	}
}