def haveBaggle():Boolean = {
  var vuBiscuit=false
  var compteur = 0
  while (vuBiscuit==false) {
    for (i<-1 to 6){
      avance()
      if (estSurBiscuit()){
        vuBiscuit=true
      }
    }
    if (vuBiscuit){
      for (i<- 1 to 6){
        recule()
      }
    }else{
      for (i<- 1 to 6){
        recule()
      }
      droite()
      avance()
      gauche()
