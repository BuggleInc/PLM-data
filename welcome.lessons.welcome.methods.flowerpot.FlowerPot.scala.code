
Color.PINK pour le rose

Color.ORANGE pour le orange
Color.GREEN pour le vert
Color.YELLOW pour le jaune

def marqueSol(c: Color) {
	setCouleurBrosse(c);baisseBrosse();leveBrosse();
}

def fleur(c: Color) {
	marqueSol(c);
	avance();
	marqueSol(Color.yellow);
	droite();
	avance();
	marqueSol(c);
	recule();
	recule();
	marqueSol(c);
	avance();
	gauche();
	avance();
	marqueSol(c);
}

def growFlowers() {
	fleur(Color.red);
	avance();
	avance();
	marqueSol(Color.cyan);
	
}
