def gnomeSort() {
  var nain = 0
  var lgr = getNombreValeurs()
  var test = false
  while (nain != lgr-1){
    test = plusPetit(nain,nain+1)
    if(test){
      nain = nain +1 
    } else {
      echange(nain , nain +1)
      if (nain ==0){
        nain=nain+1
      }else {
        nain=nain-1
      }
    }   
  }
  
}