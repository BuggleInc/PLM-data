boolean isFacingTrail() {
boolean greenCell = false;

    		if(isFacingWall()){
    			greenCell = false;
    		}		

		if(getGroundColor().equals(Color.green)){
    			greenCell = true;
    		}

		if(!getGroundColor().equals(Color.green)){
			greenCell = false;
		}    		

    		if(isOverBaggle()){
    			greenCell = true;
    		}
forward();
    return greenCell;
}//end function 
