def cocktailSort() {
  var end:Boolean = true
  while (end) {
    end = false
    for (i <- 0 to getNombreValeurs() - 2) {
      if (getValeur(i) > getValeur(i+1)) {
        echange(i,i+1)
        end = true
      }
    }
    for (j <- 0 to getNombreValeurs() - 3) {
      if (getValeur(getNombreValeurs()-2-j) < getValeur(getNombreValeurs()-2-(j+1))) {
        echange(getNombreValeurs()-2-j,getNombreValeurs()-2-(j+1))
        end = true
      }
    }
  }
}
