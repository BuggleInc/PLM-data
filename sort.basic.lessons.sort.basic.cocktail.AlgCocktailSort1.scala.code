def cocktailSort() {
  var n = getValueCount();
  var fin = false;
  
  while(!fin) {
    fin = true;
    
    for(i <- 0 to n-2) {
      if(getValue(i) > getValue(i+1)){
        swap(i,i+1);
        fin = false;
      }
    }
    
    for(i <- n-2 to 0 by -1){
      if(getValue(i) > getValue(i+1)){
        swap(i , i+1);
        fin = false;
      }
    }
  }  
}