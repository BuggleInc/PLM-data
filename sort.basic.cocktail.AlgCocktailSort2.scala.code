def cocktailSort2() {
  var a = 0
  var b = getValueCount() - 2
  var i = 0
  var t = false
  do {
  t = true
    
    
  i = 0  
  for (i <- a to b) {
    if (isSmaller(i+1,i)){
      
      t = false
      swap(i,i+1)}
    else {
      i+=1
    }
    
  } 
  i = 0
    while (i<b){
		if (isSmaller(b-i+1,b-i)){
      t = false
      swap(b-i,b-i+1)
      i = i+1
    }
    else{
      i= i +1
    }
    
  }
  a+=1
    
  b-=1   
    


    
  
} while (t == false)
}