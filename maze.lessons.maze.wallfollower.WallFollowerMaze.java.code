public void run() {
	keepHandOnSideWall();
}


void keepHandOnSideWall(){
	while(!overBaggle()){
		faceEmptyAndLeftWall();
		forward();	
		if(!wallOnLeft()){
			left();
			forward();
		}
	}
	
	faceEmpty();
	forward();
}


void faceEmpty(){
	while(isFacingWall())
		left();
}

void faceEmptyAndLeftWall(){
	while(isFacingWall() && !wallOnLeft())
		left();
}

boolean wallOnLeft(){
	left();
	boolean a = isFacingWall();
	right();
	return a;
}
