def gnomeSort() {
  var i=0

  var sorted=false
  while(!sorted){
    sorted=true

    if (i==getNombreValeurs()-1){
      sorted=true
    }
    else if (i==0){
      i=i+1
    }

    else (plusPetit(i+1,i+2)){
      i=i+1
      sorted=false
    }
    else{
      echange(i+1,i+2)
      i=i-1
      sorted=false
    }

  }
}