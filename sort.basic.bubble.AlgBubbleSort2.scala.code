def bubbleSort2()  {
  var inchange = true ;
  var perfect = false;
  var j = getNombreValeurs-2;
  while(!perfect){
    perfect = true
    for(i <- 0 to j){
      var k = getNombreValeurs-2-i
    	if (getValeur(k) > getValeur(k+1)) {
        echange (k, k+1);
        perfect = false;
      }
      
    }
    j -= 1;
  }
  for (i <- 1 to 20) getValeur(0);
}

