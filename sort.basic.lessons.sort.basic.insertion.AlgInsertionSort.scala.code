def insertionSort() {
  var v = 0
  var c = -1
  var k = 1
    for (i <- 2 to getNombreValeurs()-1) {
      c = 0
      v = getValeur(i)
      k = 1
      while ((i-k>0)&&(plusPetit(v, i-k))) {
        setValeur(i-k+1, getValeur(i-k));
        k=k+1;
        c=1
      }
      k=k-1
      if (c==1) {
        setValeur(i-k,v)
      }
    }
}