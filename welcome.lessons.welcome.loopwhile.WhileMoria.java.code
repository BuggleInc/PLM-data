public void run() 
{
	retourne();
	while(!estFaceMur())
	{
		avance();
		while(!estSurBiscuit() && !estFaceMur())
			avance();
		if(estSurBiscuit())
			prendBiscuit();
		if(porteBiscuit())
			recule();
		while(!estSurBiscuit())
			recule();
		avance();
		if(porteBiscuit())
			poseBiscuit();
	}
	droite();
	avance();
	gauche();
	avance();
}
