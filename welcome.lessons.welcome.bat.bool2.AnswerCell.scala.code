def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
  (isMorning,isMom,isAsleep) match {
    case (true,false,true) => return false
    case (false,false,true) => return false
    case (true,true,true) => return false
    case (false,true,true) => return false
    case (true,false,false) => return false
    case (false,true,false) => return true
    case (true,true,false) => return true
    case (false,false,false) => return true
  }
}