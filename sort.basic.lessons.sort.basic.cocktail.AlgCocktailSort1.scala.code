def cocktailSort() {
  val n=getNombreValeurs()
  var change1 =true
  var change2=true
  
  while (change1 || change2){
    
    change1=false
    change2=false
  for (i <- 0 to n-2 ){
    if (getValeur(i)>getValeur(i+1)){
      echange(i,i+1)
      change1=true
    }
  }
  for (j<- n-2 to 0 by -1){
    if (getValeur(j)>getValeur(j+1)){
      echange(j,j+1)
      change2=true
    }
  }
}
}