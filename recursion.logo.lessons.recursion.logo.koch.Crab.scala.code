def crab(levels:Int, length:Double) {
  gauche(45)
  if(levels!= 0){
    crab(levels-1, length/Math.sqrt(2))
  	gauche(60)
  	crab(levels-1, length/Math.sqrt(2))
    droite(120)
    crab(levels-1, length/Math.sqrt(2))
    gauche(60)
    crab(levels-1, length/Math.sqrt(2))
  }
  if(levels == 0){
    avance(length)
  }
}
