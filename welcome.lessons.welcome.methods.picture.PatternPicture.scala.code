override def run() {
  faireCarre()
}

  

def faireL(c: Color, d: Color) {
  avance()
  setCouleurBrosse(c)
  baisseBrosse()
  leveBrosse()
  avance()
  setCouleurBrosse(d)
  baisseBrosse()
  leveBrosse()
  avance()
  gauche()
}

def faireCarre() {
  faireL(Color.RED, Color.BLUE)
  faireL(Color.BLUE, Color.GREEN)
  faireL(Color.GREEN, Color.YELLOW)
  faireL(Color.YELLOW, Color.RED)
}