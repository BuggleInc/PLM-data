override def run() {
  for (i<- 1 to 4) {
  	maison(); forward(30); leveCrayon(); forward(20); baisseCrayon(); left(90);
  }
}


def maison() {
  forward(30); right(90); forward(30); left(120); forward(30); left(120); forward(30); left(30); forward(30); left(90); forward(30); left(90); forward(30); left(90); forward(30); left(90); forward(30); right(270); forward(30)
}