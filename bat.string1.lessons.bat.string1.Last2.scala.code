def last2(str:String):Int = {
  var str1 = str.charAt(str.length-1);
  var str2 = str.charAt(str.length-2);
  
  var nb:Int = 0;
  
  println(str);
  println(str.length);

  if(str.length>2){
    for(i<-0 to str.length-2){
      
      if(str.charAt(i)==str1 && str.charAt(i+1)==str2){
        nb+=1;
      }
    }
  }
  
  return nb;
}