def cocktailSort2() 
{
		var i : Int = 0;
		var echEffect : Int = 1;
		var n : Int = 0;
		var nfin : Int = 0;
		
	while (echEffect == 1)
	{
		echEffect = 0;
			
		while (i<getNombreValeurs()-1-nfin)
		{
			if(plusPetit(i+1,i))
			{
				echange(i,i+1);
				echEffect = 1;
			}
			i=i+1;
		}
		nfin = nfin + 1;
          i = i - nfin
	
		while(i>0+n)
		{
			if(plusPetit(i,i-1))
			{
				echange(i,i-1);
				echEffect = 1;
			}
			i=i-1;
		}
		n = n+1;
		
		
	}
}