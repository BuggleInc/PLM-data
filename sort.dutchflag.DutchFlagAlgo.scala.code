def solve() {
  var n :Int = getSize;
  var li:Int = getColor(0);
  var lj:Int = getColor(1);
  
  for (i<-0 to n-2) {
    li = getColor(i);
    for (j<-i+1 to n-1) {
      lj = getColor(j);
      if (lj != li && li !=Color.BLUE && lj != Color.RED) {
      	if ( (lj== Color.WHITE && li==Color.RED) || (lj== Color.BLUE)) {
          swap(i,j);
        }
        
      }
    }
  }
}