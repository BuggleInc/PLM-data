def cocktailSort() {
var changement:Boolean=true
while (changement) {
	changement=false
	for (j <- 0 to getNombreValeurs()-2) {
		if (plusPetit(j+1,j)==true) {
			echange(j,j+1)
			changement=true
			}
               }
	for(l<- getNombreValeurs()-2 to 0 by -1) {
		if (plusPetit(l+1,l)==true) {
			echange(l,l+1)
			changement=true
			}
			}
}
}

