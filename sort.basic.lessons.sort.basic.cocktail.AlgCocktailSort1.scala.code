def cocktailSort()
{

var trie: Int = 0;
while (trie == 0) {
   trie = 1;
 for(i <- 0 to (getNombreValeurs()-2))
       {if(getValeur(i+1) < getValeur(i))
        {  
  echange(i+1,i);
  trie = 0;
        }
      }
 
 for(i <- (getNombreValeurs()-2) to 0 by -1) {
  if(getValeur(i+1) < getValeur(i)) {
   echange(i+1,i);
   trie = 0;
  }
 }
}
 }

