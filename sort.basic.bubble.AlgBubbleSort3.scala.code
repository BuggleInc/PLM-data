def bubbleSort3() {
  var n : Int =getValueCount()
  
  var i : Int = n-1
  
  while(i >= 0 ){
    var inv : Boolean = true
    var j : Int = 0
    
    for(j<-0 to i-1){
      
      if( isSmaller(j+1,j)){
        inv=false
	    	swap(j,j+1)
      }
    }
    
    i=i-1
    if(inv)
    	return;
  }

}