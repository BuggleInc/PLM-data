override def run() {
  while(getY()!=0){
    trouverNord()
  }
  while(!verifierMurGauche()){
    gauche()
  }
  while(!estSurBiscuit()){
    keepHandOnSideWall()
  }
  prendBiscuit()
}

def keepHandOnSideWall(){
  if (estFaceMur()){
    verifierMurGauche() match{
      case true => droite();if(!estFaceMur()){avance()}
      case false => while (estFaceMur()){gauche()};avance()
    }
  }
  else{
  	verifierMurGauche() match{
      	case true => avance()
      	case false => gauche();avance()
  	} 
	}
}

def trouverNord(){
  setDirection(Direction.NORTH)
  while(!estFaceMur()){
    avance()
  }
  while(!verifierMurGauche()){
    droite()
  }
  while(getDirection!=Direction.NORTH){
    keepHandOnSideWall()
  }
}

def verifierMurGauche():Boolean={
  gauche()
  estFaceMur() match{
    case true => droite();return true
    case false => droite();return false
  }
  
}