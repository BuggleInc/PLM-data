def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {

	if (isMom==true && isAsleep==false)
	{
		return true
	}
	else
	{
		if (isAsleep==true)
		{
			return false
		}
		else 
		{
			if (isMom==false && isMorning==true)
			{
				return false
			}
			else
			{
				return true
			}
		}
	}
}
