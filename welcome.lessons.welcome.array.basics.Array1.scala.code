override def run() {
	def color() {
		getCouleurSol()
	}
	var T = new Array[Color](getMondeHauteur());
	for (i <- 1 to T.length) {
		T(i) = color(i)
	}
	for (i<- 1 to getMondeHauteur()){
		color()
		avance()
		T(i)=color(i)
	}
	for(i<-1 to getMondeHauteur()-i) {
		baisseBrosse()
		leveBrosse()
		avance()
	}
	
	
}
