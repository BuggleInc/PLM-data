// isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle(). 
public void run() { 
	// Turn around!
	right();
	right();

	// Find first baggle
	while (!isOverBaggle()) {
		forward();
	}

	// Move baggle
	pickupBaggle();
	right();
	right();
	while (isOverBaggle()) {
		forward();
	}
	
	dropBaggle();
}
