def combSort() {
  var ecart = getNombreValeurs()
  var echanges = true
  do{
    if(ecart>1){
      ecart = (ecart.toDouble/1.3).toInt
    }
    var i=0
    echanges = false
    for(i<- i + ecart to getNombreValeurs()-1){
      if(!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        echanges = true
      }
    }
  }while((ecart > 1) || (!echanges))
}