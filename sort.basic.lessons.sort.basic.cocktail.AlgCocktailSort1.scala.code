def cocktailSort() {
  var echanges = false
  do{
     echanges = true
     for(i<-0 to getNombreValeurs()-2){
    	 if(!plusPetit(i,i+1)){
      	echange(i,i+1)
        echanges = false
    	 }
  	 }
  	 for(i<-getNombreValeurs()-2 to 0 by -1){
    	 if(!plusPetit(i,i+1)){
      	echange(i+1,i)
        echanges = false
    	 }
  	 }
  }while(!echanges)
}
