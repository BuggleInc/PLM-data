def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
  if( die1==die2 && noDoubles == true){
    if (die1==6){
      die1==1
    }
    if (die2==6){
      die2==1
    }
    return die1+die2
  }
  else if (die1==die2 && noDoubles == false){
    return die1+die2
  }
  else {
    return die1+die2
  }
}