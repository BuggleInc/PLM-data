def bubbleSort3() {
  
  var n:Int = getNombreValeurs()
  var echange:Boolean = true
  var i:Int = 0
  
  while ((i <= n-2) && echange){
    echange = false
  
    for (j <- 0 to n-2-i){

      if(plusPetit(j+1, j)){
        var tampon:Int = getValeur(j)
        setValeur(j, getValeur(j+1))
        setValeur(j+1, tampon)
        echange = true
      }
    }
	}
  
}