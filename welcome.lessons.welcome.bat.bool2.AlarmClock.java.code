String alarmClock(int day, boolean vacation) {
   if (vacation   ){  if ( day = 0 || day = 6) return "off"; else return "10:00";}
   else {
  day match  {
   
    case 0 => return("10:00");
    case 1 => return("7:00");
    case 2 => return("7:00");
    case 3 => return("7:00");
    case 4 => return("7:00");
    case 5 => return("7:00");
    case 6 => return("10:00");}
    
    
    }













}
