def bubbleSort3()
{
  var n = getNombreValeurs()-1;
  var isEchange = true;
  var i = n;
  var j = n;
  
  while (i>0)
  {
    j = i-1;
    isEchange = false
    
    while (j>=0)
    {
      if (plusPetit(i,j))
      {
        echange(i,j);
        isEchange = true;
      }
      j-=1;
    }
    if (!isEchange) return;
    i-=1;
  }
}