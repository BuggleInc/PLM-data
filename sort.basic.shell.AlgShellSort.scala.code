def shellSort()  {
  var compt = 0 
  var v = 0
  var a = 0
  var long = getNombreValeurs()
  var ecart = long/2
  while(ecart > 0) {
    for (i<- ecart to long - 1) {
    	v = getValeur(i)
   		a = getValeur(i-ecart)
   		compt = 0
    while (a>v && i-(compt+1)*ecart> 0) {
      setValeur(i-(compt+1)*ecart,a)
      compt += 1
      if (i-(compt+1)*ecart>=0) a = getValeur(i-(compt+1)*ecart)
    }
    setValeur(i-compt*ecart,v)
  }
    ecart /= 2
  }
}