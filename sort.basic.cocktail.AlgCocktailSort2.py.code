n=getNombreValeurs()-1

debut=0
fin=n
trie=False

while trie==False:
  trie=True
  for i in range(fin):
    if plusPetit(i,i+1)==False:
      echange(i,i+1)
      fin=fin-1
      trie=False
  fin=n
  for i in range(fin,debut,-1):
    if plusPetit(i-1,i)==False:
      echange(i-1,i)
      trie=False
  if trie==True:
    break