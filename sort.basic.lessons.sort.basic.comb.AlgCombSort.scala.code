def combSort() {
  ecart = getValueCount();
  var i = O;
  do{
    if(ecart > 1){
      ecart = ecart / 1.3
      i = O;
      while(i + ecart < getValueCount()){
        if(isSmaller(i,i+ecart)){
          swap(i,i+ecart);
          i = i+1;
        }
        
      }
    }
  }
}
