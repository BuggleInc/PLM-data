def stringX(str:String):String = {
    
  println(str);
  
  var debut:String = "";
  var entre:String = "";
  var fin:String = "";
  
  for(i<-0 to str.length-1){
    if(str.charAt(i)!='x'){
      entre+=str.charAt(i);
    }
  }
  
  if(entre!="" && entre!=str){


    if(str.charAt(0)=='x'){
      debut="x";
    }

    if(str.charAt(str.length-1)=='x'){
      fin="x";
    }
    
  }
  
  
  println(debut+entre+fin);
  
  return debut+entre+fin;
}