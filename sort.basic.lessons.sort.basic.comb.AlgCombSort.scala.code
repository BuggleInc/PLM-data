def combSort() {
  var n= getNombreValeurs()
  var ecart = n
  var m = true
  do{ 
    if ( ecart>1){
      ecart = (ecart.toDouble / 1.3).toInt    
    }
    var i = 0
    m = false
    do{
      if( !plusPetit(i, i+ecart)){ 
        echange (i, i+ecart)
        
        m = true
      }
      i = i+1
    }while (i + ecart < n )    
 }while (ecart>1 || m)
}