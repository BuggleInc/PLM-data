def hunt(c:Color) {
  avance()
  while(!estSurBiscuit()){
    if (isFacingTrail()){
      avance()
      baisseBrosse()
      leveBrosse()
    }
    else {
      gauche()
    }
  }

  prendBiscuit()
}

def isFacingTrail():Boolean = {
  var k=false
  var c=getCouleurSol()
  if (!estFaceMur()){
    avance()
    if (getCouleurSol()==c){
      k=true
    }
  }
  return k 
}