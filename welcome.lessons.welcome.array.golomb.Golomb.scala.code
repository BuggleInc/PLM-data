def golomb(num:Int): Int = {
	var tab = new Array[Int](num)
	var avancee = 1
	var progres = 1
	tab(0) = 1
	if(num == 1){
		return 1
	}
	while (avancee < num -1){
		var x = tab(progres-1)
		while (x >= 1){
			tab(avancee) = progres + 1
			x = x-1
			avancee = avancee + 1
			if(avancee == num - 1){
				return tab(avancee)
			}
		}
		progres = progres + 1
		
	}
	return tab(avancee-1)
}
