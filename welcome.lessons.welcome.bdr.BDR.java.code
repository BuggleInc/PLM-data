while(isOverMessage()) {
	char val = readMessage()[0];

	switch (val):
		case 'R':
			right();
			forward();
			break;
		case 'L':
			left();
			forward();
			break;
		case 'I':
			right();
			right();
			forward();
			break;
		case 'C':
			forward();
		case 'B'
			forward();
		case 'A':
			forward();
			break;
		case 'X':
			backward();
		case 'Y':
			backward();
		case 'Z':
			backward();
			break;
}