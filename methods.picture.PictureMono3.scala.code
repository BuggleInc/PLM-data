override def run() {
def marquer(){baisseBrosse();leveBrosse()}
def faireV(){avance();avance();marquer();avance;gauche();avance();marquer();recule();droite();avance();marquer();avance();avance()}
def faireMotif(){for(x<-1 to 4){faireV();gauche()}}
def grandMotif(){for(x<-1 to 3){(for(y<-1 to 3){faireMotif();avance(7)});gauche();avance(7);droite()}}
for(x<-1 to 3){(for(y<-1 to 3){grandMotif();avance(3*7)});gauche();avance(3*7);droite()}
}