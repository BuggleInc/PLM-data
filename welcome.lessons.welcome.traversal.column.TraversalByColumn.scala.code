override def run() {
  var xf:Int = getWorldWidth() - 1
  var yf:Int = getWorldHeight() - 1
  var counter:Int = 0
  
  while (true){
      
   	writeMessage(counter)
    
    if (xf == getX() && yf == getY()){
      return
    }
    
  	if (getY() + 1 <= yf){
      setPos(getX(), getY() + 1)
      
    }else{
      setPos(getX() + 1, 0)
    }
    
    counter += 1
	}
  
}
