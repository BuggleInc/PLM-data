def solve() {
  var l=getStackSize()
  var i = l-1
  while(i > 0){

    for(j <- 0 to i-1){
      if(getPancakeRadius(j)>getPancakeRadius(j+1))
	    	swap(j)
    }
    if(isPancakeUpsideDown(i)){
      flip(i+1)
      flip(i+1)
    }
    
    
    i-=1
  }
}




def swap(n : Int){
  flip(n)
  flip(n+1)
}