def combSort() {
	var lgr = getNombreValeurs(); var ecart = lgr; var ech = false;
	do {
		ech = false;
		if(ecart > 1) {
			ecart = (ecart.toDouble / 1.3).toInt;
		}
		var i = 0;
		while(i+ecart < lgr) {
			if(plusPetit(i+1, i)) {
				echange(i i+1);
				ech = true;
				i+=1;
			}
		}
		
	}while(ecart > 1 || ech)
}