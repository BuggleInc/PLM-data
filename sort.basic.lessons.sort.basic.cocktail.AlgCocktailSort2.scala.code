def cocktailSort2() {
	var inverse=false
	var fin=getNombreValeurs()
	var debut=0
	do{
		inverse=false
		for (i<- 0 to fin-2) {
			if (plusPetit(i+1,i)) {
			echange(i,i+1)
			inverse=true
			}
		fin=fin-1
		}
		for (i<-getNombreValeurs()-2 to debut by -1) {
			if (plusPetit(i+1,i)) {
			echange(i,i+1)
			inverse=true
			}
		debut=debut+1	
		}
	} while (inverse)
}
