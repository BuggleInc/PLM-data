def isFacingTrail():
    if estFaceMur!=True:
       forward()
       if getCouleurSol()==Color.green:
           backward()
           return True
       else:
           backward()
           return False
    return False

while estFaceMur()!=True and estSurBiscuit!=True:
    if isFacingTrail()==True:
        avance()
        baisseBrosse()
        leveBrosse()
    else:
        left()
    