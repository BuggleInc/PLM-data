def insertionSort() {
  var len = getNombreValeurs()
  var continueboucle:Boolean = false
  var v = 0
  var rg = 0
  for (i <- 1 to len-1) {
    rg = i-1
    v = getValeur(i)
    continueboucle = getValeur(rg)>v
    while (continueboucle) {
      copie(rg,rg+1)
      if (rg>0) {
      	rg-=1
      	continueboucle = getValeur(rg)>v
    	}
      else
      	continueboucle = false
    	setValeur(rg+1,v)	
    }
	}
}