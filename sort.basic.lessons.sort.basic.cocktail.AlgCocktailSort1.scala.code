def cocktailSort() {
  var a=true
    if (a==false){
      return
    }
  a=false
  for (j <- 0 to getNombreValeurs()-1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true
		}
}
for (j <- getNombreValeurs()-1 to 0 by -1){
    if (!isSmaller(j,j+1)){
      swap(j,j+1)
      a=true
		}
}   
}