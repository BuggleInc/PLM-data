override def run() {
 var sommeAngle:Int=(-1)
 var dir:Direction=getDirection()
 var Angle:Boolean=false
 
  
 trouverNord()
 droite()
 
 while(Angle==false){
    dir=getDirection()
   	keepHandOnSideWall()
    if(getDirection()!=dir){
      if(dir==Direction.NORTH && getDirection==Direction.EAST){
        sommeAngle-=1
      }
      else if(dir==Direction.NORTH && getDirection==Direction.WEST){
        sommeAngle+=1
      }
      else if(dir==Direction.SUD && getDirection==Direction.EAST){
        sommeAngle+=1
      }
      else if(dir==Direction.SUD && getDirection==Direction.WEST){
        sommeAngle-=1
      } else  if(dir==Direction.EST && getDirection==Direction.NORTH){
        sommeAngle+=1
      }
      else if(dir==Direction.EST && getDirection==Direction.SOUTH){
        sommeAngle-=1
      }
      else if(dir==Direction.WEST && getDirection==Direction.NORTH){
        sommeAngle-=1
      }
      else if(dir==Direction.WEST && getDirection==Direction.SOUTH){
        sommeAngle+=1
      }
    }else{
      sommeAngle=sommeAngle
    }

    if (sommeAngle==0){
      Angle=true
    }else{
      Angle=false
    }
	}
  
 
  
}












