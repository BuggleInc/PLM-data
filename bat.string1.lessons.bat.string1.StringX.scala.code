def stringX(str:String):String =
{
  var chaine = ""
  
  if (str.length > 2)
  {
    chaine += str(0).toString()
    for (i <- 1 to str.length - 2)
    {
      if (str(i) != 'x')
      	chaine += str(i).toString()
    }
    chaine += str(str.length - 1).toString()
  }
 	else
  	chaine = str
  
  return chaine
}