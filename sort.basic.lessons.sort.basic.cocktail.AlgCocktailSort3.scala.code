def cocktailSort3() {
	var x:Boolean = false
	var a:Int = 0
	var b:Int = getNombreValeurs()-2
	do {
		x = false;
		for (i <- a to b) {
			if (plusPetit(i+1,i)) {
			echange(i,i+1);
			x = true;
			} 
		}
		b=b-1
		
		if (!x) {return} else {
			x= false
			for (i <- b to a by -1) {
				if (plusPetit(i+1,i)) {
				echange(i,i+1);
				x = true;
				} 
			}
		} 
		if (!x) {return}
		a=a+1
	}while(x)
}