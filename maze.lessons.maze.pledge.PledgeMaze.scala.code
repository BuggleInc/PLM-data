override def run() {
	var sommeAngle:Int = 0
	while (!estFaceMur()) {
		avance();
	}
	droite();
	sommeAngle = sommeAngle - 1;

	while (!estSurBiscuit()) {
		if (getDirection() != Direction.NORD) {
			gauche();
			if (estFaceMur()) {
				while (estFaceMur()) {
					droite();
				}
				sommeAngle = sommeAngle - 1;
				avance();
			} else {
				avance();
			}
		} else if (getDirection() == Direction.NORD) {
		 	while(!estFaceMur) {
		 		avance();
		 	}
		 	droite();
			}
	}
	prendBiscuit();
}
