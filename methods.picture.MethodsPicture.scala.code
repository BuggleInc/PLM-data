override def run() 
{
  var L:Array[Color]=Array(Color.yellow,Color.red,Color.blue,Color.green)
  for (i <- L)
  {
    avance(2)
    gauche()
    faireV(i)
  }
}

def faireV(color:Color)
{
  for (i<-1 to 2)
  {
  	marquer(color)
  	avance()
  	droite()
  	avance()
  }
  marquer(color)
  gauche()
  avance(2)
  gauche()
}

def marquer(color:Color)
{
  setCouleurBrosse(color)
  baisseBrosse()
  leveBrosse()
}