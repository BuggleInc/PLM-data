def solve() {
  var c=0
  while (c!=15){
   for (i <- 0 to getTaille-2){
     for (j <- 0 to getTaille-i-2){
       if (getCouleur(i)==0){
         if (getCouleur(getTaille-j-1)==2 || getCouleur(getTaille-j-1)==1){
           echange(i,getTaille-j-1)
         }
     	 } else if (getCouleur(i)==1) {
         if (getCouleur(getTaille-j-1)==2){
           echange(i,getTaille-j-1)
         }
       }
   		}
		}
    c=c+1
	}
}