 public void hunt(Color c) {
 	while(!isOverBaggle()){
 		
 		if(isFacingTrail(c)){
 			brushDown();
 			forward();
 			brushUp();
 		}else{
 			right();
 		}
 	}

 	pickUpBaggle();
     
 }

boolean isFacingTrail(Color c) {
boolean cell = false;

	if(isFacingWall()){
		return false;
	}

	forward();
	cell = getGroundColor().equals(c); 
	backward();
	
	return cell;
}//end isFacingTrail()

