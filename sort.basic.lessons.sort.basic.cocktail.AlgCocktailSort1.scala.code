def cocktailSort() {

	var modif:Boolean = true;
	var taille:Int = getNombreValeurs();

	do
	{
		modif = false;

	for (j <- 0 to taille - 2)
	{
		if (!plusPetit(j, j + 1)) 
		{
				echange(j, j + 1);
				modif = true;
		}
	}

	for (j <- taille - 2 to 0 by -1)
	{
		if (!plusPetit(j, j + 1)) 
		{
				echange(j, j + 1);
				modif = true;
		}
	}
		
	} while(modif);
}



