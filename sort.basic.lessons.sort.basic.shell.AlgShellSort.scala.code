def shellSort()  {
  var v : Int =0
  var k : Int =0
  var ecart : Int = getNombreValeurs()/2
  while(ecart>=1){
    for(i<-ecart-1 to getNombreValeurs()-1){
      v = getValeur(i)
      k=i
      while( k>=ecart &&getValeur(k-ecart)>v){setValeur(k,getValeur(k-ecart));k+= -ecart;}
      setValeur(k,v);
    }
    ecart = ecart/2
  }
}