def bubbleSort3() {
		var n = getNombreValeurs()
  	var B : Boolean = false
  	var x = 0
  	do {
  		for	(i <- n-2 to 0 by -1) {
      	for (j<- 0 to i) {
          if (plusPetit(j+1,j)) {
            echange(j+1,j)
            x+=1
          }
          if (x>0)
          	B = true
          else
          	B = false
         
          
        
      }
      x=0
    }
  } while (B)
}