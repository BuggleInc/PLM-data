def cocktailSort2() {

  var fini = false;
  var n = getNombreValeurs() -2 ;
  var p = 0;
  while(!fini) {
 	  p += 1;
    fini = true ;
    for (i<- p to n-p){
      if (!plusPetit(i,i+1)){
        echange(i,i+1);
        fini = false
      }
    }
    for (i<- p to n-p){
      if (!plusPetit(n - i,n - i + 1)){
        echange(n - i,n - i + 1);
        fini = false
      }
    }
    
   
  }

}