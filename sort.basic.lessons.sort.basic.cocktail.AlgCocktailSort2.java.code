public void cocktailSort2() {
  boolean swap = true;
  int debut = 0;
  int fin = getValueCount()-2;
  do{
    swap = false;
    for(int i = debut; i < fin+1;i++){
      if(isSmaller(i+1,i)){
        swap=true;
				swap(i+1,i);
      }
    }
    fin--;
    for(int j = fin ; j >= debut; j-- ){
      if(isSmaller(j+1,j)){
        swap =true;
        swap(j+1,j);
        
      }
    }
    debut++;
  }while(swap);
  
}