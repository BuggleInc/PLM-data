while(isOverMessage()){
  if(readMessage=="R"){
    right()
    forward()
  } else if (readMessage=="L"){
    left()
    forward()
  } else if (readMessage=="I"){
    left()
    left()
    forward()
  } else if (readMessage=="A"){
    forward()
    
  }else if (readMessage=="B"){
    forward()
    forward()
  }else if (readMessage=="C"){
    forward()
    forward()
    forward()
    
  }else if (readMessage=="Z"){
    backward()
  }else if (readMessage=="Y"){
    backward()
    bakward()
  }else if (readMessage=="X"){
    backward()
    backward()
    backward()
  }

}