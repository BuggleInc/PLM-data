def gnomeSort() {
	var position = 0
	while(position != getNombreValeurs()){
		if(position == 0){
			position += 1
		}else{
			if(position == getNombreValeurs()){
				return
			}else{
				if(plusPetit(position+1,position-1)){
					echange(position+1,position-1)
					position -= 1
				}else{
					position += 1
				}
			}
		}
	}
}
