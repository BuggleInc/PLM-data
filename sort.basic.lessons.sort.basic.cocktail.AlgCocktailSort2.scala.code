def cocktailSort2() {
  var swapped= false
  var j=getValueCount()-2
  do {
    swapped=false
    for (i <- 0 to j+1)
      if (! isSmaller(i,i+1)){
      swap(i,i+1)
        swapped=true;
        j=j-1
    }
    for (i<- j to 0 by -1)
      if (! isSmaller(i,i+1)){
        swap(i,i+1)
        swapped=true;
        j=j+1
      }
    
   
  }  while (swapped);
}
