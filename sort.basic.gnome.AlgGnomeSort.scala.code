def gnomeSort() {
  val lgr = getNombreValeurs()
  var position = 0
  while (position < lgr - 1) {
    if (!plusPetit(position, position + 1)) {
      echange(position,position+1)
      if (position > 0) {
      	position = position - 1
      }
    }
    else {
      position = position + 1
    }
  }
}