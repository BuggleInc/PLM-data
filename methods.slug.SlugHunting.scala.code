def hunt() {
  while(!estSurBiscuit()){
  	isFacingTrail()
  	avance()
  	baisseBrosse()
  	leveBrosse()
  }
}

def isFacingTrail():Boolean = {
   if (!estFaceMur()){
   	avance()
    if(getCouleurSol()==Color.green){
      recule()
      return true
    }else{
      recule()
      return false
    }
  }
  return false 
}