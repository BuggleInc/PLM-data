def isFacingTrail():Boolean = {
  var go=false
  while (!estSurBiscuit){
    if (testCouleur){
      avance()
    }
    
    return go
  }
}


def testCouleur():Boolean ={
  avance()
  var avant=false
  if (getCouleurSol()==Color.green){
    avant=true
    recule()
  }else{
    recule()
    gauche()
    avant=false
  }
  return avant
}
