def alarmClock(day:Int, vacation:Boolean): String = {
  if (vacation) {
    
  if (day==0) {
    return "off"
  }
    if (vacation && day==6) {
    return "off"
  }
  if (vacation && day>=1 && day<=5) {
    return "10:00"
  }
  } 
  if (!vacation) {
  if (day>=1 && day<=5) {
    return "7:00"
  }
  if( day == 0) {
    return "10:00"
  }
  if( day == 1) {
    return "10:00"
  }
  }
  return "0"
}