override def run() {
	while (!estSurBiscuit()) {
		setDirection(Direction.NORTH)
		while(!estFaceMur()) {
			avance();
		} 
		keepHandOnSideWall()
	}
	prendBiscuit();
}


def keepHandOnSideWall() {
	if(!estFaceMur()) {
		gauche();
		if(estFaceMur()) {
			droite();
			avance();
		} else {
			avance();
		}
	} else {
		gauche()
		if(estFaceMur()) {
			retourne();
			if (estFaceMur()) {
				droite();
				avance();
			} else {
			avance();
			}
		} else {
			avance();
		}
	}
}
