var suce = 0;
override def run() {
	for(i<- 0 to getMondeHauteur-1){
		for(j<- 0 to i){
			setPos(j,i-j)
			ecritMessage(suce)
			suce+=1
			
		}
			
	}
	for(i<- 0 to getMondeHauteur-2){
		for(j<- getMondeHauteur-2 to i by -1){
			setPos(getMondeHauteur-1-j+i,j+1)
			ecritMessage(suce)
			suce+=1
			
		}
			
	}
}
