override def run() {
  while (posFinale() == false) {
    	if (getY() == getMondeHauteur()) {
    		setPos((getX()+1), 0)
  }
  		else
  			setPos(getX(), (getY() + 1))
  }
}


def posFinale() {
  return ((getMondeLargeur() == getX()) && (getY() == getMondeHauteur()))
}

def posSuivante() {
  if (getY() == getMondeHauteur()) {
    	setPos((getX()+1), 0)
  }
  else
  		setPos(getX(), (getY() + 1))
}