def insertionSort(e : Int) = {
  var n = getNombreValeurs()
  var v = 0
  var j = e
  for (i <- 1 to n-1) {
    v = getValeur (i)
    while ( i-j >=0 && !plusPetitQue((i-j),v)) {
        copie(i-j,i-j+e)
        j += e 
      } 
    setValeur(i-j+e,v)
    j=e
  }
}

def shellSort()  {
  var e = ((getNombreValeurs())/2)
  while (e>0) {
    insertionSort(e)
    e = (e.toDouble/2).toInt
  }
}



