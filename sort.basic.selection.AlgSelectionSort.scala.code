def selectionSort() {
  var lgr=getNombreValeurs
  var indice=0
  
  var min=0
  var posMin=0
  
  while (indice!=lgr-1){
    min=getValeur(indice)
  	var oui=false
    for (i<- (indice+1) to lgr-1){
      if (plusPetitQue(i,min)){
        posMin=i
        min=getValeur(i)
        oui=true
      }
    }
    if (oui==true){
      echange(indice,posMin)
    }
    
    indice+=1
  }
}