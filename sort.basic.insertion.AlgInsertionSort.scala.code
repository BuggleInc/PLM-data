def insertionSort() {
  
  for (i <- 1 to getNombreValeurs()-1) {
    
    var v = getValeur(i)
    var last = 0
    
    for (c <- 1 to i) {
      
      if (!plusPetitQue(i-c,v)) {
        
        copie(i-c,i-c+1)
        last = c
        
      }
      
    }
    
    setValeur(i-last,v)
    
  }
  
}
