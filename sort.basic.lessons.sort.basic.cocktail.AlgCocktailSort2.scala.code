def cocktailSort2() {
val n = getNombreValeurs()
var swapped = true
var debut = 0
var fin = n-2
	while (swapped)
		{swapped = false
		for (i <- debut to fin)
			{if (!plusPetit(i,i+1))
				{echange(i,i+1)
				swapped = true}
			}
			fin -= 1
		for (k <- fin to debut by -1)
			{if (!plusPetit(k,k+1))
				{echange(k,k+1)
				swapped = true}
			}
			debut += 1
		}
}
