def bubbleSort2() {
  var term = false
  
    do{ 
      term = false
      
      for (i <- getValueCount()-2 to 0){
        for (j <- 0 to i){
          
        if (isSmaller(i+1,i)){
       		swap(i,i+1)
      	  term = true
      		}
          
        }
      } 
      
   }  while (term == true)
  
  
}