def positionFinale : Boolean = {
  return getX()==getMondeLargeur()-3
}

def moria = {
  while ((!positionFinale) && (!estSurBiscuit())) {
    setDirection(Direction.EAST:Direction)
    while ((!estSurBiscuit()) && (!positionFinale)) avance()
    prendBiscuit()
    setDirection(Direction.WEST:Direction)
    while ((!estSurBiscuit()) && (!getX()==0)) avance()
    recule()
    poseBiscuit()
    recule()
  }
  while (!positionFinale) avance()
  droite();avance();gauche();avance()
}