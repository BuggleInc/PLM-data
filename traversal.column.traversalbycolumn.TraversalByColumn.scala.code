override def run() {
  var a=getMondeHauteur()
  var b=getMondeLargeur()
  var x=0
  var cpt=0
  var k=0
  
  while (getX()!=(a-1) && getY!=(b-1)){
    for (k<-1 to (b-1)){
      ecritMessage(cpt)
      setPos(x,k)
      cpt+=1
    }
    ecritMessage(cpt)
    x+=1
    setPos(x,0)
    cpt+=1
  }
for (k<-1 to (b-1)){
  ecritMessage(cpt)
      setPos(x,k)
      cpt+=1
}
  ecritMessage(cpt)
}