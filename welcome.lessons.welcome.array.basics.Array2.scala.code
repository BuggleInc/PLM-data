override def run() {
	var T = new Array[Color](getMondeHauteur)
	val dec = litMessage().toInt
	for (i <- 0 to T.length - 1) {
		T(i)=getCouleurSol()
		avance()
	}
	for(i<-2 to getMondeLargeur()) {
		if (estFaceMur) {
			gauche()
			gauche()
			avance()
		}
		gauche()
		avance()
		droite()
		avance()
		for (j <- 0 to getMondeHauteur() - 1) {
			setCouleurBrosse(T(j))
			baisseBrosse()
			leveBrosse()
			avance()
		}
	}
}
