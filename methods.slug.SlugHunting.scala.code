def hunt() {
   while(!estSurBiscuit()) {
    if(isFacingTrail()) {
      avance()
    }  
    else {
      gauche()
    } 
   }
}

def isFacingTrail():Boolean = {
  var bool=false
  if(!estFaceMur()) {
    avance()
    if(getCouleurSol()==Color.green) {
      bool=true
    }
    recule()
  }
  return bool
}

