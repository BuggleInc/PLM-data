override def run() {
  while(!estSurBiscuit()){
    keepHandOnWall()
  }
  prendBiscuit()
}

def keepHandOnWall(){
  gauche()
  if (estFaceMur()){
    droite()
    avance()
  }else{
    avance()
    gauche()
    if(!estFaceMur()){
      avance()
      gauche()
      if(!estFaceMur()){
        avance()
      }
    }
  }
}