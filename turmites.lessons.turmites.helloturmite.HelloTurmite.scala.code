

def step(colors:Array[Color], rule:Array[Array[Array[Int]]] ) {
    def colore(c:Color){
    setCouleurBrosse(c)
    baisseBrosse()
    leveBrosse()
  }
  var col =0
  while(getCouleurSol!=colors(col)){
    col+=1
  }
  def mouv(m:Int){
    if(m==0){}
    if(m==1){avance()}
    if(m==2){gauche();avance()}
    if(m==4){retourne();avance()}
    if(m==8){droite();avance()}
  }
  colore(rule(col)(0))
  mouv(rule(col)(1))
  col=rule(col)(2)
}
