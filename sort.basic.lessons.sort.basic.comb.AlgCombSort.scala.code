def combSort() {
	val n = getNombreValeurs()
	var ecart:Int=n
	var swapped:Boolean=true
	var i:Int = 0
	while (ecart>1||swapped) {
		swapped = false
		if(ecart>1) {ecart = (ecart.toDouble / 1.3).toInt}
		i=0
		while (i+ecart <n) {
			if (plusPetit(i+ecart,i)) {echange(i+ecart,i);swapped=true}
		}
	}
	
}
