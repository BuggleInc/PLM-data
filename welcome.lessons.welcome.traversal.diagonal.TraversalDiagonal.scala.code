var diag = 0;
override def run() 
{
  var avancement = 0
  ecritMessage(avancement)
  while ( (getX(),getY()) != (getMondeLargeur()-1,getMondeHauteur()-1))
  {
    while (diag != getMondeHauteur()-1)
    {
      while (getY() != 0)
      {
        go()
        avancement = avancement + 1
        ecritMessage(avancement)
      }
      diag = diag + 1
      setPos(0,diag)
    }
  }
}

def go()
{
  var X = getX()
  var Y = getY()
  setPos(X+1,Y-1)
}