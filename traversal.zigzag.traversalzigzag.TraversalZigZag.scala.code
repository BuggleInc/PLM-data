override def run() {
  var cpt=0
  ecritMessage(0)
  setPos(1,0)
  ecritMessage(1)
  cpt=1
  while(!testFin()){
    go()
    cpt+=1
    ecritMessage(cpt)
  }
}

def testFin():Boolean= {
  if(getX()+1==getMondeLargeur && getY()+1==getMondeHauteur){
    return true
  }else{
    return false
  }
}

def go(){
  if(getX()+1<getMondeLargeur || getX()==0){
    setPos(getX(),getY()+1)
  }else if(getY()%2==0){
    setPos(getX()+1,getY())
  }else{
    setPos(getX()-1,getY())
  }
}





