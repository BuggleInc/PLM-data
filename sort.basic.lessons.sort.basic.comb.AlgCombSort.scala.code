def combSort() {
  var ecart = getNombreValeurs()
  do{
    var echanges = true
    if(ecart>1){
      double(ecart) = double(ecart)/1.3
    }
    var i=0
    while(i + int(ecart)< getNombreValeurs()){
      if(!plusPetit(i,i+ecart)){
        echange(i,i+ecart)
        echanges = false
      }
      i++
    }
  }while((ecart<1)||(!echanges))
}