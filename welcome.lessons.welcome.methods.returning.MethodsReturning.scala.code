def haveBaggle():Boolean = {

var vuBiscuit:Boolean = false;
var vSteps:Int = 0;



	while(!estSurBiscuit())
	{


		avance();
		vSteps = vSteps + 1;
		if(vSteps==7)
		{
			vSteps = 0;
			droite();
			avance();
			gauche();
		}

	
	}
		if(estSurBiscuit())
		{
			vuBiscuit = true;
		}

		for(i <- 1 to vSteps)
		{
			recule();
		}
		
				return vuBiscuit;

}
