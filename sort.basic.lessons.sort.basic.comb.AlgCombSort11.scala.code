def combSort11() { 
	var ecart = getNombreValeurs()
	var is_echange = false
	do {
		is_echange = false
		if (ecart > 1) {
			ecart = (ecart.toDouble / 1.3).toInt
			}
		if (ecart == 9 || ecart == 10) {
			ecart = 11
		}
		var i:Int = 0
		while ((i+ecart) < getNombreValeurs()-1) {
			if (plusPetit(i+ecart,i)) {
				echange(i,i+ecart)
				i += 1
				is_echange = true
			}
		} 
	} while ((ecart > 1) || is_echange)
}
