override def run() {
  var ord=getY()
  var abs=getX()
  println(ord)
  println(abs)
  var h=getMondeHauteur()
  var m=0
  while (arrive()==false){
    println("Yo")
    ecritMessage(m)
    m=m+1
    if (ord==h-1){
      setPos(abs+1,0)
    }
    else setPos(abs,ord+1)
  }
  ecritMessage(m)
}

def arrive(){
  var h=getMondeHauteur()
  var l=getMondeLargeur()
  if (getX()==l-1 && getY()==h-1){
    return true
  }
  else return false
}