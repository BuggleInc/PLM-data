def gnomeSort() {
  var k=0
  while (k<=getValueCount()-1){
    if (isSmaller(k,k+1)){
      k=k+1
    }
    if (k==getValueCount()-1){
      return
    }
    if(!isSmaller(k,k+1)){
      swap(k,k+1)
      if (k==0){
        k=k+1
      } else{
        k=k-1
      }
    }
    //if (k==0){
     // k=k+1
    //}
  }
}