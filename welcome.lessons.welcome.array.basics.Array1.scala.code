override def run() {
	def tabColor(x:Int):Array[Color] = {
		var T = new Array[Color](x)
		for (i <- 0 to x-1) {
			T(i)=getCouleurSol();
			avance();
		}
	return T
		
	}
	val T = tabColor(getMondeHauteur)
	for (i <- 1 to getMondeLargeur()) {
		avance();
		gauche();
		avance();
		droite();
		for (j <- 1 to getMondeHauteur-1) {
			setCouleurBrosse(T(j-1));
			baisseBrosse();
			leveBrosse();
			avance();
			
		}
	}
}
