def pattern(color1,color2,color3,color4):
  setBrushColor(color1)#rouge
  for i in range(2):
    forward()
  setBrushColor(color2)#bleu
  forward()
  setBrushColor(color3) #jaune
  forward()
  setBrushColor(color4) #vert
  for i in range(2):
    forward()

pattern(Color.red,Color.blue,Color.green,Color.yellow)
