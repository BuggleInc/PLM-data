$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class island extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( island((int[])t.getParameter(0)) ); }
	int island(int[] num) {
		int nbisland=0;
		for(int i=0;i<num.length-1;i++){
			if(num[i]<num[i+1]){
				nbisland++;
			}
		}
		return nbisland;
	}
 }