def cocktailSort() {
  var lgr=getNombreValeurs()
  var stop=false
  
  while(stop!=true){
    stop=true
    for (i<-0 to lgr-2){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        stop=false
      }
    }

    for (i<-lgr-2 to 0 by -1){
      if (plusPetit(i+1,i)){
        echange(i,1+i)
        stop=false
      }
    }
  }
}