override def run() {
var x=0	
var y=0
var nb=0


def finale():Boolean = {
	if (getX() == getMondeLargeur()-1 && getY() ==  getMondeHauteur()-1 ) return true
	else return false
}

def avancer() {

	while ( getY() != getMondeHauteur()-1) {
	setPos(x, y)
	y=y+1
	nb=nb+1
	}
	y=0
	x=x+1
}

setPos(0,0)
while ( finale != true ) {
	
ecritMessage(nb)
avancer()

}