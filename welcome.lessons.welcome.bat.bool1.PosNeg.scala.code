def posNeg(a:Int, b:Int, negative:Boolean):Boolean = {
	if (a<=0 == true && b>=0 == true && negative == false) {
		return true
	} else if (b<=0 == true && a>=0 == true && negative == false) {
		return true
}else if (a<=0 == true && b<=0 == true && negative == true) {
	return true
	
}else
