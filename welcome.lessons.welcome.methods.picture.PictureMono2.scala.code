override def run() {
  for (ligne <- 1 to 3) {for (colone <- 1 to 3) {faireMotif();} ; reposition()};
}

def marqueSol() {baisseBrosse(); leveBrosse();}

def faireV() {avance(2); marqueSol(); avance(); gauche(); avance(); marqueSol(); recule() ; droite(); avance(); marqueSol(); avance(2); gauche();}

def faireMotif() {for (cote <- 1 to 4) {faireV();};}

def reposition() {recule(21); gauche(); avance(7); droite();}