override def run() {
  def keepHandOnSideWall() = {
    gauche()
    if (!estFaceMur()){
      avance()
    }
    else {
      droite()
      if (!estFaceMur()){
        avance()
      }
      else {
        droite()
        if (!estFaceMur()){
          avance()
        }
        else {
          droite()
          avance()
        }
      }
    }
  }

  while (getY()!=0){
    while (!estFaceMur() && getY()!=0){
      avance()
    }
    if (getY!=0){
      keepHandOnSideWall()
    }

    while (getDirection()!=Direction.NORTH && getY()!=0){
      droite()
    }
  }
  while (!estSurBiscuit()){
    keepHandOnSideWall()
  }
  prendBiscuit()
}