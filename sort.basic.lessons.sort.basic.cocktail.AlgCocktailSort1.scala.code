	while (trié==true) {
		trié=false
	for (i<-0 to getNombreValeurs()-2) {
		trié=false
		if (plusPetit(i:Int, i+1:Int)==false) {
			echange(i:Int, i+1:Int)
				trié=true
		}
	}
	for (i<-getNombreValeurs()-2 to 0 by -1) {
		trié=false
		if (plusPetit(i:Int, i+1:Int)==false) {
			echange(i:Int, i+1:Int)
				trié=true
		}
	}
	
}
}