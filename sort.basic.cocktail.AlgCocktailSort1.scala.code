def cocktailSort() {
  var len=getValueCount()
  var bool=false
  for(j<-0 to len-2){
		bool=true

  	for(i<-0 to len-2){
      if(!isSmaller(i,i+1)){
        swap(i,i+1)
        bool=false}
      }
      

      for(i<-0 to len-2){
      if(!isSmaller(len-2-i,len-2-i+1)){
        swap(len-2-i,len-2-i+1)
        bool=false}}
    
    if (bool==true){
      return
    }
  }
}
