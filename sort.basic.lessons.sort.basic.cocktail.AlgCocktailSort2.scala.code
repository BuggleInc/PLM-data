def cocktailSort2() {
var f=getNombreValeurs()-2
var d=0
var x=false 
do{


	for(i<- d to f){
		
		if (plusPetit(i,i+1)==false){
			echange(i,i+1)
				x=true
		}
		
	}

	f=f-1

	for(i<- f to d by-1){
		if (plusPetit(i,i+1)==false){
			
			echange(i,i+1)
			 x=true
		}
	 }
	
	 d=d+1

} while(x==true)
}