def combSort() {
	var ecart=getNombreValeurs()
	var inverse=true
	do{  
		inverse=false
		if (ecart>1){
			ecart = (ecart.toDouble / 1.3).toInt
		}
		var i=0
		while ((i+ecart)<getNombreValeurs()){
			if (!plusPetit(i,i+ecart)){
				echange(i,i+ecart)
				inverse=true
			}
			i+=1
		}
	}while(ecart>1 || inverse)
}
