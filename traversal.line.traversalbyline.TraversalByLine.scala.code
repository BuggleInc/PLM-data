override def run() {
  val largeur : Int = getMondeLargeur()
  val hauteur : Int = getMondeHauteur()
  var cpt : Int = 0 
  var fini : Boolean = false
  while (!fini) {
  (getX(),getY()) match {
    case (6,6) => ecritMessage(cpt); fini = true
    case (6,_) => ecritMessage(cpt); cpt= cpt + 1; setPos(0,getY()+1)
    case (_,_)=> ecritMessage(cpt); cpt=cpt+1; setPos(getX()+1,getY())
  }
  }
}