def shellSort()  {

	var taille:Int = getNombreValeurs();
	var v:Int = 0;
	var j:Int = 1;

	for (i <- 1 to taille - 1)
	{
		v = getValeur(i);
		j = 1;
		while (i - j >= 0 && getValeur(i - j) > v)
		{
			setValeur(i - j + 1, getValeur(i - j));
			j += 1;
		}

		setValeur(i - j + 1, v);
	}
}

