def solve() {
  var afterBlue = 0;
  var whiteRed, beforeWhite = getSize-1;
  
  while(afterBlue <= beforeWhite){
  	var colorAB = getColor(afterBlue);
    
    if(colorAB == RED){
      swap(afterBlue, beforeWhite);
      swap(whiteRed, beforeWhite);
      beforeWhite -= 1;
      whiteRed -= 1;
    }
    if(colorAB == WHITE){
      swap(afterBlue, beforeWhite);
      beforeWhite -= 1;
    }
    if(colorAB == BLUE){
      afterBlue += 1;
    }
    
  }
}