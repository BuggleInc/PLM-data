def withoutDoubles(die1:Int, die2:Int, noDoubles:Boolean):Int = {
  if (noDoubles == false){
    return die1+die2
  }
  else{
    if( die1!=die2){
      return die1+die2
    }
    else{
      if (die1=1 || die1==2 | die1==3 || die1==4 || die1==5){
        die1=die1 + 1
        return die1+die2
      }
      if (die1 == 6){
        die1=1
        return die1+die2
      }
    }
  }
}