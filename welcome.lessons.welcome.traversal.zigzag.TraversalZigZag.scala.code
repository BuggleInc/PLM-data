def parcoursDouble(x:Int,y:Int,d:Boolean) {
  if (d) {
    if (x==getMondeLargeur()-1) {
    setPos(x,y+1)
   	parcoursDouble(getX(),getY(),false)
  }
  else setPos(x+1,y)
  }
  if (!d) {
    if (x==0) {
    setPos(x,y+1)
   	parcoursDouble(getX(),getY(),true)
  }
  else setPos(x-1,y)
  }
}