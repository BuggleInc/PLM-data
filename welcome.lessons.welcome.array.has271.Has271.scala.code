def has271(nums:Array[Int]): Boolean = {
  var n =nums.length
  for (i <- 1 to n-2){
    if ( (nums(i+1)==(nums((i)+5))) && (nums (i+2)==(nums((i)-1)))){
      return true
    }
    else if ((nums(i+1)==nums((i)+5)) && (nums (i+2)<=nums((i)+1 ))&& (nums(i+2)>=nums((i)-3))){
      return true
    }
  }
  return false
}