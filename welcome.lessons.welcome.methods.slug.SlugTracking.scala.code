def isFacingTrail():Boolean = {
  avance()
  while (!estSurBiscuit) { 
    if (getCouleurSol() = Color.green) {
      return true
      avance()}
    else if (estFaceMur()) {return false
                            recule()}
    else if (getCouleurSol() != Color.green) {
      return false 
      recule()      
      droite()
      avance()
    }
  }
}
