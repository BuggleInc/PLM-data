def cocktailSort2() {
  var debut:Int=0
  var fin:Int=getNombreValeurs()-2
  for (j <-debut to fin){
    for (i <- debut to fin-j) {
      if (plusPetit(i+1, i)){
        echange(i, i+1)  
      }
      fin=fin-1
    }
    for(i<- 0 to fin){
      if (plusPetit(fin-i, fin-i-1)){
        echange(getNombreValeurs()-2-i, getNombreValeurs()-1-i)
      }
  	}	
	}
}