var condition = false
def combSort() {
  var ecart:Double = getNombreValeurs()
  while(ecart>1 && condition){
    condition=false
    if(ecart>1)
      ecart = ecart/1.3
    var i=0
    while(i+ecart<getNombreValeurs()){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        i=i+1
        condition=true
    }
    }
  }
}