def angleBleu():
    setCouleurBrosse(Color.BLUe)
    for b in range(2):
        baisseBrosse()
        leveBrosse()
        forward()
        left()
        forward()
        baisseBrosse()
        leveBrosse()

def angleRouge():
    setCouleurBrosse(Color.RED)
    for r in range(2):
        forward()
        baisseBrosse()
        leveBrosse()
        left()
        forward()
        left()


angleBleu()