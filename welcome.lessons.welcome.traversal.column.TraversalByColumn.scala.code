override def run() {
	var x=0
	var y=0
	var cpt=0
	def positionFinale():Boolean= {
	if(cpt==getMondeHauteur()*getMondeLargeur()) {
		return true
	} else {
		return false
	}
	}
	while (!positionFinale) {
		if(getY()==getMondeHauteur()-1) {
			y=0
			x+=1
		} else {
			y=y+1
		}
		ecritMessage(cpt);
		cpt+=1
		setPos(x,y);
	}
}
