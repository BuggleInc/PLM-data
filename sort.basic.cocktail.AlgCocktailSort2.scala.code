def cocktailSort2() {

  var fini = false;
  var n = getNombreValeurs();
  do {
    fini = true ;
    n -= 1;
    for (i<- 0 to n -2){
      if (!plusPetit(i,i+1)){
        echange(i,i+1);
        fini = false
      }
    }
    for (i<- 0 to n -2){
      if (!plusPetit(n - i - 2,n - i - 1)){
        echange(n - i - 2,n - i - 1);
        fini = false
      }
    }
    
  } while(!fini)

}