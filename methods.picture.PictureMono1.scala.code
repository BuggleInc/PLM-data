override def run() {for (stepper <- 1 to 4) {forward()
                                             forward()
                                             makeV()}
}

def makeV() {paint()
             forward()
             left()
             forward()
             paint()
             backward()
             right()
             forward()
             paint()
            forward()
            forward()
            left()}

def paint() {brushDown()
             brushUp()}