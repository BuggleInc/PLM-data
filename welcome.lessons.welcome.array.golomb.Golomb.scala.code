def golomb(num:Int): Int = {
	var T = new Array[Int](num)
	T(1)=1
	for (i<-2 to num) {
		var cpt = 0
		for (j<-1 to i-1) {
			if (T(i-1)==T(j)) {
				cpt=cpt+1
			}
		}
		if (cpt==T(T(i-1))) {
			T(i)=T(i-1)+1
		} else {
			T(i)=T(i-1)
		}	
	}
	return T(num)
}

def occurrences(nums:Array[Int],lookingFor:Int):Int = {
	var compteur:Int = 0
	for (i<-0 to nums.length-1) {
		if (nums(i)==lookingFor) {
			compteur = compteur +1
		}
	}
	return compteur
}