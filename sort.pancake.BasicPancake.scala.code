def solve() {
  var length:Int = getTaillePile()-1
  var max:Int = 0
  do{
    for (i <- 0 to length){
      if (getRayonCrepe(i) >= getRayonCrepe(max)){
        max = i
      }
    }
    if (max < length){
      retourne(max+1)
      retourne(length+1)
    } else {
      length -= 1
    }
    length -= 1
    if (length == 1){
      retourne(2)
    }
  }while(!estTriee() && length > 0)
}