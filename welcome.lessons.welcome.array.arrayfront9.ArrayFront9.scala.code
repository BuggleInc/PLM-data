def arrayFront9(nums:Array[Int]): Boolean = {
	
var j:Int=0
	while(indexOfValue(nums ,9)){
		j= j+1
	}

	if (j==0){
		return false
	}else{
		return true
	}
	



}


def indexOfValue(nums:Array[Int] ,value:Int): Boolean = {
	var i = 0
  while(i < 5 && nums(i) != value){
  	i = i + 1
  }
  if(i < nums.length){
  	return true 
  }else{
  	return false
  }
}




