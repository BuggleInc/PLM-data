override def run() {
  var i = getMondeHauteur()
  var k = getMondeLargeur()
  var T = new Array [Color](i)
  for (j<- 0 to T.length-1) {
  	var m = getCouleurSol
  	T(j) = m
  	avance()
	}
  for (cpt <- 0 to k) {
	gauche()
	avance()
	droite()
	avance()
  for (cpt1<- 0 to T.length-1){
		setCouleurBrosse(T(cpt1))
		baisseBrosse()
		leveBrosse()
    avance()
	}
    
  }
  gauche()
  avance(k-2)
  droite()
  avance(i-2)
}


