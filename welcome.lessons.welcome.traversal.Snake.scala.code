override def run() {
  baisseBrosse()
  avancer()
}

def avancer(){
  while(!estFaceMur() )
  avance()
  if (estFaceMur()){
    if (getDirection()== Direction.EST){
      gauche()
      avance()
      gauche()}
    if (getDirection()==Direction.OUEST){
      droite()
      avance()
      droite()
    }
  }
}