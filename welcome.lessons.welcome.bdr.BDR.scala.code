var x: String=""
while(estSurMessage()){
	x=litMessage()
if(x=="R") {Right();avance();}
if(x=="L") {Left();avance();}
if(x=="I") {retourne();avance();}
if(x=="A") {avance();}
if(x=="B") {avance();avance();}
if(x=="C") {avance();avance();avance();}
if(x=="Z") {recule();}
if(x=="Y") {recule();recule();}
if(x=="X") {recule();recule();recule();}
}