boolean squirrelPlay(int temp, boolean isSummer) {
if(!isSummer){
	if(estDans(60,90,temp)){
		return true;
	}
	else {
		return false;
	}
}
else {
	if(estDans(60,100,temp)){
		return true;
	}
	else {
		return false;
	}
}

}

boolean estDans(int i, int j, int a){
if(a>i-1 && a<j+1){
	return true;
}
else {
	return false;
}
}


