override def run() {
  
  for (i <- 1 to 3){
    makeV(Color.YELLOW)
    left()
    forward()
    makeV(Color.RED)
  }
 
  left()
  forward(5)
}



def makeV(c: Color){
  setBrushColor(c)
  forward()
  brushDown()
  forward()
  brushUp()
  left()
  forward()
  brushDown()
  backward()
  brushUp()
  right()
  brushDown()
  forward()
  brushUp()
}
