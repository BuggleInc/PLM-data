override def run() {
  while (isOverBaggle()==true){
      while(isFacingWall()==false){
        forward()
  }
    while (getDirection != Direction.NORTH){
    keepHandOnSideWall()
    }
  }
  pickupBaggle()
}


def keepHandOnSideWall(){
  left()
  if (isFacingWall()==true){
    right()
    if (isFacingWall()==false){
      forward()
    }
    else{
      right()
    }
  }
  else {
    forward()
    left()
    if (isFacingWall()==true){
      right()
      if (isFacingWall()==false){
        forward()
      }
    }
    else{
      forward()
    }
  }
}