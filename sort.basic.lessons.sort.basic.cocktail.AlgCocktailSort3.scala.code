def cocktailSort3() {
  var enOrdre = true ;
  var fin = getNombreValeurs()-2;
  var debut = 0;
  while (enOrdre){
    enOrdre = false ;
    for (i <- debut to fin){
      if (plusPetit(i+1,i)){
        echange(i+1,i)
        enOrdre = true ;
      }    
    }

    fin-=1;
    if(enOrdre){  	
      for (j <- fin to debut by -1){
        if (plusPetit(j+1,j)){
          echange(j+1,j)
          enOrdre = true ;
        }
      } 
      debut+=1; 
    }
    enOrdre = true ;

  }
}
