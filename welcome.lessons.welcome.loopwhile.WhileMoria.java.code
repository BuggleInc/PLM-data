public void run() { 
	// on se retourne
	droite();
	droite();
	while (!estFaceMur()) {
		// on avance tant qu'on n'est pas sur un biscuit ou face au mur
		while (!(estSurBiscuit() || estFaceMur()))
			avance();

		// quand on trouve un biscuit
		if (estSurBiscuit()) {
			// on le ramasse et on fait demi-tour
			prendBiscuit();
			droite();
			droite();
			// avance jusqu'au biscuit de gauche
			while (!estSurBiscuit())
				avance();
			// se retourne, avance d'un pas et pose le biscuit
			droite();
			droite();
			avance();
			poseBiscuit();
			avance();
		}
	}
	// on a ramass√© tous les biscuits et on est face au mur
	droite();
	avance();
	gauche();
	avance();
}
