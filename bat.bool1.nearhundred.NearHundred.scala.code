def nearHundred(n:Int): Boolean = {
  if (n>=90){
    if (n<=110){
      return true
    }
    else {
      if (n>=190){
        if (n<=210){
          return true
        }
        else {
          return false
        }
      }
      else {
        return false
      }
    }
  }
  else {
    return false
  }
}