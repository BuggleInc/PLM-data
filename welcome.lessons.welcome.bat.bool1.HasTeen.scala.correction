$package
import plm.universe.bat.{BatEntity,BatWorld,BatTest}; 
import plm.universe.World; 
class hasTeen extends BatEntity { 
   override def run(t: BatTest) {
   t.setResult( hasTeen( t.getParameter(0).asInstanceOf[Int], t.getParameter(1).asInstanceOf[Int], t.getParameter(2).asInstanceOf[Int]) )
   }
def hasTeen(a:Int, b:Int, c:Int): Boolean = {
   return (a>12 && a<20) || (b>12 && b<20) || (c>12 && c<20)
} }