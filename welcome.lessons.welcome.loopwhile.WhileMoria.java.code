public void run() { 
while (balin==true)
{
	right();
	right();
	forward();
	if (isOverBaggle())
	{
		pickupBaggle();
		right();
		right();
		forward();
		forward();
		forward();
		while(isOverBaggle())
		{	
		backward();
		dropBaggle();
	}}
	
}
}
/*isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle()
*/
