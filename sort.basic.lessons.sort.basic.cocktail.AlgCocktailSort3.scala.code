def cocktailSort3() {
	var fin: Int = getNombreValeurs()-2;
	var debut: Int = 0;
	var swapped: Boolean = true
	
	while (swapped) {
		swapped = false
		for (i <- debut to fin) {
			if (plusPetit(i+1,i)) {
				echange(i,i+1)
				swapped = true
			}
		}
		if (!swapped) return
		fin -= 1
		swapped = false
		for (j <- fin to debut by -1) {
			if (plusPetit(j+1,j)) {
				echange(j,j+1)
				swapped = true
			}
		}
		if (!swapped) return
		debut += 1
	}
}