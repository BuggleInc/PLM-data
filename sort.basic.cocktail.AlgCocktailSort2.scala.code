def cocktailSort2() {
  var Start : Int = 0
  var Stop : Int = getNombreValeurs-2
  var Swap : Boolean = true
  while (Swap == true) {
    Swap = false
    for(i <- Range.inclusive(Start,Stop)) {
      if (!plusPetit(i,i+1)) {
        Swap = true
        echange(i,i+1)
      }
    	Stop-=1
    }
    for(i <- Range.inclusive(Stop,Start,-1)) {
      if (!plusPetit(i,i-1)) {
        Swap = true
        echange(i,i-1)
  		}
    	Start+=1
		}
  }
}