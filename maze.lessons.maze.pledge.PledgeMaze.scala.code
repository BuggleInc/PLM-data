override def run() {
  var virage : Int = 0
  while(!estSurBiscuit()){
    if(isDirectionFree(Direction.NORD))
    {
      setDirection(Direction.NORD)
    }
    while(!estFaceMur())
    {
      avance()
    }
		
    do{
      virage = keepHandsOnSideWall(virage)
    }while(virage!=0)

  }
  prendBiscuit()
  
}

def mG() : Boolean = /*Verifie s'il y a un mur a droite*/{
  var g = false
  gauche();g= estFaceMur();droite()
  return g
}

def mD() : Boolean = /*Verifie s'il y a un mur a gauche*/{
  var d = false
  droite();d= estFaceMur();gauche()
  return d
}

def keepHandsOnSideWall(vir : Int) : Int = {
  var virage : Int = vir
  if(!mG())
  {
    gauche();avance();virage = virage + 1
  }
  else if(!estFaceMur())
  {
    avance()
  }
  else if(!mD())
  {
    droite();avance();virage = virage - 1
  }
  else
  {
    gauche;gauche();avance();virage = virage + 2
  }
  return virage
}

def isDirectionFree(dir : Direction) : Boolean = {
  var actu : Direction = getDirection()
  setDirection(dir)
  if(estFaceMur())
  {
    setDirection(actu);return false
  }
  else
  {
    setDirection(actu);return true
  }
}

def keepGoing(vir : Int) : Int = {
  var virage = vir
  while(!isDirectionFree(Direction.NORD))
  {
    if(!mD())
    {
      droite();avance(); virage = virage + 1
    }
    else if(!mG())
    {
      gauche();avance(); virage = virage - 1
    }
    else
    {
      gauche();gauche(); virage = virage + 2
    }
  }
  return virage
}