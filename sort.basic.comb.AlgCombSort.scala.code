def combSort() {
  var swapped:Boolean=true
  var gap:Int=getValueCount()
  do {
    swapped=false
    if (gap>1) {
      gap = (gap.toDouble / 1.3).toInt
    }
    var i:Int=0
    while(i+gap < getValueCount()) {
      if (!isSmaller(i,i+gap)) {
        swap(i,i+gap)
      }
      i+=1
    }
  } while (gap>1 || swapped==true)
}