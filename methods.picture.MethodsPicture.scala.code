override def run() {
  for (a <- 1 to 3){
    	for (b <- 1 to 3){
    	makeV(Color.YELLOW)
   		 makeV(Color.RED)
    		makeV(Color.BLUE)
   		 makeV(Color.GREEN)
      }
  }
}

def makeV(c:Color) {
  setBrushColor(c)
  forward(2)
  brushDown()
  brushUp()
  left()
  forward()
  right()
  forward()
  brushDown()
  brushUp()
  forward()
  right()
  forward()
  brushDown()
  brushUp()
  left()
  forward(2)
  left()
}