$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class teenSum extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( teenSum((Integer)t.getParameter(0), (Integer)t.getParameter(1)) ); }
	int teenSum(int a, int b) {
		if ((a >= 13 && a <= 19) || (b >= 13 && b <= 19))
			return 19;
		else
			return a+b;
	}
