override def run() {
  while(!estSurBiscuit())
  {
    keepHandOnSideWall()
  }
  prendBiscuit()
}

def keepHandOnSideWall()
{
  if(getDirection()==Direction.NORTH && !estFaceMur())
  {
    droite()
    avance()
  }else{
    
    gauche()
    
  	if(estFaceMur())
  	{
    	 droite()
  	}
  	if(!estFaceMur())
  	{
    	avance()
  	}else
    {
    	droite();
  	}
  }  
}