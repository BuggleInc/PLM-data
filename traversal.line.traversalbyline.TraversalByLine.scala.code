def positionFinale():Boolean = {
  return ((getX()+1,getY()+1)==(getMondeLargeur(),getMondeHauteur()))
}

override def run() {
  var cpt:Int=0
  val xmax:Int=getMondeLargeur()
  val ymax:Int=getMondeHauteur()
  for (y <- 0 to ymax-1) {
    for (x <- 0 to xmax-1) {
      setPos(x,y)
      ecritMessage(cpt)
      cpt=cpt+1
    }
  }
}