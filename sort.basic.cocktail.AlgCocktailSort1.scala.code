def cocktailSort() {

  var swapped:Boolean =true
  var done:Int=0
  
  while (swapped) {
    swapped=false
    for (i <- 0 to getValueCount()-2-done) {
    	if ( ! isSmaller(i,i+1)) {
      	swap(i,i+1)
      	swapped=true
      }
    }
    
    for (i <- (getValueCount()-3-done) to 0) {
    	if ( ! isSmaller(i,i-1)) {
      	swap(i,i+1)
      	swapped=true
      }
    }
    done+=1
  }
}
