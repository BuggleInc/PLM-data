def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
	if (steps!=0) {
		current(steps)
		avance(length)
		droite(angle)
		tree( steps-1, shrink*length, angle , shrink)
	     gauche(angle)
	     leveCrayon()
	     recule(length)
	     baisseCrayon()
	     current(steps)
		avance(length)
		gauche(angle)
		tree( steps-1, shrink*length, angle , shrink)
	     droite(angle)
	     leveCrayon()
	     recule(length)
	     

		
	}
}
