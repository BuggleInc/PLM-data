override def run() {
  
  def posFinal() : Boolean =  {
    
    var H = getMondeHauteur();
    var L = getMondeLargeur();
        
    if ((getX() == H) && (getY() == L)) {
      return true
    }
    return false
    
  }
  
  var cpt = 0
  
  while(!posFinal) {
    
    ecritMessage(cpt);
    
    
    
  }
  
}