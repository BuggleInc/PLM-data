override def run() {
  droite()
  var compteur:Int=0
  while (!(getX() ==6) && getY() == getMondeHauteur()-1)) {
    ecritMessage(compteur)
    setPos(getX()+1,getY())
    compteur+=1
    if (estFaceMur() && !(getX()==6) ){
      ecritMessage(compteur)
      compteur+=1
      setPos(0,getY()+1)
    }   
  }
  ecritMessage(compteur)
}

