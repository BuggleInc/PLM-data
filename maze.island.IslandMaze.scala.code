override def run() {
  while (!estSurBiscuit()) {
    setDirection(Direction.NORTH)
    while (!estFaceMur()) {
      avance()
    }
    if (estFaceMur()) {
      while (getDirection() != Direction.NORTH) {
        keepHandOnSideWall()
      }
    }
  }
}

def keepHandOnSideWall() = {
  if (!estFaceMur()) {
    gauche()
  }
  while (estFaceMur()) {
    droite()
  }
  avance()
}