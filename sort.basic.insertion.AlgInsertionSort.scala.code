def insertionSort() {var v:Int=0; var test:Boolean=false
                     for (i<-(0 to (getValueCount()-1))) {test=false; v=getValue(i); for (j<-1 to i) {if (getValue(i-j+1)>v) {setValue((i-j+2), getValue(i-j+1))} else {if (!test) {setValue((i-j+2), v); test=true}}
                                                                                                     }}}