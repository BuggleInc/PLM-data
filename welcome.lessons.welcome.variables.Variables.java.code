public void run() { 
	int nbPas = 0;
	int dejaFait = 0;

     // avance jusqu'au biscuit en comptant le nombre de pas
	while (!estSurBiscuit()) {
		avance();
		nbPas++;
	}
	// ramasse le biscuit et se retourne
	prendBiscuit();
	droite();
	droite();
	// revient sur ses pas
	while (dejaFait != nbPas) {
		avance();
		dejaFait++;
	}
	droite();
	droite();
	poseBiscuit();
}
