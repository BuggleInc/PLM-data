override def run() {
  def positionFinale():Boolean={
    var c=0
    while ((getX()!=getWorldWidth()) && getY()!=getWorldHeight()){
      writeMessage(c)
      if (getY()!=6){
        setPos(getY()+1,getX())
        c=c+1
      }
      else {
        setPos(0,getX()+1)
        c=c+1
      }
    }
    return(true)
  }
  
positionFinale()
}