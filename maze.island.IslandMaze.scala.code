override def run() 
{
  while(!estSurBiscuit())
  {
    if(getDirection()!=Direction.NORTH || estFaceMur())
    {
    	keepHandOnSideWall()
  	}
    else
    	{
        avance()
      }
  }
	prendBiscuit()
}

def keepHandOnSideWall()
{
  do
  	{
      avance()
      gauche()
    }
  while(!estFaceMur())  
  //tant qu'on a pas de mur devant nous après avoir tourné à gauche, on avance puis on tourne à gauche 
  droite() //si on a un mur à notre gauche, on revient tout droit puis à nouveau on avance et tourne à gauche
}





