def cocktailSort2() {
  var a=true;
  var c=getValueCount()
  while(a){
  	a=false;
  	for (i<- 0 to c-2){
    	if (isSmaller(i+1, i)){
      	swap(i, i+1);
        a=true;
    	}
    }
    for (i<- 0 to c-2){
      if (isSmaller(c-i-1, c-i-2)){
        swap(c-i-2, c-i-1);
        a=true
      }
    }
  }
}