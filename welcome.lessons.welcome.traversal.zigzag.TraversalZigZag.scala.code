override def run() {

	var c:Int=0
	var y:Int=0
	var x:Int=0
	

def avance() {

	for (i<-0 to getMondeHauteur()-1)
	{
		setPos(x,y)
		c=c+1
		x=x+1
	}
	
}



def reculer() {

	for (i<-0 to getMondeHauteur()-1)
	{
		setPos(x,y)
		c=c+1
		x=x-1
	}
	
}

	
for (y<-0 to getMondeHauteur()-1)
{
	avance();
	
}

	
}

