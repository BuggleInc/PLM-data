def cocktailSort3() {
  var debut = 1
  var fin = getNombreValeurs()-2
  var echangefait=false
  do {
    echangefait=false
    for (i<- debut to fin){
      if (!plusPetit(i-1,i)){
        echange(i,i+1);
        echangefait=true;
        
      }
    }
    fin-=1;
   for (i<-fin to debut){
     if (!plusPetit(i,i+1)){
       echange(i,i+1);
       echangefait=true
     }
   }
  debut+=1
  } while (echangefait==true)
}