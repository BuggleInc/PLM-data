//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle()

def move() = {
 	if(!isCarryingBaggle()){
   while(getDir() != Direction.EAST){
     right()
   }
   while(!isOverBaggle()){
     forward()
   }
   backward()
   dropBaggle()
   backward()
  } else {
   	while(getDir() != Direction.WEST){
      right()
    }
    while(!isOverBaggle()){
      forward()
    }
    pickupBaggle()
    backward()
  }
}

def leave() = {
  right();forward();left();forward()
}




def makeHisWayOut() = {
  left();left()
  while(!isFacingWall()){
    move()
  }
  leave()
}



makeHisWayOut()