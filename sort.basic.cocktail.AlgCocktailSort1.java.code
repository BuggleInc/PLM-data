public void cocktailSort() {
  n = getNombreValeurs()
  fin = false
    
  while (fin == false){
    for (k <- 0 to n-1){
      if (plusPetit(k, k+1) == false){
        echange(k,k+1)
      }
      else{
      fin = true
    	}
    }
    for (k <- 0 to n-1){
      if (plusPetit(k, k+1) == true){
        echange(k,k+1)
      }
      else{
      fin = true
      }
    }
  }
}