def close10(a:Int, b:Int): Int = {

var compteurA = Math.abs(a - 10)
var compteurB = Math.abs(b - 10)

	if (compteurA <= compteurB) {
		return a
	} else {
		if (compteurA >= compteurB) {
			return b
		} else {
			if (Math.abs(a - 0) <= Math.abs(b - 0)) {
				return a
			} else { 
				return b
				} 
			}
		}
	}
