public void run() {
	left();
	left();
	while (isFacingWall() == false) {
	while (isOverBaggle() == false) {
		forward();}
	pickupBaggle();
	while (isCarryingBaggle() == true) {
		if (isOverBaggle() == false) {
			backward();}
		}
		else {forward();}
		forward();
		dropBaggle();		
	}
	right();
	forward();
	left();
	forward();
}
	/* isFacingWall() isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().*/
	 
