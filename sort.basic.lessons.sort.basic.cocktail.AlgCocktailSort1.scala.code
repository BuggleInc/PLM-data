def cocktailSort() {
	for(j<-0 to getNombreValeurs()-1){
    var count=0
    for(i<- 0 to getNombreValeurs()-2-j){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        count+=1
      }
    }
		for(i<- getNombreValeurs()-2-j to 1){
      if(!plusPetit(i,i-1)){
        echange(i,i-1)
        count+=1
      }
    } 
    if(count<1){
      return 
    }
  }
}