

def max1020(a:Int, b:Int):Int = {
  if ((10>a || a>20)&&(10>b || b>20)){
    return 0
  }
  if ((10<=a && a<=20)&&(10<=b && b<=20)){
    if (a>b){
      return a
    }
    else{
      return b
    }
  }
  if ((10>a || a>20)&&(10<=b && b<=20)){
    return b
  }
  else {
    return a 
  }
  
} 
  

