def insertionSort() {
  for(i<- 1 to getNombreValeurs()-1){
    val v = getValeur(i)
    var x = i
    var y:Boolean = false
    var j = i-1 
    while(!y && j >= 0){
      y = true
      if(!plusPetitQue(j,v)){
        copie(j,j+1)
        x = j
        y = false
      }
      j -= 1
    }
    setValeur(x,v)
  }
}
