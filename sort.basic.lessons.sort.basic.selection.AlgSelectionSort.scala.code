def selectionSort() {
  var min = 0
  var fini = false
	while (!fini && min<getNombreValeurs()-1) {
    fini=true
    if (plusPetit(min+1,min)) {
      min=min+1
      fini=false
    }
    for (i <- 0 to getNombreValeurs()-1) {
      if (plusPetit(min,i)) {
        echange(min,i)
      }
    }
  }
}
