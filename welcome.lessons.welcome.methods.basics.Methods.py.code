
def goAndGet():
  """Avance, récupère le biscuit, et le ramène à la position d'origine

  Ne vérifie pas la présence de mur; à ne pas l'utiliser en cas de risques de mur."""
    compteur=0
    while estSurBiscuit()==False:
        avance()
        compteur+=1
    prendBiscuit()
    for retour in range(compteur):
        recule()
    poseBiscuit()     
   
 

  

for i in range(7):
