def array123(nums:Array[Int]): Boolean = {
	var flag1:Boolean = false;
	var flag2:Boolean = false;
	var flag3:Boolean = false;
	for(i<- 0 to nums.length-1) {
		if (nums(i) == 1) flag1 = true;
		else if (nums(i) == 2) flag2 = true;
		else if (nums(i) == 3) flag3 = true;
	}
	flag1 && flag2 && flag3;
}
