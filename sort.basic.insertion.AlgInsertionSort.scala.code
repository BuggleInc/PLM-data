def insertionSort() {
  var n:Int=getNombreValeurs()
  for ( k <- 1 to (n-1)){
    var stock:Int=getValeur(k)
    var compteur:Int=0
    var x:Int=0
    for (l <- (k-1) to 0 by -1){
      var compa:Int=getValeur(l)
      if (compa > stock){
        setValeur((l+1),compa)
      }else if(compteur==0) {
        compteur=1
        x=l        
      }
    }
    setValeur(x,stock)
  }
}
