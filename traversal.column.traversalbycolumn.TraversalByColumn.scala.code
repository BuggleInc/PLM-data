override def run() {
  var cpt=0
  var x=0
  var y=0
  do {
    ecritMessage(cpt)
    y+=1
    setPos(x,y)
    cpt+=1
    if (y==6) {
      x+=1
      y=0
      ecritMessage(cpt)
      setPos(x,y)
      cpt+=1
    }
  } while (fonctionTest(x,y)==false)
}

def fonctionTest(x:Int, y:Int) {
  var res:Boolean=false
  if (x==6) {
    if (y==6) {
      res=true 
    }
  }
  return res
}