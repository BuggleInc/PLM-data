def cocktailSort() {
  var flag = false;
  var count = 0;
  var nb = getValueCount();
	do{
		flag = false;
    if(count % 2 == 0){
      for(i <- 0 to nb-2){
        if(!isSmaller(i,i+1)){
          swap(i,i+1);
          flag = true;     
        }
      }
    } if(count % 2 == 1){
      for(i <- nb-1 to 1){
        if(isSmaller(i+1,i)){
          swap(i+1,i);
          flag = true;     
        }
      }
    }
    count = count + 1;
  }while(flag);
}