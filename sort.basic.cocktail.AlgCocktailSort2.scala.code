def cocktailSort2() {
  var mouv:Boolean=true
  val lgr:Int=getNombreValeurs()
  var fin=lgr-2
  var deb=0
  while(mouv){
    mouv=false
   for(i<-deb to fin){if(plusPetit(i+1,i)){echange(i,i+1);mouv=true}}
    fin-=1
   for(i<-fin to deb){if(plusPetit(i+1,i)){echange(i,i+1);mouv=true}}
    deb+=1
  }
}