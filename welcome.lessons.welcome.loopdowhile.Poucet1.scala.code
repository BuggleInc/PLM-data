var pascouloir = 0
var nbcookie = 0
var sortie = 0
while (sortie==0) {
  nbcookie = 0;
  for (pascouloir <- 1 to 5) {
    avance();
    if (estSurBiscuit()) {
      nbcookie = nbcookie + 1;
    };
  }
  if (nbcookie<3) {
    droite();
  } else {
    gauche();
  }
  if (sortieTrouvee()) {
    sortie=1;
    avance();
  }
}