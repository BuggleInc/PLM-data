def gnomeSort() {
  
  var len:Int = getNombreValeurs()
  var end:Boolean = false
  var i:Int = 0
  
  while (!end) {
    if (plusPetit(i,(i+1))) {
      i += 1
    } else {
      echange(i,(i+1))
      i -= 1
    }
    if (i == -1) {
      i += 1
    } else if (i == len-1) {
      end = true
    }
  }
}