var diag = 0;
override def run() {
	var x=0
	var y=0
	var cpt=0
	ecritMessage(cpt);
	
	for (i<- 1 to getMondeHauteur()-1) {
		x=0
		y=i
		setPos(x,y);
		diag=x+y
		for (c<- 0 to diag) {
			cpt+=1
			ecritMessage(cpt);
			if (estSurMessage()) {
			y-=1
			x+=1
			setPos(x,y);
			}
		}	
	}
	for(i<- 0 to getMondeLargeur()-1) {
		x=i
		y=getMondeHauteur()
		setPos(x,y);
		diag=y-i
		for (c<- 0 to diag-1) {
			cpt+=1
			ecritMessage(cpt);
			if (!estSurMessage()) {
			y-=1
			x+=1
			setPos(x,y);
			}
		}
	}
}
