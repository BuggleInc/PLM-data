def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
	if (ASpeed(speed, isBirthday)<60){
		return 0;
	}
	if (ASpeed(speed, isBirthday)<80 && ASpeed(speed, isBirthday)>60 ){
		return 1;
	}else{
		return 2;
	}
}


def ASpeed (speed:Int, isBirthday:Boolean) :Int = {
	if(!isBirthday){
		return speed;
	}
	if(isBirthday){
		return ( 5 + speed);
	}
}
