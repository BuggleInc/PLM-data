def hunt() {
	while(!estSurBiscuit()){
    while(isFacingTrail()){
    	baisseBrosse()
      leveBrosse()
      avance()
  	}
    gauche()
  }
  prendBiscuit()
}

def isFacingTrail():Boolean = {
  if(!estFaceMur()){
    avance()
 	 	if(getCouleurSol()==Color.green){
   		recule()
    	return true
  	}
  	else{
    	recule()
    	return false
  	}
  }
  else{
    return false
  }
}