override def run() {
  var dir:Int=0;
  setDirection(Direction.NORTH)
  while(!isOverBaggle()) {
    while (!isFacingWall()) {
      forward()
    }
    while (!isOverBaggle() && dir !=4) {
      right();dir+=1
      if (!isFacingWall()) {
        forward()
      }
      else {
        left();dir-=1
        if (isFacingWall()) {
          left();dir-=1
          if (isFacingWall()) {
            left();dir-=1
            forward()
          }
          else {
            forward()
          }
        }
        else {
          forward()
        }
      }
    }
  }
  pickupBaggle()
}