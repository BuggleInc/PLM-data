n=0
while getY() != getWorldHeight()-1 or getX() != getWorldWidth()-1:
    writeMessage(n)
    if getY()<getWorldHeight()-1 and getX()<= getWorldWidth()-1:
        setPos(getX(),getY()+1)
    elif getY()==getWorldHeight()-1 and getX()<= getWorldWidth()-1:
        setPos(getX()+1,0)
    n +=1
writeMessage(n)