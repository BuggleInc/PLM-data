def step(rule:Array[Char], colors:Array[Color]) {
  var x =0
  var c= getCouleurSol()
  for(i<-0 to colors.length - 1){
    if(c == colors(i)){x=i}
  }
       setCouleurBrosse(colors((x+1)%colors.length));baisseBrosse();leveBrosse()
    if(rule(x)=='L'){gauche();avance();}
    else{droite();avance()}
  }
