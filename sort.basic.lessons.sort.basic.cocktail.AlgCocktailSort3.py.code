


debut = 0
fin = getNombreValeurs()-1



fini = False

while not fini:
    action  = False
    for i in range(debut,fin):
        if plusPetit(i+1,i):
            echange(i+1,i)
            action = True
    fin = fin - 1
    if action == False:
        break
    action  = False
    for i in range(fin,debut,-1):
        if plusPetit(i,i-1):
            echange(i,i-1)
            action = True
    debut = debut + 1
    if action == False:
        fini = True
        





