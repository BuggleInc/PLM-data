def cocktailSort2() {
  var debut = 0;
  var fin = getNombreValeurs() -2;
  var nbOps = 1;
  while (nbOps > 0) {
    nbOps = 0;
    for (i<- debut to fin) {
      if (plusPetit(i+1, i)) {
        echange(i, i+1);
        nbOps += 1;
      }
    }
    fin -= 1;
    for (i<- fin to debut by -1) {
      if (plusPetit(i+1, i)) {
        echange(i, i+1);
        nbOps += 1;
      }
    }
    debut += 1;
  }
}