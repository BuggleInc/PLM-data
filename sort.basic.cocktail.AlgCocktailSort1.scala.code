def cocktailSort() 
{
  var echanger=false
  do
  {
    for (i<-0 to getNombreValeurs()-2)
    	{
        if (plusPetit(i+1,i))
        	{
            echange(i,i+1)
            echanger=true
          }
      }
    for (j<- getNombreValeurs()-2 to 0)
    	{
        if (plusPetit(j,j-1))
        	{
            echange(j,j-1)
            echanger=true
          }
      }
  }
  while(echanger)
}
