def bubbleSort3() {
  
  for (i <- 0 to getNombreValeurs()-2 by -1) {
    
    for (j <- 0 to i) {
      
    	var comptr = 0
      
      if (!plusPetit(j,j+1)) {
          
      	echange(j,j+1)
          
      }
    
      comptr = comptr + 1
         
    	}
      
      if (comptr == i+1) {
        
        return
        
      }
      
  }
  
}