def cocktailSort() {
  var debut:Int=0;
  var fin:Int=getValueCount()-2;
  var echanges:Boolean=true
  
	while(echanges==true){
    echanges=false;
    for (i<-debut to fin){
      if(isSmaller(i+1,i)){
        echanges=true;
        swap(i,i+1);
      }
    }
    fin-=1;
    for(i<-fin to debut by (-1)){
       if(isSmaller(i+1,i)){
        echanges=true;
        swap(i,i+1);
    	}
  	}
    debut+=1;
	}
}