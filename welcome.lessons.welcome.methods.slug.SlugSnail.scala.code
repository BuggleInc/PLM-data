def hunt(c:Color) { 
	while (!estSurBiscuit) {
   		leveBrosse()
    		if (isFacingTrail(c) == true) {
	    		avance()
	    		baisseBrosse()
	    		
	    	}else{
    			gauche()}
    	
    	}
    	prendBiscuit()
}
    


def isFacingTrail(c:Color):Boolean = {      
	var couleur :Boolean = false
   	if (!estFaceMur()) {
		avance()
		if(getCouleurSol() == c){
			couleur = true
		}
			recule()
   	}
	return couleur	
}