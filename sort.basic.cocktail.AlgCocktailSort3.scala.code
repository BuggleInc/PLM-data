def cocktailSort3() {
var fini=false
var nop =0
var pon=0
var a=0
var b=0
  while(!fini){
    for(i<-b to getNombreValeur()-2-a){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        nop=1
      }
    }
    if(nop==0){
      fini=true
    }
    else{
      nop=0
    }
    a+=1
    for(i<-a to getNombreValeurs()-2-b){
      if(!plusPetit(getNombreValeurs()-i-2, getNombreValeurs()-i-1)){
        echange(getNombreValeurs()-i-2, getNombreValeurs()-i-1)
        pon=1
      }
    }
    b+=1
    if (pon==0){
      fini=true
    }
    else{po,=0}
  }