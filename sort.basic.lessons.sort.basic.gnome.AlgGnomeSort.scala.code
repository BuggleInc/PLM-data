def gnomeSort() {
  var taille = getNombreValeurs()
  var avance = 1
  while (avance != taille)
  {
    if (avance != 0)
    {
      if (!plusPetit(avance,avance+1))
      {
        echange(avance,avance+1)
        avance = avance-1
      }
      else
      {
        avance = avance + 1
      }

    }
    else 
    {
      avance = avance +1
    }
  }
}