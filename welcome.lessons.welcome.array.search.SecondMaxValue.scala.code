def max2Value(nums:Array[Int]): Int = {
  var max= nums(0)
  var indiceMax= 0
  for( i <- 1 to nums.length-1){
    if(max<nums(i)){
      max= nums(i)
      indiceMax=i
    }
  }
  var max2=0
  
  if(indiceMax==0){max2=nums(1)}
  else{max2=nums(0)}
  
  for( i <- 0 to nums.length-1){
    if(max2<nums(i) && i!=indiceMax){max2=nums(i)}
  }
  return max2
}
