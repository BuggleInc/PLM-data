public void run() { 
	while(!isOverBaggle()){ // bacwards till baggle
		backward();
		//pickUpBaggle();
		while(isOverBaggle()) {
			pickUpBaggle();
			while(!isOverBaggle()){
				forward();
				if(isOverBaggle()){
					backward();
					dropBaggle();
				}
			}
		}
		
	}
}
//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().
