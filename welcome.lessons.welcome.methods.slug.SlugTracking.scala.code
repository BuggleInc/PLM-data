def isFacingTrail():Boolean = {
	var solVert:Boolean = false
	if(estFaceMur) {
		gauche()
		return solVert	
	}
	else {
		avance()
		getCouleurSol()
		if(getCouleurSol == Color.green) {
			solVert = true
		}
		recule()
		if(solVert == true) {
			avance()
		}
		
		else {
			gauche()
		}
	}
	return solVert
}