override def run() {
	def finale():Boolean = {
		if(!estFaceMur) {
			return false
		}
		val dir = getDirection()
		setDirection(Direction.NORTH)
		val mur = estFaceMur()
		setDirection(dir)
		return mur
	}
	def snake() {
		while(!estFaceMur) {
			avance()
		}
		if(getDirection() == Direction.EST) {
			gauche()
			if(!estFaceMur) {
				avance()
			}
			gauche()
		}
		else {
			droite()
			if(!estFaceMur) {
				avance()
			}
			droite()
		}
	}
	snake()
	}
}

	
		
