def lessBy10(a:Int, b:Int, c:Int):Boolean = {
  var res:Int=0
  if ((Math.abs(a))>10){
    res=a-10
    return ((res<=b)||(res<=c))
  }
  else if ((Math.abs(b))>10){
    res=b-10
    return ((res<=a)||(res<=c))
  }
  else if ((Math.abs(c))>10){
    res=a-10
    return ((res<=b)||(res<=a))
  }
  else {
    return false
  }
}

