def bubbleSort3() 
{
  var echanger=false
  do
  	{
  		for (i<- getNombreValeurs()-2 to 0 by -1)
  			{
      		for (j<-0 to i)
      			{
      				if (plusPetit(j+1,j))
      					{
          				echange(j+1,j)
              		echanger=true
        				}
    				}
    		}
    }
  while(echanger)
}