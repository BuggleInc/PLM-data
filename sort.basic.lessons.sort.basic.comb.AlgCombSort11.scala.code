def combSort11() {
  var ecart:Int = getNombreValeurs()
  var sorted = false
  do{
    sorted = true
    if (ecart >1) {
      ecart = (ecart.toDouble/1.3).toInt
      if (ecart==10 || ecart==9) ecart=11
    }
    var i:Int =0
    while( i+ecart < getNombreValeurs()){
      if (! plusPetit(i,i+ecart)) {
        echange(i,i+ecart)
        sorted = false
      }
      i = i+1
    }
  } while(ecart > 1 || ! sorted )
}