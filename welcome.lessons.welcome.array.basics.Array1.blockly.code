color = None
i = None

def mark(color):
  global i
  setBrushColor(color)
  brushDown()
  brushUp()

def upRange(start, stop, step):
  while start <= stop:
    yield start
    start += abs(step)

def downRange(start, stop, step):
  while start >= stop:
    yield start
    start -= abs(step)


i_end = float(getWorldHeight())
for i in (0 <= i_end) and upRange(0, i_end, 1) or downRange(0, i_end, 1):
  mark(None)
