
def shellSort()  {
  
  var n : Int =getValueCount()
  var gap : Int = (n/2).toInt
  
  
  while(gap>0){
    
      var n : Int = getValueCount()
  
      for(it<-gap to n-1){

				var k : Int = 1
        
        while(it - k*gap >=0 && isSmaller(it-(k-1)*gap,it-k*gap)){
          swap(it-k*gap,it-(k-1)*gap)
          k+=1
        }
        
      }
    	
    	gap=(gap/2).toInt
  }
}