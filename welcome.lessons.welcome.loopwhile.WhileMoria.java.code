public void run() { 
	while(!isOverBaggle()){
		backward();
		if(isOverBaggle()){
			pickUpBaggle();
			while(isCarryingBaggle()){
				forward();
				while(isOverBaggle()){
					backward();
					if(!isOverBaggle()){
						dropBaggle();
					}
					
				}
			}
			
		}else {
			backward();
		}
	}
}
//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().
