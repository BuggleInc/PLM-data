def shellSort()  {
	val l = getNombreValeurs() -1
	var gap = getNombreValeurs()/2
	while ( gap > 0) {
		for (i<- 1 to l ) {
			var p = i
			var v = getValeur(i)
			var ind = 1
				while ((p >  0) && !plusPetitQue(p-(ind*gap),v)) {
					setValeur(p,getValeur(p-(ind*gap)))
					p -=1
					ind += 1
			}
			setValeur(p,v)
					
			
		}
		gap = gap/2
	}
	
}
