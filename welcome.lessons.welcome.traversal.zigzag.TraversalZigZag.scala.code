def test():Boolean= {
  if ((getX()==getMondeHauteur()-1) & (getY()==getMondeLargeur()-1)) {
    return true
  }
  return false
}


def exo() {
  var w=0 
  while (!test()) {
    if (getX() == getMondeHauteur() -1){
      ecritMessage(w)
      setPos(getMondeHauteur()-1,getY()+1)
      w=w+1
    }
    ecritMessage(w)
    setPos(getX()+1,getY())
    w=w+1
   
  }
  ecritMessage(48)
}







override def run() { exo()

