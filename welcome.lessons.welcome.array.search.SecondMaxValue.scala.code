def max2Value(nums:Array[Int]): Int = {
  var max1 : Int=nums(0)
  var max2 : Int=nums(0)
  if (nums(0)>nums(1)){
    max2=nums(1)
  }
  var l : Int=nums.length
  for (j<- 1 to l-1){
    if (nums(j)>=max1){
      max2=max1
      max1=nums(j)
    }
    if (nums(j)<max1 && nums(j)>max2)
    max2=nums(j)
  }
  return max2
}
