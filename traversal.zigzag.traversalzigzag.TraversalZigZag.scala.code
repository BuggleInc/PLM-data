override def run() {
  var n=0
  marque(n)
  setPos(1,0)
  n=1
  marque(1)
    while(getMondeLargeur()-1!=getX()){
    setPos(getX()+1,getY())
    n=n+1
    marque(n)
  }
  while(!positionFinale){
    if(getX()==0){
      setPos(getX(),getY()+1)
      n=n+1
      marque(n)
      while(getMondeLargeur()-1!=getX()){
    setPos(getX()+1,getY())
    n=n+1
    marque(n)
  }
   } else {
      setPos(getX(),getY()+1)
      n=n+1
      marque(n)
       while(0!=getX()){
    setPos(getX()-1,getY())
    n=n+1
    marque(n)
    
    }
  
   }
  }
}

def positionFinale():Boolean = {
  if(getMondeHauteur()-1==getY() && getMondeLargeur()-1==getX()){
    return true
  } else {
    return false
  }
}



def marque(x:Int){
  ecritMessage(x)
}

