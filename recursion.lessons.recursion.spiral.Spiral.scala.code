def spiral(steps:Int, angle:Int, length:Int, increment:Int)    {

var longueur=length
var nbPas=steps

	if(steps==0)
	{
	return
	}
	
	avance(length)
	gauche(angle)
	nbPas-=1
	longueur=longueur+increment

		
		else
		{
		spiral(steps:Int, angle:Int, length:Int, increment:Int)
		}

}

























