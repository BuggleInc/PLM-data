override def run() {

var T = new Array[Color](getMondeHauteur());
for(n<-0 to getMondeHauteur()-1){
	for(i<-0 to getMondeHauteur()-1){
		T(i)=getCouleurSol();
		avance();
	}
	for(k<-0 to getMondeHauteur()-1){recule()}
	gauche();avance();droite();
	val decalage=litMessage().toInt;
	for(j<-decalage to getMondeHauteur()-decalage){
		setCouleurBrosse(T(j));
		baisseBrosse();
		leveBrosse();
		avance();
	}
	for(h<-0 to decalage){
		setCouleurBrosse(T(h));
		baisseBrosse();
		leveBrosse();
		avance();
	}
}
}