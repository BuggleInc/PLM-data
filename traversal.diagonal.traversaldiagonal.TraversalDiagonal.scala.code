var diag:Int = 1;
override def run() {
  var x:Int = 0
		ecritMessage(x)
   while (getMondeHauteur()-1 !=getY() || getMondeLargeur()-1 != getX()){
     if (diag<getMondeHauteur()){
for (i <- 0 to diag){
  x+=1
  setPos(i,diag-i)
  ecritMessage(x)
}
       diag+=1
     }
     else{
       for (i <-0 to diag-1){
  x+=1
  setPos(i+1,diag-1-i)
  ecritMessage(x)
     }
       diag-=1
     }
     
      
}
}