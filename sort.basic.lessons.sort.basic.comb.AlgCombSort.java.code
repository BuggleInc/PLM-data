public void combSort() {

	boolean fini=false;
	int gap = getNombreValeurs();

	while(!fini || gap>1){
		fini=true;
		
		if(gap>1)
			gap=gap/1.3;
		
		int i=0;
		while(i+gap < getNombreValeurs()){
			if(plusPetit(i+gap, i)){
				fini=false;
				int temp = getValeur(i);
				copie(i+gap, i);
				setValeur(i+gap, temp);
			}
		}

		i++;
	}
}
