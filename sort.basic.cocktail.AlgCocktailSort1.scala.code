def cocktailSort() {
  var echangefait:Boolean = true
  do {
    echangefait= false
    for (i <- 0 to getNombreValeurs()-2) {
      if (plusPetit(i+1, i)){
        echange(i, i+1)
        echangefait=true    
      }
    }
    for (i <- getNombreValeurs()-2 to 0) {
      if (plusPetit(i+1, i)){
        echange(i, i+1)
        echangefait=true
      }
  	}	
  } while (echangefait)
}