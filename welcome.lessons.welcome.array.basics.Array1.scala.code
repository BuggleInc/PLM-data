override def run() {
	var T=new Array[Color](getMondeHauteur())
	for (i <- 1 to getMondeHauteur()){
		T(i-1) = getCouleurSol();
		avance();
	}
	for (k <- 1 to getMondeLargeur()) {
		avance();
		gauche();
		avance();
		droite();
		for (j <- 1 to getMondeHauteur()) {
			setCouleurBrosse(T(j-1));
			baisseBrosse();
			leveBrosse();
			avance();
		}
	}
	for (l <- 1 to getMondeHauteur()) {
		avance();
	}
	gauche();
	for (m <- 1 to getMondeLargeur()) {
		avance();
	}
	droite();
}
