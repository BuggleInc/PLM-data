def hunt() {
  while (!estSurBiscuit()){ 
        if (isFacingTrail()){
           baisseBrosse()
           avance()
           leveBrosse()
        }
       else gauche()
       
  }
  prendBiscuit()
}



def isFacingTrail():Boolean = {
    
  var verte: Boolean = false
  if (estFaceMur()){
    return false
  }
  avance()
  verte=getCouleurSol()==Color.green
  
  recule()
  return verte
}