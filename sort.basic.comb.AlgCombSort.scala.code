def combSort()  {
  
  var tri = false
  var a = 0
  var b = 0
  var long = getNombreValeurs()-1
  var ecart = long
  
  while (ecart > 0 && tri == false)
  {
    tri = true
    for (i <- 0 to long-ecart)
    {
      a = getValeur(i)
      b = getValeur(i+ecart)
      if (a > b)
      {
        setValeur(i,b)
        setValeur(i+ecart,a)
        tri = false
      }
    }
		ecart = (ecart.toDouble / 1.3).toInt
  }
  return
}