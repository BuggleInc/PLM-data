def insertionSort() {
  var lgr = getNombreValeurs()
  var v = 0
  var j =1
  var casePourV = 0
  for (i <- 1 to lgr -1){
    v=getValeur(i)
    casePourV = i
    while (j<= i && !plusPetitQue(i-j,v)) {
        copie(i-j,i-j+1)
        casePourV = i-j
        j=j+1
    }
    setValeur(casePourV-1 , v) 
  }
}

