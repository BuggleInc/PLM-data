def island(num:Array[Int]): Int = {
  var compt:Int = 0
  var niveauActuel:Int = num(0)
  for (n<-1 to num.length -1){
    if (num(n)>niveauActuel){
      niveauActuel = niveauActuel +1
    } else if (num(n)<niveauActuel){
      niveauActuel = niveauActuel +1
      compt = compt + 1
    }
  }
  return compt
}

