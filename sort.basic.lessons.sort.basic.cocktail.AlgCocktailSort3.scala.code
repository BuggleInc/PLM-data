def cocktailSort3() {
	
var length = getNombreValeurs(); 
var debut=0; var fin=length-2;
var echangé:Boolean=false;
do
{
	echangé=false
	
	for(i<-debut to fin){
		if(plusPetit(i+1,i)){
			echange(i,i+1);
			echangé=true;
			
		}
		
	}
	fin-=1;
	for(i<-fin to debut by -1){
		if(plusPetit(i+1,i)){
			echange(i,i+1);
			echangé=true;
			
		}
		
	}
	debut += 1;
}
while(echangé)

}
