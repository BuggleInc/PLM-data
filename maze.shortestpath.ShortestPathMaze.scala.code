def trouveDonut() : (Int, Int) = {
  var i : Int = 0 
  var j : Int = 0 
  while (!aBiscuit(i,j)){
    if (i==getMondeLargeur()-1){
      i=0
      j=j+1
    }
    i=i+1
  }
  return(i,j)
}

def Cartographie(i:Int, j:Int, done: Int) {
  var done : Int = done
  while(done != (getMondeHauteur()*getMondeLargeur())){
    if (getIndication(i,j)!=9999){
      setIndication(i,j,done)
      done=done+1
      (i,j) match {
        case (getMondeLargeur()-1,getMondeHauteur-1) => if (!aMurNord(i,j)) {Cartographie(i,j-1,done)}; if (!aMurOuest(i,j)){Cartographie(i-1,j,done)}
      }
    }
  }
  
}

override def run() {
  var x : Int = trouveDonut()._1
  var y : Int = trouveDonut()._2
  var done : Int = 0
  Cartographie(x,y,done)
}