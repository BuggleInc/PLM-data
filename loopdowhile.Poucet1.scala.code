var s=0
 for (stepper<-1 to 5){
    if (isOverBaggle()){
      forward()
      s=s+1
    }
do{
      if (s <= 2){
        right()
        for (stepper1 <- 1 to 5){
          forward()
        }
      }else {
        left()
        for (stepper2 <- 1 to 5){
          forward()
        }
      }
}while (!exitReached())