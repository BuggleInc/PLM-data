var diag = 0;
override def run() {
  var diag = 0
  var compt:Int=0
  for(a<-0 to 6){
    for (b<-0 to a){
      setPos(b,a-b)
      ecritMessage(""+compt)
      compt=compt+1
    }
  }
  for (a<-0 to 5){
    for (b<-0 to a){
      setPos(getMondeLargeur()-(a-b)-1,getMondeLargeur()-b-1)
      ecritMessage(""+compt)
      compt=compt+1
    }
  }
}
