def cocktailSort3() {

  var debut=0;  var fin=getNombreValeurs()-2
  var a=0

  do {
    a=0
    for (i <- debut to fin){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        a+=1
      }
    }
      fin=fin-1
    
    for (i <- fin to debut by -1){
      if (plusPetit(i+1,i)){
        echange(i,i+1)
        if (a>=1){
          a=2}
      }
    }
      debut=debut+1
    
  }
  while (a==2)
}
