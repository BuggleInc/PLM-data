def close10(a:Int, b:Int): Boolean = {
  if(a < 30){
    return false
  }
  if(a > 50){
    return false
  }
  if(b < 30){
    return false
  }
  if(b > 50){
    return false
  }
  if(a < 40){
    if(b < 40){
      return true
    }
    return false
  }
  if(a > 40){
    if(b > 40){
      return true
    }
    return false
  }
}