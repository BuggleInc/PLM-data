public void cocktailSort() {
	boolean swapped;
	do {
		swapped = false;
		for(int i = 0; i < getValueCount() - 1; i++)
			hasToSwap(i);
		for(int i = getValueCount() - 2; i >= 0 ; i--)
			hasToSwap(i);
	} while(swapped);
}	

boolean hasToSwap(int i){
	if(isSmaller(i + 1, i)){
		swap(i, i+1);
		return true;
	} else 
		return false;
}
