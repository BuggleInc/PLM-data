def alarmClock(day:Int, vacation:Boolean): String = {
  var x : String="7:00"
  if (vacation) {
    if (day==0) {
      x= "off"
    } else if (day==6) {
      	x= "off"
    } else {
      	x= "10:00"
    }
  } else {
    	if (day==0) {
      	x= "10:00"
    	} else if (day==6) {
      	x= "10:00"
    	}
  }
  x
}