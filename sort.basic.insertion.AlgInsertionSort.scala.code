def insertionSort() {
  var n:Int=getNombreValeurs()
  for ( k <- 1 to (n-1)){
    var stock:Int=getValeur(k)
    for (l <- (k-1) to 0 by -1){
      var compa:Int=getValeur(l)
      if (compa > stock){
        setValeur((l+1),compa)
      }else {
        setValeur((l+1),stock)
        return
      }
    }
  }
}