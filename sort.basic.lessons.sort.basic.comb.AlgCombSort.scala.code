def combSort() {
  var taille = getNombreValeurs()
  var ecart  = getNombreValeurs()
  var parcours = true
  
  	while(ecart > 1 || parcours)
  	 {if (ecart>1){
       ecart = (ecart.toDouble / 1.3).toInt}
        var i = 0
        while(i+ecart<taille)
        {parcours = false
          if(plusPetit(i+ecart,i))
          	{
              echange(i,i+ecart)
              
              parcours = true
            }
         i = i + 1
        }
        
      }
  
}