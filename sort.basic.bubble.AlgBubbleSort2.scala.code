def bubbleSort2()  {
  var listeTriee:Boolean=true
  var x=0
  x=getNombreValeurs
  do {
    listeTriee=true
    for (i <- x-2 to 0) {
      listeTriee=true
      for (j <- 0 to i) {
       if (plusPetit(j+1,j)) {
         echange(j+1,j)
         listeTriee=false
       }
       }
    }
  } while (listeTriee)
}