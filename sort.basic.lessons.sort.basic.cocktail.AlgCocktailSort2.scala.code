def cocktailSort2() {
  var s = true
  var debut = 0
  var fin = getNombreValeurs()-2
  do{
    s=false
    for (i<- debut to fin){
      if (!plusPetit(i, i+1)){
        echange(i, i+1)
        s=true
      }
    }
    fin = fin-1
    for (i<- fin to debut by -1){
      if (!plusPetit(i, i+1)){
        echange(i, i+1)
        s=true
      }
    }
    debut = debut +1
  }while (s && debut>1)
}