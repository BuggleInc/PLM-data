$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class fizzBuzz extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( fizzBuzz((Integer)t.getParameter(0)) ); }
	String fizzBuzz(int a) {
		if (a%5 == 0 && a%3 ==0) 
			return "Fizz Buzz";
		else if (a%5 ==0)
			return "Buzz";
		else if (a%3 == 0)
			return "Fizz";
		return ""+a;
	}
 }