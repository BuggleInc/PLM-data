def cocktailSort3() {
  var tri=true;
  var deb=0;
  var fin=getValueCount()-2;
while(tri==true){
  tri=false;
  for(i<- deb to fin){
    if(isSmaller(i+1,i)){
 			swap(i,i+1);
      tri=true;
      }
  }
  if(tri){
    fin=fin-1;
 
  for(i<- fin to deb by -1){
    if(isSmaller(i+1,i)){
 			swap(i,i+1);
       tri=true;
      }
   
    }
    deb=deb+1;
    
}

}
}