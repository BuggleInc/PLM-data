def fairemotif():
    couleur=[Color.YELLOW,Color.RED,Color.BLUE,Color.GREEN]
    for i in range (len(couleur)):
        color=couleur[i]  
        motif(color)  
        forward(2)


def motif(color):
    setCouleurBrosse(color)
    for i in range(2):
        forward()
        baisseBrosse()
        leveBrosse()
        gauche()
    forward()
    gauche()

fairemotif()
    
