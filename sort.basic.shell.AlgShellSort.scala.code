def shellSort()  {

  var lgr = getNombreValeurs();
  var ecart = lgr/2;
  while(ecart>0){
    insertionSort(ecart);
    ecart = (ecart.toDouble / 2).toInt;
  }

}

def insertionSort(ecart:Int){
  var lgr = getNombreValeurs();
  var v = 0 ;
  var j = 1 ;
	for(i<- 1 to lgr-1){
    j = 1 ;
    v = getValeur(i);
    while((j*ecart)<=i && v<getValeur(i-(j*ecart))){
      copie(i-(j*ecart),i-(j*ecart)+1);
      j += 1;
    }
    setValeur(i-j,v)
  }
}