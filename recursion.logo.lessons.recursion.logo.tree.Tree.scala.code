def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
  if(steps<1){
    return

  }else{
    current(steps)
    avance(length)
    droite(angle)
    (tree(steps-1,shrink*length,angle,shrink))
    if(steps<1){
      recule(length)
      gauche(angle)
      current(steps)
      
    
      return (tree(steps-1, length*shrink, angle, shrink))
    }



  }
}