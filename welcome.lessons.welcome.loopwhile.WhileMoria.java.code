public void run() { 
	while(!isOverBaggle()){
		backward();
		if(isOverBaggle()){
			pickUpBaggle();
			while(isCarryingBaggle()){
				while(!isOverBaggle()){
					forward();
					if(isOverBaggle()){
						backward();
						dropBaggle();
					}
				}
			}
			
		}else {
			backward();
		}
	}
}
//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle().
