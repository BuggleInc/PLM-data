def cocktailSort() {
	var x:Boolean=true
	do{x=false
		for(i<-0 to getNombreValeurs()-2){
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				x=true
			}
			return
		}
		for(i<-getNombreValeurs()-2 to 0){
			if(!plusPetit(i,i+1)){
				echange(i,i+1)
				x=true
			}
			return
		}
		
	}
	while(x==true)
}
