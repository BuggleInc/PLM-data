def bubbleSort2()  {
  var swapped = false;
  
  do {
    swapped = false;
    for (k <- 0 to getValueCount()) {
      var i = getValueCount()-2 - k
      print(i + " " + getValueCount() + "\t")
      
      for (j <- 0 to i) {
        if (isSmaller(j+1, j)) {
          swap(j, j+1);
          swapped = true;
        }
      }
    }
    
  } while (swapped);
}