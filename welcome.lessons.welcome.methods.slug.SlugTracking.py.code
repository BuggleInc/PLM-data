def isFacingTrail():
    if True:
       forward()
       if getCouleurSol()==Color.green:
           backward()
           return True
       else:
           backward()
           return False
    return False

while estSurBiscuit==False:
    if isFacingTrail()==True:
        avance()
        baisseBrosse()
        leveBrosse()
    else:
        left()    