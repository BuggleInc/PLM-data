def cocktailSort3() {
	var up=false
	var down=false
	var debut=0
	var fin=getNombreValeurs()-2
	while (!up && !down) {
		up=true
		down=true
		for (i<-debut to fin) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				up=false
			}
		}
		fin-=1
		for (i<-fin to debut by -1) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				down=false
			}
		}
		debut+=1
	}
}