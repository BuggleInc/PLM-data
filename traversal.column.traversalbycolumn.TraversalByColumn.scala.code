override def run() {
  var cpt:Int = 0
  while(!positionFinale) {
    ecritMessage()
    if(getY()<(getMondeHauteur()-1)) {
      setPos(getX(), getY()+1)
    } else setPos(getX()+1,0)
    cpt+=1
  }
}

def positionFinale():Boolean = {
  if (getX()==(getMondeHauteur()-1) && getY()==(getMondeLargeur()-1)) return true
  return false
}
