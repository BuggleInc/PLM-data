def cocktailSort() {
  
  var trier:Boolean=false
  
  
  do{
    for(i<-0 to getNombreValeurs()-2){
      trier=false
      if(plusPetit(i,i+1)){
        echange(i,i+1);
        trier=true;
      }
    }
    
    for(i<- getNombreValeurs()-2 to 0 by -1){
      trier=false
      if(plusPetit(i,i+1)){
        echange(i,i+1);
        trier=true;
      }
    }
    
  } while(trier);
  
}