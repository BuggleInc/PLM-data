while sortieTrouvee() == False:
    premiereFois = True
    while premiereFois or not croisement():
        premiereFois = False
        nbBiscuit = 0
        forward()
        if estSurBiscuit() == True:
            nbBiscuit = nbBiscuit + 1
        if nbBiscuit >= 3:
            gauche()
        else:
            droite()
forward()
