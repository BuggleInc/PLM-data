def golomb(num:Int): Int = {
  var c:Int = 0;
  var T1 = new Array[Int](num);
  var T2 = new Array[Int](num);
  for ( i <- 1 to num ){
    T1(i-1) = i;
  }
  for ( i <- 0 to T1.length-1 ){
    if ( c > num ){
      return T2(num);
    }
    while( T1(i) != 0 ){
      T2(c) = i;
      c += 1
    }
  }
  return T2(num);
}
  