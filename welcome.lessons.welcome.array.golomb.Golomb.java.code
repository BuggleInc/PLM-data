int golomb(int num) {
	int[] result = new int[num + 1];
	result[1] = 1;
	for (int i = 1; i <= num; i++){
		int timesthisnumber = result[i];
		int pointer = i;
		while (result[pointer] == 0) {pointer+;}
		for (int iii = pointer; iii < pointer + timesthisnumber; iii++){
			result[iii] = i;
			pointer++;
		}
	}
	return result[num];
}
