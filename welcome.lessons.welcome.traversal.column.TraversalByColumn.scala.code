override def run() {
  while (positionFinale()==false){
    positionSuivante()
  }
}

def positionFinale(){
  if (getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1){
    return true
  }
  return false
}

def positionSuivante(){
  var cpt : Int = 0
  for (i <- 0 to getMondeLargeur()-1){
    for (j <- 0 to getMondeHauteur()-1){
      cpt=cpt+1
      setPos(i,j)
      ecritMessage(cpt)
    }
  }
}