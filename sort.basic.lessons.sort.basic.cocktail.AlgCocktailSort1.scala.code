def cocktailSort() {
  var ech=true
  var taille=getNombreValeurs()
	var j=0
  
  while (ech){
   
    ech=false
    for (i<-0 to taille-2)
      if (!plusPetit(i,i+1)){
        echange(i,i+1)
        ech=true
      }
    j=taille-2
    while (j>0){
      if (!plusPetit(j,j+1)){
        echange(j,j+1)
        ech=true
      }
      j-=1
      
    }
    
  }
      
  
}