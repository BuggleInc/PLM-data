//isOverBaggle(), isCarryingBaggle(), pickupBaggle() or dropBaggle()

def move() = {
 	if(isCarryingBaggle()){
   do{
     forward()
   } while(!isOverBaggle())
   backward()
   dropBaggle()
   right();right()
  } else {
    do{
      forward()
    } while(!isOverBaggle())
    pickupBaggle()
    right();right()
  }
}

def leave() = {
  right();forward();left();forward()
}




def makeHisWayOut() = {
  left();left()
  while(!isFacingWall()){
    move()
  }
  leave()
}



makeHisWayOut()