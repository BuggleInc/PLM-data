def cocktailSort2() {
  var debut:Int = 0
  var fin:Int = getNombreValeurs()-2 
  var l:Int = 0
  for (k <- 0 to getNombreValeurs()-1) {
    for (i <- (debut + l) to (fin-l)) {
      
     if (plusPetit(i, i+1) == false) {
       
       echange(i, i+1)
       fin = fin - 1
     }  
    }
    for (j <- (fin-l) to (debut+1) by -1) {
      
      if (plusPetit(j, j+1) == false) {
        
        echange(j, j+1)
        debut = debut + 1
      }  
    }
    l = l + 1
	}
}
