def stringTimes(str:String, n:Int):String = {
  var s = ""
                                             for (i <- 0 to n-1) {s=s+str};return s}

def frontTimes(str:String, n:Int):String = {
  var s = ""
  	if (str.length <3) {s = str}
  	else s = str.charAt(0).toString + str.charAt(1).toString + str.charAt(2).toString
  val st = stringTimes(s,n)
  return st}
