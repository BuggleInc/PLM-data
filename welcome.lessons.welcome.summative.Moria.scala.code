def sortirMoria(){
  var a:Boolean=true
  while (getCouleurSol!=Color.green){
    retourne()
    avance()
    while (a){
      if (estFaceMur()){a=false}
      else{if (estSurBiscuit()){a=false}else{avance()}}
    }
    a=true
    if (estSurBiscuit()){
      prendBiscuit()
    	retourne()
    	while (!estSurBiscuit()){
      	avance()
    	}
    	recule()
      poseBiscuit()
    }else{droite();avance();gauche();avance()}
	}
}
sortirMoria()