$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; import lessons.recursion.cons.universe.*;import java.util.Vector;public class remove extends BatEntity { public RecList cons(int head, RecList tail){  return new RecList(head, tail);} @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( remove((RecList)t.getParameter(0), (Integer)t.getParameter(1)) ); }
	RecList remove(RecList seq, int v) {
		if (seq == null)
			return null;
		if (seq.head == v)
			return remove(seq.tail, v);
		return cons(seq.head, remove(seq.tail, v));
	}
 }