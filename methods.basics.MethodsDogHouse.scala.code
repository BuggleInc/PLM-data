var bx = true

def dogHouse()
{
  if(bx)
  {
    bx = false
    
    for(i <- 1 to 6)
    {
      forward()
    }

    setPos(1, 6)

    left()

    for(i <- 1 to 6)
    {
      forward()
    }

    setPos(1, 6)
  }
  else 
  {
    //setPos(0, 6)
    var x = getX()
    var y = getY()
    
    setBrushColor(Color.white)
    setPos(4, 0)
    setBrushColor(Color.red)
    setPos(x, y)
  }
}