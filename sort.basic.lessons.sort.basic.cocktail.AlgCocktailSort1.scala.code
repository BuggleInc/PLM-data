def cocktailSort() {
  var nbOps:Int = 0;
  do {
    nbOps = 0;
    for (i<- 0 to getNombreValeurs()-2) {
      if (plusPetit(i+1, i)) {
        echange(i, i+1);
        nbOps = nbOps + 1;
      }
    }
    for (j<- getNombreValeurs()-2 to 0 by -1) {
      if (plusPetit(j, j+1)) {
        echange(j, j+1);
        nbOps = nbOps + 1;
      }
    }
  } while (nbOps > 0)
}
