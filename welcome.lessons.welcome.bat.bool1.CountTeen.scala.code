def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var compteur: Int =0
  if (a<=19 && a>=13 ){
    compteur = compteur +1
  }
  else if (b<=19 && b>=13){
    compteur = compteur +1
  }
  else if (c<=19 && c>=13){
    compteur = compteur +1
  }
  else (d<=19 && d>=13){
    compteur = compteur +1
  }
  return compteur