override def run() {
  var cpt:Int=0
  ecritMessage(cpt)
  while (!positionFinale()){
    if (getY()==getMondeHauteur()-1){
      setPos(getX()+1,0)
    }else{
      avance()
    }
    cpt=cpt+1
    ecritMessage(cpt)
  }
}

def positionFinale(){
  return getX()==getMondeLargeur()-1 && getY()==getMondeHauteur()-1
}