def insertionSort() {
  var len = getNombreValeurs()
  var rgpositif:Boolean = true
  var v = 0
  var rg = 0
  var ind = 0
  for (i <- 1 to len-1) {
    rg = i-1
    rgpositif = true
    v = getValeur(i)
    while (rgpositif) {
    	if (getValeur(rg)>v) {
    		copie(rg,rg+1)
      	if (rg>0)
      		rg-=1
      	else
      		rgpositif = false
      }
      else
      	rgpositif = false
    }
    setValeur(rg,v)
  }
}
    	