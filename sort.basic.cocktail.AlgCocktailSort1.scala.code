def cocktailSort() {
  val n = getNombreValeurs()
  var i:Int=n-2
  var j:Int=0
  var faire:Boolean=true
  while(faire){
  faire=false
  for (j <- 0 to i){
  	if (plusPetit(j+1,j)){
      	echange(j,j+1)
      	faire = true
    	}
  	}
  for (j <- i to 0 by -1){
    if (plusPetit(j+1,j)){
      echange(j,j+1)
      faire = true
    }
  }
  }
}