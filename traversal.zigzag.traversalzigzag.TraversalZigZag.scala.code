override def run() {
  var cpt=0
  var x=0
  var y=0
  while (!(x==6 & y==6)) {
    if (y%2==0) {
    ecritMessage(cpt)
    x+=1
    setPos(x,y)
    cpt+=1
    if (x==6 & y!=6) {
      y+=1
      x=0
      ecritMessage(cpt)
      setPos(x,y)
      cpt+=1
    }
    } else if (y%2==1) {
      if (x==6 & y!=6) {
        y-=1
        ecritMessage(cpt)
        setPos(x,y)
        cpt+=1
      }
    }
  }
  ecritMessage(cpt)
}