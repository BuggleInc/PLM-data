override def run() {
  var cpt=1
  val h=getMondeHauteur()
  
  ecritMessage("0")
  
  while (!posFinale()){
    parcours()
    ecritMessage(cpt.toString)
  }
    
  
  }

def parcours()={
  var l=getMondeLargeur()
  if ((getX())==(l-1)){
    setPos(0,getY()+1)
  }
  else{
    setPos(getX()+1,getY())
    
  }
}




def posFinale():Boolean={
  val l=getMondeLargeur()
  val h=getMondeHauteur()
  
  return ((getX()==(l-1))&&(getY()==(h-1)))
}