override def run() {
var c=0
var x=0:Int
var y=0:Int

def avancer() {
	for (x<-0 to getMondeHauteur()-1)
	{setPos(x,y)
	ecritMessage(c)
	c=c+1
	x=x+1}
}


def reculer() {
	for (x<-0 to getMondeHauteur()-1)
	{setPos(x,y)
	ecritMessage(c)
	c=c+1
	x=x-1
}
}


def finale():Boolean={
 if (getX()==getMondeHauteur()-1 && getY()==getMondeLargeur()-1) 
 {
 	return true
 	} else {
 		return false
 		}
}

setPos(x,y)
while(!finale)
{
	avancer()
	y=y+1
	reculer()
	y=y+1
}
}