def cocktailSort() {
  val taille=getNombreValeurs()
  var fin=true
  do {
    fin=true
    for (i<- 0 to taille-2) {
      if (!plusPetit(i,i+1)) {
        echange (i,i+1)
        fin=false
      }
    }
    for (i<- taille-2 to 0 by -1) {
      if (!plusPetit(i,i+1)) {
        echange (i,i+1)
        fin=false
      } 
    }
  } while (!fin)
}
