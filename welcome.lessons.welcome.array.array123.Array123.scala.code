def array123(nums:Array[Int]): Boolean = {
  var isOne:Boolean = false
  var isTwo:Boolean = false
  var isThree:Boolean = false
  for (i <- 0 to nums.length -1) { 
    if (nums(i) == 1) {isOne = true}
    else if (nums(i) == 2) {isTwo = true}
    else if (nums(i) == 3) {isThree = true}   
  }
  if ((isOne == isTwo) && (isTwo == isThree) && (isOne == true)) {return true}
  else {return false}
}