override def run() {
  var i=0
	while(posFinale()==false){
    ecritMessage(i+"")
    nextPos()
    i=i+1
  }
  ecritMessage(i+"")
}

def posFinale(): Boolean={
  if (getX()==getMondeLargeur-1 & getY()==getMondeHauteur()-1)
  	return true
  return false
}

def nextPos(){
  if (getY()%2==0){
    if (getX()==getMondeLargeur()-1){
    setPos(getX(),getY()+1)
  	}
  	else{
    	setX(getX()+1)
  	}
  }
  else{
    if (getX()==0)
    	setPos(0,getY()+1)
    else
    	setPos(getX()-1,getY())
  }
}
