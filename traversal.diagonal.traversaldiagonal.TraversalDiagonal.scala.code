var diag = 0;
var finalpos : Boolean = true;
override def run() {
  while (finalpos==false){

    if (diag==6){
      setPos(diag,diag)
    writeMessage(diag)
    finalpos=true
  }
    else {
      for (a <- 0 to diag +1){
      setPos(diag-a+1,diag-a+1)
      writeMessage(diag)
      diag = diag + 1
      }
    }
  }
  
}