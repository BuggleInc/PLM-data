def clearMoria(){
  droite();droite();
  while(!estFaceMur()){
    while( !estSurBiscuit() && !porteBiscuit() && !estFaceMur()){
      avance();
    }
    if(estSurBiscuit()){
    	prendBiscuit();droite();droite()
    }
    while(!estSurBiscuit() && porteBiscuit()){
      avance();
    }
    if(porteBiscuit()){
    	recule();poseBiscuit();droite();droite();avance();
    }
  }
  setPos(getX() + 1,getY() - 1)
}


clearMoria();