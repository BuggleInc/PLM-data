def shellSort()  {
	
  var n: Int=getNombreValeurs
  for (i<- 1 to n-1){
    var ecart: Int=getNombreValeurs()/2
    while (ecart>0){
      ecart=ecart/2
      if (ecart!=0){
      	var v: Int=getValeur(i)
      	var l: Int=i
      	for (j<- i-ecart to 0 by -ecart){
        	if (!plusPetitQue(j,v)){
          	echange(j,j+1)
          	l=j
        	}
        };setValeur(l,v)}
    }
    
  }
}