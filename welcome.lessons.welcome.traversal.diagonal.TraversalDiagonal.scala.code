var diag = 0;
override def run() {
  var cpt:Int = 0
  ecritMessage(cpt)
  cpt+=1
  for (i <- 0 to 47) {
    if(estFaceMur()) {
      if (diag<6){
        setPos(0,diag+1)
      }
      if (diag>=6) {
        setPos(diag-5,6)
      }
    }
    else {
      setPos(getX()+1,getY()-1)
    }
    diag+=1
    ecritMessage(cpt)
    cpt+=1
  }
}
