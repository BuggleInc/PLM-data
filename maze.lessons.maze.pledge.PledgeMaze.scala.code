override def run() {
  var nordTrouve=false
  var virages=0
  while(!estSurBiscuit()){
    while (nordTrouve==false){
      while(nordTrouve==false && virages>0){
        choix2(virages)
        goNord()
        if (getY()==0){
          nordTrouve=true
          droite()
        }
      }
      while (nordTrouve==false && virages<4){
        choix1(virages)
        goNord()     
        if (getY()==0){
          nordTrouve=true
          droite()
        }
      }
    }
    choix1(virages)
  }
  prendBiscuit()
}




def keepHandOnSideWall():Boolean={
  gauche()
  if (estFaceMur()){
    droite()
    return true
  }
  else{
    droite()
    return false
  }
}

def goNord(){
  setDirection(Direction.NORTH)
  while(!estFaceMur()){
    avance()
  }
}

def choix1(virageIn:Int):Int={
  var virages = virageIn
  if (estFaceMur() && keepHandOnSideWall()){
    droite()
    avance()
    virages=virages-1
  }    
  else if (!estFaceMur() && keepHandOnSideWall()){
    avance()
  }
  else if (estFaceMur() && !keepHandOnSideWall()){
    gauche()
    avance()
    virages=virages+1
  }
  else if (!estFaceMur && !keepHandOnSideWall()){
    gauche()
    avance()
    virages=virages+1
  }
  return virages
}

