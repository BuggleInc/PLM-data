def max2Value(nums:Array[Int]): Int =
{
  var max1:Int = nums(0)
  var max2:Int = 0
  var max3:Int = nums(1)
  var id:Int = 0
  
  for(i <- 1 to nums.length-1)
  {
    if(max1 <= nums(i))
    {
      max2 = max1
      max1 = nums(i)
      id=i
    }
    if(max3 <= nums(i))
    {
      max3 = nums(i)
    }
  }
  
 	if(id==0)
  {
    max2 = max3
  }
  
  return max2
}