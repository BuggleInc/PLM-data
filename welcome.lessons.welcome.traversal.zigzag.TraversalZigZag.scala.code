override def run() {
  var cpt=0
  var h=getMondeHauteur
  var l=getMondeLargeur
  var x:Int=0
  var y:Int=0
  //var x:Int=0
  //var y:Int=0
    for (x <- 0 to l-1){   
      for (y <- 0 to h-1){
        if(y%2==0){
          setPos(x,y)
