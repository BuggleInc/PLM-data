def gnomeSort() {
  var finTri = false
  var i = 0
  while(finTri==false) {
    if(plusPetit(i+1,i)) {
      echange(i,i+1)
      if(i>0) {
        i=i-1
      }
      else {
        i=i+1
      }
    }
    if(plusPetit(i,i+1)) {
      if(i==getNombreValeurs()-1) {
        i=i-1
        finTri = true
      }
      else {
        i=i+1
      }
    }
  }
}