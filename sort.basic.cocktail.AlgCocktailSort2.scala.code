def cocktailSort2() {
  var debut:Int =0
  var fin:Int = getNombreValeurs()-2
  var aucuntri:Int =1
  while(aucuntri!=0){
    aucuntri=0
  for(i<- debut to fin){
 
     if(!plusPetit(i,i+1)){
      echange(i,i+1)
       aucuntri+=1
       fin-=1
    }
  }
    
   for(j<- fin to debut by -1){
     if(!plusPetit(j,j+1)){
      echange(j,j+1)
       debut +=1
       aucuntri+=1
    }
   } 
  
}
}