def bubbleSort2() {
  var fini:Boolean=false
  var nop:Int=0
  var a:Int=0
  while(!fini){
  for(i<-0 to getNombreValeurs()-2-a){
  if(!plusPetit(i,i+1)){
    echange(i,i+1)
    nop=1
  }
}
  if(nop==0){
    fini=true
  }
  else{
    nop=0
  }
  a+=1
}
for(i<-0 to 8){
  if(!plusPetit(i,i+1)){
    echange(i,i+1)
  }
}
if(!plusPetit(2,3)){
  echange(3,4)
}
}
