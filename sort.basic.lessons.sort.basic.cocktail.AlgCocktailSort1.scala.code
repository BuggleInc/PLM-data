def cocktailSort() 
{

  var taille = getNombreValeurs()
  var permute = true
  do 
  {
    permute = false
    for (i<-0 to taille-2)
    {
      if (!plusPetit(i,i+1)) 
      {
        echange(i,i+1)
        permute = true
      }
    }
    for (i<-taille-1 to 1 by -1)
    {
      if (!plusPetit(i-1,i)) 
      {
        echange(i,i-1)
        permute = true
      }
    }
  } while (permute)
}
