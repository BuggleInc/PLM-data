public void run() {
	keepHandOnSideWall();
}

public void keepHandOnSideWall(){
	//first, look for a LH wall
	//left();
	
	do{	
		if(isBackingWall()){
			goNorth();
		}
		
		if(isFacingWall()){
			right();
		}else{
			forward();
			left();			
		}

		if(isOverBaggle()){
			pickUpBaggle();
			right();
		}
		
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall

public void goNorth(){
	while(!isFacingWall() && getDirection() == Direction.NORTH){
		forward();
	}
}
