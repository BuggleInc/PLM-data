def isFacingTrail():
       if estFaceMur():
            return False
       forward()
       coul = getCouleurSol()
       backward()
       if coul == Color.green:
           return True

def hunt():
    if estSurBiscuit():
        prendBiscuit()
    elif isFacingTrail() == True:
        baisseBrosse()
        avance()
        leveBrosse()
    else:
        left()
    

while not porteBiscuit():
    hunt()
