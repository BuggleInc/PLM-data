def stringX(str:String):String =
{
  var s=""
  var  suppression=false;
 	var i=0;
  s+=str.drop(0).take(1);
 for(k <- 1 to str.length()-2)
  {
    
    if(str.drop(k).take(1)!="x" )
    {
      s+=str.drop(k).take(1)
    }
    
  }
  
  if(str.length()>1)s+=str.drop(str.length()-1).take(1);

  return s
}