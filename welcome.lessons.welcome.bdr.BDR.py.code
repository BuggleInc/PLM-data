while estSurMessage():
    if litMessage()=="A":
        avance(1)
    if litMessage()=="B":
        avance(2)
    if litMessage()=="L":
        gauche()
        avance(1)
    if litMessage()=="R":
        droite()
        avance(1)
    if litMessage()=="I":
        gauche()
        gauche()
        avance(1)
    if litMessage()=="Z":
        recule(1)
    if litMessage()=="X":
        recule(3)
