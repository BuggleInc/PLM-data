def cocktailSort2() {
  var debut:Int = 0
  var fin:Int = getNombreValeurs()
  var continue:Int = 1
  while (continue != 0){
    continue = 0
  	for (i <- debut to fin-2){
    	if (plusPetit(i+1,i)){echange(i,i+1)}
      continue = continue + 1
  	}
    fin = fin - 1
  	for (i <- fin to debut+1 by -1){
    	if (plusPetit(i,i-1)){echange(i,i-1)}
    	continue = continue + 1
  	}
    debut = debut + 1
  }
}