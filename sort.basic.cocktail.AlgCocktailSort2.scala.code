def cocktailSort2() {
  var mouv:Boolean=true
  val N=getNombreValeurs()-2
  var fin=N
  var deb=0
  while(mouv){mouv=false
             for(x<-deb to fin){if(plusPetit(x+1,x)){echange(x,x+1);mouv=true}}
              fin-=1
             for(x<-deb to fin){
               var j=N-x
               if(plusPetit(j+1,j)){echange(j,j+1);mouv=true}}
              deb+=1
             }
}