def cocktailSort() {
  var echangefait:Boolean = false
  do {
    echangefait= true
    for (i <- 0 to getNombreValeurs()-2) {
      if (plusPetit(i+1, i)){
        echange(i, i+1)
        echangefait=false    
      }
    }
    for (i <- getNombreValeurs()-2 to 0) {
      if (plusPetit(i+1, i)){
        echange(i, i+1)
        echangefait=false
      }
  	}	
  } while (!echangefait)
}