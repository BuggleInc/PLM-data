void solve() {
  
  int bleu = getTaille();
  int rouge  = 0;
  int blanc = 0;
  int i = 0;
  
  while(apresBleu<=avantBlanc)
  {
  	if(getCouleur(i)==BLEU)
      {
      bleu++;
    }
    else if(getCouleur(i)==ROUGE)
      {
      echange(rouge,blanc);
      rouge++;blanc++;
    }
    else
      {
     echange(avantBlanc,blancRouge);
    
   	 blancRouge++;
      
    }

    i++;
	}
}