override def run() {
  var n :Int= getMondeHauteur()
  var T =new Array [Color](n)
  for (i<-0 to n-1){
    T(i)=getCouleurSol()
    avance()
  }
  
  def reproduireMotif(){
    gauche()
    avance()
    droite()
    for (k<-0 to n-1){
      avance()
      setCouleurBrosse(T(k))
      baisseBrosse()
      leveBrosse()
    }
  }
  for (compt<-1 to n-1){
    reproduireMotif()
    gauche()
    avance()
    droite()
  }
}