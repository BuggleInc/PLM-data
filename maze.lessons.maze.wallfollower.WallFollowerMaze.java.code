public void run() {
	keepHandOnsidewall();
}

public void keepHandOnsidewall(){
int count = 0;

	do{
		if(count > 1){
			//back();
		}
		
		if(isFacingWall() ){
			count++;
			right();
		}
		if(!isFacingWall()){
			count = 0;
			forward();
			left();
		}
	
		if(isOverBaggle()){
			pickUpBaggle();
		}
	}while(!isCarryingBaggle());
}//end keepHandOnSideWall
