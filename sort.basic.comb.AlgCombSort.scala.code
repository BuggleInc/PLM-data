def combSort() {
  var lgr:Double=getNombreValeurs();
  var ecart:Double=lgr
  var i:Double=1
  var a=1
  while (ecart>1||a==1)
    if (ecart>1) {
      ecart=ecart/(1.3)
    }
    i=0
    while (i+ecart < lgr) {
      a=0
      if (!plusPetit(i,i+ecart)) {echange(i,i+ecart);i+=1;a=1}
    } 

                
}