def island(num:Array[Int]): Int = {
  return maxValue(maxValue():Array[Int]))
}

def isPresent(nums:Array[Int] ,lookingFor:Int): Int = {
  for (i<- 0 to nums.length-1)
  {
    if (nums(i) == lookingFor)
    	return true
  }
  return false
  
}
def maxValue(nums:Array[Int]): Int = {
  var max = 0
  max = nums(0)
  for(i<-nums)
  {
    if (i>max)
    	max = i
  }
  return max
}