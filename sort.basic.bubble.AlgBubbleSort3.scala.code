def bubbleSort3() {
  var nb=false
  
  do {
    nb = false
    for(i<- 0 to getNombreValeurs()-2){
      
      for (j <- 0 to getNombreValeurs()-2-i){
        if( plusPetit(j+1,j)){
          echange(j,j+1)
          nb=true
        }
        else {
          return true
          
        }
      }
    }
  }
  while(nb==true)
}