public void run() {
int laps = 7;
int steps = 16;
int sides = 4;

	//loop for laps
	for(int x = 1; x <= laps; x++){
		//loop all four sides
		for(int count = 1; count <= sides; count++){
			//loop for one side
			for(int y = 1; y <= steps; y++){
				forward();
				if(y == 4){left();}
				if(y == 6){right();}
				if(y == 10){right();}
				if(y == 12){left();}
				if(y == 16){left();}
			}
		}

	}
}
