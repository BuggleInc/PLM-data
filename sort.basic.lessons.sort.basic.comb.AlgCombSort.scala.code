def combSort() {
  var ecart:Int=getNombreValeurs()
  var compteur:Int=0
  var i:Int=0
  do {
    if (ecart>1) {
      ecart = (ecart.toDouble / 1.3).toInt}
    i=0
    compteur=0
    while(i+ecart>1){
      if( plusPetit(i+ecart,i)) {
        echange(i+ecart,i)
        compteur+=1}
      }
    i+=1
    }
  while( ecart>1 || compteur!=0)
  
}