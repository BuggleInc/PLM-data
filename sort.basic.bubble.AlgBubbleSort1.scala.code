def bubbleSort()  
{
  for(i<-0 to (getNombreValeurs()-2)) 
  //on prend chaque élément de la liste pour le remonter
  	{
      var depart=getValeur(i)//valeur de la case de départ de chaque boucle
      var indice=i+1 //indice de l'élément auquel on va comparer i
      while(getValeur(i)==depart)
      	{
          if (plusPetit(getValeur(indice),getValeur(i)))
      			{
          		echange(i,indice)
        		}
          indice+=1
        }
		}
}