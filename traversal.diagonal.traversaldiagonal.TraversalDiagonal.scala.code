var diag = 0;
override def run() {
  var cpt = -1
  while ((getX()!=getMondeLargeur()-1) || (getY()!=getMondeHauteur()-1)){
    if (diag<7){
      setPos(0,diag)
    } else {
      setPos(diag-6,6)
    }
    while ((getX()!=6) && (getY()!=0)){
      cpt += 1
      ecritMessage(cpt)
      setPos(getX()+1,getY()-1)
    }
    diag=diag+1
    if ((getX()==6) || (getY()==0)){
      cpt+=1
      ecritMessage(cpt)
    }
  }
}