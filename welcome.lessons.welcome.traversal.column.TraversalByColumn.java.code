public void run() {
	colByCol();
}

void colByCol(){
int x = getWorldWidth();
int y = getWorldHeight();

setPos(0, 0);

	for(int i = 0; i < x; i++){
		for(int j = 0; j < y; j++){
			setPos(i, j);			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth()){
		return true;
	}

	return false;
}//end finalPos
