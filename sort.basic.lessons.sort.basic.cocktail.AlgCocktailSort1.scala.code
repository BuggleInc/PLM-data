def cocktailSort() {
  var estTriee=false
  while(!estTriee){
    estTriee=true
    for (i<-0 to getNombreValeurs()-2){
      if(!plusPetit(i,i+1)){
        echange(i,i+1)
        estTriee=false
      } 
    }
    for (i<-getNombreValeurs()-2 to 0){
      if(!plusPetit(i,i-1)){
        echange(i-1,i)
        estTriee=false
      }      
    }
  }
}