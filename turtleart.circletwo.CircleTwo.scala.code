override def run()
{  
  cercleTroue(2);
  cercleTroue(1);
}

def cercleTroue(cote: Int)
{  
  for (i <- 1 to 180)
  {
    avance(cote);
    droite(1);
	}
  
  leveCrayon();
  for (i <- 1 to 4)
  {
    avance(cote);
    droite(1);
	}
  baisseCrayon();
  
  for (i <- 1 to 176)
  {
    avance(cote);
    droite(1);
	}
}