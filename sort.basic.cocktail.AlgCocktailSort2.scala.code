def cocktailSort2() {
  var d:Int=0
  var c:Int=0
  var f:Int=getNombreValeurs()-2
  while( c==0){
    c=1
  for (i <-d to f){
    if(plusPetit(i+1,i)){
      echange(i,i+1)
      
      c=0
    }
  }
    
  for (i <-f to d){
    if(plusPetit(i+1,i)){
      echange(i+1,i)
      c=0
      
}
       }

    f-=1
    d+=1
       }
}