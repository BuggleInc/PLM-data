def tag(i:Int){
  ecritMessage(i)
}

override def run() {
  var nb=0
  var x=0
  var y2=0
  for(y<-0 to getMondeHauteur()-1){
    setPos(0,y)
    tag(nb)
    nb=nb+1
    x=0
    while(y>0 && x<getMondeLargeur()-5){
      y2=y
      x=x+1
      y2=y2-1
      setPos(x,y2)
      tag(nb)
      nb=nb+1
    }
  }
}