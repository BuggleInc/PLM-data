def shellSort()  {
  var ecart=(getNombreValeurs())/2
  while (ecart>0) {
    insertionSort(ecart)
    ecart=ecart/2
  }
}


def insertionSort(ecart:Int) {
  var v=0
  var j =0
  for (i <- 1 to getNombreValeurs()-1) {
    v=getValeur(i)
    j=i-ecart
    while (j>=0 && getValeur(j)>v) {
      setValeur(j+ecart,getValeur(j))
      j-=ecart
    }
    j+=ecart
    setValeur(j,v)
  }
}
