override def run() {
var a = getMondeHauteur()
var b = getMondeLargeur()
var compteur = 0
var y = getX()
var x = getY()
var R = true

ecritMessage(compteur)

while (x != a-1 || y != b-1)
{if ((y==a-1 && R) || (y == 0 && !R)) 
	{if (R)
	{y = a-1; x = x + 1; R = false}
	else {y = 0; x = x + 1}; R = true}
else {if (R) 
{y = y + 1}
else {y = y - 1}
}

setPos(y, x)
compteur = compteur +1
ecritMessage(compteur)
	}

}
