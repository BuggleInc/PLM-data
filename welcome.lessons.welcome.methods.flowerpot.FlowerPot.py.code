def growFlowers(color):
    setCouleurBrosse(color)
    baisseBrosse()
    forward(2)
    backward()
    gauche()
    forward()
    backward(2)
    forward()
    setCouleurBrosse()
    leveBrosse
    droite()
    forward(3)


couleur=[Color.RED,Color.CYAN,Color.GREEN,Color.PINK]
for i in range(len(couleur)):
    color=couleur[i]
    growFlowers(color)


        