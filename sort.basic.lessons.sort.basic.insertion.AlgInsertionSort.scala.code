def insertionSort() {
  var tampon:Int =0
  var fini:Boolean =false
  var j:Int=0
  for (i <- 1 to getNombreValeurs()-1){
    tampon = getValeur(i)
    j=i-1
    while (fini==false){
      fini==true
      if (plusPetit(i,j)==true){
        copie(j,j+1)
        fini ==false
      }
      else {
        setValeur(j+1,tampon)
      }
      j-=1
    }
    
  }
}
