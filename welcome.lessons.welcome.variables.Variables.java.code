public void run() {

	int StepAmount = 0;
	
	while (isOverBaggle()==false) {
		forward();
		StepAmount = StepAmount +1;
	}

	while (isOverBaggle()==true) {
		pickupBaggle();
		//StepAmount = StepAmount -1;
	}

	while (isCarryingBaggle()==true && StepAmount > 0) {
		backward();
		StepAmount = StepAmount -1;
		
	}

	dropBaggle()






	
	
}
