public void run() {
int laps = 7;
int steps = 64;

	for(int x = 1; x <= laps; x++){
		
		for(int y = 1; y <= steps; y++){
			forward();
			if(y == 4){left();}
			if(y == 6){right();}
			if(y == 10){right();}
			if(y == 12){left();}
			if(y == 16){left();}
			if(y == 20){left();}
			if(y == 22){left();}
		}

	}
}
