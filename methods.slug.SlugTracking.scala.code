def isFacingTrail():Boolean = {
  while (!estSurBiscuit()){
    forward()
    if (getCouleurSol()!=Color.green){
      backward()
    }
    left()
    forward()
    if (getCouleurSol()!=Color.green){
      backward()
      right()
    }
    right()
    forward()
    if (getCouleurSol()!=Color.green){
      backward()
      left()
    }
  }
  return true
