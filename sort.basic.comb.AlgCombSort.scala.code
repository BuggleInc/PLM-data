def combSort() {
  var ecart=getNombreValeurs()
  var ok :Boolean = false
  do{
    ok = false
    if (ecart>1){
      ecart=(ecart.toDouble / 1.3).toInt
    }
    i=0
    
    while(i+ecart < getNombreValeur()){
      if(getValeur(i)>getValeur(i+ecart)){
        echange(i,i+ecart)
        i=i+1
        ok = true
      }
    }
  } while(ecart>1 || ok==true)
}