def testArrive (): Boolean = {
  if (!isFacingWall()){
  	forward()
  	return false
  }
  var d = getDirection()
  d match {
    case Direction.EAST => left(); if (isFacingWall()) {return true}else{forward()}; left;
    case Direction.WEST => right(); if (isFacingWall()) {return true}else{forward()}; right;
  }
  return false
}