def cocktailSort2() {
  var beg=0;
  var end=getValueCount()-2;
  var swapped = false;
  do {
    swapped=true;
    
    for(i <- beg to end){
      if(isSmaller(i+1,i)){
      	swap(i,i+1);
        swapped = true;
      }
    end-=1;
    }
    for(i <- beg to end by -1){
      if(isSmaller(i+1,i)){
      	swap(i,i+1);
        swapped = true;
      }
    beg+=1;
    }
  } while(swapped)
}