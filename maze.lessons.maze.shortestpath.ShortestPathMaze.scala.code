override def run() {
var x = 0
var y = 0

def indic(a:Int,b:Int){
	if(!aMurNord(a,b)){setIndication(a,b-1,getIndication(a,b)+1);indic(a,b-1)}
	if(!aMurOuest(a,b)){setIndication(a-1,b,getIndication(a,b)+1);indic(a-1,b)}
	indic(a,b+1)
	indic(a+1,b)
	
}

for(i <- 0 to getMondeLargeur() - 1){
	for(c <- 0 to getMondeHauteur() - 1){
		 if(aBiscuit(i,c)){x = i; y = c}
	}
}



setIndication(x,y,0)
indic(x,y)

    

}
