override def run() {
  def positionFinale():Boolean={
    var c=0
    while ((getX()!=getWorldWidth()) && getY()==getWorldHeight()){
      if (getX()!=6){
        setPos(getX()+1,getY())
        writeMessage(c)
        c=c+1
      }
      else {
        setPos(0,getY()+1)
        writeMessage('j')
        c=c+1
      }
    }
    return(true)
  }
  
positionFinale()
}