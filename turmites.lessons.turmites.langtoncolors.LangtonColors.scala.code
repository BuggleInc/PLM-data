def step(rule:Array[Char], colors:Array[Color]) {
  var cou=Color.white
 	var dir = 0
  cou = getCouleurSol()
  for (i <- 0 to rule.length-1){
    if (cou==colors(i)){
      dir = i
    }
  }
  setCouleurBrosse(Color((i+1)%(rule.length-1))
 	baisseBrosse()
 	leveBrosse()
  if (rule(i)=='L'){
   	gauche()
    avance()
  }
  else {
    droite()
    avance()
  }
}