public void run() {
	while (!isFacingWall()){
		right();
		right();
		forward();
		while(!isOverBaggle() && !isFacingWall()){
			forward();
		}
		if (isOverBaggle()){
			pickupBaggle();
			left();
			left();
			while(!isOverBaggle()){
				forward();
				}
				backward();
				dropBaggle();}
				}
				right();
				forward();
				left();
				forward();
				}