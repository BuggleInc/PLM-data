def cocktailSort2() {
  var n = getNombreValeurs()
  var echanges = 0
  var stop = false
  var debut = 0;
  var fin = n-2
  do{
  	for (i <- debut to fin){
    	if (plusPetit(i+1,i)){
      	echange(i,i+1);
      	echanges = echanges +1}
 		}
    fin = fin -1
  	for (i <- fin to debut+1 by -1){
    	if (plusPetit(i,i-1)){
      		echange(i,i-1);
      		echanges = echanges +1}
  	}
    debut = debut + 1
   if (echanges==0){stop=true};
   echanges = 0;
  }while(stop==false)
}