boolean in3050(int a, int b) {
	return in3040(a,b) || in4050(a,b);
}


boolean areIn(int a, int b, int intS, int intE){
	return between(a,intS,intE) || between(b,intS,intE);
}

boolean in3040(int a, int b) {
	return areIn(a,b,30,40);
}

boolean in4050(int a, int b) {
	return areIn(a,b,40,50);
}

boolean between(int x,int a,int b){
	return a <= x && x <= b;
}
