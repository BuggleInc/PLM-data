override def run() {
  droite()
  while (!estSurBiscuit()) {
    keepHandOnSideWall()
  }
  prendBiscuit()
}

def keepHandOnSideWall() {
  if (estFaceMur()) {
    gauche()
  }
  else {
    avance()
    if (!murADroite()) {
      droite()
      avance()
      if (!murADroite()) {
        droite()
        avance()
      }
    }
  }
}

def murADroite():Boolean = {
  droite()
  if (estFaceMur()) {
    gauche()
    return true
  }
  else {
    gauche()
    return false
  }
}
