def deplacerBiscuit(){
  setDirection(dir:est)
  while (!estSurBiscuit()){
    avance()
  }
  prendBiscuit()
  setDirection(dir:west)
  while (!estSurBiscuit()){
    avance()
  }
  recule()
  poseBiscuit()
}

def positionFinale(){
  if (getCouleurSol==Color.GREEN){
    return true
  }
  return false
}