def bubbleSort2() {

	var estTrie = false;
	var compteurIter = 0;
	
	while(compteurIter < getNombreValeurs())
	{
	
	estTrie = true;
	
		for(i <- 0 to getNombreValeurs() - 2 - compteurIter)
		{
			if(!plusPetit(i,i+1))
			{
				echange(i,i+1);
				estTrie = false;
			}
		}

	compteurIter = compteurIter + 1;
	
	}

}
