def cocktailSort() {
  var enOrdre = true ;
  while (enOrdre){
    enOrdre = false ;
  for (i <- 0 to getNombreValeurs()-2){
    if (plusPetit(i+1,i)){
    	echange(i+1,i)
      enOrdre = true ;
    }
  }
  for (j <- getNombreValeurs()-2 to 0 by -1){
    if (plusPetit(j+1,j)){
    	echange(j+1,j)
    	enOrdre = true ;
    }
  }    
 }
}
