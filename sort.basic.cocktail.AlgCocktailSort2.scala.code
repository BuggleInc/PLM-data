def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs() - 2
	var cpt = 1
  while (debut != fin -1 ) {
    for (i <- debut to fin){
      if (plusPetit(i+1,i)) {
        echange(i,i+1)
      }
    }
    fin -= 1
    for (i <- fin to debut) {
      if (plusPetit(i,i+1)) {
        echange(i,i+1)
      }
    }
    debut += 1
  }
}

