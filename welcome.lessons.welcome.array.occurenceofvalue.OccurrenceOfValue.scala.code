def occurrences(nums:Array[Int],lookingFor:Int):Int = {
   val taille = nums.length-1;
   val pasla = 0;
   var nbVal = 0;
   var pos = 0;
   for (i <-1 to 4) {
   while ({nums(pos) != lookingFor}&&{pos != nums.length-1}) {
   	pos = pos + 1;
   }
   if ( nums(pos) == lookingFor ) { nbVal = nbVal + 1; pos = pos + 1;}
   }
   if ( nums(taille) == lookingFor ) {
   	nbVal;
   } else {
   	pasla;
   }
   
}
