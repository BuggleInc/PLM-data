public void run() {
	colByCol();
}

void colByCol(){
int x = getWorldWidth();
int y = getWorldHeight();


	for(int x = 0; i < width; i++){
		for(int y = 0; j < height; j++){
			if(!finalPos()){
				setPos(x, y);
			}			
		}
	}
}

boolean finalPos(){
	if(getX() == getWorldWidth()){
		return true;
	}

	return false;
}//end finalPos
