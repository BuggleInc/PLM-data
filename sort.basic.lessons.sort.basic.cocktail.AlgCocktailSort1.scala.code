def cocktailSort() {
  var a:Boolean = true
  do {
    a = true
    for (j <- 0 to getNombreValeurs()-2) {
      if (getValeur(j)>getValeur(j+1)) {
        echange(j,j+1)
        a=false
      }
    }
    for (j <- getNombreValeurs()-2 to 0 by -1) {
      if (getValeur(j+1)<getValeur(j)) {
        echange(j,j+1)
        a=false
      }
    }
  } while (a == false)
}