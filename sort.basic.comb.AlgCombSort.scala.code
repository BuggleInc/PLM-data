def combSort() {
  var lgr = getNombreValeurs()
  var ecart = lgr
  var cpt = 0
  var i = 0
  do
  {
    cpt=0
    if (ecart>1)
    {
      ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    }
    
    i=0
    
    while (i+ecart<lgr)
    {
      if (plusPetit(i,i+1))
      {
        echange(i,i+1)
        cpt+=1
      }
      
      i+=1
      
    }
  }
  while(ecart>1 || cpt!=0)
}