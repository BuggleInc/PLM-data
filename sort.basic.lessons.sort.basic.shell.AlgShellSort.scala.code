def shellSort() {
  
  var ecart = getNombreValeurs()/2;
  
  while(ecart>=1){
    for(i <- 0 to getNombreValeurs()-1){
      
      var v = getValeur(i);
      var j = i;
      
      while(j>ecart && !plusPetitQue(j-ecart,v)){
        copie(j-ecart,j);
        j -= ecart;
      }
      setValeur(j,v);
    }
    ecart = ecart/2;
  }
}