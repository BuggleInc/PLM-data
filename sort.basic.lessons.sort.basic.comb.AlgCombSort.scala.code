def combSort() {
	var ecart: Int = getNombreValeurs()
	var i: Int = 0;
	var swapped: Boolean = false

	while (ecart > 1 || swapped) {
		swapped = false
		if (ecart>1)
			ecart = (ecart.toDouble / 1.3).toInt
		if (ecart<1)
			ecart=1
		i = 0;
		while (i+ecart < getNombreValeurs()) {
			if (plusPetit(i+ecart,i)) {
				echange(i,i+ecart)
				swapped = true
			}
			i = i+1
		}
	}
}

