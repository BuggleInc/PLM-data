def combSort() {
	var ecart = getNombreValeurs()
	var swapped = true

	do{
		swapped = true
		if(ecart>1){
			ecart = ecart/1.3
		}
		var i = 0
		while(i+ecart<getNombreValeurs()){
			if(plusPetit(i+ecart, i)){
				echange(i, i+ecart)
				swapped = true
			}
			i +=1
		}
	}while(ecart>1 || swapped)
}
