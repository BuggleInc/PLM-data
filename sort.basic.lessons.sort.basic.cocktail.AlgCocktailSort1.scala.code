def cocktailSort() {
  
  var sorted = false
  
  var nbVal = getNombreValeurs()
  
  
  do{
    sorted=true
    for(i<-0 to nbVal-2){
      if(plusPetit(i,i+1)){
        echange(i,i+1)
        sorted=false
      }
    }
    for(i<-nbVal-2 to 0){
      if(plusPetit(i,i+1)){
        echange(i,i+1)
        sorted=false
      }
    }
    
    
  }while(!sorted)
}


