def combSort() {
  var gap = getValueCount()
  var n = true
  var m = true
  while (gap>1 || n || m) {
    var n = false
    var m = false
    if (gap>1) {gap= (gap.asInstanceOf[Double] / 1.3).asInstanceOf[Int]}
    var i = 0
    while (i+gap<getValueCount()) {
      if (isSmaller(i+gap,i)) {swap(i+gap,i)}
      i= i+1
    }
  }
}