def cocktailSort() {
	var faitechange:Boolean=true
	for(i<-0 to getNombreValeurs()-2){
			if(plusPetit(i+1,i){
				echange(i,i+1)
			}else{
				if(faitechange==false){
					return
				}
			}
		}
		for(i<-getNombreValeurs()-2 to 0){
			if(plusPetit(i+1,i){
				echange(i,i+1)
	}else{
		if(faitechange==false){
			return
		}
	}
}

}
