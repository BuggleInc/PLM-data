def parcoursDiag(diag:Int,cpt:Int): Int={
  var y=diag
  var x=0
  var cpt2=cpt
  while (y!=0){
    x+=1
    y-=1
    setPos(x,y)
    ecritMessage(cpt2)
    cpt2=cpt2+1
  }
  return cpt2
}