override def run() {
  while (posFinale() == false) {
    	posSuivante()
    	ecritMessage(0)
  }
}


def posFinale() {
  return ((getMondeLargeur() == getX()) && (getY() == getMondeHauteur()))
}



def posSuivante() {
  if (getY() == getMondeHauteur()) {
    	setPos((getX()+1), 0)
  }
  else {
  		setPos(getX(), (getY()+ 1))
  }
}