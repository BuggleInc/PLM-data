override def run() {
	move(2, true);
	gauche();
	move(2,true);
	for (carre <- 1 to 3) {
		if (carre>1){
			droite();
			move(5,true)
			gauche();
		}
	}
			
}

def faireV(c:Color) {
	avance();
	setCouleurBrosse(c);
	baisseBrosse();
	leveBrosse();
	avance();
	gauche();
	avance();
	baisseBrosse();
	leveBrosse();
	recule();
	recule();
	baisseBrosse();
	leveBrosse();
	avance();
	droite();
	recule();
	recule();
