def cocktailSort() {
  var swapped:Boolean =true
  
  while (swapped){
    swapped=false
    for (k<-0 to getNombreValeurs-2){
      if (plusPetit(k,k+1)){
        echange(k,k+1)
        swapped=true
      }
    }
    for (i<-getNombreValeurs-2 to 0 by -1){
      if (plusPetit(i,i+1)){
        echange(i,i+1)
        swapped=true
      }
    }
  }
}
