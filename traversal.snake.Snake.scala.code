override def run() {
  var test:Boolean = false
  brushDown();
  while (test ==false) {
    forward();
    if (estFaceMur) {
      if (getDirection() == Direction.EST) {
        left();
        if (!estFaceMur) {
          forward(); left();
        } else {
          test=true
        }
      }
      if (getDirection() == Direction.OUEST) {
        right();
        if (!estFaceMur) {
          forward(); right();
        } else {
          test=true
        }
      }
    }
  return test
  }
}
