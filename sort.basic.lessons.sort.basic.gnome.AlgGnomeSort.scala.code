def gnomeSort() {
  var position:Int=0
  while(position!=getNombreValeurs()) {
    if(position==0){
      if (!plusPetit(position,position+1)) {echange(0,1)
                                           position=1}}
    if (plusPetit(position-1,position)) {position+=1}
    else {echange(position-1,position)
          position-=1}}
}