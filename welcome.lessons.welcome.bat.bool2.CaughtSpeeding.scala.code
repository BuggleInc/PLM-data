def caughtSpeeding(speed:Int, isBirthday:Boolean):Int = {
  if (!isBirthday) {
  	if (speed <= 60) {
    	return 0;
  	} else if (61 <= speed && speed <= 80) {
    	return 1;
  	} else {
    	return 2;
  	}
  } else {
    if (speed <= 65) {
    	return 0;
  	} else if (66 <= speed && speed <= 85) {
    	return 1;
  	} else {
    	return 2;
  	}
  }
}