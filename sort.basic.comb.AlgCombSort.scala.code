def combSort() {
  var gap = getValueCount()
  var n = false
  do {
    var n = false
    if (gap>1) {
      gap= (gap.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
    }
    var i = 0
    for (i <- 0 to getValueCount() - gap - 1) {
      if (
        isSmaller(i+gap,i)) {
        swap(i+gap,i)
        n = true
      }
      i= i+1
    }
  } while (gap>1 || n)
}