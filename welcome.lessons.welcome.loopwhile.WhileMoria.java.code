public void run() { 
	boolean hasBaggle = false;
	left();
	left();
	while(!isFacingWall()) {
		if(hasBaggle) {
			forward();
			if(isOverBaggle()) {
				backward();
				dropBaggle();
				hasBaggle = false;
				left();
				left();
			}
		}
		else {
			forward();
			if(isOverBaggle()){
				pickUpBaggle();
				hasBaggle = true;
				left();
				left();
			}
		}
	}
}
