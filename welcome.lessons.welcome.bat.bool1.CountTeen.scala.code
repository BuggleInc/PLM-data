def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var compteur = 0
  if(a<=19 && a>=13) {
    compteur = compteur + 1
  }
  if(b<=19 && b>=13) {
    compteur = compteur + 1
  }
  if(c>=13 && c<=19) {
    compteur = compteur + 1
  }
  if(d>=13 && d<=19) {
    compteur = compteur + 1
  }
  return compteur
}