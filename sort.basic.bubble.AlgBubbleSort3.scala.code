def bubbleSort3() {

  var term = false

      for (j <- 0 to getValueCount()-2){
        
        term = false
        for(i <- 0 to getValueCount()-2-j){

        if (isSmaller(i+1,i)){
       		swap(i,i+1)
      	  term = true
       			 }
        
      		}
        if (term == false){return}
          
        }

  
  
}