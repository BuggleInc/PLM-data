def insertionSort() {
  for (i<-1 to getNombreValeurs()-1){
    var stop=false
    var j=i
    var memory=getValeur(i)
    while (stop==false && j>=1){
      if (plusPetit(j, j-1)){
      	echange(j, j-1)
      }
      else{
        setValeur(j, memory)
        stop=true
      }
      j=j-1
    
    }
  }
}