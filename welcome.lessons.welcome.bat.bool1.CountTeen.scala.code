def countTeen(a:Int, b:Int,c:Int,d:Int): Int = {
  var comp:Int=0
  comp=test(a)+test(b)+test(c)+test(d)
  return comp
}
  
                                               

def estTeen(a:Int):Boolean={return (a>=13&&(a<=19))
}
def test(a:Int):Int={
  if (estTeen(a)){
    return 1
  }
  else{return 0}
}
