def insertionSort() {
	for(i <- 1 to getNombreValeurs() - 1){
		var v: Int = getValeur(i);
		var last: Int = 0;
		var aEcrit: Boolean = false;
		var j: Int = i;
		while(j != 0 && getValeur(j - 1) > v){
			setValeur(j, getValeur(j - 1));
			j = j - 1;
			aEcrit = true;
			}
		setValeur(j, v);
	}
}
