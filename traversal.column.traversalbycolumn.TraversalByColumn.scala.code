override def run() {
  var i = 0
  var x = 0
  var y = 0
  ecritMessage(0)
  while (x != getMondeLargeur()-1) {
    while (y != getMondeHauteur()-1) {
      setPos(x,y+1)
      i += 1
      y += 1
      ecritMessage(i)      
    }
    setPos(x+1,0)
    x += 1
    i += 1
    y = 0
    ecritMessage(i)
  }
}