var diag = 0;
var compt = 0;
override def run() {
  val n = getMondeHauteur()-1
  while (diag<12) {
    for(i <- 0 to n) {
      if (i<(diag+1)) {
        var j = diag-i
        if (j<(n+1)) {
          ecritMessage(compt)
          setPos(i,j)
          compt +=1
        }
      }
    diag +=1
    }
  }
}
