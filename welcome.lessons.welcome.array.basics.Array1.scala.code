def marque() {baisseBrosse();leveBrosse()}

override def run() {var n:Int=getMondeHauteur()
                   var T= new Array[Color](n)
                   for (i<-0 to n-1) {T(i)=getCouleurSol();avance()}
                    for (i<-1 to n-1) {gauche();avance();droite();avance();for (i<-T) {setCouleurBrosse(i);marque();avance()} }
                   recule()}
