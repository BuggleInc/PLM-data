listeA_E = ['A','B','C','D','E','F']
while estSurMessage():
  message = litMessage
  if message == 'R':
    droite()
    avance()
  elif message == 'L':
    gauche()
    avance()
  elif message == 'I':
    retoune()
    avance()
  elif message in listeA_E:
    avance(listeA_E.index(message))
  else:
    break