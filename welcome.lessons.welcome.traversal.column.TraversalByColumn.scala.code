override def run() {
  while(!posFinale()){posSuivante()}
}

def posFinale():Boolean ={
  val y:Int = getMondeHauteur()
  val x:Int = getMondeLargeur()
  var z:Boolean = false
  if(getX() == x-1 && getY() == y-1){z = true}
  return(z)
}

def posSuivante(){
  var i:Int = 0
  val x:Int = getMondeLargeur()
  val y:Int = getMondeHauteur()
  ecritMessage(i)
  while(getX() < x-1){
    if(getY() < y-1){setY(getY()+1)}
    else if(getY() == y-1){setPos(getX()+1,0)}
  }
}
