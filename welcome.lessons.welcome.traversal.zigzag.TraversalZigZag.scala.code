override def run() {
	var suce=0
	for (i <- 0 to getMondeHauteur()-1){
		if(suce%(2)==0 && suce!=0){
		for(j <- 0 to getMondeLargeur()-1){
			setPos(i, j-7)
			ecritMessage(suce)
			suce+=1}
		}else{
					for(j <- 0 to getMondeLargeur()-1){
					setPos(i, j)
					ecritMessage(suce)
					suce+=1
					}
			}
}
	}


