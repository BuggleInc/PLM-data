override def run() {
  var fin=false
  while (!fin){
    if(!estFaceMur()){
      avance()
    }else{
      if(getDirection() == Direction.EST){
        gauche()
        if (estFaceMur()){
          fin=true
        }else{
          avance()
          gauche()
        }
      }else{
        droite()
        if (estFaceMur()){
          fin=true
        }else{
          avance()
          droite()
        }
      }
    }
  }
}

