def shellSort()  {
  var gap:Int = getValueCount() /2
  var v:Int=0
  var j:Int=0
  while (gap>0) {
    for (i <- gap-1 to getValueCount()-1) {
    v=getValue(i)
    j=i
    while ((j>gap-1) && (getValue(j-gap)>v) ) {
      setValue(j, getValue(j-gap))
      j=j-gap
    }
    setValue(j,v)
    }
  	gap=gap/2
  }
}






