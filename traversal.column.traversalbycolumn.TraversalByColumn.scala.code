override def run() {
  var n = 0
  marque(n)
  while(!positionFinale()){
    if(getY()==getMondeHauteur()){
      setPos(getX()+1,0)
      n=n+1
      marque(n)
    } else {
      setPos(getX(),getY()+1)
      n=n+1
      marque(n)
    }
  }
}

def positionFinale():Boolean = {
  if(getMondeHauteur()-1==getY() && getMondeLargeur()-1==getX()){
    return true
  } else {
    return false
  }
}



def marque(x:Int){
  ecritMessage(x)
}