def gnomeSort2() {
  var x=0
  var ok:Boolean=true
  x=getNombreValeurs
  for (i <- x-2 to 0 by -1) {
    ok=true
    for (j <- 0 to i) {
      if (plusPetit(j+1,j)) {
        echange(j+1,j)
        ok=false
      }
    }
    if (ok) {
      return
    }
  }
}

def gnomeSort() {
  var nain=0
  var lgr=getNombreValeurs
  do {
    if (plusPetit(nain+1,nain)) {
      echange(nain,nain+1)
      nain+=1
    } else {
      nain+=1
    }
  } while (nain<lgr)
}