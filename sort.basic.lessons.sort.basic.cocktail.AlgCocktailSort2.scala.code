def cocktailSort2() {
	var d:Int=0;
	var f:Int=getNombreValeurs-2;
	var test1, test2:Boolean=false;

	while (test1== true ||test2=true) {
		test1=false;
		test2=false;
		for (i<-d to f) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				f=f-1;
				test1=true;
			}
		}
		for (i<-f to d by -1) {
			if (plusPetit(i+1,i)) {
				echange(i+1,i);
				d=d+1;
				test2=true;
			}
		}
	}
}
