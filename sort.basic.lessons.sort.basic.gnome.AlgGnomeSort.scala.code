def gnomeSort() {
	
var length = getNombreValeurs(); 
	var echangé:Boolean = false;
	
	do
	{
		echangé=false;
		
		for(i<-0 to length-2 by +1)
		{
			if(!plusPetit(i,i+1))
			{
				echange(i,i+1);
				echangé=true;
			}
			
		}
		for(i<-length-2 to 0 by -1)
		{
			if(!plusPetit(i,i+1))
			{
				echange(i,i+1);
				echangé=true;
			}
			
		}
		
	} while (echangé)
}
