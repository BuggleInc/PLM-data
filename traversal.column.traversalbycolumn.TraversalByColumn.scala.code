override def run() {
  var cpt=0
  ecritMessage(cpt)
  while (PositionFinale()==false){
    PositionSuivante()
    cpt+=1
    ecritMessage(cpt)
  }
}

def PositionFinale():Boolean={
  if (getMondeLargeur()==getX() && getMondeHauteur()==getY()){
    return true 
  }
  return false
}

def PositionSuivante(){
  while (getX()<=5){
    if (getY()==6){
    	setPos(getX()+1,0)
    } else {
      setPos(getX(),getY()+1)
    }
  }
  if (getX()==6){
    for (k<-1 to 6){
      setPos(getX(),getY()+1)
    }
  }
}
