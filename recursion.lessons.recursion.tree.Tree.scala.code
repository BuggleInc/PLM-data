def tree(steps:Int, length:Double, angle:Double, shrink:Double)    {
gauche(angle)
	if (steps!=0){
	current(steps)
	droite(angle)
	avance(length)
	current(steps)
	tree(steps-1,shrink*length,angle,shrink)
	gauche(angle)
	recule(length)
	gauche(2*angle)
	current(steps)
	avance(length)
	tree(steps-1,shrink*length,angle,shrink)
	recule(length)
}
}