def golomb(num:Int): Int = {
  var tab = new Array[Int](num)
  var repere = 0
  tab(0) = 1
  for (k <- 0 to num-1) {
    var j = 0
    while (compteur(tab, k+1) != tab(k)) {
      tab(repere+j)=k+1
      j = j + 1
    }
    tab(j)=k+2
    repere = j    
  }
  return tab(num-1)
}

def compteur(nums:Array[Int], nb:Int): Int = {
  var compteur = 0
  for (k <- 0 to nums.length-1) {
    if (nums(k) == nb) {
      compteur = compteur + 1
    }
  }
  return compteur
}