public void run() {
makeSquare();
square();
	
}

public void makeSquare()
{
	for(int p=0;p<2;p++)
	{
	paint();
	turn();
	}

}
public void square()
{
	for(int w=0;w<2;w++)
	forward();
	brush();
	forward();
	left();
	forward();
	brush();
	forward();
	left();
	forward();
	right();
	for(int o=0;o<2;o++)
	forward();
	back();
}

public void brush()
{
	brushDown();
	brushUp();
}

public void paint(){
	forward();
	brush();
}
public void turn()
{
	left();
	forward();
	left();
}
