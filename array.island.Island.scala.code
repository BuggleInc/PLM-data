def island(num:Array[Int]): Int = {
  val res : Int = 0
  for (compteur <- 1 to island.length) {
    val aux : Boolean = true
    for (compteur1 <-compteur to len(island)) {
      if (island[compteur]>island[compteur1]){aux=false}
      if ((island[compteur]==island[compteur1]) && aux) {res = res + 1}
    }
  }
}