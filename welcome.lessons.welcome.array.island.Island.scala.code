def island(num:Array[Int]): Int = {
  var compt:Int=0
  var n :Int = 0
  while(n<num.length){
    if(num(n)>0){
      compt = compt + 1
      if(num(n)>1){
        compt = compt + 1
        if(num(n)>2){
          compt = compt + 1
          if(num(n)>3){
            compt=compt+1
          }
        }
      }
    }
    n = n +1
  }
  return compt
}