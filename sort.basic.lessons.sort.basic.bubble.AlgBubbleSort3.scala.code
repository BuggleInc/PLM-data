def bubbleSort3() {
	var nbValue = getValueCount();
  var boucle = 0;
  var fin = false;
  
  while(!fin) {
    fin = true;
    var i = 0;
    while(i < nbValue-1-boucle) {
			if( getValue(i) > getValue(i+1) ) {
        swap(i,i+1);
        fin = false;
      }
      i = i+1;
    }
    boucle = boucle +1;
  }
}
