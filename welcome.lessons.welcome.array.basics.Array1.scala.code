override def run() {
  
	var tab = new Array[Color](getMondeHauteur()) 
  
	for (i <- 1 to getMondeHauteur()) {
 	 tab(i) = getCouleurSol();
 	 avance()
	} //remplissage du tableau
  
  for (n <- tab) {
   	setCouleurBrosse(n);
   	baisseBrosse();
   	leveBrosse();
   	avance()
 	}//fais une colonne
}

