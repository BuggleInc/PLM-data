override def run() {
  carreVColor()
  for (i<-1 to 7){
    avance()
  }
  carreVColor()
  avance()
  gauche()
  for (i<-1 to 7){
    avance()
  }
  droite()
  carreVColor()
}

def faireVolre(c:Color){
  setCouleurBrosse(c)
  avance()
  avance()
  baisseBrosse()
  leveBrosse()
  avance()
  leveBrosse()
  gauche()
  avance()
  baisseBrosse()
  leveBrosse()
  recule()
  leveBrosse()
  droite()
  avance()
  baisseBrosse()
  leveBrosse()
  avance()
  leveBrosse()
  avance()
  gauche()
}

def carreVColor(){
  faireVolre(Color.YELLOW)
  faireVolre(Color.RED)
  faireVolre(Color.BLUE)
  faireVolre(Color.GREEN)
}