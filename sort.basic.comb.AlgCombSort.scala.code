def combSort() {
  var l = getNombreValeurs;
  var ecart = l;
  var fini = false;
  var i = 0;
  
  while(!fini){
    i = 0;
    fini = true;
    if(ecart > 1) ecart=(ecart.toDouble/1.3).toInt ;
    do{
      if(plusPetit(i+ecart, i)){ 
        echange(i, i+ecart);
        fini = false;
      }
   	i += 1;
    }while(i < l-ecart)
  }
}