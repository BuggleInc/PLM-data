override def run() {

	
var c:Int=0


	do {
		for (x<-0 to getMondeHauteur()-1)
		{
			setPos(x,0)
				ecritMessage(c)
				c=c+1
		}
		for (y<-1 to  getMondeHauteur()-1)
		{
			for (x<-0 to getMondeHauteur()-1)
			{
				setPos(y,x)
				ecritMessage(c)
				c=c+1
			}
		}
		}
	while (
		getX!=getMondeHauteur()-1
		&&
		getY!=getMondeHauteur()-2
		)
	
}
