def cocktailSort() {
  var switch:Boolean =true
  
   do {
     
    switch=false
    
    for(i<- 0 to getNombreValeurs()-2 ){
      if (plusPetit(i+1,i)){echange(i+1,i);switch=true}}
    
    
     for(j<- getNombreValeurs()-2 to 0 by -1){
       if(plusPetit(j+1,j)){echange(j+1,j);switch=true}}
     

    
  }while (switch)
}
