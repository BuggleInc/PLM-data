var diag = 1;
var cpt = 1;
override def run() {
  ecritMessage(0)
  while(!finalPos())
  {
    setPos(diag/7,diag%7)
    doDiag()
    diag = diag+1
  }
  
}

def doDiag()
{
  do
  {
    ecritMessage(cpt)
    cpt=cpt+1
    if(getY()>0)
    {
    	setPos(getX()+1,getY()-1)
      
    }
  }while(getY()!=0);
  ecritMessage(cpt)
  cpt=cpt+1
}

def finalPos():Boolean = 
{
  return (getMondeHauteur()==getY()+1)&&(getMondeLargeur()==getX()+1)
}