var diag = 0;
override def run() {
  var cpt = 0
  while ((getX()!=getMondeLargeur()-1) || (getY()!=getMondeHauteur()-1)){
    ecritMessage(cpt)
    if (diag<7){
      setPos(0,diag)
    } else {
      setPos(diag-6,6)
    }
    while ((getX()!=6) && (getY()!=0)){
      ecritMessage(cpt)
      cpt += 1
      setPos(getX()+1,getY()-1)
    }
    cpt +=1
    diag=diag+1
  }
}