def cocktailSort3() {
  var incr:Boolean=false;
  var debut:Int=0;
  var fin:Int=getNombreValeurs()-2;
  
  do{
    incr=false;
  	for(i<- debut to fin){
  	  if(plusPetit(i+1,i)){
        echange(i,i+1);
        incr=true;
      }
  	}
    fin=fin-1;
    if(incr==true){
      incr=false;
    	for(i<- fin to debut by -1){
    		if(plusPetit(i+1,i)){
    	  	echange(i+1,i);
    	  	incr=true;
    		}
    	}
  		debut=debut+1;
    }
  }while(incr!=false);
}
