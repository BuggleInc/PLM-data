var etat = 0
override def run() {
  while (!estSurBiscuit){
  	etat match {
    	case 0 => auNordChef()
    	case 1 => aGaucheToute()
  	}
  }
  prendBiscuit()
}

def aGaucheToute(){
  while (getDirection!=Direction.NORD){
    droite()
    while (estFaceMur){
      droite()
    }
    forward()
  }
 etat=0   
}

def auNordChef(){
  while(!estFaceMur){
    forward()
  }
  droite()
  if (estFaceMur){
    etat=1
  } 
}