def combSort11() {
  var ecart:Int = getNombreValeurs()
  var echangeFait:Boolean = false
  var i:Int = 0
  do {
    i = 0
    echangeFait = false
    
    if (ecart > 1)
      ecart = (ecart.toDouble / 1.3).toInt
    
    if (ecart == 9 || ecart == 10)
    	ecart = 11
    
    while (i + ecart < getNombreValeurs()) {
      if(plusPetit(i + ecart, i)) {
        echangeFait = true
        echange(i + ecart, i)
      }
      i += 1
    }
  } while(ecart > 1 || echangeFait)
}