def isFacingTrail():
    couleur=False       
    if not isFacingWall():
        forward()
        if getGroundColor()==Color.green:
            brushDown()
            brushUp()
            couleur=True
    backward()
    return couleur

def hunt():
    if isFacingTrail():
        forward()
    else:
        left()
    
while not isOverBaggle()
    hunt()
        