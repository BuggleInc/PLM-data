def combSort()
{
  var sortie:Boolean = false
  var ecart:Int = getNombreValeurs()
  var i:Int = 0
  
  do
  {
    sortie = true
    
    if(ecart>1)
    {
      ecart = (ecart.toDouble / 1.3).toInt
    }
    
    i=0
    
    while(i+ecart < getNombreValeurs())
    {
      if(plusPetit(i+ecart,i))
      {
        echange(i+ecart,i)
        sortie = false
      }
      i=i+1
    }   
  }while((ecart>1)||(!sortie))
}