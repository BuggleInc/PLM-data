def cocktailSort2() {
	var debut=0
	var fin=getNombreValeurs()-2
	var inverse=true
	do{
		inverse=false
		for(i<-debut to fin){
			if(plusPetit(i+1,i)){
				inverse=true
				echange(i,i+1) 
				fin=fin-1
			}
		}

		for(i<-fin to debut by -1){
			if(plusPetit(i+1,i)){
				inverse=true
				echange(i,i+1) 
				debut=debut+1
			}
		}
	} while(inverse)
}

