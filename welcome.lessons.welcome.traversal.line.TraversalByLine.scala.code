override def run() {
def posFinale():Boolean={
	if((getX()==getMondeLargeur()-1) && (getY() == getMondeHauteur()-1)){true}else{false}
}
var x = 0
while(!posFinale()){
	for(c<-0 to getMondeHauteur() - 1){
		setPos(0,c)
		for (l <- 1 to getMondeLargeur()-1){
			ecritMessage(""+x)
			x = x+1
			setPos(l,c)
		}
		ecritMessage(""+x)
		x = x+1
		if(!posFinale()){
		}
	}
}
}
