$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class stringSplosion extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( stringSplosion((String)t.getParameter(0)) );  }
	String stringSplosion(String str) {
		String result = "";
		
		for (int i=0; i<str.length(); i++) {
			result = result + str.substring(0, i+1);
		}
		return result;
	}
 }