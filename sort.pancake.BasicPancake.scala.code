def solve() {
  var k=0
  var l=0
  var max=0
  var cmax=0
  
  var n=getTaillePile()
  
  while(!estTriee){
    max=0
    cmax=0
    for(k<- 0 to n-1-l){
      
      if(getRayonCrepe(k)>max){
        max = getRayonCrepe(k)
        cmax = k
      }
      
    }
    if(cmax==0){
        return
      }
      retourne(cmax+1)
      retourne(n-l)
      l+=1
  }
}