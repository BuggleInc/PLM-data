public void run() { 

	//Setup variables
	int BaggleCount = 0;
	int OrangeCells = 0;

	//Move forward until correct number reached
	while (2 * BaggleCount != OrangeCells + 1) {
		forward();
		if isOverOrange() {
			OrangeCells = OrangeCells + 1;
		}
		if isOverBaggle() {
			BaggleCount = BaggleCount + 1;
		}
	}
	
}
