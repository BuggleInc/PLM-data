def nearHundred(n:Int): Boolean = {
  var sol = false
  if (n <= 100) {
    if (n + 10 >= 100) {
      sol = true
    } else {
      sol = false
    }
  } else {
    if ((210 >= n + 10) && ((n + 10)>= 200) || n - 10 == 100 || n - 10 == 200) {
      sol = true
    } else {
      sol = false
    }
  }
  return sol
}