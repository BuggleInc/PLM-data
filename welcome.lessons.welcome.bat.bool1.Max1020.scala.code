def max1020(a:Int, b:Int):Int = {
  if ((estDans(a,10,30))&&(estDans(b,10,20))){
    return Math.max(a,b)}
  if ((estDans(a,10,30))&&(!estDans(b,10,20))){
    return a}
  if ((!estDans(a,10,30))&&(estDans(b,10,20))){
    return b}
  return 0
}

def estDans(a:Int,b:Int,c:Int):Boolean={
  return (a>=b)&&(a<=c)}
  