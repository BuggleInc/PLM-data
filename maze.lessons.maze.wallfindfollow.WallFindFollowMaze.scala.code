override def run() {
  if (!murAGauche()) {
    gauche()
  }
  while (!estSurBiscuit) {
    keepHandOnSideWall()
  }
  prendBiscuit()
}
def keepHandOnSideWall() {
  gauche()
  while (estFaceMur) {
    droite()
  }
  avance()
}
def murAGauche():Boolean = {
  gauche()
  if (estFaceMur) {
    droite()
    return true
  } else droite; return false
}
