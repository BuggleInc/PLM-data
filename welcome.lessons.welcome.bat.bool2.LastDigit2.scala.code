def lastDigit(a:Int, b:Int, c:Int):Boolean = {
  
  
  
  
  var reste1= a%10
  var reste2=b%10
  var reste3=c%10
  
  while (reste1>10 || reste2>10 || reste3>10){
    if (reste1>10){
      reste1=reste1%10
    }
    if (reste2>10){
      reste2=reste2%10
    }
    if (reste3>10){
      reste3=reste3%10
    }
  }
  if (reste1==reste2 || reste2==reste3 || reste1==reste3){
    return true
  }else{
    return false
  }
}