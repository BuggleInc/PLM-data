def isFacingTrail():Boolean = {
  	var vuVert: Boolean = false
  	if (estFaceMur()==false) {
      forward();
      if (getCouleurSol()==Color.green) {
        vuVert=true
      } else {
        forward();
        isFacingTrail();
      }
    } else {
      left();
      isFacingTrail();
    }
  	backward();
  	return vuVert
}

