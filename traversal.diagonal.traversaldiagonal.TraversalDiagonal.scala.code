var diag = 0
def fin () : Boolean = {
  return getX() == 6 && getY() == 6
}

override def run() {
  var p = 1
  var i = 0
  while(!fin()) {
  ecritMessage(i)
  i += 1
  if (p == 1) {
    if (getY()==0) {
      if (getX()==6) {setPos(1,6);p=2;diag=0}
      else{setPos(0,diag+1)
           diag += 1}
    }
    else {
      setPos(getX()+1,getY()-1)
    }
  }
  else{
    if (getX() == 0) {
      setPos(diag+1,6)
    }
    else {
      setPos(getX()+1,getY-1)
    }
  }
  }}