def hunt(c:Color) {
  while(!isOverBaggle()){
    if(isFacingTrail(c)==true){
      brushDown();
      brushUp();
      forward();
    }else{
      left();
    }
    }
  brushDown();
  brushUp();
  pickupBaggle();
}

def isFacingTrail(c):Boolean ={
    if(isFacingWall()){
      return false
    }else{
      forward();
    if(getCouleurSol()==c){
      backward();
      return true
    }else{
      backward();
      return false
    }
    }
}