def bubbleSort()  {
  
  var x=getValueCount()
  var i = 0
  var echange = true
  
  while ((echange==true) && (i<x)){
    
    echange = false;
    
    for (j <- 0 to x-i){
      
      if (isSmaller(getValue(j+1), getValue(j))){
        
        swap(j,j+1);
        echange = true;
        
      }
      
      i=i+1
      
    }
    
  }
  
}