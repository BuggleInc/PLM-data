override def run() {
var hauteur = getMondeHauteur()-1;
var largeur = getMondeLargeur();
var lar = largeur-1;
var tab = new Array[Color] (hauteur+1)
for (i<- 0 to hauteur ) {tab(i) = getCouleurSol();avance()} 
for (i<-0 to lar-1) {gauche();avance();droite();avance()
for(h<- 0 to hauteur) {setCouleurBrosse(tab(h));baisseBrosse();leveBrosse();avance()}}
}
