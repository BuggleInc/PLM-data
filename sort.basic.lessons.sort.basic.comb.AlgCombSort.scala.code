def combSort() {
  var taille = getNombreValeurs()
  var ecart :Double = getNombreValeurs()
  var parcours = true
  
  	while(ecart > 1 || parcours)
  		{ecart = ecart / 1.3
        var i = 0
        while(i+ecart<taille)
        {
          if(getValeur(i + ecart)<getValeur(i))
          	{
              echange(i,i+ecart)
              i = i + 1
            }
        }
        
      }
  
}