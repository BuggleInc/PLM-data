def cocktailSort() {
  var switch:Boolean =true
  
   do {
     
    switch=false
    
    for(i<- 0 to getNombreValeurs()-2 ){
      if (plusPetit(i+1,i)){echange(i+1,i);switch=true}}
    
    
     for(i<- getNombreValeurs()-2 to 0){
       if(plusPetit(i,i+1)){echange(i,i+1);switch=true}}
     

    
  }while (switch)
}