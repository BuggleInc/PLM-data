def cocktailSort2() {
  var Change1: Boolean=true
  var L =getNombreValeurs()
  
  do{
    Change1=false
    
 	  for (k<- L-2 to 0 by -1 ){
   
   		 if (getValeur(k) > getValeur(k+1)){
     		  echange(k,k+1)
      		Change1=true
  	} 	     
  }     
  	for (l<-0 to L-2){
    		if (getValeur(l) > getValeur(l+1)){
      		 echange(l,l+1)
       		 
    
  		}
  }
  
  
	} while (Change1==true)
}
