def combSort() {
  var lgr = getNombreValeurs();
  var ecart = lgr;
  var ech : Boolean = true;
  
  while (ecart>1 | ech){
    ech=false;
    if (ecart > 1) {
      ecart = (ecart.toDouble / 1.3).toInt;
    }
    var i = 0;

    while ( (i+ecart)<lgr ){
      if ( getValeur(i)>getValeur(i+ecart)){
        echange(i,i+ecart);
        i+=1;
        ech=true;
      }
    }
}
}