def dessinerFleur(couleur : String)
{
  /**
	*Dessine une fleur en partant du petale du bas, on peut choisir la couleur
  */
  
  setCouleurBrosse(couleur)
  baisseBrosse()
  avance(2)
  recule()
  gauche()
  avance()
  droite(2)
  avance(2)
  droite(2)
  avance()
  setCouleurBrosse(Color.YELLOW)
  leveBrosse()
  droite()
  recule()
    
}
dessinerFleur("RED")