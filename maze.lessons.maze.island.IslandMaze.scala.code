override def run() {

  
  
  while (! estSurBiscuit()) {  
    if (getDirection() == Direction.NORTH && ! estFaceMur()) {
      avanceVersNord();      
      droite();
    }
    longerMurGauche();
  }
  prendBiscuit()
}

def avanceVersNord() {
  setDirection(Direction.NORD);
  while (! estFaceMur()) {
    avance();    
  }
}


def longerMurGauche() {    
    while (! estFaceMur) {
      avance();
      gauche();
    }
  	droite();
}