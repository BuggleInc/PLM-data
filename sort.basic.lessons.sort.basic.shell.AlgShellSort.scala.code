def shellSort() {
  
  var ecart = getNombreValeurs();
  
  while(ecart>0){
    ecart = ecart/2;
    for(i <- 1 to getNombreValeurs()-1){
      
      var v = getValeur(i);
      var j = i;
      
      while(j>ecart && !plusPetitQue(j-ecart,v)){
        copie(j-ecart,j);
        j -= ecart;
      }
      setValeur(j,v);
    }
  }
}