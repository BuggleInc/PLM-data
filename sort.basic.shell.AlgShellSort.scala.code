def shellSort()  {
  var lgr=getNombreValeurs()
  var ecart=lgr/2
  
  while (ecart>0){
    var l=0
    for (i<- 1 to getNombreValeurs()-1){
      var v=getValeur(i-l*ecart)
      var k=getValeur(i-(l+1)*ecart)
      while (k>=1 && !plusPetitQue(k,v)){
        copie(k, k)
        l+=1
      }
      echange(k, v)
  	}
	}
}