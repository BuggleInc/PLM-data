$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; public class maxValue extends BatEntity {  @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( maxValue( (int[])t.getParameter(0) )); }
	
	public int maxValue(int[] tab) {
		int max = tab[0];
		for (int i=1; i<tab.length; i++) 
			if (tab[i] >= max)  
				max = tab[i];
				
		return max;
	}
 }