var diag = 0;
override def run() {
  var cpt : Int = 0
  ecritMessage(cpt)
  cpt += 1
  for(i<-1 to 13){
  	cpt = diagonale(cpt)  
  }
  
}

def diagonale(cpt : Int) : Int = {
  var compteur = cpt
  for(i<-0 to diag-1){
    ecritMessage(compteur)
    setPos(getX()+1,getY()-1)
    compteur = compteur + 1
  }
  diag = diag + 1
  if(diag < getMondeHauteur()){
    setPos(0,diag)
  }
  return compteur
}