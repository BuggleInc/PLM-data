def cocktailSort() {
  var nb1=false
  var nb2=false
  
  do{
    for( i<- 0 to getNombreValeurs()-2){
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        nb1=true
      }
    }
    for(i<- 0 to getNombreValeurs()-2){
      
      if(plusPetit(i+1,i)){
        echange(i,i+1)
        nb2=true
      }
    }
  }
  while((nb1||nb2)||(nb1&&nb2))
}