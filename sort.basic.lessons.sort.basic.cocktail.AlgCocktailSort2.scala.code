def cocktailSort2() {
  var n = getNombreValeurs();
  var parcours = true;
  var k = 0
  while (parcours) {
    parcours=false
  	for (i<-k to n-2) {
    	if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
        parcours=true
      }
      
    }
    for (i<-n-2 to k by -1) {
      if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
        parcours=true
      
    }
  }

k=k+1
}
}