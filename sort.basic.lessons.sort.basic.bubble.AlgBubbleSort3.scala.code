def bubbleSort3()
{
  var cpt = 0
  var tour = 0
  var tri = false
  while (!tri)
  {
    for (k<- 0 to getNombreValeurs()-2-tour)
    {
      if (!plusPetit(k,k+1))
      {
        echange(k,k+1)
        cpt+=1
      }
    }
    if (cpt == 0)
    {
      tri = true
    }
    cpt = 0
    tour+=1
  }
}