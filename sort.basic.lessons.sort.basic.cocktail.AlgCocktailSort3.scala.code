def cocktailSort3() {
var nbr=getNombreValeurs();
var debut=0;
var fin=nbr-2;
var indice=1;

do{ 
     indice=0;
	for(i<-debut to fin){
		if(plusPetit(i,i+1)==false){
           	echange(i,i+1);
           	indice +=1;
           }
     }
     fin -=1;
     for(i<-fin to debut by -1)   {
     	if(plusPetit(i,i+1)==false){
           	echange(i,i+1);
           	indice += 1;
           }
     }
	debut +=1;
}while(indice!=0 )
}
