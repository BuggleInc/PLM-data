def max2Value(nums:Array[Int]): Int = {
  var max1=nums(0)
  var max2=nums(1)
  if (max2>max1) {
    max1=nums(1)
    max2=nums(0)
  }
  for (i <- 0 to nums.length-1) {
    if (nums(i)>max1) {
      max2=max1
      max1=nums(i)
    } else if (nums(i)>max2) {
      max2=nums(i)
    }
  }
  return max2
} 