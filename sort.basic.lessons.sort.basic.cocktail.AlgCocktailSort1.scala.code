def cocktailSort() {
  var n = getNombreValeurs();
  var parcours = true;
  while (parcours) {
    parcours=false
  	for (i<-0 to n-2) {
    	if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
        parcours=true
      }
      
    }
    for (i<-n-1 to 1) {
      if (getValeur(i+1)<getValeur(i)) {
      	echange(i,i+1)
        parcours=true
      
    }
  }
}
}