var move : Int = 0
override def run() {
    while (isOverBaggle == false){
      move = random3()
      if (isFacingWall()){
        if (move == 0){
          left()
          move = random3()
        }
        else if ( move == 0){
          forward()
        }
        else if ( move == 1){
          left()
        }
        else if (move == 2){
          right()
        }
          
        }
      }
}