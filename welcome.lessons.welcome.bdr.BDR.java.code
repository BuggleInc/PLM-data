var msg:String=""
var fini=false
while (fini!=true)
{
  
  if (estSurMessage()){
    msg=litMessage()
 
  	if (msg=="R"){ droite(); avance()}
  	if (msg=="L"){gauche();avance()}
  	if (msg=="I"){gauche(2);avance()}
  	if (msg=="A"){avance()}
  	if (msg=="B"){avance(2)}
  	if (msg=="C"){avance(3)}
  	if (msg=="Z"){recule()}
  	if (msg=="Y"){recule(2)}
  	if (msg=="X"){recule(3)}
  }else
  {
    fini=true
  }
}
