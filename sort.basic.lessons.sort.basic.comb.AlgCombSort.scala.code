def combSort() {
  var n = getNombreValeurs();
  var ecart = getNombreValeurs();
  var c = 1;
  while ( ecart > 1 || c != 0 ){
    if ( ecart > 1 ){
      ecart = (ecart.toDouble / 1.3).toInt;
    }
    var i = 0;
    while ( i+ecart < n ){
      if ( plusPetit(i+ecart:Int,i:Int) ){
        echange(i,i+ecart);
      }
      i += 1;
    }
  }
}