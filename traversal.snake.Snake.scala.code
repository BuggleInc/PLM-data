def estSurPositionFinale():Boolean={
  var res : Boolean = true
  while(!(getDirection() == Direction.EST))
  	{gauche()}
  if (!(estFaceMur()))
  	{res=false}
  gauche()
  if (!(estFaceMur()))
  	{res=false}
  droite()
  return(res)
}

override def run() {
  baisseBrosse()
  while (!(estSurPositionFinale())){
    while(!(estFaceMur())){avance()}
    if(getDirection() == Direction.EST){gauche();avance();gauche()}
    else {droite();avance();droite()}
  }
}