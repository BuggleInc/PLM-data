def shellSort()  {

	var ecart = Math.floor(getNombreValeurs()-1)
	var v = 0
	var cpt = 1

	while (ecart>0) {
		//algo du tri par insertion
		for (i<-1 to getNombreValeurs()-1) {
		v=getValeur(i)
		cpt=i
			while (cpt>0 && !plusPetitQue(cpt-1, v)) {
			copie(cpt-1, cpt)
			cpt-=ecart
			}
		setValeur(cpt, v)
		//fin
	}

}
