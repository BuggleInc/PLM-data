def bubbleSort2() {	
  var n:Int=getNombreValeurs()
  var i:Int=0
  var compteur:Int=0
  var nbBoucle:Int=0
  while (compteur <(n-1-nbBoucle)){
    while (i<(n-1)){
      if (!plusPetit(i,i+1)){
      echange(i,i+1)
      i=i+1
      compteur=0
    } else{
      i=i+1
      compteur=compteur+1
    }
  }
   i=0
    nbBoucle=nbBoucle+1
}
}