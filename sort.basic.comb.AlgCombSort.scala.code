def combSort() {
  var ecart=getNombreValeurs()
  var nb=false
  var n=getNombreValeurs()
  var i= 0
  do{
    nb=false
    if(ecart >= 1){
      ecart=(ecart.toDouble/1.3).toInt
      
    }
    
    while(i+ecart < n){
      if(plusPetit(i+ecart,i)){
        echange(i,i+ecart)
        i=i+1
        nb=true
      }
    }
  }
  while(ecart >=1 || nb)
}