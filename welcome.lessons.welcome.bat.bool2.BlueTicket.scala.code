def blueTicket(a:Int, b:Int, c:Int):Int = {var s1:Int=a+b
                                           var s2:Int=b+c
                                           var s3:Int=c+a
                                           if ((s1==10)||(s2==10)||(s3==10)) {return 10}
                                           else if ((s1==(s2+10))||(s1==s3+10)) {return 5}
                                           else return 0}