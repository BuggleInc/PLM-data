override def run() {
  octogone()
  motif2()
  motif3()
}

def octogone(){
  for (i <- 1 to 8){
    forward(50); right(45)
  }
}

def motif2(){
  forward(25);
  left(45);
  for (i <- 1 to 8)
  right(90); forward(50); left(45); forward(50)
}

def motif3(){
  right(90); forward(50); left(45);
  for (i <- 1 to 8){
  right(135); forward(50); right(45); forward(50); backward(50); left(135); forward(50)
  }
}