override def run() {
  var X:Double = getX()
  var Y:Double = getY()
  ligneHouse()
  setPos(X,Y-75)
  house()
  setPos(X+150,Y-75)
  house()
  setPos(X,Y-150)
  ligneHouse()
  setPos(X,Y-225)
  
  
}


def ligneHouse() {
  for (i <- 0 to 3) {
    house()
    setPos(getX()+50,getY())
  }
}


def house() {
  for (i <- 0 to 3) {
    avance(30)
    droite(90)
  }
  avance(30)
  droite(30)
  for (i <- 0 to 2) {
  	avance(30)
  	droite(120)
  }
  droite(150)
  avance(30)
  droite(180)
}