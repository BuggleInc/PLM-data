override def run() {
    def keepHandOnRightWall() = {
    	droite();
    	if (estFaceMur) {gauche(); if (!(estFaceMur)) {avance()} else gauche()}
    	else if (!(estFaceMur())) {avance()}
    		else {droite()}
    }
    while (!(estSurBiscuit)) {
    		if (!(getDirection==Direction.EST)) {
    			keepHandOnRightWall();
    	
    		} else
    			if ((getDirection==Direction.EST) && !(estFaceMur)) {
    	 		avance()	
    			} else {keepHandOnRightWall()}
	}
}