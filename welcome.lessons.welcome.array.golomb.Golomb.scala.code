def golomb(num:Int): Int = {
  var T = new Array[Int](num+1)
  var compteur=1
  T(0)=0
  T(1)=1
  if (num==1){
    return 1
  }else{
    for (i<-1 to num){
      if (i+compteur>num){
        for(j<-1 to num){
          T(j)=i
        }
      }else{
        for(j<-1 to i+compteur){
          T(j)=i
          compteur=T(i-1)
        }
      }
      
    }
  }
  return T(num)
}
