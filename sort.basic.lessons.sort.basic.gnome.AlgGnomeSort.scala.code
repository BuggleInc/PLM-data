def gnomeSort() 
{
	var curseur:Int = 1;
	var hasSwap: Boolean = false;
	do
	{
		
		hasSwap = false;
		if(!plusPetit(curseur-1,curseur+1) )
			{
				echange(curseur+1,curseur-1);
			hasSwap = true;
			curseur=curseur-1;
				
			}else{
				curseur=curseur+1;
			}
		if (curseur == 0)
			{curseur = curseur+1}
	
	}while(curseur == getNombreValeurs())
	
} 
