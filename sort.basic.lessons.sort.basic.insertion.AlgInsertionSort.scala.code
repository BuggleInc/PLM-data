def insertionSort() 
{
var v:Int=0

	for (i <- 1 to getNombreValeurs()-1)
	{
		v=getValeur(i)
		for (j <- i to 1 by -1)
		{
			if (!(plusPetitQue(j-1,v)))
			{
				copie(j-1,i)
			}
			else 
			{
				setValeur(j,v)
			}
		}
	}

}

