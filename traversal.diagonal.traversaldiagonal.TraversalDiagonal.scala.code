var diag = 1;
var cpt = 1;
override def run() {
  while(cpt<=20)//!finalPos())
  {
    setPos(0,diag)
    doDiag()
    diag = diag+1
  }
  
}

def doDiag()
{
  do
  {
    ecritMessage(cpt)
    cpt=cpt+1
    if(getY()>0)
    {
    	setPos(getX()+1,getY()-1)
      
    }
  }while(getY()!=0);
  ecritMessage(cpt)
  cpt=cpt+1
}

def finalPos():Boolean = 
{
  return (getMondeHauteur()==getY()+1)&&(getMondeLargeur()==getX()+1)
}