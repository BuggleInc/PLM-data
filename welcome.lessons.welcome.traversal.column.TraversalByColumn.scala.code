override def run() {
  var avancement = 0
  ecritMessage(avancement)
  while(getY()!=getMondeHauteur() && getX!=getMondeLargeur())
  {
    go()
    avancement = avancement + 1
    ecritMessage(avancement)
  }
}

def go(){
  var Hauteur = getMondeHauteur()-1
  var Largeur = getMondeLargeur()-1
  var X = getX()
  var Y = getY()
  if (Y==Hauteur)
  {
    setPos(0,X+1)
  }
  else
  {
    setPos(Y+1,X)
  }
}

