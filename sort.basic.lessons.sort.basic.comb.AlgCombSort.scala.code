def combSort() {
   var n = getNombreValeurs()
   var ecart = n
   var i = 0
   var c = false
   do {
     c=true
     if (ecart>1) {
        ecart = (ecart.toDouble / 1.3).toInt
     }
     i=0
     while (i+ecart<n) {
          if ((getValeur(i))>getValeur(i+ecart)) {
              echange(i,(i+ecart))
              c=false
            //print(i+ecart)
            //print(" ")
          }  
          i=i+1
      }
  } while ((!c)||(ecart>1)) 
}


