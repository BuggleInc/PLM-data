def bubbleSort()  {
  var nbValue = getValueCount();
  var fin = false;
  while(!fin) {
    var echange = false;
    
    var i = 0; 
    while(i < nbValue-1) {
      
      if( getValue(i) > getValue(i+1) ) {
        swap(i,i+1);
        echange = true;
      }
      
      i+=1;
    }
    
    
    if(echange) {
      fin = false;
    }
  }
}
