def bubbleSort2() {
  var n=getNombreValeurs()
  var count= -1
  var dejT=0
  while (count != 0){
    
    for (dejt <- 0 to n-2){
      count=0
      for (j <- 0 to dejt ){
        if (getValeur(j)>getValeur(j+1)){
          echange(j,j+1)
          count=count+1
          dejT=dejT+1
        }
      }
    }
  } 
}
