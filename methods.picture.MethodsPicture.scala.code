override def run() {
  for(y<-0 to 2){
    for(x<-0 to 2){
			faireMotif()
      avancerColonne()
    }
    avancerLigne()
  }
}


def avancerLigne(){
  gauche()
  for(i<-0 to 6){
    avance()
  }
  droite()
}

def avancerColonne(){
  for(i<-0 to 6){
    avance()
  }
}

def marquerSol(){
  baisseBrosse()
  leveBrosse()
}

def faireMotif(){
  avance()
    setCouleurBrosse(Color.YELLOW)
    faireV()
    setCouleurBrosse(Color.RED)
    faireV()
    setCouleurBrosse(Color.BLUE)
    faireV()
    setCouleurBrosse(Color.GREEN)
    faireV() 
  recule()
}

def faireV(){
  avance()
  marquerSol()
  avance()
  gauche()
  avance()
  marquerSol()
  recule()
  droite()
  avance()
  marquerSol()
	avance()
  avance()
  gauche()
  avance()
}