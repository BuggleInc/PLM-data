def gnomeSort() {
  var lgr = getNombreValeurs();
  var fini = false;
  var i = 0;
  while(!fini){
    fini = true;
    if (!plusPetit(i,i+1)){
      echange(i,i+1);
      if (i!=0){
      	i -= 1;
      } else {
        i+=1
      }
    } else {
    	i += 1;
    }
    if (i != lgr-1){
      fini = false
    }
  }
}
