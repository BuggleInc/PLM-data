def selectionSort() {
  var m = 0
  var nbEchanges=0
  while(m<getNombreValeurs()) {
    var k = m
    nbEchanges=0
    for(i <- m to getNombreValeurs()-1) {
      if(plusPetit(i,k)) {
        k=i
        nbEchanges+=1
      }
    }
    echange(m,k)
    if(nbEchanges=0) {
      return
    }
    m+=1
  }
}