def golomb(num:Int): Int = {
  var tab = new Array[Int](num)
  var repere = 0
  tab(0)=1
  for (k <- 0 to num-1) {
    var occ = tab(k)
    for (i <- repere to repere+occ-1) {
      if (i <= num-1) {
      	tab(i) = k
    }
    }
    repere = k
  }
  return tab(num-1)
}

def compteur(nums:Array[Int], nb:Int): Int = {
  var compteur = 0
  for (k <- 0 to nums.length-1) {
    if (nums(k) == nb) {
      compteur = compteur + 1
    }
  }
  return compteur
}