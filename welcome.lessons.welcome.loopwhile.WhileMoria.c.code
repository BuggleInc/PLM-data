void run(){
	while(!estFaceMur()){
		retourne();

		/**
		 * On avance pour aller chercher un biscuit 
		 */
		while(!estSurBiscuit()){
			avancePas();
		}
		
		retourne();
		prendBiscuit();

		/**
		 * On va déposer le biscuit à la bonne place
		 */
		while(!estSurBiscuit()){
			avancePas();
		}

		reculePas();
		poseBiscuit();
		
	}
}


