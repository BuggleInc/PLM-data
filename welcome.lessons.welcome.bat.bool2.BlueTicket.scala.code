def blueTicket(a:Int, b:Int, c:Int):Int = {
  if ((a+b==10) | (a+c==10) | (b+c==10) ) {
    return 10
  }
  else if ((a+b==10) & (a+c==10) & (b+c==10) ) {
    return 5
  }
  else if (a+b==c+10) {
  return 5
  }
  else if (a+b==10+b+c) {
    return 5
  }
    else if((a+b)==(10+a+c) {
      return 5
    }
  
  else {
    return 0
  }
}