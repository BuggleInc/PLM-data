def getRankOf(size):    
	for rank in range(getStackSize()):
		if getPancakeRadius(rank) == size:
			return rank
	return -99 # be robust to border cases

def showStack():
    s = "{"
    for rank in range(getStackSize()):
        if isPancakeUpsideDown(rank):
            s = s + "-"
        s = s + str(getPancakeRadius(rank)) + ", "
    s = s + "}"
    print(s)
        
showStack()