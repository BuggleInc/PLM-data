override def run() {

  
  
  while (! estSurBiscuit()) {
  
    avanceVersNord();
	  droite();
  
    while (getDirection() != Direction.NORTH && ! estFaceMur()) {
      longerMurGauche();
    }
  
  }
  prendBiscuit()
}

def avanceVersNord() {
  setDirection(Direction.NORD);
  while (! estFaceMur()) {
    avance();    
  }
}


def longerMurGauche() {    
    while (! estFaceMur) {
      avance();
      gauche();
    }
  	droite();
}