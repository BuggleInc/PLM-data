def island(num:Array[Int]): Int = {
	var levelNb = 0
	var level = 0
	var i=0
	while (i != num.length) {
		while (num(i) != 0) {
			if ((level==0 && num(i)==1) || (level==1 && num(i)==2) || (level==2 && num(i)==3) || (level==3 && num(i)==4)) level = level + 1
			i = i+1
		}
		levelNb += level
		level=0
		i += 1
	}
	return levelNb
	
}
