def sortirMoria(){
  var a:Boolean=false
  while (getCouleurSol()!=Color.green){
    retourne()
    avance()
    while (a==false){
      if (estFaceMur()){
        a=true
      }else{
      	avance()
        if (estSurBiscuit()){a=true}
      }
    }
  	if (estFaceMur()){
      droite();avance();gauche();avance()
    }else{
    	prendBiscuit()
    	retourne()
    	while (!estSurBiscuit()){
      	avance()
    	}
    	recule()
      poseBiscuit()
  	}
	}
}
sortirMoria()