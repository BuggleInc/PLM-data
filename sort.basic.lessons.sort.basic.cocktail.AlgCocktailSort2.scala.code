def cocktailSort2() {
var u=0
var x=0
var y=0
var z=getNombreValeurs()-2
do {
	x=0
	y=0
for (i <- z to u by -1) {
	if (getValeur(i)>getValeur(i+1)) {
			echange(i,i+1)
			x=1
			u += 1
	}
}
for (i <- u to z) {
		if (getValeur(i)>getValeur(i+1)) {
			echange(i,i+1)
			y=1
			z -= 1
	}
}
}
while (x==1 && y==1)
}
