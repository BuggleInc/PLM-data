override def run() {
  var cpt = 0
  var y = 0
  var finale = 0
  while (finale==0) {
		for (i<-0 to 6) {
      if (cpt/2==0) {
 	     setPos(y,i)
      } else {
        setPos(y,6-i)
      }
      ecritMessage(cpt)
      cpt = cpt + 1
    }
    if (cpt<48) {
      y = y + 1
      setPos(y,0)
    } else {
      finale=1
    }
  }
}
