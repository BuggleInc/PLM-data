def cocktailSort2() {
  var debut = 0
  var fin = getNombreValeurs()-1
  var n= false
  do{
    n= false
    for (i<- debut to fin-1){
      if (!plusPetit(i, i+1)){
        	echange(i, i+1)
          fin = fin-1
          n= true     
     }
    }
    for (j<- fin-1 to debut by -1){
      if (!plusPetit(j,j+1 )){
        	echange(j, j+1)
          debut = debut + 1
          n= true      
     }
    }
  } while(n)
}