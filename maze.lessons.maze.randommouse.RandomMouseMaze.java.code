public void run() {
	while(!isOverBaggle()) {
    move();
	}
	pickUpBaggle();
}

public void move() {
	int nextMove = random3();
    switch(nextMove) {
    	case 0:
    	left();
    	break;
    	case 1:
    	right();
    	break;
    	case 2:
    	if(!isFacingWall()){
    	forward();
    	}
    	break;
    }
}
