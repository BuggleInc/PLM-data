def combSort() {
  var ecart=getNombreValeurs() 
  var s=true
  do{
    s=true
    if (ecart>1){
      ecart = ecart / 1.3
    }
    i=0
    while (i+ecart <getNombreValeurs()){
      if (!plusPetit(i, i+1) ){
        echange(i,i+1)
        i=i+1
        s=false
      }
    }
  }while(ecart>1 || !s)
}