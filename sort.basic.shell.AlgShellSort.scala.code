def shellSort()  {
  var ecart = (getNombreValeurs)/2
  while (ecart > 0){
   var j = 1
   for ( i <- 1 to getNombreValeurs()-1){
    if (i-j*ecart>0){
   		var a = getValeur(i-(j-1)*ecart)
    	var c = getValeur(i-j*ecart)
    	while ( a<c && j>0){
      	setValeur(i, getValeur(i-j*ecart))
    		j += 1
      	if (j>0) {
      	}
    	}
   		setValeur(i-j*ecart , a)
    }
   }
   ecart = ecart/2
	}
}
