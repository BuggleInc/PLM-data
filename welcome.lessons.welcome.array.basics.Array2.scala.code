override def run() {
var T = new Array[Color](getMondeHauteur);
T(0)=getCouleurSol()
for (c <- 0 to T.length -1)
{
	avance()
	T(c) = getCouleurSol
}

for (i <- 0 to T.length - 2)
{
	gauche()
	avance()
	droite()
for (j <- 0 to T.length - 2)
{
	setCouleurBrosse(T(j))
	baisseBrosse()
	leveBrosse()
	avance()
}
}
}
