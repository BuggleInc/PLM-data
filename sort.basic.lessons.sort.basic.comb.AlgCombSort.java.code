public void combSort() {
  int gap = getValueCount();
  boolean swap;
 	int i = 0;
  do{
    swap=true;
    if(gap>1){
      gap = gap / 1.3;
    }
    i = 0;
    while(i+gap < getValueCount()){
      if(isSmaller(i+gap,i)){
        swap(i,i+gap);
        swap=false;
        i++;
      }
    }
  }while(gap>1 || swap)
}
}