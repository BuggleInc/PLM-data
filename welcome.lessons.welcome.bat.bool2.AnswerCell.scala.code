def answerCell(isMorning:Boolean, isMom:Boolean, isAsleep:Boolean):Boolean = {
  if (isAsleep==true) {
    return false
  } else {
    if (isMorning==true & isMom==true) {
      return true
    } else {
      if (isMorning==true & isMom==false) {
        return false
      } else {
        return true
      }
    }
  }
}