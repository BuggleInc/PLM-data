def arrive() : Boolean = 
{
  if ( getX()==(getMondeLargeur()-1) && getY()==(getMondeHauteur()-1) )
  {
    return true
  }
  else
  {
    return false
  }
}

def deplacement() 
{
  if (!estFaceMur() && getX()!=6) 
  {
    setPos(getX()+1,getY()-1)
  }

  if (estFaceMur() && getX()!=6)
  {
    setPos(0,getX()+1)
  }
  if (getX==6)
  {
    setPos(getY()+1,6)
  }
  
}

override def run() {
  var cpt = 0
  while(!arrive)
  {
    ecritMessage(cpt)
    cpt+=1
    deplacement()
  }
  
}