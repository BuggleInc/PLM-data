//4 steps forward, 2 steps to the left, 4 steps to the right, 2 steps to the right, 4 steps to the left,
for(i <- 1 to 7) {
	for(j <- 1 to 7) {
		forward(); forward(); forward(); forward();
		left(); forward(); forward();
		right(); forward(); forward(); forward(); forward();
		right(); forward(); forward();
		left(); forward(); forward(); forward(); forward(); left();
	}
}
