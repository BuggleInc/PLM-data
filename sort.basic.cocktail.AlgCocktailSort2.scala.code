def cocktailSort() {
  var debut = 0
  var fin = getNombreValeurs()-2
  var inversion:Boolean = false
  do {
    inversion = false
    for (i <- debut to fin) {
      if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
        fin-=1
        inversion = true
      }
    }
    for (i <- fin to debut by -1) {
       if (getValeur(i)>getValeur(i+1)) {
      	echange(i,i+1)
         debut+=1
        inversion = true
       }
    }
   }
  while (inversion)
}