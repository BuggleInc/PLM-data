def combSort() {
var ecart=getNombreValeurs()
var x=getNombreValeurs()
var y=1
var i=0
do {
	if(ecart > 1){
	ecart = (ecart.asInstanceOf[Double] / 1.3).asInstanceOf[Int]
	}
	i=0
	y=0
	while( i+ecart < x){
		if (!plusPetit(i,i+ecart)){
			echange(i,i+ecart)
			y=y+1
		}
			i=i+1
	}
} while ((ecart >1) || ( y!=0))
}