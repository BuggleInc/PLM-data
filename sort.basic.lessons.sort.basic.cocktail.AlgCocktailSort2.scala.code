def cocktailSort2() {
	var sorted=false
	var debut=0
	var fin=getNombreValeurs()-2
	while (!sorted) {
		sorted=true
		for (i<-debut to fin) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				sorted=false
			}
			fin-=1
		}
		for (i<-fin to debut by -1) {
			if (!plusPetit(i,i+1)) {
				echange(i,i+1);
				sorted=false
			}
			debut+=1
		}
	}
}