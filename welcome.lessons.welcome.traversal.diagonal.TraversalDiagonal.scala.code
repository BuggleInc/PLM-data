var diag = 0;
override def run() {
	var x = 0 
	var y = 0
	
	while (diag != 12)
	{
		if (estFaceMur)
		{
			diag = diag + 1

			 if (0 < diag && diag < getMondeHauteur + 1)
			{
				x = 0 
				y = diag - 1  
				setPos(x , y )
			}

			if (getMondeHauteur < diag && diag < getMondeHauteur * getMondeLargeur)
			{
				x = diag - getMondeHauteur
				y = getMondeHauteur - 1   
				setPos(x , y )
			}

			
		}
		
		y = y - 1
			x = x + 1
			setPos(x , y )

		
	}
}
