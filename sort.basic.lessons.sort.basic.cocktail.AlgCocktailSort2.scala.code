def cocktailSort2() { 
	var tri: Boolean = false;
	var debut: Int = 0;
	var fin: Int = getNombreValeurs() - 2;
	do{
		tri = false;
			for(i <- debut to fin){
				if(!plusPetit(i, i + 1)){
					echange(i, i + 1);
					tri = true;
				}
			}
			for(i <- fin to debut by -1){
				if(plusPetit(i, i - 1)){
					echange(i, i - 1);
					tri = true;
				}
			}
		debut = debut + 1;
		fin = fin - 1;
	}while(tri)
}
