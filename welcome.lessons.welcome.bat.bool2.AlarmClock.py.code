def alarmClock(day, vacation):
    if (day == 0 or day == 6) and not vacation :   #WE pas vac
        return "10:00"
    elif (day == 0 or day == 6) and vacation:
        return "off"
    elif (day == 1 or day == 2 or day == 3 or day == 4 or day == 5) and vacation:
        return "10:00"
    else:
        return "7:00"    