def combSort() {
  var ecart = getNombreValeurs()
  if(ecart>1){
    ecart = (ecart.toDouble / 1.3).toInt
  }
  var i = 0
  var tri = true
  while(i+ecart<getNombreValeurs() || tri){
    tri= false
    if(!plusPetit(i,i+ecart)){
      echange(i,i+ecart)
      i+=1
      tri = true
    }
    if(ecart<=1){
      return 
    }
  }
}