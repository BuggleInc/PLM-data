def cocktailSort() {
  var n = getNombreValeurs()
  var d = 1
      while (d!= 0) {
        d = 0
        for (i<- 0 to n-2) {
          if (!plusPetit(i,i+1)) {
            echange(i,i+1)
            d=1
          }
        }
        for (i<- n-2 to 0) {
          if (!plusPetit(i-1,i)) {
            echange(i,i-1); d =1
          }
        }
  }
}
