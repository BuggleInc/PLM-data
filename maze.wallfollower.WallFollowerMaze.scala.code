override def run() {
  def go(){
    if(!isFacingWall()){
      forward();
    }
  }
  
  def keepHandOnSideWall() {
    go();
    left();
    if(!isFacingWall()){
      forward();
      left();
      if(!isFacingWall()){
        forward();
        left();
      }
    }
    right();
  }
  
  while(!isOverBaggle()){
    keepHandOnSideWall()
  }
  pickupBaggle();
  
}