public void run() {
  int nbBiscuit = 0;
  int nbCase = 0;
    
  do{
    if(estSurBiscuit()){
      nbBiscuit++;
    }
    
    if(!sortieTrouvee()){
      avance();
    } else
      break;
    
    if(crossing() && nbBiscuit >=3 && !sortieTrouvee()){
