def cocktailSort() {
  var n = getNombreValeurs()
  var ech = false
  while(!ech)
  {
    for(i<-0 to n-2)
    {
      if(!plusPetit(i,i+1))
      {
        echange(i,i+1)
        ech=true
      }
    }
    for(i<-n-2 to 0 by -1)
    {
      if(!plusPetit(i,i+1))
      {
        echange(i,i+1)
        ech=true
      }
    }
  }
  
}