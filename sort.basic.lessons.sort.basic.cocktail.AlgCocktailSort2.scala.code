def cocktailSort2() {
  var nb : Int = getNombreValeurs()
  var bool : Boolean = true
  var debut : Int = 0
  var fin : Int = nb-2
  
  while (bool == true) {
    bool = false
    for (i<- debut to fin) {
     	if (plusPetit(i+1,i)) {
        echange(i,i+1)
        bool = true
      }
    }
    fin-=1
    
    for (i<- fin to debut by -1) {
     	if (plusPetit(i+1,i)) {
        echange(i,i+1)
        bool = true
      }
    }
    debut+=1
    
  }
}