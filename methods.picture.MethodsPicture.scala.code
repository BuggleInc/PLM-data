override def run()
{
  for (j <- 1 to 3) faireRectangle();
}

def ecrire()
{
  baisseBrosse();
  leveBrosse();
}

def faireV(c:Color)
{
  setCouleurBrosse(c)
  
  avance();
  ecrire();
  gauche();
  avance();
  droite();
  avance();
  ecrire();
  avance();
  droite();
  avance();
  ecrire();
  gauche();
  avance();
  gauche();
}

def faireRectangle()
{
  for (j <- 1 to 3)
  {
    faireV(Color.YELLOW);
    faireV(Color.RED);
    faireV(Color.BLUE);
    faireV(Color.GREEN);
    for (i <- 1 to 6) avance()
  }
  
  gauche();
  for (i <- 1 to 6) avance()
  droite();
}