def posFinale():Boolean = {
  if (getX() == (getMondeLargeur() - 1) && getY() == (getMondeHauteur() - 1)){
    return true
  }
  return false
}
      
def colone(cpt:Int) {
  ecritMessage(cpt)
  setPos(getX() + 1, getY())
}

override def run() {
  var cpt:Int = 0
  while(!posFinale()){
    colone(cpt)
    cpt += 1
  }
}