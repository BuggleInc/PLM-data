$package import plm.universe.bat.BatEntity; import plm.universe.bat.BatWorld; import plm.universe.bat.BatTest; import plm.universe.World; import lessons.recursion.cons.universe.*;import java.util.Vector;public class plusOne extends BatEntity { public RecList cons(int head, RecList tail){  return new RecList(head, tail);} @SuppressWarnings("unchecked") public void run(BatTest t) {
		t.setResult( plusOne(RecList.fromVector((Vector<Integer>)t.getParameter(0))) ); }
	RecList plusOne(RecList seq) {
		if (seq == null)
			return null;
		return cons(seq.head+1, plusOne(seq.tail));
	}
 }