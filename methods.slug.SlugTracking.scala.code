def isFacingTrail():Boolean = {
	var estVerte = false
  if (!estFaceMur()) {
  	avance()
  }
  if (getCouleurSol() == Color.green) {
    estVerte = true
  } else {
    estVerte = false
  }
  recule()
  return estVerte
}        

while (!estSurBiscuit) {
  if (isFacingTrail()) {
    avance()
  } else {
    gauche()
  }
}