def cocktailSort3() {
  var debut=0
  var fin=getValueCount()-2
  var echangeC=1
  var echangeD=1
  while((echangeC!=0) && (echangeD!=0)){
    echangeC=0
    echangeD=0
    for (k<- debut to fin){
      if (!isSmaller(k,k+1)){
        swap(k+1,k)
        echangeC=echangeC+1
      }
    }
    fin=fin-1
    for (k<- fin to debut by -1){
      if (!isSmaller(k,k+1)){
        swap(k,k+1)
        echangeD=echangeD+1
      }
    }
    debut=debut+1
  }
}
