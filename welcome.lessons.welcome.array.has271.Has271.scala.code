def has271(nums:Array[Int]): Boolean = {
  val n = nums.length
  for (i <- 0 to n-3) {
    if (nums(i+1) == nums(i)+5 && nums(i+2) == nums(i)-1 || nums(i+1) == nums(i)+5 && nums(i+2) == nums(i)-1-2 || nums(i+1) == nums(i)+5 && nums(i+2) == nums(i)-1+2) {
      return true
    }
  }
  return false
}
