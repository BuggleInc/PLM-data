def combSort() {
	var ecart = getNombreValeurs
	var inverse = true	
	do {
		if (ecart > 1) {
			ecart = (ecart.toDouble / 1.3).toInt
		}
		var i = 0
		while (i+ecart < getNombreValeurs) {
			inverse = false
			if (!plusPetit(i,i+ecart)) {
				echange(i,i+ecart)
				i +=1
				inverse = true
			}
		}
	} while ((ecart>1)||(inverse)) 
}