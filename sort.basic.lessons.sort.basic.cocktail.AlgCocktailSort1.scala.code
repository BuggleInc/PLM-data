def cocktailSort() {
	var chgtfait : Boolean = true
	while (chgtfait){
		chgtfait = false
		for (i<- 0 to getNombreValeurs()-2){
			if(plusPetit(i+1,i)){
				echange(i+1,i)
				chgtfait = true
			}
		}
		for (i<- getNombreValeurs() -2 to 0 by -1){
			if (plusPetit(i+1,i)){
				echange(i+1,i)
				chgtfait = true	
			}
		}
	}
}
